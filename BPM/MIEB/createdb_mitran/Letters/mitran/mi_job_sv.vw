CREATE OR REPLACE FORCE VIEW MI_JOB_SV AS
SELECT "JOB_ID","JOB_NAME","JOB_DESCRIPTION","DAILY_RUN_ENABLED","DAILY_RUN_SCRIPT","ENABLED","CREATE_DT","CREATED_BY","UPDATE_DT","UPDATED_BY" FROM MI_JOB;
grant select on MI_JOB_SV to MAXDAT_MITRAN_OLTP_SIU;
grant select on MI_JOB_SV to MAXDAT_MITRAN_OLTP_SIUD;
grant select on MI_JOB_SV to MAXDAT_MITRAN_READ_ONLY;


