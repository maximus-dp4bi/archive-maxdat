CREATE OR REPLACE FORCE VIEW D_LETTER_ADJUST_REASON_SV AS
SELECT LETTER_ADJUST_REASON_CODE
, NAME
, DESCRIPTION
, REPORT_LABEL
, EFFECTIVE_FROM_DATE
, EFFECTIVE_THRU_DATE
FROM D_LETTER_ADJUST_REASON;
grant select on D_LETTER_ADJUST_REASON_SV to MAXDAT_MITRAN_OLTP_SIU;
grant select on D_LETTER_ADJUST_REASON_SV to MAXDAT_MITRAN_OLTP_SIUD;
grant select on D_LETTER_ADJUST_REASON_SV to MAXDAT_MITRAN_READ_ONLY;


