CREATE OR REPLACE FORCE VIEW D_LETTER_INVOICE_GROUP_SV AS
SELECT LETTER_INVOICE_GROUP_CODE
, LETTER_INVOICE_GROUP_NAME
, DESCRIPTION
, REPORT_LABEL
, EFFECTIVE_FROM_DATE
, EFFECTIVE_THRU_DATE
FROM D_LETTER_INVOICE_GROUP;
grant select on D_LETTER_INVOICE_GROUP_SV to MAXDAT_MITRAN_OLTP_SIU;
grant select on D_LETTER_INVOICE_GROUP_SV to MAXDAT_MITRAN_OLTP_SIUD;
grant select on D_LETTER_INVOICE_GROUP_SV to MAXDAT_MITRAN_READ_ONLY;


