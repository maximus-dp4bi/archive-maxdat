    <sql>SELECT 
  doc.ID_PARENT
, doc.ID
, doc.C_DOCUMENT_TYPE 
, doc.C_ICN 
, doc.C_SOURCE  
, doc.C_MAILED_DATE  
, doc.C_DUE_DATE 
