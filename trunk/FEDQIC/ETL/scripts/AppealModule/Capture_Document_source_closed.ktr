, doc.C_DATE_RECEIVED 
, doc.C_REQUESTED_INFORMATION  
, doc.C_REQUEST_SENT_TO 
, doc.C_REQUESTOR   
, doc.C_DATE_OF_REQUEST
FROM T_DOCUMENT doc
join t_appeal app on doc.id_parent = app.id
WHERE (app.c_appeal_status in (47,49))
and (app.c_appeal_request_received_date is null )</sql>
