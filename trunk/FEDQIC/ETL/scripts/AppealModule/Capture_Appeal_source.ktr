    <sql>SELECT APPEAL.*,
etka.subject_id as c_adjudicator,
--MAD.C_APPELLANT_DISMISSAL as C_APPELLANT_DISMISSAL,
TCLAIM.C_DISPOSITION as C_DISPOSITION,
TCLAIM.C_DISPOSITION_EXPLANATION as C_DISPOSITION_EXPLANATION,
TCLAIM.C_PROCEDURAL_DECISION_REASON as C_PROCEDURAL_DECISION_REASON,
TCLAIM.C_SUBSTANTIVE_REASON as C_SUBSTANTIVE_REASON,
case when TMD.MIN_START_DATE is null and c_appeal_status in (47,49) then trunc(sysdate,&apos;hh24&apos;) else TMD.MIN_START_DATE end as T_MAXDAT_CLOSED_DATE 
FROM (
SELECT
tap.ID APPEAL_ID,
tap.C_APPEAL_REQUEST_RECEIVED_DATE as CREATE_DATE,
case when tap.C_VERBAL_NOTICE is not null then tap.C_VERBAL_NOTICE ELSE tap.C_DECISION_LETTER_MAILED_DATE END as COMPLETE_DATE,
null as CANCELLED_DATE,
--etka.SUBJECT_ID as C_ADJUDICATOR,
tap.C_APPEAL_DEADLINE_DATE,
tap.C_APPEAL_ISSUE,
tap.C_ITEM_SERVICE,
tap.C_APPEAL_NUMBER,
tap.C_APPEAL_PRIORITY,
tap.C_APPEAL_REQUEST_RECEIVED_DATE,
tap.C_APPEAL_STAGE,
tap.C_APPEAL_STATUS,
tap.C_APPEAL_TYPE,
--null as C_APPELLANT_DISMISSAL, --mad.C_APPELLANT_DISMISSAL,
tap.C_APPELLANT_DISMISSAL,
tap.C_APPELLANT_DISMISSAL_REASON,
tap.C_AUTO_FORWARD as C_AUTO_FORWARD,
tap.C_CASE_FILE_REQUEST_DATE,
tap.C_DATE_ACKNOWLEDGEMENT_LETTER_ ,
tap.C_DECISION_LETTER_MAILED_DATE,
tap.C_DECISION_SENT_TO_PLAN_DATE,
tap.C_MEDICAL_REVIEW_CHECK,
tap.C_PART,
tap.C_APPEAL_REASON,
tap.C_TOLLING_DATE,
tap.C_VERBAL_NOTICE,
tap.C_CASE_FILE_RECEIVED_DATE,
tap.C_CLAIMED_DATE,
tap.C_DECISION,
tap.C_DECISION_NOTIFICATION_METHOD,
tap.C_HPMS,
tap.C_HPMS_REQUESTED_DATE,
tap.C_IS_REQUEST_FOR_INFORMATION_P,
tap.C_MAC,
tap.C_MEDICARE_TYPE,
tap.C_MSP,
tap.C_NEW_DOCUMENTATION_REVIEWED,
tap.C_PHYSICIAN_SPECIALTY,
tap.C_PRECHECK_COMPLETED,
substrb(dbms_lob.substr(tap.C_REASON_FOR_APPEAL,2000, 1 ),1,2000)|| substrb(dbms_lob.substr(tap.C_REASON_FOR_APPEAL,2000, 2001),1,2000) as C_REASON_FOR_APPEAL,
tap.C_WITHDRAWAL,
tap.C_WITHDRAWAL_REQUEST_SUBMITTED,
tap.C_NON_ENGLISH,
tap.C_NON_ENGLISH_OTHER,
--TCLAIM.C_DISPOSITION as C_DISPOSITION,
--TCLAIM.C_DISPOSITION_EXPLANATION as C_DISPOSITION_EXPLANATION,
--TCLAIM.C_PROCEDURAL_DECISION_REASON as C_PROCEDURAL_DECISION_REASON,
--TCLAIM.C_SUBSTANTIVE_REASON as C_SUBSTANTIVE_REASON,
tmr.C_FIRST_REVIEW_CASE_ATTESTATIO as C_FIRST_REVIEW_CASE_ATTESTATIO,
tmr.C_FIRST_MEDICAL_REVIEW_DECISIO as C_FIRST_MEDICAL_REVIEW_DECISIO,
tmr.C_FIRST_REVIEWER as C_FIRST_REVIEWER,
tmr.C_THIRD_MEDICAL_REVIEW_DECISIO as C_THIRD_MEDICAL_REVIEW_DECISIO,
tmr.C_THIRD_REVIEW_CASE_ATTESTATIO as C_THIRD_REVIEW_CASE_ATTESTATIO,
tmr.C_THIRD_REVIEWER as C_THIRD_REVIEWER,
tmr.C_EXPERT_REVIEW_CASE_ATTESTATI as C_EXPERT_REVIEW_CASE_ATTESTATI,
substrb(dbms_lob.substr(tmr.C_EXPERT_REVIEW_CITATION,2000, 1 ),1,2000)|| substrb(dbms_lob.substr(tmr.C_EXPERT_REVIEW_CITATION,2000, 2001),1,2000) as C_EXPERT_REVIEW_CITATION,
(select mds.c_user_id from t_md mds where mds.c_md_id = tmr.C_EXPERT_REVIEWER_MD_ID) as C_EXPERT_REVIEWER_MD_ID,
null as CLOSED_DATE,
null as WITHDRAWN_DATE,

tdr.C_DEMO_REOPENING_TYPE,
tdr.C_OTHER_REOPENING_TYPE,
tdr.C_CASE_CURRENTLY_WITH_ALJ,
tdr.C_DATE_OF_REQUEST_TO_ALJ,
tdr.C_DEMO_REOPENING_DUE_DATE,
tdr.C_DEMO_REO_SENT_TO_OMHA_DATE,
tdr.C_OMHA_RESPONSE_RECEIVED,
tdr.C_RESPONSE_FROM_OMHA,
tdr.C_ADDITIONAL_INFO_REQUESTED,
tdr.C_REQUESTED_INFORMATION_DUE,
tdr.C_FOLLOW_UP as C_DEMO_REOPEN_FOLLOW_UP,
tdr.C_ADDITIONAL_INFO_RECEIVED,
tdr.C_REOPENING_DECISION_RESULTS,
tdr.C_NOT_REOPENED_REASON,

tdr.C_OMHA_REMAND_REQUEST,
tdr.C_REMAND_ELIGIBILITY_RESPONSE,
tdr.C_REMAND_RECEIVED_DATE,
tdr.C_OMHA_REMAND_REQUEST_RESPONSE,
tdr.C_OMHA_WITHDRAW_FORM_SENT,
tdr.C_OMHA_WITHDRAW_FORM_RETURNED,
tdr.C_OMHA_NOTIFIED_OF_WITHDRAWL,
tdr.C_ALJ_WITHDRAWAL,
tdr.C_DEMO_REOPENING_APPEAL_NUMBER,
tdr.C_REOPENING_ANALYSIS_COMPLETED,
tdr.C_REOPENING_ANALYSIS_OUTCOME,
tdr.C_NOT_PURSUED_BY_CONTR_REASON,
tdr.C_ACK_LETTER_MAILED,
tdr.C_REO_DECISION_LETTER_MAILED,
tdr.C_REOPENING_OUTCOME,
tdr.C_DECLINE_TO_REOPEN_DECISION,
tdr.C_APPEAL_ATTESTATION_DATE,
tdr.C_APPEAL_ATTESTATION,

td.C_DEMO_SCHEDULED,
td.C_DEMO_NOTIFICATION_LETTER_SEN,
td.C_RESPONSE_DUE,
td.C_RESPONSE_RECEIVED,
td.C_DEMO_ACCEPTANCE_STATUS,
td.C_FOLLOW_UP as C_TELE_DEMO_FOLLOW_UP,
td.C_DEMO_NOTIFICATION_LTR_RESENT,
td.C_RESCHEDULED_RESPONSE_DUE,
td.C_RESCHEDULE_RESPONSE_RECEIVED,
td.C_VERBAL_CONFIRMATION,
td.C_RESCHEDULED_DEMO_STATUS,
td.C_PROVIDER_OR_SUPPLIER_NAME,	
td.C_DEMO_CONFERENCE_STATUS,
tmr.C_REVIEW_TYPE,
C_SECOND_MEDICAL_REVIEW_DECISI as C_EXPERT_REVIEW_DECISION,		
tmr.C_REVIEW_NUMBER	

FROM entellitrak.T_APPEAL tap
--LEFT OUTER JOIN entellitrak.M_APPELLANT_DISMISSAL mad
--ON tap.id = mad.id_owner
--LEFT OUTER JOIN entellitrak.M_AUTO_FORWARD maf
--ON tap.id = maf.id_owner
LEFT OUTER JOIN entellitrak.T_MEDICAL_REVIEW tmr
ON tap.id = tmr.id_base
LEFT OUTER JOIN T_DEMO_REOPENING tdr 
ON tap.id = tdr.id_parent
LEFT OUTER JOIN T_DEMONSTRATION td
ON tap.id = td.id_parent
--LEFT OUTER JOIN entellitrak.T_CLAIM tc
--ON tap.id = tc.id_base
) APPEAL
LEFT OUTER JOIN 
(select c_appeal_id, min(c_start_date) as min_start_date from entellitrak.t_maxdat where c_status in (47,49) group by c_appeal_id) TMD
ON APPEAL.appeal_id = TMD.c_appeal_id
LEFT OUTER JOIN
(select id_base,C_DISPOSITION, C_DISPOSITION_EXPLANATION, C_PROCEDURAL_DECISION_REASON, C_SUBSTANTIVE_REASON from 
(select id_base,C_DISPOSITION, C_DISPOSITION_EXPLANATION, C_PROCEDURAL_DECISION_REASON, C_SUBSTANTIVE_REASON,  
rank() over (partition by id_base order by id) as rnk 
from entellitrak.t_claim) 
where rnk=1) TCLAIM
ON APPEAL.appeal_id = TCLAIM.id_base

--LEFT OUTER JOIN
--(select id_owner,C_APPELLANT_DISMISSAL from 
--(select id_owner,C_APPELLANT_DISMISSAL,  
--rank() over (partition by id_owner order by id) as rnk 
--from entellitrak.m_appellant_dismissal) 
--where rnk=1) MAD
--ON APPEAL.appeal_id = MAD.id_owner


LEFT OUTER JOIN
(select subject_id, tracking_id from 
(select tracking_id,data_object_key,subject_type, subject_id, is_current, assignment_id,  
rank() over (partition by tracking_id order by assignment_id desc) as rnk 
from 
(select tracking_id,data_object_key,subject_type, subject_id, is_current, assignment_id
from entellitrak.etk_assignment
where data_object_key = &apos;T_APPEAL&apos;
and subject_type = 1
and is_current = 1) ETKA_BASE
) ETKA_RANKED
where rnk=1
)ETKA
ON APPEAL.appeal_id = etka.tracking_id</sql>
