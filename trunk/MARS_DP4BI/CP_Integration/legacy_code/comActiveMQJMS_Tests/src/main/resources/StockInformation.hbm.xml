<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "StockInformation" table = "CISCO_ENTERPRISE_CC.JMS_MSG_STORE">

        <meta attribute = "class-description">
            This class contains the stock detail.
        </meta>

        <id name = "id" type = "int" column = "id">
            <generator class="increment"/>
        </id>

        <property name = "stockName" column = "STOCK_NAME" type = "string"/>
        <property name = "stockPrice" column = "STOCK_PRICE" type = "float"/>

    </class>
</hibernate-mapping>