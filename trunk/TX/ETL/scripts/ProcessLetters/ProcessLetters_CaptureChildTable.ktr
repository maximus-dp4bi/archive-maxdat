    <sql>select distinct lmreq_id, CS.client_id ,program_type_cd,ll.plan_type_cd,CLIENT_STATUS, CS.SUBPROGRAM_TYPE from (
SELECT  DISTINCT
LR.LMREQ_ID,
LL.CLIENT_ID,
program_type_cd,
plan_type_cd,
MIN(CS.ELIG_STATUS_CD) OVER (PARTITION BY LR.lmreq_id,LL.CLIENT_ID) AS CLIENT_STATUS
FROM LETTER_REQUEST LR
LEFT OUTER JOIN LETTER_REQUEST_LINK LL
ON LL.LMREQ_ID= LR.LMREQ_ID
LEFT OUTER JOIN CLIENT_ELIG_STATUS CS
ON CS.CLIENT_ID = LL.CLIENT_ID
where lr.lmreq_id = ?
and program_type_cd = &apos;MEDICAID&apos;
and plan_type_cd IN &apos;MEDICAL&apos;
and cs.start_date  &lt;= lr.create_ts
AND END_DATE IS NULL
AND CS.ELIG_STATUS_CD IN (&apos;M&apos;,&apos;V&apos;)) LL 
JOIN CLIENT_ELIG_STATUS CS
ON CS.CLIENT_ID = LL.CLIENT_ID
AND LL.CLIENT_STATUS = CS.ELIG_STATUS_CD
and cs.plan_type_cd = ll.plan_type_cd

UNION ALL 


select distinct lmreq_id, CS.client_id ,program_type_cd,ll.plan_type_cd,CLIENT_STATUS, CS.SUBPROGRAM_TYPE from (
SELECT  DISTINCT
LR.LMREQ_ID,
LL.CLIENT_ID,
program_type_cd,
plan_type_cd,
MIN(CS.ELIG_STATUS_CD) OVER (PARTITION BY LR.lmreq_id,LL.CLIENT_ID) AS CLIENT_STATUS
FROM LETTER_REQUEST LR
LEFT OUTER JOIN LETTER_REQUEST_LINK LL
ON LL.LMREQ_ID= LR.LMREQ_ID
LEFT OUTER JOIN CLIENT_ELIG_STATUS CS
ON CS.CLIENT_ID = LL.CLIENT_ID
where lr.lmreq_id = ?
and program_type_cd = &apos;MEDICAID&apos;
and plan_type_cd IN &apos;BEHAVIORAL&apos;
and cs.start_date  &lt;= lr.create_ts
AND END_DATE IS NULL
AND CS.ELIG_STATUS_CD IN (&apos;M&apos;,&apos;V&apos;)) LL 
JOIN CLIENT_ELIG_STATUS CS
ON CS.CLIENT_ID = LL.CLIENT_ID
AND LL.CLIENT_STATUS = CS.ELIG_STATUS_CD
and cs.plan_type_cd = ll.plan_type_cd
AND LL.CLIENT_ID NOT IN (select CS.client_id from (
SELECT  DISTINCT
LR.LMREQ_ID,
LL.CLIENT_ID,
program_type_cd,
MIN(CS.ELIG_STATUS_CD) OVER (PARTITION BY LR.lmreq_id,LL.CLIENT_ID) AS CLIENT_STATUS
FROM LETTER_REQUEST LR
LEFT OUTER JOIN LETTER_REQUEST_LINK LL
ON LL.LMREQ_ID= LR.LMREQ_ID
LEFT OUTER JOIN CLIENT_ELIG_STATUS CS
ON CS.CLIENT_ID = LL.CLIENT_ID
where lr.lmreq_id = ?
and program_type_cd = &apos;MEDICAID&apos;
and plan_type_cd IN &apos;MEDICAL&apos;
and cs.start_date  &lt;= lr.create_ts
AND END_DATE IS NULL
AND CS.ELIG_STATUS_CD IN (&apos;M&apos;,&apos;V&apos;))
)

UNION ALL 


select distinct lmreq_id, CS.client_id ,program_type_cd,ll.plan_type_cd,CLIENT_STATUS, CS.SUBPROGRAM_TYPE from (
SELECT  DISTINCT
LR.LMREQ_ID,
LL.CLIENT_ID,
program_type_cd,
plan_type_cd,
MIN(CS.ELIG_STATUS_CD) OVER (PARTITION BY LR.lmreq_id,LL.CLIENT_ID) AS CLIENT_STATUS
FROM LETTER_REQUEST LR
LEFT OUTER JOIN LETTER_REQUEST_LINK LL
ON LL.LMREQ_ID= LR.LMREQ_ID
LEFT OUTER JOIN CLIENT_ELIG_STATUS CS
ON CS.CLIENT_ID = LL.CLIENT_ID
where lr.lmreq_id = ?
and program_type_cd = &apos;CHIP&apos;
and plan_type_cd IN &apos;MEDICAL&apos;
and cs.start_date  &lt;= lr.create_ts
AND END_DATE IS NULL
AND CS.ELIG_STATUS_CD IN (&apos;M&apos;,&apos;V&apos;)) LL 
JOIN CLIENT_ELIG_STATUS CS
ON CS.CLIENT_ID = LL.CLIENT_ID
AND LL.CLIENT_STATUS = CS.ELIG_STATUS_CD
and cs.plan_type_cd = ll.plan_type_cd
</sql>
