    <rowlimit>0</rowlimit>
    <sql>SELECT MAX(CPW_REFERRAL_DT) CPW_REFERRAL_DT,
MAX(CPW_REFERRAL_SOURCE_TYPE) CPW_REFERRAL_SOURCE_TYPE,
MAX(CPW_REFERRAL_SOURCE_NAME) CPW_REFERRAL_SOURCE_NAME,
MAX(CPW_REFERRAL_REASON) CPW_REFERRAL_REASON,
MAX(CPW_CALL_BACK_IND) CPW_CALL_BACK_IND,
MAX(CPW_REFERRAL_PRIORITY_STATUS) CPW_REFERRAL_PRIORITY_STATUS,
MAX(CPW_NAME_OF_REFERRER) OLTP_CPW_NAME_OF_REFERRER,
MAX(CPW_REFERRAL_SOURCE_PHONE) OLTP_CPW_REFERRAL_SOURCE_PHONE
FROM 
(SELECT 
       CASE WHEN R.TEMPLATE_QUESTION_ID = ?
            THEN R.ANSWER_TEXT
       END AS CPW_REFERRAL_DT,
       CASE WHEN TEMPLATE_QUESTION_ID = ?
            THEN AN.ANSWER_TEXT
       END AS CPW_REFERRAL_SOURCE_TYPE,
       CASE WHEN TEMPLATE_QUESTION_ID = ?
            THEN R.ANSWER_TEXT
       END AS CPW_REFERRAL_SOURCE_NAME,
       CASE WHEN TEMPLATE_QUESTION_ID = ?
            THEN R.ANSWER_TEXT
       END AS CPW_REFERRAL_REASON,
       CASE WHEN TEMPLATE_QUESTION_ID = ?
            THEN AN.ANSWER_TEXT
       END AS CPW_CALL_BACK_IND,
       CASE WHEN TEMPLATE_QUESTION_ID = ?
            THEN AN.ANSWER_TEXT
       END AS CPW_REFERRAL_PRIORITY_STATUS,
       CASE WHEN TEMPLATE_QUESTION_ID = ?
            THEN R.ANSWER_TEXT
       END AS CPW_NAME_OF_REFERRER,
       CASE WHEN TEMPLATE_QUESTION_ID = ?
            THEN R.ANSWER_TEXT
       END AS CPW_REFERRAL_SOURCE_PHONE
FROM SURVEY_RESPONSE R
LEFT OUTER JOIN SVEY_TMPL_ANSWER_TEXT AN
ON AN.SURVEY_TEMPLATE_ANSWER_ID = R.SURVEY_TEMPLATE_ANSWER_ID
WHERE SURVEY_ID = ?
)</sql>
