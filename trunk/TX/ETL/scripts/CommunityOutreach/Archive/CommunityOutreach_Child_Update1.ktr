    <sql>SELECT 
CHLD.OUTREACH_CLIENT_ID,
CASE WHEN NVL(CLIENT_ID,0) &lt;&gt;  NVL(O_CLIENT_ID,0)
     THEN CLIENT_ID
     ELSE O_CLIENT_ID
END AS UPD1_840_CLIENT_ID,
CASE WHEN NVL(CASE_ID,0) &lt;&gt;  NVL(O_CASE_ID,0)
     THEN CASE_ID
     ELSE O_CASE_ID
END AS UPD1_850_CASE_ID,

CASE WHEN NVL(CLIENT_GENDER,&apos;X&apos;) &lt;&gt;  NVL(O_CLIENT_GENDER,&apos;X&apos;)
     THEN CLIENT_GENDER
     ELSE O_CLIENT_GENDER
END AS UPD1_860_CLIENT_GENDER,
CASE WHEN NVL(CLIENT_RACE,&apos;X&apos;) &lt;&gt;  NVL(O_CLIENT_RACE,&apos;X&apos;)
     THEN CLIENT_RACE
     ELSE O_CLIENT_RACE
END AS UPD1_870_CLIENT_RACE,
CASE WHEN NVL(CLIENT_ETHNICITY,&apos;X&apos;) &lt;&gt;  NVL(O_CLIENT_ETHNICITY,&apos;X&apos;)
     THEN CLIENT_ETHNICITY
     ELSE O_CLIENT_ETHNICITY
END AS UPD1_880_CLIENT_ETHNICITY,
CASE WHEN NVL(CLIENT_ATTENDED_IND,0) &lt;&gt;  NVL(O_CLIENT_ATTENDED_IND,0)
     THEN CLIENT_ATTENDED_IND
     ELSE O_CLIENT_ATTENDED_IND
END AS UPD1_890_CLIENT_ATTENDED_IND,
CASE WHEN NVL(CLIENT_SURVEY_COLLECTED_IND,0) &lt;&gt;  NVL(O_CLIENT_SUR_COL_IND,0)
     THEN CLIENT_SURVEY_COLLECTED_IND
     ELSE O_CLIENT_SUR_COL_IND
END AS UPD1_900_CLIENT_SUR_COL_IND,
CASE WHEN NVL(CLIENT_CREATE_DT,&apos;1-JAN-1111&apos;) &lt;&gt;  NVL(O_CLIENT_CREATE_DT,&apos;1-JAN-1111&apos;)
     THEN CLIENT_CREATE_DT
     ELSE O_CLIENT_CREATE_DT
END AS UPD1_910_CLIENT_CREATE_DT,
CASE WHEN NVL(CLIENT_CREATED_BY,&apos;X&apos;) &lt;&gt;  NVL(O_CLIENT_CREATED_BY,&apos;X&apos;)
     THEN CLIENT_CREATED_BY
     ELSE O_CLIENT_CREATED_BY
END AS UPD1_920_CLIENT_CREATED_BY,
CASE WHEN NVL(CLIENT_LAST_UPDATE_DT,&apos;1-JAN-1111&apos;) &lt;&gt;  NVL(O_CLIENT_LAST_UPDATE_DT,&apos;1-JAN-1111&apos;)
     THEN CLIENT_LAST_UPDATE_DT
     ELSE O_CLIENT_LAST_UPDATE_DT
END AS UPD1_930_CLIENT_LAST_UPDATE_DT,
CASE WHEN NVL(CLIENT_LAST_UPDATED_BY,&apos;X&apos;) &lt;&gt;  NVL(O_CLIENT_LAST_UPD_BY,&apos;X&apos;)
     THEN CLIENT_LAST_UPDATED_BY
     ELSE O_CLIENT_LAST_UPD_BY
END AS UPD1_940_CLIENT_LAST_UPD_BY,
CASE WHEN NVL(OUTREACH_SESSION_ID,0) &lt;&gt;  NVL(O_OUTREACH_SESSION_ID,0)
     THEN OUTREACH_SESSION_ID
     ELSE O_OUTREACH_SESSION_ID
END AS UPD1_950_OUTREACH_SESSION_ID,

CASE WHEN NVL(CLIENT_ID,0) &lt;&gt;  NVL(O_CLIENT_ID,0)
     OR NVL(CASE_ID,0) &lt;&gt;  NVL(O_CASE_ID,0)
     OR NVL(CLIENT_GENDER,&apos;X&apos;) &lt;&gt;  NVL(O_CLIENT_GENDER,&apos;X&apos;)
     OR NVL(CLIENT_RACE,&apos;X&apos;) &lt;&gt;  NVL(O_CLIENT_RACE,&apos;X&apos;)
     OR NVL(CLIENT_ETHNICITY,&apos;X&apos;) &lt;&gt;  NVL(O_CLIENT_ETHNICITY,&apos;X&apos;)
     OR NVL(CLIENT_ATTENDED_IND,0) &lt;&gt;  NVL(O_CLIENT_ATTENDED_IND,0)
     OR NVL(CLIENT_SURVEY_COLLECTED_IND,0) &lt;&gt;  NVL(O_CLIENT_SUR_COL_IND,0)
     OR NVL(CLIENT_CREATE_DT,&apos;1-JAN-1111&apos;) &lt;&gt;  NVL(O_CLIENT_CREATE_DT,&apos;1-JAN-1111&apos;)
     OR NVL(CLIENT_CREATED_BY,&apos;X&apos;) &lt;&gt;  NVL(O_CLIENT_CREATED_BY,&apos;X&apos;)
     OR NVL(CLIENT_LAST_UPDATE_DT,&apos;1-JAN-1111&apos;) &lt;&gt;  NVL(O_CLIENT_LAST_UPDATE_DT,&apos;1-JAN-1111&apos;)
     OR NVL(CLIENT_LAST_UPDATED_BY,&apos;X&apos;) &lt;&gt;  NVL(O_CLIENT_LAST_UPD_BY,&apos;X&apos;)
     OR NVL(OUTREACH_SESSION_ID,0) &lt;&gt;  NVL(O_OUTREACH_SESSION_ID,0)
     THEN &apos;Y&apos;
     ELSE NULL
END AS UPDATED
FROM CORP_ETL_COMM_OUTREACH_CLI_CHD CHLD
JOIN (
select
OUTREACH_CLIENT_ID  
,CLIENT_ID O_CLIENT_ID
,CASE_ID  O_CASE_ID
,CLIENT_GENDER  O_CLIENT_GENDER
,CLIENT_RACE  O_CLIENT_RACE
,CLIENT_ETHNICITY  O_CLIENT_ETHNICITY
,CLIENT_ATTENDED_IND O_CLIENT_ATTENDED_IND
,CLIENT_SURVEY_COLLECTED_IND  O_CLIENT_SUR_COL_IND
,CLIENT_CREATE_DT  O_CLIENT_CREATE_DT
,CLIENT_CREATED_BY  O_CLIENT_CREATED_BY
,CLIENT_LAST_UPDATE_DT  O_CLIENT_LAST_UPDATE_DT
,CLIENT_LAST_UPDATED_BY  O_CLIENT_LAST_UPD_BY
,OUTREACH_SESSION_ID  O_OUTREACH_SESSION_ID
from CORP_ETL_COMM_CLI_CHLD_OLTP
)OLTP
on CHLD.OUTREACH_CLIENT_ID = OLTP.OUTREACH_CLIENT_ID</sql>
