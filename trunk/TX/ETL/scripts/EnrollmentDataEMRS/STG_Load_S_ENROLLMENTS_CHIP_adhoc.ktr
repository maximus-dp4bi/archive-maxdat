  <step>
    <name>Database join - Get CHIP case client info</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>10</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT COALESCE(cl.clnt_citizen,&apos;0&apos;) citizenship_status_code
       ,COALESCE(cl.client_language,&apos;0&apos;) language_code
       ,COALESCE(cl.clnt_race,&apos;0&apos;) race_code
       ,COALESCE(?,addr_county) county_code
       ,case_id source_case_id
       ,district_cd csda_code
       ,COALESCE(?,cscl_adlk_id) aid_category_code
       ,LEAST(cc.cscl_elig_begin_dt,cc.creation_date) eligibility_receipt_date
FROM client cl 
  LEFT OUTER JOIN(SELECT cc.program_cd, cc.cscl_clnt_client_id, cc.cscl_adlk_id,cc.cscl_elig_begin_dt, cc.creation_date,cc.cscl_status_begin_date
                        ,cs.addr_county
                        ,cs.case_id
                       -- ,ec.attrib_district_cd                        
                        ,sa.district_cd
           FROM case_client cc
		   INNER JOIN client_supplementary_info cs
            -- ON cc.cscl_id = cs.case_client_id
              ON cc.cscl_clnt_client_id = cs.client_id
              AND cc.program_cd = cs.program_cd
           --INNER JOIN enum_county ec
           --  ON cs.addr_county = ec.value
		   LEFT OUTER JOIN (SELECT * 
                       FROM county_service_area_mapping sa
                       WHERE plan_type_cd = ?
                       AND subprogram_type_cd =  ?
                       AND program_type_cd = ?) sa
		     ON sa.county_cd = COALESCE(?,cs.addr_county)
           LEFT OUTER JOIN enum_aid_category ac
             ON cc.cscl_adlk_id = ac.value
		   --WHERE (cscl_elig_end_dt is NULL OR TRUNC(cscl_elig_end_dt) &gt;= ?)
           WHERE cscl_status_begin_date = (SELECT MAX(cscl_status_begin_date)
	                                       FROM case_client cc1
    	                                   WHERE cc1.cscl_clnt_client_id = cc.cscl_clnt_client_id
                                           AND cc1.program_cd = cc.program_cd)
          ) cc
  ON cl.clnt_client_id = cc.cscl_clnt_client_id                                             
WHERE cl.clnt_client_id = ?
AND program_cd = ?
ORDER BY cscl_status_begin_date DESC</sql>
