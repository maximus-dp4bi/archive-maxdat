  <step>
    <name>Table input - CHIP Selection segments from EB</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>SELECT ss.selection_segment_id source_selection_record_id
       ,&apos;SELECTION_SEGMENT&apos; source_table_name
       ,ss.client_id source_client_id
       ,ss.program_type_cd program_type_code
       ,ss.plan_type_cd plan_type_code
       ,COALESCE(ss.plan_id_ext,&apos;0&apos;) source_plan_code
       ,COALESCE(ss.network_id,0) source_network_id
       ,COALESCE(ss.disenroll_reason_cd_1,&apos;0&apos;) change_reason_code
       ,COALESCE(st.selection_source_cd,ss.selection_source_cd,&apos;0&apos;) selection_source_code
       ,CASE WHEN st.choice_reason_cd IS NULL AND ss.choice_reason_cd IS NULL THEN &apos;N&#47;A&apos; ELSE COALESCE(st.choice_reason_cd,ss.choice_reason_cd) END selection_reason_code
       ,CASE WHEN COALESCE(st.selection_source_cd,ss.selection_source_cd) = &apos;ST&apos; AND st.transaction_type_cd IS NULL THEN &apos;DefaultEnroll&apos; 
         ELSE COALESCE(st.transaction_type_cd,&apos;0&apos;) END  enrollment_action_status_code               
       ,ss.start_date managed_care_start_date
       ,ss.end_date managed_care_end_date
       --,ss.end_nd --segment start date in number format
       ,COALESCE(TRUNC(ss.end_date),TRUNC(sysdate)) eligibility_date
       ,ss.status_date enrollment_status_change_date
       ,1 number_count -- always set to 1
      -- ,CASE WHEN ss.plan_type_cd = &apos;MEDICAL&apos; THEN &apos;CHIP&apos; ELSE &apos;DENTAL&apos; END plan_service_type     
      -- ,CASE WHEN ss.plan_type_cd = &apos;MEDICAL&apos; THEN &apos;CHIP&apos; ELSE &apos;CHIP-DEN&apos; END sub_program_code
      --  ,ss.plan_type_cd
        ,TO_CHAR(COALESCE(ss.create_ts,SYSDATE),&apos;mm&#47;dd&#47;yyyy&apos;) create_ts
        ,TO_CHAR(COALESCE(ss.create_ts,SYSDATE),&apos;hh24:mi&apos;) create_ts_hrmin
        ,TO_NUMBER(TO_CHAR(COALESCE(ss.create_ts,SYSDATE),&apos;ss&apos;)) create_ts_second
        ,ss.county_cd
        ,ss.client_aid_category_cd
FROM selection_segment ss 
 LEFT OUTER JOIN (SELECT * FROM selection_txn st
				  WHERE selection_txn_id = (SELECT MAX(selection_txn_id)
											FROM selection_txn st1
											WHERE st.selection_segment_id = st1.selection_segment_id)) st
  ON ss.selection_segment_id = st.selection_segment_id   
WHERE ss.program_type_cd = &apos;CHIP&apos;</sql>
