    <sql>SELECT dept_name, first_name, last_name, middle_initial
       ,gender, phone, login_id, login_group
       ,manager_staff_id, role, start_date, end_date
       ,source_record_id, user_type, managed_care_program
FROM(SELECT tmp_groups.dept_name
               ,s.first_name
               ,s.last_name
               ,s.middle_name middle_initial
               ,s.gender_cd gender
               ,s.phone_number phone 
               ,su.login_name login_id
			   ,sr.portal login_group
               ,tmp_groups.manager_staff_id
               ,sr.role_name role
               ,tmp_groups.start_date
               ,tmp_groups.end_date
               ,s.staff_id source_record_id
               ,s.staff_type_cd user_type
               ,&apos;MEDICAID&apos; managed_care_program
     FROM staff s
      INNER JOIN sec_user su
       ON s.staff_id = su.staff_id
      INNER JOIN sec_user_role sur
       ON su.sec_user_id = sur.sec_user_id    
      INNER JOIN  sec_role sr
       ON sur.sec_role_id = sr.sec_role_id
      INNER JOIN  (SELECT staff_id, gs.start_date, g.group_name dept_name, gs.end_date, g.supervisor_staff_id manager_staff_id
                   FROM group_staff gs
                   INNER JOIN GROUPS g
                   ON gs.group_id = g.group_id) tmp_groups
    ON s.staff_id = tmp_groups.staff_id)
WHERE end_date IS NULL
ORDER BY source_record_id, dept_name, role, managed_care_program,start_date</sql>
