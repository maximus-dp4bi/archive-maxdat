  <step>
    <name>Database join - OLTP Doc IA Task data</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>4</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT DISTINCT Si.Ref_Id		ref_id
     , Si.Ref_Type
     , Si.Step_Definition_Id
     , Si.Step_Instance_Id		
     , Si.Create_Ts		assd_create_ia_task 
 	 ,(SELECT MIN(s1.Create_Ts) FROM Step_Instance_history s1 
		 WHERE s1.Step_Instance_id = SI.Step_Instance_id
   		   AND s1.Status = &apos;CLAIMED&apos;) ased_create_ia_task
  	,case when (SELECT MIN(s1.Create_Ts) FROM Step_Instance_history s1 
		 WHERE s1.Step_Instance_id = SI.Step_Instance_id
   		   AND s1.Status = &apos;CLAIMED&apos;) is not null then &apos;Y&apos; end asf_create_ia_task

FROM   STEP_INSTANCE SI
     , DOCUMENT   Ds
     , Step_Definition Sd
WHERE  Si.Ref_Type = &apos;IMAGING&apos; --&apos;DOCUMENT_ID&apos;
AND    Si.Ref_Id = Ds.Document_Id
AND    Ds.Document_Id = ?
--AND    Si.Create_Ts &gt; Ds.Create_Ts   -- Get the Step instance record doc data was created
AND    Si.Step_Definition_Id = Sd.Step_Definition_Id
AND    Sd.Step_Type_Cd IN (&apos;HUMAN_TASK&apos;, &apos;VIRTUAL_HUMAN_TASK&apos;)
AND    Sd.Step_Definition_Id  IN (${IA_REF_VALS})
and    si.status &lt;&gt; &apos;TERMINATED&apos;
order by Si.Step_Instance_Id desc</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>DOCUMENT_ID</name>
        <type>Number</type>
      </field>
    </parameter>
