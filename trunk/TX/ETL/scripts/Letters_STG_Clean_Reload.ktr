    <sql>&#47;*
This SQL gets all the letters from the OLTP source system. This is part of the one-time effort to load all letters into the LETTERS_STG table.
*&#47;
select distinct 
       LR.LMREQ_ID      as LETTER_ID,
       LR.REQUESTED_ON  as LETTER_REQUESTED_ON,
       LR.STATUS_CD     as LETTER_STATUS_CD,
       ST.REPORT_LABEL   AS LETTER_STATUS,
       LR.CREATE_TS     as LETTER_CREATE_TS,
       LR.UPDATE_TS     as LETTER_UPDATE_TS,
       LR.SENT_ON       as LETTER_SENT_ON,
       LR.PROGRAM_TYPE_CD as PROGRAM_CODE,
       PR.REPORT_LABEL AS PROGRAM,
       LD.DRIVER_TABLE_NAME as DRIVER_TABLE_NAME,
       LR.MAILED_DATE   as LETTER_MAILED_DATE,
       LR.REJECT_REASON_CD as LETTER_REJECT_REASON_CD,
       ER.REPORT_LABEL AS LETTER_REJECT_REASON,
       LR.PRINTED_ON as LETTER_PRINTED_ON,
       LR.ERROR_CODES as LETTER_ERROR_CODES,
       LH.RESIDENCE_COUNTY as RESIDENCE_COUNTY,
       LH.RESIDENCE_ZIP_CODE as RESIDENCE_ZIP_CODE,
       LR.CASE_ID       as LETTER_CASE_ID,
	     LR.PARENT_LMREQ_ID as LETTER_PARENT_LMREQ_ID,
       LL.REFERENCE_TYPE  as LETTER_REF_TYPE,
       LD.name            as LETTER_TYPE_CD,
       LD.DESCRIPTION     as LETTER_TYPE,
       LR.REQUEST_TYPE    as LETTER_REQUEST_TYPE,
       LR.LANGUAGE_CD    as LETTER_LANG_CD,
       LN.REPORT_LABEL   AS LANGUAGE,
       LR.DRIVER_TYPE    as LETTER_DRIVER_TYPE,
       LR.MATERIAL_REQUEST_ID as LET_MATERIAL_REQUEST_ID,
       LR.CREATED_BY     as LETTER_CREATED_BY,
       LR.RETURN_DATE     as LETTER_RETURN_DATE,
       LR.RETURN_REASON_CD as LETTER_RETURN_REASON_CD,
       RR.REPORT_LABEL AS RETURN_REASON,
       LR.UPDATED_BY as LETTER_UPDATED_BY
from  LETTER_REQUEST      LR, 
     letter_out_header lh,
     LETTER_REQUEST_LINK LL , 
     LETTER_DEFINITION   LD,
     ENUM_LM_STATUS ST,
     ENUM_LANGUAGE LN,
     ENUM_MAILHOUSE_ERROR_REASONS ER,
     ENUM_RETURN_REASON RR,
     ENUM_PROGRAM_TYPE PR
where LR.LMREQ_ID = LL.LMREQ_ID (+)
and LR.LMREQ_ID = LH.LETTER_REQ_ID (+)
and LR.LMDEF_ID = LD.LMDEF_ID (+)
and LR.STATUS_CD = ST.VALUE (+)
and LR.LANGUAGE_CD = LN.VALUE (+)
and LR.REJECT_REASON_CD = ER.VALUE (+)
and LR.RETURN_REASON_CD = RR.VALUE (+)
and LR.PROGRAM_TYPE_CD = PR.VALUE (+)
---and rownum &lt;= 1000</sql>
