    <sql>SELECT event_id
     , event_type_cd    supp_event_type_cd
     , context          supp_event_context
     , ref_type
     , ref_id
     , call_record_id
     , create_ts        event_create_dt
     , created_by       supp_event_created_by
     ,COALESCE((SELECT TRIM(LAST_NAME) || DECODE(LAST_NAME, NULL, NULL, &apos;,&apos;) || TRIM(FIRST_NAME) ||
                        RTRIM(LPAD(SUBSTR(MIDDLE_NAME, 1, 1),2, &apos; &apos;))
                  FROM staff
                 WHERE TO_CHAR(staff_id) = ev.created_by), ev.created_by) oltp_created_by
  FROM event ev
 WHERE event_type_cd = &apos;MANUAL_ACTION&apos; --AND event_id &gt;= 86052653 
   AND call_record_id &gt;= 26209061
--and rownum &lt;= 1
