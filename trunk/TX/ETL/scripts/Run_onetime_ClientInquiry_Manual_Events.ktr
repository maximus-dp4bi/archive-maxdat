    <sql>-- Get Completed instances only. Actives are processed by the normal hourly runs

SELECT i.ceci_id
     , ma.ref_type lkup_manual_action_category 
     , ea.out_var  lkup_event_action
  FROM corp_etl_client_inquiry i, corp_etl_list_lkup ea, corp_etl_list_lkup ma
 WHERE contact_record_id = ?
   AND ea.name         = &apos;CLIENT_INQUIRY_EVENT_ACTION&apos;
   AND ea.ref_type     = &apos;ENUM_BIZ_EVENT_TYPE&apos;
   AND ma.name         = &apos;CLIENT_INQUIRY_MANUAL_ACTION&apos;
   AND ea.value        = ?
   AND ma.value        = ?
&#47;*
   AND NOT EXISTS
       (SELECT 1 FROM corp_etl_client_inquiry_event e
         WHERE e.contact_record_id = i.contact_record_id
           AND e.event_id = **)
*&#47;</sql>
