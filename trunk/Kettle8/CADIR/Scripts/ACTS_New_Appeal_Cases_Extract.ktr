  <step>
    <name>Execute SQL script - Identify cases closed multiple times</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>MAXDAT</connection>
    <execute_each_row>N</execute_each_row>
    <single_statement>Y</single_statement>
    <replace_variables>N</replace_variables>
    <quoteString>N</quoteString>
    <sql>Begin

Update acts_appeal_case a
   Set completed = 'N'
 where completed = 'Y'
   and exists (select '1'
                from cadir_maxdat_stg s  
                    ,cadir_role_stg r
               where s.role_id = r.role_id
                 and r.name = 'Closed Cases Queue'
                 and s.c_case_number = a.case_number
                 and stg_extract_date > trunc(sysdate) - 30 )
   and (select count(1)
          from cadir_maxdat_stg s  
              ,cadir_role_stg r
         where s.role_id = r.role_id
           and r.name = 'Closed Cases Queue'
           and s.c_case_number = a.case_number) > 1;  


commit;

