  <step>
    <name>Database join - Get app receipt date</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>2</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT TO_CHAR(received_date,'mm/dd/yyyy') received_date       
FROM (
SELECT MIN(received_date) received_date
FROM document_stg d 
 INNER JOIN document_set_stg s 
   ON d.document_set_id = s.document_set_id 
 INNER JOIN doc_link_stg dl 
   ON d.document_id = dl.document_id  
WHERE doc_type_cd = 'APPLICATION' 
AND link_type_cd = 'APPLICATION'
AND link_ref_id = ?)

</sql>
    <outer_join>Y</outer_join>
    <replace_vars>N</replace_vars>
    <parameter>
      <field>
        <name>APPLICATION_ID</name>
        <type>Number</type>
      </field>
    </parameter>
    <attributes/>
    <cluster_schema/>
    <remotesteps>
      <input>
      </input>
      <output>
      </output>
    </remotesteps>
    <GUI>
      <xloc>413</xloc>
      <yloc>90</yloc>
      <draw>Y</draw>
    </GUI>
  </step>
  <step>
    <name>Database join - Get client data</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>2</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT
 cl.case_id
,cl.client_id
,cl.clnt_fname first_name
,cl.clnt_lname last_name
,TO_CHAR(cl.clnt_dob,'mm/dd/yyyy') dob
,CASE WHEN FLOOR(months_between(COALESCE(?,TRUNC(sysdate)),cl.clnt_dob)/12) &lt;= 18 THEN 'YES' ELSE 'NO' END q_client_under18
,cl.clnt_ssn
,DECODE(cl.clnt_gender_cd, 'M','Male','F','Female',null) gender
,CASE WHEN cl.supplemental_nbr IS NOT NULL THEN null ELSE cl.clnt_cin END rids_id
,CASE WHEN cl.supplemental_nbr IS NOT NULL THEN cl.supplemental_nbr ELSE cl.case_cin END state_case_id
,CASE WHEN cl.supplemental_nbr IS NOT NULL THEN null ELSE cl.clnt_generic_field8_txt END aid_category
FROM  client_stg cl
WHERE client_id = ?
</sql>
    <outer_join>Y</outer_join>
    <replace_vars>N</replace_vars>
