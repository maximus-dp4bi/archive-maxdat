    <connection>MAXDAT</connection>
    <rowlimit>1</rowlimit>
    <sql>
SELECT COALESCE(date_hcfa_referral,'${DEFAULT_RUNDATE}') date_hcfa_referral, referral_task_type
FROM app_header_stg ah
  LEFT JOIN(SELECT DISTINCT s.ref_id,TO_CHAR(s.create_ts,'MM/DD/YYYY') date_hcfa_referral,  t.task_name referral_task_type
            FROM step_instance_stg s
			  INNER JOIN d_task_types t ON s.step_definition_id = t.task_type_id
		    WHERE task_name IN(${LTSSMSP_TASKS})
			AND ref_type = 'APP_HEADER'
			AND s.ref_id = ?
			AND step_instance_id = (SELECT MAX(si.step_instance_id)
            			            FROM step_instance_stg si
                        			 INNER JOIN d_task_types ti ON si.step_definition_id = ti.task_type_id
			                        WHERE s.ref_type = si.ref_type
            			            AND s.ref_id = si.ref_id
                        			AND ti.task_name IN(${LTSSMSP_TASKS}) )
 ) i
  ON ah.application_id = i.ref_id
WHERE ah.application_id = ?</sql>
