    <sql>SELECT ls.letter_id,TO_CHAR(ls.letter_mailed_date,'MM/DD/YYYY') packet_mailed_date,TRIM(REPLACE(ls.letter_type_cd,'TN','')) letter_type,lr.reference_id application_id
      ,ai.client_id,TO_CHAR(termltr.response_due_date,'MM/DD/YYYY') termination_date, TO_CHAR(termltr.response_due_date + 90,'MM/DD/YYYY') recon_period_end,doc.link_date
FROM letters_stg ls
 JOIN letter_request_link_stg lr ON ls.letter_id = lr.lmreq_id 
 JOIN app_individual_stg ai ON lr.reference_id = ai.application_id
 JOIN (SELECT *
        FROM (SELECT letter_id,letter_case_id, letter_mailed_date,response_due_date, reference_id,letter_type_cd,ROW_NUMBER() OVER (PARTITION BY ll.reference_id ORDER BY response_due_date DESC) rn            
              FROM letters_stg lr INNER JOIN letter_request_link_stg ll ON lr.letter_id = ll.lmreq_id AND ll.reference_type = 'APP_HEADER'
              WHERE lr.letter_type_cd IN('TN 411', 'TN 408ftp','TN 408') 
              AND lr.letter_status_cd = 'MAIL'
              AND lr.letter_request_type IN('L','S'))                   
        WHERE rn = 1 ) termltr   
     ON lr.reference_id = termltr.reference_id 
     AND termltr.letter_mailed_date > ls.letter_mailed_date
 JOIN (SELECT *
       FROM(SELECT dl.link_ref_id application_id, dl.create_ts link_date, s.received_date, ROW_NUMBER() OVER (PARTITION BY dl.link_ref_id ORDER BY s.received_date) rn
            FROM document_stg d
              INNER JOIN document_set_stg s ON d.document_set_id = s.document_set_id
              INNER JOIN doc_link_stg dl ON d.document_id = dl.document_id
            WHERE dl.link_type_cd = 'APPLICATION')
       WHERE rn = 1 ) doc 
   ON lr.reference_id = doc.application_id 
 
WHERE ls.letter_type_cd = 'TN 401a'
AND ls.letter_status_cd = 'MAIL'
AND lr.reference_type = 'APP_HEADER'
AND lr.reference_id >= 320
AND ai.applicant_ind = 1
AND doc.received_date > termltr.response_due_date + 90
AND doc.link_date >= TO_DATE('${LTSSMSP_REPORT_START_DATE}','YYYY/MM/DD HH24:MI:SS') 
AND doc.link_date &lt;= TO_DATE('${LTSSMSP_REPORT_END_DATE}','YYYY/MM/DD HH24:MI:SS') 
UNION 
SELECT pkt.letter_id,TO_CHAR(pkt.letter_mailed_date,'MM/DD/YYYY') packet_mailed_date,TRIM(REPLACE(pkt.letter_type_cd,'TN','')) letter_type,ah.application_id
      ,ai.client_id,TO_CHAR(termltr.response_due_date,'MM/DD/YYYY') termination_date,TO_CHAR(termltr.response_due_date + 90,'MM/DD/YYYY') recon_period_end,doc.link_date
FROM app_header_stg ah
  JOIN app_individual_stg ai ON ah.application_id = ai.application_id
  JOIN client_stg cs ON ai.client_id = cs.client_id
  JOIN (SELECT *
        FROM (SELECT letter_id,letter_case_id, letter_mailed_date,response_due_date, reference_id,letter_type_cd,ROW_NUMBER() OVER (PARTITION BY ll.reference_id ORDER BY response_due_date DESC) rn            
              FROM letters_stg lr INNER JOIN letter_request_link_stg ll ON lr.letter_id = ll.lmreq_id AND ll.reference_type = 'APP_HEADER'
              WHERE lr.letter_type_cd IN('TN 401', 'TN 401a') 
              AND lr.letter_status_cd = 'MAIL'
              AND lr.letter_request_type = 'L')                   
        WHERE rn = 1 ) pkt
   ON ah.application_id = pkt.reference_id
  JOIN (SELECT *
        FROM (SELECT letter_id,letter_case_id, letter_mailed_date,response_due_date, reference_id,letter_type_cd,ROW_NUMBER() OVER (PARTITION BY ll.reference_id ORDER BY response_due_date DESC) rn            
              FROM letters_stg lr INNER JOIN letter_request_link_stg ll ON lr.letter_id = ll.lmreq_id AND ll.reference_type = 'APP_HEADER'
              WHERE lr.letter_type_cd IN('TN 411', 'TN 408ftp','TN 408') 
              AND lr.letter_status_cd = 'MAIL'
              AND lr.letter_request_type IN('L','S'))                   
        WHERE rn = 1 ) termltr   
     ON ah.application_id = termltr.reference_id 
     AND termltr.letter_mailed_date > pkt.letter_mailed_date
 JOIN (SELECT *
       FROM(SELECT dl.link_ref_id application_id, dl.create_ts link_date, s.received_date, ROW_NUMBER() OVER (PARTITION BY dl.link_ref_id ORDER BY s.received_date) rn
            FROM document_stg d
              INNER JOIN document_set_stg s ON d.document_set_id = s.document_set_id
              INNER JOIN doc_link_stg dl ON d.document_id = dl.document_id
            WHERE dl.link_type_cd = 'APPLICATION')
       WHERE rn = 1 ) doc 
   ON ah.application_id = doc.application_id    
  LEFT JOIN(SELECT *
       FROM(SELECT link_ref_id,ds.received_date,d.scan_date,dl.create_ts link_date,doc_form_type,ROW_NUMBER() OVER (PARTITION BY link_ref_id ORDER BY received_date) rn
            FROM document_set_stg ds 
              JOIN document_stg d ON ds.document_set_id = d.document_set_id
              JOIN doc_link_stg dl ON d.document_id = dl.document_id AND link_type_cd = 'APPLICATION' 
            WHERE d.doc_type_cd = 'APPLICATION' 
            AND d.doc_form_type = 'LTSS' )
       WHERE rn = 1) doc_rcvd
  ON ah.application_id = doc_rcvd.link_ref_id      
WHERE ah.application_id >= 320
AND ai.applicant_ind = 1
AND doc.received_date > termltr.response_due_date + 90
AND (cs.clnt_generic_field8_txt IN('MA M','MA L') OR doc_rcvd.link_date IS NOT NULL)
AND doc.link_date >= TO_DATE('${LTSSMSP_REPORT_START_DATE}','YYYY/MM/DD HH24:MI:SS') 
AND doc.link_date &lt;= TO_DATE('${LTSSMSP_REPORT_END_DATE}','YYYY/MM/DD HH24:MI:SS') 



</sql>
