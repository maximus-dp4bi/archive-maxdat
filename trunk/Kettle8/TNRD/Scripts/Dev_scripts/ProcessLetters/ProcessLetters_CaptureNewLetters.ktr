    <connection>MAXDAT</connection>
    <sql>Select 
L.LETTER_ID,
L.LETTER_CREATE_TS,
COALESCE((CASE WHEN S.STAFF_ID &lt;0 THEN 'MAXeb' 
ELSE S.DISPLAY_NAME 
END),L.LETTER_CREATED_BY) AS USERNAME,
L.LETTER_REQUESTED_ON,
'Active' AS INSTANCE_STATUS,
L.LETTER_TYPE,
L.PROGRAM,
L.LETTER_CASE_ID,
L.RESIDENCE_COUNTY,
L.RESIDENCE_ZIP_CODE,
L.LANGUAGE,
CASE WHEN L.LETTER_REQUEST_TYPE IN('R','S')
THEN 'Y'
ELSE 'N'
END AS REPRINT,
CASE WHEN L.LETTER_DRIVER_TYPE = 'CASE'
THEN 'CASE'
WHEN LETTER_DRIVER_TYPE = 'CLNT'
THEN 'CLNT'
ELSE NULL
END AS REQUEST_DRIVER_TYPE,
L.DRIVER_TABLE_NAME,
'Requested' AS STATUS,
L.LETTER_CREATE_TS AS ASSD_PROCESS_LETTER_REQ,
'N' as  ASF_PROCESS_LETTER_REQ,
'N' as ASF_TRANSMIT,
'N' as ASF_RECEIVE_CONFIRMATION,
'N' as ASF_CREATE_ROUTE_WORK
FROM 
LETTERS_STG L
LEFT OUTER JOIN STAFF_LKUP S
ON L.LETTER_CREATED_BY = S.STAFF_ID
WHERE 
--LETTER_ID > ${Last_LTR_ID}
--AND TRUNC(LETTER_CREATE_TS) > TRUNC(SYSDATE - 4)
LETTER_CREATE_TS >=  to_date('${LastSuccessfulETLRunDate}','yyyy/mm/dd hh24:mi:ss')
ORDER BY LETTER_ID</sql>
