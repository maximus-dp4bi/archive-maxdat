  <step>
    <name>Kofax Source</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>KOFAX_CENTRAL</connection>
    <sql>SELECT "StatsBatch"."BatchGUID",
 "StatsBatch"."ExternalBatchID",
 "StatsBatch"."BatchName",
 "StatsBatch"."CreationStationID",			
 "StatsBatch"."CreationUserName",			
 "StatsBatch"."CreationUserID",			
 "StatsBatch"."BatchClass",			
 coalesce("StatsBatch"."BatchClassDescription",' ') "BatchClassDescription",	
 'Unknown' BATCH_TYPE,		
 "StatsBatchModule"."StartDateTime",
 (case when exists(select 1 from "${DB_KOFAX_CENTRAL_DB}".."StatsBatchModule" sbm2 where sbm2."ExternalBatchID"="StatsBatch"."ExternalBatchID" and sbm2."PagesScanned">0) then 'Y' else 'N' end) "PagesScanned",
 (case when exists(select 1 from "${DB_KOFAX_CENTRAL_DB}".."StatsBatchModule" sbm2 where sbm2."ExternalBatchID"="StatsBatch"."ExternalBatchID" and sbm2."DocumentsCreated">0) then 'Y' else 'N' end) "DocumentsCreated",
 (case when exists(select 1 from "${DB_KOFAX_CENTRAL_DB}".."StatsBatchModule" sbm2 where sbm2."ExternalBatchID"="StatsBatch"."ExternalBatchID" and sbm2."DocumentsDeleted">0) then 'Y' else 'N' end) "DocumentsDeleted",
 (case when exists(select 1 from "${DB_KOFAX_CENTRAL_DB}".."StatsBatchModule" sbm2 where sbm2."ExternalBatchID"="StatsBatch"."ExternalBatchID" and sbm2."PagesReplaced">0) then 'Y' else 'N' end) "PagesReplaced",
 (case when exists(select 1 from "${DB_KOFAX_CENTRAL_DB}".."StatsBatchModule" sbm2 where sbm2."ExternalBatchID"="StatsBatch"."ExternalBatchID" and sbm2."PagesDeleted">0) then 'Y' else 'N' end) "PagesDeleted",
 'Active' INSTANCE_STATUS,
 "StatsBatchModule"."Priority",
 "StatsBatchModule"."Deleted",
 getdate() sysdate,
 'CENTRAL' source_server
FROM
 (SELECT "StatsBatch"."BatchGUID",
  min("StatsBatchModule"."StartDateTime") "StartDateTime"
 FROM
  "${DB_KOFAX_CENTRAL_DB}".."StatsBatchModule", 
  "${DB_KOFAX_CENTRAL_DB}".."StatsBatch" 
 WHERE "StatsBatch"."BatchGUID"="StatsBatchModule"."BatchGUID"
 AND "StatsBatchModule"."ModuleLaunchID" in (select "ModuleLaunchID" from "${DB_KOFAX_CENTRAL_DB}".."StatsModuleLaunch" where "ModuleName"='${SCAN_MODULE_NAME}')
  and "StatsBatchModule"."StartDateTime">= convert(datetime,'${CENTRAL_START_DATE}')
 GROUP BY "StatsBatch"."BatchGUID") "BatchFirstScanned",
 "${DB_KOFAX_CENTRAL_DB}".."StatsBatchModule",
 "${DB_KOFAX_CENTRAL_DB}".."StatsBatch" 
WHERE "StatsBatch"."BatchGUID"="StatsBatchModule"."BatchGUID"
 AND "StatsBatchModule"."ModuleLaunchID" in (select "ModuleLaunchID" from "${DB_KOFAX_CENTRAL_DB}".."StatsModuleLaunch" where "ModuleName"='${SCAN_MODULE_NAME}')

 and "StatsBatchModule"."StartDateTime">= convert(datetime,'${CENTRAL_START_DATE}')
 AND "StatsBatch"."BatchGUID"="BatchFirstScanned"."BatchGUID"
 AND "StatsBatchModule"."StartDateTime"="BatchFirstScanned"."StartDateTime"
 AND "StatsBatch"."BatchClass" in ('${BATCH_CLASS_LIST}')
</sql>
