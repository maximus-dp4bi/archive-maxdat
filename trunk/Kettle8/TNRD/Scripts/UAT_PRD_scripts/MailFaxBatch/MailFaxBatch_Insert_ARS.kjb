    <entry>
      <name>Clear out WIP tables and load with active batch instances for each</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>BEGIN
DELETE FROM CORP_ETL_MFB_BATCH_WIP;
DELETE FROM CORP_ETL_MFB_BATCH_EVENTS_WIP;
DELETE FROM CORP_ETL_MFB_FORM_WIP;
DELETE FROM CORP_ETL_MFB_ENVELOPE_WIP;
DELETE FROM CORP_ETL_MFB_DOCUMENT_WIP;

insert into CORP_ETL_MFB_BATCH_WIP select * from CORP_ETL_MFB_BATCH WHERE COMPLETE_DT IS NULL;
insert into CORP_ETL_MFB_BATCH_EVENTS_WIP select * from CORP_ETL_MFB_BATCH_EVENTS Z WHERE EXISTS(SELECT 1 FROM CORP_ETL_MFB_BATCH B WHERE B.BATCH_ID=Z.BATCH_ID AND COMPLETE_DT IS NULL);
insert into CORP_ETL_MFB_FORM_WIP select * from CORP_ETL_MFB_FORM Z WHERE EXISTS(SELECT 1 FROM CORP_ETL_MFB_BATCH B WHERE B.BATCH_ID=Z.BATCH_ID AND COMPLETE_DT IS NULL);
insert into CORP_ETL_MFB_ENVELOPE_WIP select * from CORP_ETL_MFB_ENVELOPE Z WHERE EXISTS(SELECT 1 FROM CORP_ETL_MFB_BATCH B WHERE B.BATCH_ID=Z.BATCH_ID AND COMPLETE_DT IS NULL);
insert into CORP_ETL_MFB_DOCUMENT_WIP select * from CORP_ETL_MFB_DOCUMENT Z WHERE EXISTS(SELECT 1 FROM CORP_ETL_MFB_BATCH B WHERE B.BATCH_ID=Z.BATCH_ID AND COMPLETE_DT IS NULL);


UPDATE CORP_ETL_MFB_BATCH_WIP set UPDATED=NULL;
UPDATE CORP_ETL_MFB_BATCH_EVENTS_WIP set UPDATED=NULL;
UPDATE CORP_ETL_MFB_FORM_WIP set UPDATED=NULL;
UPDATE CORP_ETL_MFB_ENVELOPE_WIP set UPDATED=NULL;
UPDATE CORP_ETL_MFB_DOCUMENT_WIP set UPDATED=NULL;

COMMIT;
END; 

</sql>
