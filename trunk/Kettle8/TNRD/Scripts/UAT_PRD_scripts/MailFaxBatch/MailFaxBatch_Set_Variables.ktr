  <step>
    <name>CORP_ETL_MFB_CONTROLS</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT to_date(RSD.VALUE)-RSL.VALUE remote_start_date,
 to_date(CSD.VALUE)-CSL.VALUE central_start_date,
 to_date(ASD.VALUE)-ASL.VALUE ars_start_date,
 SCANM.VALUE SCAN_MODULE_NAME,
 QCM.VALUE QC_MODULE_NAME,
 CLASSM.VALUE CLASSIFICATION_MODULE_NAME,
 RECM.VALUE RECOGNITION_MODULE_NAME,
 VALM.VALUE VALIDATION_MODULE_NAME,
 PDFM.VALUE PDF_MODULE_NAME,
 REPM.VALUE REPORT_MODULE_NAME,
 EXPM.VALUE EXPORT_MODULE_NAME,
 BCL.VALUE BATCH_CLASS_LIST,
 AF.VALUE ARS_FLAG,
 CF.VALUE CENTRAL_FLAG,
 RF.VALUE REMOTE_FLAG

FROM CORP_ETL_CONTROL RSD,
 CORP_ETL_CONTROL CSD,
 CORP_ETL_CONTROL ASD,
 CORP_ETL_CONTROL RSL,
 CORP_ETL_CONTROL CSL,
 CORP_ETL_CONTROL ASL,
 CORP_ETL_CONTROL SCANM,
 CORP_ETL_CONTROL QCM,
 CORP_ETL_CONTROL CLASSM,
 CORP_ETL_CONTROL RECM,
 CORP_ETL_CONTROL VALM,
 CORP_ETL_CONTROL PDFM,
 CORP_ETL_CONTROL REPM,
 CORP_ETL_CONTROL EXPM,
 CORP_ETL_CONTROL AF,
 CORP_ETL_CONTROL CF,
 CORP_ETL_CONTROL RF,
(SELECT SUBSTR (SYS_CONNECT_BY_PATH (value , ''','''), 4) VALUE
      FROM (SELECT value , ROW_NUMBER () OVER (ORDER BY value ) rn,
                   COUNT (*) OVER () cnt
              FROM CORP_ETL_CONTROL where NAME like 'MFB_BATCH_CLASS_LIST%')
     WHERE rn = cnt
START WITH rn = 1
CONNECT BY rn = PRIOR rn + 1
) BCL
WHERE RSD.NAME='MFB_REMOTE_LAST_UPDATE_DATE'
 AND CSD.NAME='MFB_CENTRAL_LAST_UPDATE_DATE'
 AND ASD.NAME='MFB_ARS_LAST_UPDATE_DATE'
 AND RSL.NAME='MFB_REMOTE_LOOKBACK_DAYS'
 AND CSL.NAME='MFB_CENTRAL_LOOKBACK_DAYS'
 AND ASL.NAME='MFB_ARS_LOOKBACK_DAYS'
 AND SCANM.NAME='MFB_SCAN_MODULE_NAME'
 AND QCM.NAME='MFB_QC_MODULE_NAME'
 AND CLASSM.NAME='MFB_CLASSIFICATION_MODULE_NAME'
 AND RECM.NAME='MFB_RECOGNITION_MODULE_NAME'
 AND VALM.NAME='MFB_VALIDATION_MODULE_NAME'
 AND PDFM.NAME='MFB_PDF_MODULE_NAME'
 AND REPM.NAME='MFB_REPORT_MODULE_NAME'
 AND EXPM.NAME='MFB_EXPORT_MODULE_NAME'
 AND AF.NAME='MFB_ARS_FLAG'
 AND CF.NAME='MFB_CENTRAL_FLAG'
 AND RF.NAME='MFB_REMOTE_FLAG'
 



</sql>
