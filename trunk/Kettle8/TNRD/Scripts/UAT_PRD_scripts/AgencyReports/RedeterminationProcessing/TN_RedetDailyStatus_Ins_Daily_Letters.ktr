    </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>SELECT s.lmreq_id
       ,COALESCE(ls.reference_id,pkt.application_id) application_id
       ,s.case_id
       ,ls.client_id        
       ,s.mailed_date
       ,lm.process_ts
       ,ld.name letter_type_cd
       ,sysdate create_date
FROM letter_request s 
 JOIN letter_request_link ls ON s.lmreq_id = ls.lmreq_id 
 JOIN letter_definition ld ON s.lmdef_id = ld.lmdef_id
JOIN (SELECT *
       FROM (SELECT job_id,process_ts,lmreq_id,ROW_NUMBER() OVER (PARTITION BY lmreq_id ORDER BY job_id DESC) rn
             FROM etl_l_mailhouse lm 
             WHERE process_ts IS NOT NULL)
       WHERE rn = 1  ) lm        
       ON s.lmreq_id = lm.lmreq_id
LEFT JOIN (SELECT lmreq_id, application_id,case_id
            FROM(
                  SELECT lr.lmreq_id,lr.case_id,ll.reference_id application_id,
                         ROW_NUMBER() OVER(PARTITION BY lr.case_id ORDER BY lr.create_ts) rn
                  FROM letter_request lr
                    JOIN letter_request_link ll ON lr.lmreq_id = ll.lmreq_id
                    JOIN letter_definition ld ON lr.lmdef_id = ld.lmdef_id
                  WHERE lr.status_cd = 'MAIL'
                  AND ll.reference_type = 'APP_HEADER')
            WHERE rn = 1 ) pkt ON pkt.case_id = s.case_id   
WHERE status_cd = 'MAIL'
AND ld.name != 'TN 407'
AND lm.process_ts >= TO_DATE('${RDSF_LTR_REPORT_START_DATE}','YYYY/MM/DD HH24:MI:SS')
AND lm.process_ts &lt;= TO_DATE('${RDSF_LTR_REPORT_END_DATE}','YYYY/MM/DD HH24:MI:SS')
AND s.mailed_date IS NOT NULL
UNION
SELECT s.lmreq_id
       ,COALESCE(ls.reference_id,pkt.application_id) application_id
       ,s.case_id
       ,ls.client_id        
       ,s.mailed_date
       ,lm.process_ts
       ,COALESCE(l.notification_type_cd_old,ld.name) letter_type_cd
       ,sysdate create_date
FROM letter_request s 
 JOIN letter_request_link ls ON s.lmreq_id = ls.lmreq_id 
 JOIN letter_definition ld ON s.lmdef_id = ld.lmdef_id
LEFT JOIN (SELECT *
       FROM (SELECT job_id,process_ts,lmreq_id,ROW_NUMBER() OVER (PARTITION BY lmreq_id ORDER BY job_id DESC) rn
             FROM etl_l_mailhouse lm 
             WHERE process_ts IS NOT NULL)
       WHERE rn = 1  ) lm        
       ON s.lmreq_id = lm.lmreq_id
JOIN app_missing_info m ON s.lmreq_id = m.ext_ref_id
JOIN enum_app_mi_type t ON m.mi_type_cd = t.value
JOIN enum_history_mi_letter_config l ON t.mi_category = l.value         
 LEFT JOIN (SELECT lmreq_id, application_id,case_id
            FROM(
                  SELECT lr.lmreq_id,lr.case_id,ll.reference_id application_id,
                         ROW_NUMBER() OVER(PARTITION BY lr.case_id ORDER BY lr.create_ts) rn
                  FROM letter_request lr
                    JOIN letter_request_link ll ON lr.lmreq_id = ll.lmreq_id
                    JOIN letter_definition ld ON lr.lmdef_id = ld.lmdef_id
                  WHERE lr.status_cd = 'MAIL'
                  AND ll.reference_type = 'APP_HEADER')
            WHERE rn = 1 ) pkt ON pkt.case_id = s.case_id   
WHERE s.status_cd = 'MAIL'
AND ld.name = 'TN 406'
AND lm.process_ts >= TO_DATE('${RDSF_LTR_REPORT_START_DATE}','YYYY/MM/DD HH24:MI:SS')
AND lm.process_ts &lt;= TO_DATE('${RDSF_LTR_REPORT_END_DATE}','YYYY/MM/DD HH24:MI:SS')
AND s.mailed_date IS NOT NULL</sql>
