    <connection>MAXDAT</connection>
    <sql>WITH totbybot AS (
	SELECT sum(
	CASE WHEN name like ('%File an%') 
	THEN 
	  missedopportunities
	ELSE
	   defbybot
	END) def_by_bot
	FROM maxdat_support.MC_TOPIC_METRIC WHERE NAME NOT LIKE '%None%' 
), totmissed AS (
	SELECT sum(missedopportunities+noagentnecessary) missed_opps 
	FROM maxdat_support.MC_TOPIC_METRIC)
SELECT to_char(sum(total)) AS sheet_data FROM MAXDAT_SUPPORT.MC_TOPIC_METRIC
UNION ALL
SELECT to_char(sum(missedchats)) FROM MAXDAT_SUPPORT.MC_TOPIC_METRIC
UNION ALL
SELECT CASE WHEN (missed_opps - def_by_bot) &lt; 0 THEN '0' ELSE to_char(missed_opps - def_by_bot) end FROM totbybot JOIN totmissed ON 1=1
UNION ALL
SELECT case when (TO_CHAR(TRUNC(averagewaitseconds/3600),'FM9900')) is null then '00' else TO_CHAR(TRUNC(averagewaitseconds/3600),'FM9900') end || ':' || 
       case when (TO_CHAR(TRUNC(MOD(averagewaitseconds,3600)/60),'FM00')) is null then '00' else TO_CHAR(TRUNC(MOD(averagewaitseconds,3600)/60),'FM00') end || ':' || 
       case when (TO_CHAR(MOD(averagewaitseconds,60),'FM00')) is null then '00' else TO_CHAR(MOD(averagewaitseconds,60),'FM00') end FROM MAXDAT_SUPPORT.MC_CHAT_TOTALS
UNION ALL
SELECT case when (TO_CHAR(TRUNC(averageholdseconds/3600),'FM9900')) is null then '00' else TO_CHAR(TRUNC(averageholdseconds/3600),'FM9900') end || ':' || 
       case when (TO_CHAR(TRUNC(MOD(averageholdseconds,3600)/60),'FM00')) is null then '00' else TO_CHAR(TRUNC(MOD(averageholdseconds,3600)/60),'FM00') end || ':' || 
       case when (TO_CHAR(MOD(averageholdseconds,60),'FM00')) is null then '00' else TO_CHAR(MOD(averageholdseconds,60),'FM00') end FROM MAXDAT_SUPPORT.MC_CHAT_TOTALS
UNION ALL
SELECT trim(both '"' from trim(trailing '}' FROM min(weekday))) FROM MAXDAT_SUPPORT.MC_busiest_time WHERE busytimecount = (SELECT max(busytimecount) FROM MAXDAT_SUPPORT.MC_busiest_time)
UNION ALL
SELECT trim(both '"' from min(busiesttime)) FROM MAXDAT_SUPPORT.MC_busiest_time WHERE busytimecount = (SELECT max(busytimecount) FROM MAXDAT_SUPPORT.MC_busiest_time)
Union all
select to_char(total - missedchats - (select CASE WHEN (missedopportunities - def_by_bot) &lt; 0 THEN 0 ELSE missedopportunities - def_by_bot end FROM MAXDAT_SUPPORT.MC_CHAT_TOTALS join totbybot ON 1=1)) from MAXDAT_SUPPORT.MC_CHAT_TOTALS
</sql>
