  <step>
    <name>Get Program and Sub program types</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>5</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>WITH driver AS
(SELECT subprogram_type
        ,CASE
             WHEN elig_status_cd in ('M','V') AND
                  subprogram_type = 'ICP' AND
                  exists (select 1 from client_elig_status
                    WHERE client_id = ?
                    and start_date &lt;= ?
                    and nvl(end_date,to_date('7777-07-07 00:00:00','yyyy-mm-dd hh24:mi:ss')) >= ?
                      AND elig_status_cd = 'X' and subprogram_type = 'PCCM')
             THEN  5
             WHEN elig_status_cd  ='M' AND
                  subprogram_type = 'PCCM' AND
                  (exists (select 1 from client_elig_status
                    WHERE client_id = ?
                    and start_date &lt;= ?
                    and nvl(end_date,to_date('7777-07-07 00:00:00','yyyy-mm-dd hh24:mi:ss')) >= ?
                      AND elig_status_cd = 'X' and subprogram_type = 'VMC')
                   OR   
                   exists (select 1 from client_elig_status
                    WHERE client_id = ?
                    and start_date &lt;= ?
                    and nvl(end_date,to_date('7777-07-07 00:00:00','yyyy-mm-dd hh24:mi:ss')) >= ?
                      AND elig_status_cd = 'V' and subprogram_type = 'ICP')               
                  )
             THEN 4
             WHEN elig_status_cd = 'V' AND
                  subprogram_type = 'VMC' AND
                  exists (select 1 from client_elig_status
                    WHERE client_id = ?
                    and start_date &lt;= ?
                    and nvl(end_date,to_date('7777-07-07 00:00:00','yyyy-mm-dd hh24:mi:ss')) >= ?
                      AND elig_status_cd = 'M' and subprogram_type = 'PCCM')
             THEN 3
             WHEN elig_status_cd  IN ('V','M') AND
                  subprogram_type = 'MMAI' AND
                  (exists (select 1 from client_elig_status
                    WHERE client_id = ?
                    and start_date &lt;= ?
                    and nvl(end_date,to_date('7777-07-07 00:00:00','yyyy-mm-dd hh24:mi:ss')) >= ?
                      AND elig_status_cd = 'X' and subprogram_type = 'LTSS')
                   OR   
                   NOT exists (select 1 from client_elig_status
                    WHERE client_id = ?
                    and start_date &lt;= ?
                    and nvl(end_date,to_date('7777-07-07 00:00:00','yyyy-mm-dd hh24:mi:ss')) >= ?
                      AND  subprogram_type = 'LTSS')               
                  )
              THEN 2
              WHEN elig_status_cd = 'M' AND
                  subprogram_type = 'LTSS' 
              THEN 1    
             ELSE
              0
         END weight
    FROM client_elig_status
   WHERE client_id = ?
                    and start_date &lt;= ?
                    and nvl(end_date,to_date('7777-07-07 00:00:00','yyyy-mm-dd hh24:mi:ss')) >= ?
)
SELECT DISTINCT CASE
                    WHEN weight >= 1 THEN
                     subprogram_type
                    ELSE
                     NULL
                END program_subtype
               ,'MEDICAID' program_type
  FROM driver
WHERE weight = (SELECT MAX(weight)
                   FROM driver)

</sql>
