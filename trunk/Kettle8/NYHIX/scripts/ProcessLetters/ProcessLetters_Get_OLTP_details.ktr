    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT distinct M.CASE_CIN ACCOUNT_ID_M
FROM LETTER_REQUEST L
LEFT JOIN LETTER_REQUEST_LINK LR
ON L.LMREQ_ID = LR.LMREQ_ID
LEFT JOIN MANUAL_NOTICE_CASE M
ON LR.REFERENCE_ID = M.MANUAL_NOTICE_CASE_ID
Where L.LMREQ_ID = ?
</sql>
