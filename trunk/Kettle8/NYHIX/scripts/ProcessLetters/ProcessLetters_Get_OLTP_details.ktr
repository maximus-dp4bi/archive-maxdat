  <step>
    <name>Database join 2</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>10</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT DISTINCT LR. LMREQ_ID , CANCEL_DT,CANCEL_BY,TRANSMIT_FILE_NAME, TRANSMIT_FILE_DT,LETTER_RESP_FILE_NAME,LETTER_RESP_FILE_DT ,ERR_DATE,
LR.FILE_NAME
FROM LETTER_REQUEST LR
LEFT OUTER JOIN (SELECT distinct L.LMREQ_ID, L.STATUS_CD,
MAX(L.CREATE_TS) over (partition BY L.LMREQ_ID) AS CANCEL_DT,
COALESCE((CASE WHEN S.STAFF_ID &lt;0 THEN 'MAXeb' 
ELSE CASE WHEN S.LAST_NAME IS NULL THEN NULL ELSE S.LAST_NAME||','||S.FIRST_NAME END
END),L.CREATED_BY) AS CANCEL_BY 
FROM  
letter_req_status_history L 
LEFT JOIN STAFF S
ON L.CREATED_BY = TO_CHAR(S.STAFF_ID)
WHERE LMREQ_ID = ?
AND STATUS_CD IN  ('CANC','VOID','OBE','COMBND')
) CAN
ON LR. LMREQ_ID = CAN. LMREQ_ID
LEFT OUTER JOIN (SELECT DISTINCT LD.LMREQ_ID ,LD.JOB_ID,JB.FILENAME,substr(jb.filename, instr(jb.filename,'/',-1)+1) as TRANSMIT_file_name,jb.create_ts AS TRANSMIT_FILE_DT
FROM (SELECT LR.LMREQ_ID,MAX(ld.job_id) over (partition BY LR.LMREQ_ID) JOB_ID 
FROM LETTER_REQUEST LR
JOIN letter_out_data_content LD
ON LD.LETTER_REQ_ID = LR.LMREQ_ID
WHERE LMREQ_ID = ?
--GROUP BY LR.LMREQ_ID
) LD
JOIN JOB_RUN_DATA JB
ON JB.JOB_RUN_DATA_ID = LD.JOB_ID) TRAN 
ON LR. LMREQ_ID = TRAN. LMREQ_ID
LEFT OUTER JOIN (
SELECT DISTINCT LD.LMREQ_ID ,LD.JOB_ID,JB.FILENAME,substr(jb.filename, instr(jb.filename,'/',-1)+1) as letter_resp_file_name,jb.create_ts AS letter_resp_FILE_DT
FROM (SELECT LMREQ_ID,MAX(job_id) over (partition BY LMREQ_ID) JOB_ID 
FROM etl_l_mailhouse
where lmreq_id = ?
--GROUP BY LMREQ_ID
) LD
JOIN JOB_RUN_DATA JB
ON JB.JOB_RUN_DATA_ID = LD.JOB_ID
) RES
ON LR. LMREQ_ID = RES. LMREQ_ID
LEFT OUTER JOIN 
(SELECT distinct lmreq_id ,STATUS_CD ,MAX(create_ts) over (partition BY LMREQ_ID) as err_date FROM letter_req_status_history
where status_cd = 'ERR'
and lmreq_id = ?
--group by LMREQ_ID,status_cd
) ER
ON LR. LMREQ_ID = ER. LMREQ_ID
WHERE LR.LMREQ_ID = ?</sql>
