    <connection>MAXDAT</connection>
    <sql>select LETTER_REQUEST_ID,
LETTER_ID
,LETTER_REQUESTED_ON AS REQUEST_DT
,A.LETTER_TYPE AS LETTER_TYPE
,A.PROGRAM AS PROGRAM
,LETTER_CASE_ID AS CASE_ID
,RESIDENCE_COUNTY AS COUNTY_CODE
,RESIDENCE_ZIP_CODE AS ZIP_CODE
,A.LANGUAGE AS LANGUAGE
,CASE WHEN LETTER_REQUEST_TYPE = 'R'
THEN 'Y'
ELSE 'N'
END AS REPRINT
,CASE WHEN LETTER_DRIVER_TYPE = 'CASE'
THEN 'CASE'
WHEN LETTER_DRIVER_TYPE = 'CLNT'
THEN 'CLNT'
ELSE NULL
END AS REQUEST_DRIVER_TYPE
,DRIVER_TABLE_NAME
,LETTER_STATUS AS STATUS
,NVL(LETTER_UPDATE_TS,LETTER_CREATE_TS) AS STATUS_DT
,LETTER_UPDATE_TS AS LAST_UPDATE_DT
,LETTER_SENT_ON AS SENT_DT
,LETTER_MAILED_DATE AS MAILED_DT
,LETTER_REJECT_REASON AS REJECT_REASON
,LETTER_PRINTED_ON AS PRINT_DT
,LETTER_ERROR_CODES  AS ERROR_REASON
,A.RETURN_REASON AS RETURN_REASON
,COALESCE((CASE WHEN S.STAFF_ID &lt;0 THEN 'MAXeb' 
ELSE S.DISPLAY_NAME 
END),A.LETTER_UPDATED_BY) AS UPDATED_BY
,LETTER_RETURN_DATE AS RETURN_DT
,ST.STEP_INSTANCE_ID AS TASK_ID
,ST.STEP_DEFINITION_ID 
,ST.CREATE_TS AS TASK_CREATE_DT
from CORP_ETL_PROC_LETTERS_OLTP NF
JOIN letters_stg A
ON NF.LETTER_REQUEST_ID = A.LETTER_ID
LEFT OUTER JOIN STAFF_LKUP S
ON A.LETTER_UPDATED_BY = S.staff_id
LEFT OUTER JOIN 
(SELECT * FROM STEP_INSTANCE_STG WHERE STEP_DEFINITION_ID IN (${LTR_REF_VAL})) ST
ON NF.CASE_ID = ST.CASE_ID 
WHERE STAGE_DONE_DATE IS NULL
</sql>
