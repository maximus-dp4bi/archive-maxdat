    <connection>MAXDAT</connection>
    <sql>SELECT 
 A.LastLetterReq_ID,
 C.LRSHISTORY_ID,
 B.last_successful_run_date
 
FROM 
(select max(a) LastLetterReq_ID from  ( SELECT to_number(Value) a  FROM corp_etl_control  WHERE Name = 'PL_LAST_LMREQ_ID' union select NVL(max(LETTER_REQUEST_ID),0) a from CORP_ETL_PROC_LETTERS ) ) A,
(select nvl(max(job_start_date)-${g_PL_SUCCESS_RUN_LOOK_BACK_DAYS}, to_date('2013/03/05','yyyy/mm/dd')) as last_successful_run_date from corp_etl_job_statistics where job_status_cd = 'COMPLETED' and job_name = 'Process_Letters_runall') B,
(select NVL(max(LRSHISTORY_ID),0) LRSHISTORY_ID from LETTER_REQ_STATUS_HISTORY)C



</sql>
