    <sql>SELECT  WFM_TASK_YYYYMM, WFM_STAFF_ID   
FROM DP_SCORECARD.PP_WFM_TASK_AUDIT_CNTRL_DIFFS 
JOIN ( SELECT 
            CASE WHEN VALUE = '190000' 
            THEN TO_CHAR(ADD_MONTHS(SYSDATE,-24),'YYYYMM') 
            ELSE VALUE END AS VALUE 
    FROM MAXDAT.CORP_ETL_CONTROL
    WHERE NAME = 'PP_WFM_TASK_AUDIT_TASK_START_YYYYMM'
	) STARTED
ON WFM_TASK_YYYYMM >= STARTED.VALUE	
JOIN ( SELECT 
        CASE WHEN VALUE = '190000' THEN TO_CHAR(SYSDATE-30,'YYYYMM') 
        ELSE VALUE END AS VALUE 
    FROM MAXDAT.CORP_ETL_CONTROL
    WHERE NAME = 'PP_WFM_TASK_AUDIT_TASK_END_YYYYMM'
	) ENDED
ON WFM_TASK_YYYYMM &lt;= ENDED.VALUE	
WHERE WFM_STAFF_ID IS NOT NULL 
AND WFM_TASK_YYYYMM IS NOT NULL
and MOD(WFM_STAFF_ID,4) = 0
ORDER BY WFM_TASK_YYYYMM DESC, WFM_STAFF_ID   </sql>
