    <connection>WFM</connection>
    <sql>SELECT TASK_YYYYMM, STAFF_ID, COUNT(TASK_ID) AS TASK_COUNT, SUM(TASK_ID) AS TASK_SUM, SYSDATE AS RUN_DATE
FROM ( SELECT TO_CHAR(cast(from_tz(cast(TASK_START as timestamp), 'UTC') at time zone ('${cc.timezone}') as Date),'YYYYMM') as TASK_YYYYMM,
		TASK_ID, TASK_START, STAFF_ID, SCHEDULE_INSTANCE_ID
	 	FROM PIPKINS.TASK 
		WHERE SCENARIO_GROUP_ID = 1
		AND TASK_EDIT_ID = 0
		AND TASK_START 	&lt;= trunc(sysdate-30)
	) TASK
JOIN PIPKINS.SCHEDULE_INSTANCE SI 
   	ON (TASK.SCHEDULE_INSTANCE_ID = SI.SCHEDULE_INSTANCE_ID)
WHERE TASK.TASK_YYYYMM 	>= '201601'
GROUP BY TASK_YYYYMM, STAFF_ID
ORDER BY TASK_YYYYMM DESC, STAFF_ID

</sql>
