    <name>GET_PIPKINS_TASK_CONTROL  MOD 4 = 3</name>
    <type>TableInput</type>
    <description>Staff Eligibility
</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>WFM</connection>
    <sql>select staff_id, 
    task_yyyymmdd, 
    count(task_id) task_count, 
    sum(task_id) task_sum, 
    sum(orahash) orahash, 
    sysdate as run_date
from ( SELECT
	staff_id, 
	TO_CHAR(cast(from_tz(cast(TASK_START as timestamp), 'UTC') at time zone ('${cc.timezone}') as Date),'YYYYMMDD') as TASK_YYYYMMDD,
	task_id,
	ora_hash(STAFF_ID|| 
	TASK_CATEGORY_ID|| 
	DURATION|| 
	EVENT_ID|| 
	SUPERVISOR|| 
	TASK_MODIFICATION_REQUEST_REF|| 
	TASK_ID|| 
	task.SCENARIO_GROUP_ID|| 
	task.SCHEDULE_INSTANCE_ID|| 
	task.TASK_EDIT_ID|| 
	EDIT_STATE|| 
	ALT_TASK_EDIT_ID) as orahash
FROM pipkins.task
JOIN PIPKINS.SCHEDULE_INSTANCE SI 
   	ON (TASK.SCHEDULE_INSTANCE_ID = SI.SCHEDULE_INSTANCE_ID)
where 1=1
and task.scenario_group_id = 1 
and task.task_edit_id = 0
and mod(staff_id,4) = 3
)
where task_yyyymmdd BETWEEN to_char(sysdate-30,'YYYYMMDD') AND to_char(sysdate,'YYYYMMDD')
group by staff_id, task_yyyymmdd
</sql>
