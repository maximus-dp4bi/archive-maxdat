    <name>PP_WFM_EE_CLOCKING_STAGE</name>
    <type>TableInput</type>
    <description>Pipkins_Agent_Clocking_Extract</description>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>WFM</connection>
    <sql>-- PP_WFM_AGENT_CLOCKING_WORK_DAY environment variable is PPWFMAGENTCLOCKINGWORKDAY
---------------------------------------------------                 
WITH  DATE_TIMES 
---------------------------------------------------                 
AS ( 
  SELECT RUN_DATE,
        CAST(From_Tz(Cast(trunc(RUN_DATE) As Timestamp),'US/EASTERN')   At Time Zone ('UTC') AS DATE)            AS GMT_START_DATE_TIME,
        CAST(From_Tz(Cast((trunc(SYSDATE+1))-(1/(24*60*60)) as Timestamp),'US/EASTERN') At Time Zone ('UTC') as date)   AS GMT_END_DATE_TIME
	FROM ( SELECT  
			to_date('${PPWFMAGENTCLOCKINGWORKDAY}','YYYY/MM/DD')  AS RUN_DATE
			FROM DUAL 
		)           
 	 ),
---------------------------------------------------                 
ALL_DATA
AS (
        SELECT
        'T' 															AS SRC,
        CAST( T.TASK_START AS TIMESTAMP )           					AS SRC_TIMESTAMP_GMT,
        T.STAFF_ID                                  					AS STAFF_ID,
        T.SCENARIO_GROUP_ID												AS SRC_TYPE,
        T.EVENT_ID,
        T.TASK_START                                                   	AS TASK_START_GMT,
        T.TASK_END                                                     	AS TASK_END_GMT, 
        round((T.TASK_END  - T.TASK_START)*(24*60*60),0) as duration
        FROM TASK T
        CROSS JOIN DATE_TIMES
        WHERE 1=1
        --and TASK_START BETWEEN GMT_START_DATE_TIME AND GMT_END_DATE_TIME
        --AND TASK_END BETWEEN GMT_START_DATE_TIME AND GMT_END_DATE_TIME
        AND TASK_EDIT_ID = '0'
        AND ( SCENARIO_GROUP_ID = '1' or SCENARIO_GROUP_ID = '6' or  SCENARIO_GROUP_ID = '5' or  SCENARIO_GROUP_ID = '4')
        AND ( EVENT_ID = '1' OR EVENT_ID = '2' )
        AND TASK_START BETWEEN DATE_TIMES.GMT_START_DATE_TIME 
		AND DATE_TIMES.GMT_END_DATE_TIME
		-----
        UNION ALL
		-----
    SELECT
        'RT' 																AS SRC,
        CAST( RT.TASK_START AS TIMESTAMP )   								AS SRC_TIMESTAMP,
        RT.STAFF_ID															AS STAFF_ID,
        DATA_MODE															AS SCR_TYPE,
        RT.EVENT_ID,
         RT.TASK_START                                                      AS SCHEDULE_START_GMT,
         RT.TASK_END                                                        AS SCHEDULE_END_GMT,
        round((RT.TASK_END  - RT.TASK_START)*(24*60*60),0) as duration
        FROM RT_TASK_DATA RT
        CROSS JOIN DATE_TIMES
        WHERE 1=1
        AND TASK_EDIT_ID = '0'
        AND EVENT_ID IN ('1','2')
		AND DATA_MODE IN (4,0)
		-- DATA MODE 0 ARE SCHEDULES DATA MODE 4 ARE LOG_ENTRIES
        AND TASK_START BETWEEN DATE_TIMES.GMT_START_DATE_TIME 
		AND DATE_TIMES.GMT_END_DATE_TIME
       ----
    )
SELECT CAST(SYSDATE AS TIMESTAMP) AS EXTRACT_TIMESTAMP,
        SRC,
        SRC_TIMESTAMP_GMT,
        STAFF_ID,
        SRC_TYPE,
        EVENT_ID,
        TASK_START_GMT,
        TASK_END_GMT,
        duration as DURATION_SEC
FROM ALL_DATA
</sql>
