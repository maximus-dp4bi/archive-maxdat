    <sql>--- SELECT "Batch"."BatchID" BATCH_GUID,
--  'Undetermined' BATCH_TYPE,
--- "Batch"."DocumentCount" BATCH_DOC_COUNT,
--- "Batch"."PageCount" BATCH_PAGE_COUNT,
--- (select count(1) from "${DB_KOFAX_ARS_DB}".."FolderField" "FolderFieldENV" where  "FolderFieldENV"."BatchID"="Batch"."BatchID" 
---   and "FolderFieldENV"."FieldName"='EnvelopeID') BATCH_ENVELOPE_COUNT,
--- "Batch"."CreationDTM" CREATE_DT,
--- "BatchClassification"."ClassificationDTM" CLASSIFICATION_DT,
--- "BatchRecognition"."RecognitionDTM" RECOGNITION_DT,
--- "BatchValidation"."ValidationDTM" VALIDATION_DT,
--- getdate() sysdate
---from "${DB_KOFAX_ARS_DB}".."Batch"
---left outer join "${DB_KOFAX_ARS_DB}".."BatchClassification"
---- on "Batch"."BatchID"="BatchClassification"."BatchID"
---left outer join "${DB_KOFAX_ARS_DB}".."BatchRecognition"
--- on "Batch"."BatchID"="BatchRecognition"."BatchID"
---left outer join "${DB_KOFAX_ARS_DB}".."BatchValidation"
--- on "Batch"."BatchID"="BatchValidation"."BatchID"
---where "Batch"."CreationDTM">=convert(datetime,'${ARS_START_DATE}')
--- AND "Batch"."ClassName" in ('${BATCH_CLASS_LIST}')

--- ARSDB replacement 

SELECT distinct sb.BatchGUID BATCH_GUID,
       mr.batchid   Batch_id,
       'Undetermined' BATCH_TYPE,
       mr.BatchDocCount BATCH_DOC_COUNT, 
	   mr.docpagecount BATCH_PAGE_COUNT,
	   cast(mr.batchcreatedate as datetime) + cast(mr.Batchcreatetime as datetime) as CREATE_DT, 
       cast(mr.batchexportdate as datetime) + cast(mr.batchexporttime as datetime) as batch_export_dt,
	    (select max(sbm.EndDateTime) from ${DB_KOFAX_CENTRAL_DB}..StatsBatchModule sbm join ${DB_KOFAX_CENTRAL_DB}..StatsModuleLaunch sml on 
		sbm.ModuleLaunchID = sml.ModuleLaunchID and sml.ModuleName = 'KTM Server' and sbm.ExternalBatchID = mr.BatchID) Classification_DT,
        (select max(sbm.EndDateTime) from ${DB_KOFAX_CENTRAL_DB}..StatsBatchModule sbm join ${DB_KOFAX_CENTRAL_DB}..StatsModuleLaunch sml on 
		sbm.ModuleLaunchID = sml.ModuleLaunchID and sml.ModuleName = 'KTM Server' and sbm.ExternalBatchID = mr.BatchID) RECOGNITION_DT,
		(select max(sbm.EndDateTime) from ${DB_KOFAX_CENTRAL_DB}..StatsBatchModule sbm join ${DB_KOFAX_CENTRAL_DB}..StatsModuleLaunch sml on 
		sbm.ModuleLaunchID = sml.ModuleLaunchID and sml.ModuleName = 'KTM Validation' and sbm.ExternalBatchID = mr.BatchID) Validation_DT,
		getdate() sysdate,
        FAX_BATCH_SOURCE
from ${DB_KOFAX_CENTRAL_DB}..MaxDatReporting mr
join ${DB_KOFAX_CENTRAL_DB}..StatsBatch sb on mr.batchid = sb.ExternalBatchID
where cast(batchcreatedate as datetime)>=convert(datetime,'${CENTRAL_START_DATE}')
AND mr.batchclass in ('${BATCH_CLASS_LIST}')
</sql>
