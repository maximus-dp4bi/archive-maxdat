WHERE 1=1 
AND T.TASK_EDIT_ID=0 -- 'finalized'
AND T.task_start >= to_date('${LastSuccessfulETLRunDate}','yyyy/mm/dd')
AND T.task_start &lt; trunc(sysdate)
