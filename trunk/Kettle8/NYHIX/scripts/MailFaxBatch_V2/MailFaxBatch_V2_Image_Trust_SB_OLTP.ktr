    <connection>DB_IMAGE_TRUST</connection>
    <sql>SELECT
	SB.ID						AS STATS_BATCH_ID,
	SB.BATCHID					AS Batch_GUID,
	SB.BATCHUUID				AS BATCH_U_UID,
	SB.NODEID					AS NODE_ID,
	SB.NODEUUID					AS NODE_U_UID,
	SB.FLDCOUNT					AS FLD_COUNT,
	SB.DOCCOUNT					AS DOC_COUNT,
	SB.PAGECOUNT				AS PAGE_COUNT,
	SB.JOBID					AS JOB_ID,
	SB.PRIVAT					AS PRIVAT,
	SB.STARTDATE				AS START_DATE,
	SB.ENDDATE					AS END_DATE,
	SB.INDEXEDBATCHCOUNT		AS INDEXED_BATCH_COUNT,
	SB.INDEXEDDOCCOUNT			AS INDEXED_DOC_COUNT,
	SB.INDEXEDFLDCOUNT			AS INDEXED_FLD_COUNT,
	SB.USERID					AS USER_ID,
	SB.DELETED					AS DELETED,
	SB.SESSIONID				AS SESSION_ID,
	SB.QUEUE					AS QUEUE,
	SB.QUEUEMODE				AS QUEUE_MODE,
	SB.VERSION					AS VERSION,
	SB.ORIGBATCHID				AS ORIG_BATCH_ID,
	SB.LASTSCANNERNAME			AS LAST_SCANNER_NAME,
	SB.REASON					AS REASON,
	SB.COPIEDFROMBATCHID		AS COPIED_FROM_BATCH_ID,
	SB.BATCHNAME				AS BATCH_NAME,
	SB.BATCHPRIORITY			AS BATCH_PRIORITY,
	SB.STATSBATCHSESSIONID		AS STATS_BATCH_SESSION_ID,
	SB.BATCHRELEASERUNID		AS BATCH_RELEASE_RUN_ID,
	J.NAME  					AS BATCH_CLASS_NAME,
	null				as BATCH_DESCRIPTION,
	U.UserName					as User_Name
FROM ITSCH.STATSBATCH SB
LEFT OUTER JOIN ITSCH.BATCHES B
ON B.ID = SB.BATCHID
JOIN ITSCH.JOBS J
ON J.ID = SB.JOBID
JOIN ITSCH.USERS U
ON U.ID = SB.USERID
--WHERE SB.ID >= '${MFB_V2_IMAGE_TRUST_LAST_STATS_BATCH_ID}'
--or sb."StartDate">= convert(date,'${MFB_V2_IMAGE_TRUST_LAST_UPDATE_DATE}')
--or sb."EndDate">= convert(date,'${MFB_V2_IMAGE_TRUST_LAST_UPDATE_DATE}')
WHERE SB.ID >= '${MFB_V2_IMAGE_TRUST_LAST_STATS_BATCH_ID}'
or sb."StartDate">= convert(date,'${MFB_V2_IMAGE_TRUST_LAST_UPDATE_DATE}')
or sb."EndDate">= convert(date,'${MFB_V2_IMAGE_TRUST_LAST_UPDATE_DATE}')
</sql>
