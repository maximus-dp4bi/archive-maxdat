    <connection>KOFAX_CENTRAL</connection>
    <sql>SELECT distinct "StatsBatchModule"."BatchModuleID",
 "StatsBatchModule"."BatchGUID",
 "StatsModuleLaunch"."ModuleLaunchID",
 "StatsModuleLaunch"."ModuleUniqueID",
 (case when "StatsModuleLaunch"."ModuleName" like 'Advanced%' then 'Advanced Reports - Data Export' else "StatsModuleLaunch"."ModuleName" end) "ModuleName",
 "StatsBatchModule"."ModuleCloseUniqueID",
 (case when "StatsBatchModule"."ModuleName" like 'Advanced%' then 'Advanced Reports - Data Export' else "StatsBatchModule"."ModuleName" end) "ModuleCloseName",
 "StatsBatchModule"."BatchStatus",
 "StatsBatchModule"."StartDateTime",
 "StatsBatchModule"."EndDateTime",
 "StatsModuleLaunch"."UserName",
 "StatsModuleLaunch"."StationID",
 "StatsModuleLaunch"."SiteName",
 "StatsModuleLaunch"."SiteID",
 "StatsBatchModule"."Deleted",
 "StatsBatchModule"."PagesPerDocument",
 "StatsBatchModule"."PagesScanned",
 "StatsBatchModule"."PagesDeleted",
 "StatsBatchModule"."DocumentsCreated",
 "StatsBatchModule"."DocumentsDeleted",
 "StatsBatchModule"."PagesReplaced",
 getdate() sysdate,
 'CENTRAL' source_server
FROM
 "${DB_KOFAX_CENTRAL_DB}".."StatsModuleLaunch",
 "${DB_KOFAX_CENTRAL_DB}".."StatsBatchModule",
 "${DB_KOFAX_CENTRAL_DB}".."StatsBatch"
