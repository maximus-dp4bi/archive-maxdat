    <sql>
select x.D_DATE as "Record_date"
      ,x.d_date_number
      ,x.Carrier as "Carrier"
      ,y.CallTypeID
	  , '${carrProjectName}' carrierProjectName
      , '${carrProgramName}' carrierProgramName
      ,count (*) as Calls_Transferred
      ,sum(y.TalkTime) as Talk_Secs
      ,sum(y.HoldTime) as Hold_Secs
      ,sum(y.Duration) as Duration_Secs
      ,sum(y.RingTime) as Ringtime_Secs
      ,sum(y.DelayTime) as Delay_Secs
      ,sum(TimeToAband) as Abandon_Secs
      ,sum(WorkTime) as Work_Secs
      ,sum(NetQTime) as NetQ_Secs     
from 
(
select  distinct CAST(SWITCHOFFSET(TODATETIMEOFFSET(tcd.DateTime, -360), '-05:00') as date)  as D_DATE
        , CAST(Convert(VARCHAR(8),tcd.DateTime ,112) as INT) d_date_number
--       ,left(convert(varchar, convert(date, SWITCHOFFSET(TODATETIMEOFFSET(DateTime, -360), '-05:00')),112),6) as month_1
--       ,datename(month, convert(date, SWITCHOFFSET(TODATETIMEOFFSET(DateTime, -360), '-05:00'))) as month_2
       ,left(tcd.DigitsDialed,5) as xx
       ,right(tcd.DigitsDialed,10) as xfer
       ,  right(tcd.DigitsDialed,10) as Carrier
       ,RouterCallKeyDay, RouterCallKey
from maxco_awdb.dbo.Termination_Call_Detail tcd WITH(NOLOCK)
where 1=1
and   right(tcd.DigitsDialed,10) in (${providerList})
and   tcd.Variable1 = '${carrProjectName}'
and   tcd.DateTime > '01-01-2020'
and CAST(Convert(VARCHAR(8),tcd.DateTime ,112) as INT) >= ${startDate} --20191101
and CAST(Convert(VARCHAR(8),tcd.DateTime ,112) as INT) &lt;= ${endDate}   --20191201
--and   tcd.DateTime >= '11-01-2019' 
--and   tcd.DateTime &lt;  '12-01-2019'
and   tcd.AgentSkillTargetID is not null
and   tcd.CallReferenceID is not null
) x,
(
select 
distinct CAST(SWITCHOFFSET(TODATETIMEOFFSET(tcd.DateTime, -360), '-05:00') as date)  as D_DATE
--       ,left(convert(varchar, convert(date, SWITCHOFFSET(TODATETIMEOFFSET(DateTime, -360), '-05:00')),112),6) as month_1
 --      ,datename(month, convert(date, SWITCHOFFSET(TODATETIMEOFFSET(DateTime, -360), '-05:00'))) as month_2
       ,RouterCallKeyDay, RouterCallKey
       ,CallTypeID, PrecisionQueueID,Duration,RingTime,DelayTime,TimeToAband,HoldTime,TalkTime,WorkTime, NetQTime
from maxco_awdb.dbo.Termination_Call_Detail tcd WITH(NOLOCK)
where 1=1
and   tcd.Variable1 = '${carrProjectName}'
and   tcd.DateTime > '01-01-2020'
and CAST(Convert(VARCHAR(8),tcd.DateTime ,112) as INT) >= ${startDate} --20191101
and CAST(Convert(VARCHAR(8),tcd.DateTime ,112) as INT) &lt;= ${endDate}   --20191201
and   tcd.AgentSkillTargetID is not null
and   tcd.CallReferenceID is not null
and   tcd.PrecisionQueueID is not null
) y
where x.RouterCallKeyDay = y.RouterCallKeyDay and x.RouterCallKey = y.RouterCallKey
group by x.D_DATE,x.d_date_number,x.Carrier, y.CallTypeID
;
</sql>
