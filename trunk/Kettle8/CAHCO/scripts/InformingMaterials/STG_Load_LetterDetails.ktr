    <connection>DW_OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>select TOP 1 s.spdstatus,e.maximusstatus
from dbo.DimMailDetails m 
 LEFT JOIN dbo.DimSPDBeneficiary s WITH (NOLOCK)
ON ( m.BeneficiaryID = s.BeneficiaryID AND
(( (DATEADD(Day, DATEDIFF(Day, 0, s.RecordStartDate), 0)) &lt;= (DATEADD(Day, DATEDIFF(Day, 0, m.RecordCreateDate), 0))  AND (DATEADD(Day, DATEDIFF(Day, 0, s.RecordEndDate), 0)) >= (DATEADD(Day, DATEDIFF(Day, 0, m.RecordCreateDate), 0)) ) 
OR ((DATEADD(Day, DATEDIFF(Day, 0, s.RecordStartDate), 0)) &lt;= (DATEADD(Day, DATEDIFF(Day, 0, m.RecordCreateDate), 0)) AND s.RecordEndDate IS NULL)))
LEFT OUTER JOIN
dbo.DimBeneficiary e WITH (NOLOCK) ON (m.BeneficiaryID = e.BeneficiaryID AND
((((DATEADD(Day, DATEDIFF(Day, 0, e.RecordStartDate), 0)) &lt;= (DATEADD(Day, DATEDIFF(Day, 0, m.RecordCreateDate), 0)) AND (DATEADD(Day, DATEDIFF(Day, 0, e.RecordEndDate), 0))>= (DATEADD(Day, DATEDIFF(Day, 0, m.RecordCreateDate), 0))) 
OR ((DATEADD(Day, DATEDIFF(Day, 0, e.RecordStartDate), 0)) &lt;= (DATEADD(Day, DATEDIFF(Day, 0, m.RecordCreateDate), 0)) AND e.RecordEndDate IS NULL))))
where m.DimMailDetailsId = ?
order by DimMailDetailsId, DimSPDBeneficiaryID desc</sql>
