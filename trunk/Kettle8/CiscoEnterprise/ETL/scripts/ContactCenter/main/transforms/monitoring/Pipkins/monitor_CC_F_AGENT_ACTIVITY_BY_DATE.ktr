  <step>
    <name>Get DST Start&#47;End</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT
	  next_day(ADD_MONTHS(TRUNC(?,&apos;YEAR&apos;), START_MONTH-1)-1, &apos;SUNDAY&apos;) --Months
    +((START_WEEK-1)*7) --Weeks
    +START_WEEK_DAY --Days
    +(TO_NUMBER(TO_CHAR(START_TIME, &apos;hh24&apos;))&#47;24) --Hours 
    +(TO_NUMBER(TO_CHAR(START_TIME, &apos;mi&apos;))&#47;1440) --Minutes
    +(TO_NUMBER(TO_CHAR(START_TIME, &apos;ss&apos;))&#47;86400) DST_START --Seconds
, next_day(ADD_MONTHS(TRUNC(?,&apos;YEAR&apos;), 11-1)-1, &apos;SUNDAY&apos;) --+ interval &apos;2&apos; hour DST_END
    +((END_WEEK-1)*7) --Weeks
    +END_WEEK_DAY --Days
    +(TO_NUMBER(TO_CHAR(END_TIME, &apos;hh24&apos;))&#47;24) --Hours 
    +(TO_NUMBER(TO_CHAR(END_TIME, &apos;mi&apos;))&#47;1440) --Minutes
    +(TO_NUMBER(TO_CHAR(END_TIME, &apos;ss&apos;))&#47;86400) DST_END --Seconds 
FROM CC_S_TMP_PIPKINS_TIMEZONE tz
WHERE tz.TIMEZONE_ID = ?</sql>
