    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>select cclk.value ProjectName, ccm.value MetricName, cclk.list_type
from cc_a_list_lkup cclk, cc_a_list_lkup ccm
where cclk.name = 'AMPEXP_PROJECT_SOURCE_LIST'
--and cclk.list_type like '%'|| ccm.list_type||'%'
and ccm.name = 'AMPEXP_METRIC_SOURCE_LIST'
and cclk.value = ?
and upper(regexp_replace(ccm.value,'[^[:alnum:]]', NULL)) = upper(regexp_replace(? ,'[^[:alnum:]]', NULL)) 
and not exists (
select REGEXP_SUBSTR (list_type, '[^,]+', 1, level) output
            FROM (select cl.list_type from cc_a_list_lkup cl where cl.name = 'AMPEXP_METRIC_SOURCE_LIST'
            and cl.value = ?) a
  connect by level &lt;= regexp_count(list_type,',')+1        
minus
select REGEXP_SUBSTR (list_type, '[^,]+', 1, level) output
            FROM (select cl.list_type from cc_a_list_lkup cl where cl.name = 'AMPEXP_PROJECT_SOURCE_LIST'
            and cl.value = ? ) a
  connect by level &lt;= regexp_count(list_type,',')+1        
)  
----inlist compare above</sql>
