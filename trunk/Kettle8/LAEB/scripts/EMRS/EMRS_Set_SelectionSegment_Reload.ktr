    <sql>DECLARE
  v_reload_data VARCHAR2(1);
  v_reload_day VARCHAR2(1);
BEGIN
  SELECT value
  INTO v_reload_data
  FROM corp_etl_control
  WHERE name = 'EMRS_SELECTION_SEGMENT_RELOAD';

  SELECT CASE WHEN TO_CHAR(sysdate,'d') = value THEN 'Y' ELSE 'N' END
  INTO v_reload_day
  FROM corp_etl_control
  WHERE name = 'EMRS_SELECTIONSEGMENT_RELOAD_DAY';


  IF v_reload_data = 'Y' OR v_reload_day = 'Y' THEN
    UPDATE corp_etl_control
    SET value = '2010/01/01 00:00:00'
    WHERE name IN('MAX_SELECTION_SEGMENT_CREATE_DATE','MAX_SELECTION_SEGMENT_UPDATE_DATE');

    COMMIT;
 
   EXECUTE IMMEDIATE 'CREATE TABLE emrs_d_selection_segment_'||TO_CHAR(sysdate,'yyyymmdd')||' AS SELECT * FROM emrs_d_selection_segment';
   EXECUTE IMMEDIATE 'TRUNCATE TABLE EMRS_D_SELECTION_SEGMENT';
  END IF;
EXCEPTION
  WHEN NO_DATA_FOUND THEN
    v_reload_data := 'N';
END;</sql>
