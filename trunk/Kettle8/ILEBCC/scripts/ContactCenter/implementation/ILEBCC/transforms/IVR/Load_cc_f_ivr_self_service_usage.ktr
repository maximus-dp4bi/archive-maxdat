    <connection>MAXDAT</connection>
    <sql>SELECT 
       prj.project_id AS d_project_id,
       prg.program_id AS d_program_id,
       geo.geography_master_id AS d_geography_master_id,
       intv.d_interval_id                                       AS d_interval_id,
       dt.d_date_id                                             AS d_date_id,
       dn.uow_id                                                AS d_uow_id,
       issp.d_ivr_self_service_path_id,
       --trunc(resp.call_start_time)                              AS interval_date,
       SUM(decode(res.count_created,'Y',1,0))                   AS contacts_entering,
       SUM(decode(res.count_offered_to_acd,'Y',1,0))            AS contacts_transferred,
       SUM(decode(res.count_contained,'Y',1,0))                 AS contacts_completed,
       coalesce(SUM(CASE WHEN res.count_offered_to_acd='Y' THEN call_end_time-call_start_time END)*24*60,0)  AS MINUTES_IN_IVR_TRANSFERRED,
       coalesce(SUM(CASE WHEN res.count_contained='Y' THEN call_end_time-call_start_time END)*24*60,0)  AS MINUTES_IN_IVR_COMPLETED
FROM CC_S_IVR_RESPONSE resp      
JOIN CC_D_INTERVAL intv
     ON resp.call_start_time >= intv.interval_start_date 
    and resp.call_start_time &lt; intv.interval_end_date
JOIN CC_C_IVR_CALL_RESULT res
    ON resp.completion_code = res.completion_code
JOIN cc_c_ivr_dnis dn
   ON resp.destination_dnis = dn.destination_dnis
JOIN cc_d_dates dt
   ON dt.d_date = trunc(resp.call_start_time)
JOIN cc_d_ivr_self_service_path issp
   ON issp.begin_node = Coalesce(MAIN_menu_selection,exit_point_description)
   AND issp.end_node = exit_point_description
JOIN CC_D_PROJECT  prj
   ON prj.project_name = resp.project_name
JOIN CC_D_PROGRAM prg
   ON  prg.program_name = resp.program_name
JOIN CC_D_GEOGRAPHY_MASTER geo
   ON geo.geography_name = resp.geography_name
WHERE 1=1
and resp.call_start_time >= to_date('${startDate}','yyyy/MM/dd')
and resp.call_start_time &lt; to_date('${endDate}','yyyy/MM/dd')
--AND IVR_RESPONSE_ID = 35741
and resp.application_name NOT IN (SELECT g.VALUE FROM cc_a_list_lkup g WHERE g.list_type = 'IVR_NO_FACT_BUILD_TYPE')
GROUP BY prj.project_id, prg.program_id, geo.geography_master_id, intv.d_interval_id, dt.d_date_id, dn.uow_id, issp.d_ivr_self_service_path_id</sql>
