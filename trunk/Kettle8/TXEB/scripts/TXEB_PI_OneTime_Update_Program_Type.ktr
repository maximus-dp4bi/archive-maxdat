    <sql>Select 
replace(replace(substrb(DBMS_LOB.SUBSTR(xmltype.createxml(i.actions_taken).extract(&apos;&#47;actions_taken&#47;action_taken&#47;action&apos;).getClobVal(),2000,1),1,2000)||substrb(DBMS_LOB.SUBSTR(xmltype.createxml(i.actions_taken).extract(&apos;&#47;actions_taken&#47;action_taken&#47;action&apos;).getClobVal(),2000,2001),1,2000), &apos;&lt;action&gt;&apos;, CHR(13)||CHR(10)), &apos;&lt;&#47;action&gt;&apos;, &apos;&apos;) oltp_ACTION_COMMENTS
,substrb(dbms_lob.substr(i.resolution,2000, 1 ),1,2000)|| substrb(dbms_lob.substr(i.resolution,2000, 2001),1,2000) oltp_RESOLUTION_DESCRIPTION
,substrb(dbms_lob.substr(description,2000, 1 ),1,2000)|| substrb(dbms_lob.substr(description,2000, 2001),1,2000) oltp_incident_description
,i.reporter_first_name || &apos; &apos; ||i.reporter_last_name  REPORTER_NAME
from INCIDENT_HEADER I
where  1=1
and i.incident_header_id = ?</sql>
