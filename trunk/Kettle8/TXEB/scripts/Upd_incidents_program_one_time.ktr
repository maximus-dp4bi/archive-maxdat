  <hop> <from>Table input - Incidents OLTP Source</from><to>DB Join - Get program cd</to><enabled>Y</enabled> </hop>  <hop> <from>DB Join - Get program cd</from><to>Update</to><enabled>Y</enabled> </hop>  </order>
  <step>
    <name>DB Join - Get program cd</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT COALESCE((SELECT DECODE(case_generic_field6_txt,&apos;TA84&apos;,&apos;CHIP&apos;,&apos;TA85&apos;,&apos;CHIP&apos;,&apos;MED&apos;,&apos;MEDICAID&apos;,null) program_type
        FROM cases
        WHERE case_id = ?),
  CASE WHEN ? IS NOT NULL THEN
    (SELECT program_cd
     FROM(
      SELECT program_cd, 
         ROW_NUMBER() OVER (ORDER BY cscl_status_cd DESC,cscl_id) rn FROM case_client cc
     WHERE cscl_case_id = ?
     AND cscl_clnt_client_id = ?
     AND (cscl_status_cd = &apos;O&apos;
      OR (cscl_id = (SELECT MAX(cscl_id)
                           FROM case_client cc1
                           WHERE cc1.cscl_case_id = cc.cscl_case_id
                           AND cc1.cscl_clnt_client_id = cc.cscl_clnt_client_id) )))
     WHERE rn = 1 )
  ELSE
    (SELECT program_cd
     FROM
     (SELECT program_cd,ROW_NUMBER() OVER (ORDER BY cscl_status_cd DESC,cscl_id) rn
     FROM case_client cc
     WHERE cscl_case_id = ?     
     AND (cscl_status_cd = &apos;O&apos;
      OR (cscl_id = (SELECT MAX(cscl_id)
                     FROM case_client cc1
                     WHERE cc1.cscl_case_id = cc.cscl_case_id ) )) )
     WHERE rn = 1 )
END )program_type
FROM dual</sql>
