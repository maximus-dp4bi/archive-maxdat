    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>WITH act AS(
 SELECT h1.incident_header_id  
  ,t.rn action_taken_record_num
  , TO_DATE(REPLACE(EXTRACTVALUE(xmltype(h1.actions_taken),&apos;&#47;actions_taken&#47;action_taken[position()=&apos;||t.rn||&apos;]&#47;date&apos;), &apos;,&apos;), &apos;mm-dd-yyyy hh:mi:ss am&apos;) action_taken_dt
  , COALESCE(EXTRACTVALUE(xmltype(h1.actions_taken),&apos;&#47;actions_taken&#47;action_taken[position()=&apos;||t.rn||&apos;]&#47;action_type_cd&apos;),&apos;NA&apos;) action_type_cd
  , COALESCE(EXTRACTVALUE(xmltype(h1.actions_taken),&apos;&#47;actions_taken&#47;action_taken[position()=&apos;||t.rn||&apos;]&#47;created_by&apos;),&apos;NA&apos;) action_createdby
  , EXTRACTVALUE(xmltype(h1.actions_taken),&apos;&#47;actions_taken&#47;action_taken[position()=&apos;||t.rn||&apos;]&#47;action&apos;) action_taken_comment  
FROM incident_header h1
 CROSS JOIN
        ( SELECT ROWNUM rn
          FROM DUAL CONNECT BY ROWNUM &lt;=  ${INCIDENT_ACTION_TAKEN_LIMIT}) t
WHERE incident_header_id = ?
AND h1.actions_taken IS NOT NULL) 
SELECT act.*,
       COALESCE(t.description,&apos;No Action Entered&apos;) action_type_desc, 
       COALESCE(stf.staff_name,action_createdby) staff_name,
       stf.role_name,
       stf.team_name,
       stf.unit_name
FROM act
LEFT JOIN enum_action_taken t ON act.action_type_cd = t.value
LEFT JOIN (SELECT *
           FROM (SELECT su.staff_id, stf.last_name||&apos;, &apos;||stf.first_name as staff_name, sr.role_name, &apos;SEU Team&apos; team_name, &apos;SEU Unit&apos; unit_name
                        ,ROW_NUMBER() OVER (PARTITION BY stf.staff_id ORDER BY stf.staff_id, DECODE(role_name,&apos;SEU Supervisor&apos;,1,&apos;SEU Specialist 2&apos;,2,&apos;SEU Specialist&apos;,3,4)) rn
                FROM staff stf 
                  JOIN sec_user su
                    ON su.staff_id = stf.staff_id
                  JOIN sec_user_role sur
                    ON sur.sec_user_id = su.sec_user_id
                  JOIN sec_role sr
                    ON sur.sec_role_id = sr.sec_role_id
                WHERE sr.role_name IN (&apos;SEU Specialist&apos;,&apos;SEU Specialist 2&apos;,&apos;SEU Supervisor&apos;)) 
            WHERE rn = 1    ) stf
 ON stf.staff_name = act.action_createdby
WHERE action_taken_dt IS NOT NULL</sql>
