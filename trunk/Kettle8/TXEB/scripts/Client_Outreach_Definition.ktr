    <connection>OLTP_SOURCE</connection>
    <sql>select c.oc_id,        
       c.party_type outreach_category,
       p.report_label outreach_category_desc,
       c.party_sub_type outreach_type, 
       s.description outreach_type_desc,
       c.init_event outreach_event_type, 
       c.process outreach_process,
       ld1.lmdef_id lmdef_id1,
       ld1.name letter_name1,       
       ld2.lmdef_id lmdef_id2,
       ld2.name letter_name2,
       ld3.lmdef_id lmdef_id3,
       ld3.name letter_name3       
from 
enum_affected_party_subtype s
 inner join outreach_config c
   on s.value = c.party_sub_type
   and c.effective_end_ts is null
 inner join enum_affected_party_type p
   on p.value = c.party_type
  left outer join letter_definition ld1
    on c.lmdef_id1 = ld1.lmdef_id
left outer join letter_definition ld2
    on c.lmdef_id2 = ld2.lmdef_id    
left outer join letter_definition ld3
    on c.lmdef_id3 = ld3.lmdef_id    
where  c.effective_end_ts is null
and s.scope = &apos;OUTREACHREQUEST&apos;
and p.scope = &apos;OUTREACHREQUEST&apos;
and s.effective_end_date is null</sql>
