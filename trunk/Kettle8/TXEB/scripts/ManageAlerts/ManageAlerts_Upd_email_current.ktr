    <connection>MAXDAT</connection>
    <sql>select * from (
select am_id, alert_name, expected_rpt_run_dt, expected_rpt_run_time
, trunc(als.email_time) actual_run_dt
, to_char(als.email_time,&apos;HH24:MI&apos;) actual_run_time
, als.email_batch_id
, case when als.email_Result = &apos;FAIL&apos; then &apos;Failed&apos;
	   when (als.email_result = &apos;COMPLETE-NOEXCP&apos; or als.email_result = &apos;COMPLETE-NOEXCP-NODL&apos;) then &apos;Complete&apos;
	   when als.email_result = &apos;COMPLETE-EXCP&apos; then &apos;Complete&apos;
	   else alert_status
end alert_status
, case when (als.email_result = &apos;COMPLETE-NOEXCP&apos; or als.email_result = &apos;COMPLETE-NOEXCP-NODL&apos;) then &apos;No Exceptions&apos;
	   when als.email_result = &apos;COMPLETE-EXCP&apos; then &apos;Exception&apos;
	   else null
end alert_outcome
, row_number() over(partition by am_id order by als.email_time desc) rown
from d_am_current amc, alertemail_stg als
where alert_status = &apos;Ready&apos;
and expected_rpt_run_dt &gt;= trunc(sysdate-14)
and to_timestamp(to_char(amc.expected_rpt_run_dt,&apos;YYYY&#47;MM&#47;DD&apos;)|| expected_rpt_run_time, &apos;YYYY&#47;MM&#47;DD HH24:MI&apos;) between trunc(als.email_time,&apos;MI&apos;) - 30&#47;1440 and trunc(als.email_time,&apos;MI&apos;) + 30&#47;1440
and amc.alert_name = als.report_name
and als.email_batch_id = ${BATCH_NUM}
and als.email_result is not null
)
where rown = 1</sql>
