  <step>
    <name>Table input -COST SHARE from EB</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>EMRS</connection>
    <sql>SELECT  s.case_number
       ,s.cost_share_start_date
       ,s.cost_share_end_date
       ,s.co_pay_amount
       ,s.enrollment_fee_assessed
       ,s.enrollment_fee_paid
       ,s.enrollment_fee_paid_date
       ,s.met_cost_share_cap_date
       ,s.enrollment_fee_frequency
       ,s.chip_annual_enroll_date
       ,s.people_in_family
       ,COALESCE(edf.fpl_id,0) fpl_id
       ,s.enrollment_fee_assessed_date  
       ,f.enrollment_id         
       ,s.fpl_percentage     
       ,s.fee_status
       ,s.fee_status_date
       ,s.last_payment_date
       ,s.plan_notified_date
       ,s.cap_met_amount
       ,s.total_expense_amount                      
FROM   emrs_s_cost_share_stg s
  INNER JOIN emrs_f_enrollment f
   ON s.case_number = f.case_number
  LEFT OUTER JOIN emrs_d_federal_poverty_level edf
    ON edf.fpl_number_in_family = s.people_in_family
    AND TO_CHAR(sysdate,&apos;YYYY&apos;) = TO_CHAR(fpl_year,&apos;YYYY&apos;)
WHERE COALESCE(f.managed_care_end_date,s.chip_next_enroll_date) BETWEEN  s.cost_share_start_date AND s.cost_share_end_date
AND NOT EXISTS(SELECT 1 FROM emrs_s_enrollment_stg e WHERE s.case_number = e.source_case_id)
AND modified_date = (SELECT MAX(modified_date)
                       FROM emrs_s_cost_share_stg s1
                       WHERE s.case_number = s1.case_number)

</sql>
