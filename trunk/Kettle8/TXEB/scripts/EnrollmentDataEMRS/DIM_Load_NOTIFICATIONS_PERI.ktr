  <step>
    <name>Table input - Perinatal 835s from EB</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>SELECT e.create_ts send_date,
         e.den_plan,
         e.hmo_plan,
         e.client_id,    
         to_number(period||&apos;01&apos;) period,
  CASE WHEN e.den_plan IS NOT NULL THEN
    case den_tran_type when &apos;A&apos; then &apos;021&apos;
                 when &apos;C&apos; then &apos;001&apos;
                 when &apos;D&apos; then &apos;024&apos;
                 when &apos;G&apos; then &apos;021&apos;
                 when &apos;R&apos; then &apos;021&apos;
                 else &apos;030&apos;
     end 
   ELSE NULL END den_maint_type_cd
   ,CASE WHEN e.den_plan IS NOT NULL THEN 
      case when den_tran_type = &apos;C&apos;
             and (old_client_lname != client_lname
                   or old_client_mi != client_mi
                   or old_client_fname != client_fname
                   or old_client_gender != client_gender
                   or old_client_ssn != client_ssn
                   or old_client_dob  != client_dob
                   or old_client_race != client_race ) then &apos;25&apos;
          when den_tran_type = &apos;C&apos;
              and (trim(old_residence_addr1)  != trim(residence_addr1)
                    or trim(old_residence_addr2) != trim(residence_addr2)
                    or trim(old_residence_city)  != trim(residence_city)
                    or trim(old_residence_state) != trim(residence_state)
                    or substr(old_residence_zip, 1, 5) != substr(residence_zip, 1, 5)
                                             ) then &apos;43&apos;
          else case den_tran_type when &apos;A&apos; then &apos;28&apos;
                              when &apos;D&apos; then &apos;07&apos;
                              when &apos;C&apos; then null
                              when &apos;G&apos; then &apos;22&apos;
                              when &apos;R&apos; then &apos;41&apos;
                              else &apos;XN&apos; end
    end 
  ELSE NULL END den_maint_reason_code,
case hmo_tran_type when &apos;A&apos; then &apos;021&apos;
                 when &apos;C&apos; then &apos;001&apos;
                 when &apos;D&apos; then &apos;024&apos;
                 when &apos;G&apos; then &apos;021&apos;
                 when &apos;R&apos; then &apos;021&apos;
                 else &apos;030&apos;
   end hmo_maint_type_cd
   ,case when hmo_tran_type = &apos;C&apos;
             and (old_client_lname != client_lname
                   or old_client_mi != client_mi
                   or old_client_fname != client_fname
                   or old_client_gender != client_gender
                   or old_client_ssn != client_ssn
                   or old_client_dob  != client_dob
                   or old_client_race != client_race ) then &apos;25&apos;
          when hmo_tran_type = &apos;C&apos;
              and (trim(old_residence_addr1)  != trim(residence_addr1)
                    or trim(old_residence_addr2) != trim(residence_addr2)
                    or trim(old_residence_city)  != trim(residence_city)
                    or trim(old_residence_state) != trim(residence_state)
                    or substr(old_residence_zip, 1, 5) != substr(residence_zip, 1, 5)
                                             ) then &apos;43&apos;
          else case hmo_tran_type when &apos;A&apos; then &apos;28&apos;
                              when &apos;D&apos; then &apos;07&apos;
                              when &apos;C&apos; then null
                              when &apos;G&apos; then &apos;22&apos;
                              when &apos;R&apos; then &apos;41&apos;
                              else &apos;XN&apos; end
    end hmo_maint_reason_code    
    ,per_dl_enrl_id source_record_id
    ,&apos;TX_ETL_E_CHIP_PER_DAILY_835&apos; source_table_name
--    ,s.selection_segment_id source_record_id
FROM tx_etl_e_chip_per_daily_835 e
WHERE TRUNC(create_ts) &gt;= TO_DATE(&apos;${ETL_RUN_DATE}&apos;,&apos;MM&#47;DD&#47;YYYY&apos;)</sql>
