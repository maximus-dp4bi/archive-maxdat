  <step>
    <name>Table input- Get data from oltp and wip_bpm</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT  
 &apos;Y&apos; IN_YES
,&apos;N&apos; IN_NO
,&apos;Complete&apos; as FINAL_STATUS
,sysdate    as stage_date
,&apos;Normal&apos; CANCEL_METHOD
,&apos;Canceled prior to send to Print Vendor&apos; CANCEL_REASON
,cancel_dt
,cancel_by_id
,O_CEPN_ID
,O_LETTER_REQUEST_ID
,O_SENT_DT
,O_CANCEL_DT
,O_CANCEL_BY_ID
,O_CANCEL_METHOD
,O_CANCEL_REASON
,O_COMPLETE_DT
,O_STAGE_DONE_DATE
,O_INSTANCE_STATUS
,O_ASED_PROCESS_LETTER_REQ
,O_ASSD_TRANSMIT
,O_GWF_VALID
,O_UPDATED
FROM CORP_ETL_PROC_LETTERS_OLTP OLTP
JOIN 
(select
 CEPN_ID                 O_CEPN_ID
,LETTER_REQUEST_ID       O_LETTER_REQUEST_ID
,SENT_DT                 O_SENT_DT
,CANCEL_DT               O_CANCEL_DT
,CANCEL_BY_ID            O_CANCEL_BY_ID
,CANCEL_METHOD           O_CANCEL_METHOD
,CANCEL_REASON           O_CANCEL_REASON
,COMPLETE_DT             O_COMPLETE_DT
,STAGE_DONE_DATE         O_STAGE_DONE_DATE
,INSTANCE_STATUS         O_INSTANCE_STATUS
,ASED_PROCESS_LETTER_REQ O_ASED_PROCESS_LETTER_REQ
,ASSD_TRANSMIT           O_ASSD_TRANSMIT
,GWF_VALID               O_GWF_VALID
,&apos;N&apos; O_UPDATED
FROM CORP_ETL_PROC_LETTERS_WIP_T
WHERE INSTANCE_STATUS = &apos;Active&apos;
AND CANCEL_DT IS NULL
AND ASED_PROCESS_LETTER_REQ IS NOT NULL 
AND GWF_VALID IS NULL
) BPM
ON OLTP.CEPN_ID= BPM.O_CEPN_ID</sql>
