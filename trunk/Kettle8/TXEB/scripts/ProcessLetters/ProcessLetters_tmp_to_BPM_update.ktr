    <sql>declare 
v_count number := 0;

cursor cur is
(
select tmp.*
from corp_etl_proc_letters_wip_bpm tmp 
where tmp.UPDATED = &apos;Y&apos;
);

begin

for c in cur loop

	update CORP_ETL_PROC_LETTERS A
  set 
  REQUEST_DT		                = c.REQUEST_DT,
  LETTER_TYPE		                = c.LETTER_TYPE,
  PROGRAM		                    = c.PROGRAM,
  CASE_ID		                    = c.CASE_ID,
  COUNTY_CODE		                = c.COUNTY_CODE,
  ZIP_CODE		                    = c.ZIP_CODE,
  LANGUAGE		                    = c.LANGUAGE,
  REPRINT		                    = c.REPRINT,
  REQUEST_DRIVER_TYPE	            = c.REQUEST_DRIVER_TYPE,
  REQUEST_DRIVER_TABLE	            = c.REQUEST_DRIVER_TABLE,
  STATUS			                = c.STATUS,
  STATUS_DT		                    = c.STATUS_DT,
  SENT_DT		                    = c.SENT_DT,
  PRINT_DT		                    = c.PRINT_DT,
  MAILED_DT		                    = c.MAILED_DT,
  RETURN_DT		                    = c.RETURN_DT,
  RETURN_REASON		                = c.RETURN_REASON,
  REJECT_REASON		                = c.REJECT_REASON,
  ERROR_REASON		                = c.ERROR_REASON,
  TRANSMIT_FILE_NAME	            = c.TRANSMIT_FILE_NAME,
  TRANSMIT_FILE_DT                  = c.TRANSMIT_FILE_DT,
  LETTER_RESP_FILE_NAME	            = c.LETTER_RESP_FILE_NAME,
  LETTER_RESP_FILE_DT	            = c.LETTER_RESP_FILE_DT,
  LAST_UPDATE_DT		            = c.LAST_UPDATE_DT,
  LAST_UPDATE_BY_NAME	            = c.LAST_UPDATE_BY_NAME,
  NEWBORN_FLAG		                = c.NEWBORN_FLAG,
  CANCEL_DT		                    = c.CANCEL_DT,
  CANCEL_BY		                    = c.CANCEL_BY,
  CANCEL_REASON		                = c.CANCEL_REASON,
  CANCEL_METHOD		                = c.CANCEL_METHOD,
  COMPLETE_DT		                = c.COMPLETE_DT,
  INSTANCE_STATUS	                = c.INSTANCE_STATUS,
  ASSD_PROCESS_LETTER_REQ           = c.ASSD_PROCESS_LETTER_REQ,
  ASED_PROCESS_LETTER_REQ           = c.ASED_PROCESS_LETTER_REQ,
  ASSD_TRANSMIT		                = c.ASSD_TRANSMIT,
  ASED_TRANSMIT		                = c.ASED_TRANSMIT,
  ASSD_RECEIVE_CONFIRMATION         = c.ASSD_RECEIVE_CONFIRMATION,
  ASED_RECEIVE_CONFIRMATION         = c.ASED_RECEIVE_CONFIRMATION,
  ASF_PROCESS_LETTER_REQ	        = c.ASF_PROCESS_LETTER_REQ,
  ASF_TRANSMIT		                = c.ASF_TRANSMIT,
  ASF_RECEIVE_CONFIRMATION          = c.ASF_RECEIVE_CONFIRMATION,
  ASF_RESOLVE_RESP		            = c.ASF_RESOLVE_RESP,
  GWF_VALID		                    = c.GWF_VALID,
  GWF_OUTCOME		                = c.GWF_OUTCOME,
  GWF_WORK_REQUIRED	                = c.GWF_WORK_REQUIRED,
  STAGE_DONE_DATE		            = c.STAGE_DONE_DATE,
  NEW_LETTER_REQUEST_ID			    = c.NEW_LETTER_REQUEST_ID,
  error_count                       = c.error_count,
  last_errored_date                 = c.last_errored_date,
  last_error_fixed_by               = c.last_error_fixed_by,
  reject_flag                       = c.reject_flag,
  ASSD_RESOLVE_RESP		            = c.ASSD_RESOLVE_RESP,
  ASED_RESOLVE_RESP		            = c.ASED_RESOLVE_RESP,  
  created_by_id                     = c.created_by_id, 
  last_updated_by_id                = c.last_updated_by_id,
  cancel_by_id                      = c.cancel_by_id,
  epm_mail_dt_for_case              = c.epm_mail_dt_for_case,
  letter_definition_id              = c.letter_definition_id
  where c.cepn_id = A.Cepn_Id;
    
	v_count := v_count+1;
	if MOD(v_count,5000) = 0
	then
	  commit;	  
	end if;

end loop;
commit;

end;</sql>
