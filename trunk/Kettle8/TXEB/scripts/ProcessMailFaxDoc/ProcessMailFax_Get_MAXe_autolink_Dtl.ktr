  <step>
    <name>Database join -OTLP autolink outcome</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>8</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>select &apos;Autolink Failed&apos; autolink_failed
		,&apos;Invalid Bar code value&apos; failure_reason
from event 
where event_type_cd = &apos;DOCUMENT_AUTO_LINK_FAILED&apos; 
and ref_type = &apos;DOCUMENT&apos; 
and ref_id = ?
	</sql>
    <outer_join>Y</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>DOCUMENT_ID</name>
        <type>Number</type>
      </field>
    </parameter>
