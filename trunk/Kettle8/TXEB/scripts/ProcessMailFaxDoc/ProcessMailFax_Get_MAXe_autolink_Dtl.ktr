    </step>

  <step>
    <name>Database join -OTLP link method</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>5</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>
SELECT	DOC_LINK_ID link_number
		,case when (case_id is not null and client_id is null) then &apos;Case&apos; 
			 when client_id is not null  and case_id is null  then &apos;Client&apos; 
			 when  case_id is not null and client_id is not null then &apos;Indirect&apos;
			 when case_id is null and client_id is null and link_ref_id is not null  then LINK_TYPE_CD else null end link_via
		,decode(AUTO_LINKED_IND ,0, &apos;Manual&apos; ,1, &apos;Auto&apos;) link_method
		,decode(AUTO_LINKED_IND ,1, &apos;Autolink Successful&apos;)  gwf_autolink_outcome
	,decode(AUTO_LINKED_IND ,1,l.create_ts )	doc_autolink_date
	,&apos;Autolink Successful&apos; Auto_success
	,&apos;Auto&apos; in_auto
	--,d.document_id
	,null autolink_failure_reason
FROM	DOC_LINK l
		,DOCUMENT d
WHERE	l.DOCUMENT_ID = d.DOCUMENT_ID
--AND 	(l.client_id is not null or l.case_id is not null or l.link_ref_id is not null)
AND		l.doc_link_id =(select max(doc_link_id)  from doc_link l3 where l3.document_id =l.document_id)
AND		d.DCN =?		</sql>
    <outer_join>Y</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>DCN</name>
        <type>String</type>
      </field>
    </parameter>
