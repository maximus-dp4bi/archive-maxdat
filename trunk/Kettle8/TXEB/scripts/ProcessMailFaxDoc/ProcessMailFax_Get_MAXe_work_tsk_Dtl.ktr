  <step>
    <name>Database join - OLTP Work Task data</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <copies>4</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT DISTINCT Si.Ref_Id 
     , Si.Ref_Type
     , Si.Step_Definition_Id
     , Si.Step_Instance_Id	work_task_id
     , Si.Create_Ts		ased_create_and_route_work
	 , case when Si.create_Ts IS NOT NULL then &apos;Y&apos; else &apos;N&apos; end  asf_create_and_route_work
FROM   STEP_INSTANCE SI
     , DOCUMENT   Ds
     , Step_Definition Sd
WHERE  Si.Ref_Type = &apos;IMAGING&apos; --&apos;DOCUMENT_SET_ID&apos;
AND    Si.Ref_Id = Ds.Document_Id
AND    Ds.Document_Id = ?
--AND    Si.Create_Ts &gt; Ds.Create_Ts   -- Get the Step instance record after App doc data was created
--AND	   Si.Create_Ts &gt; ?
--AND	   Si.Create_Ts &gt; ?
AND    Si.Step_Definition_Id = Sd.Step_Definition_Id
AND    Sd.Step_Type_Cd IN (&apos;HUMAN_TASK&apos;, &apos;VIRTUAL_HUMAN_TASK&apos;)
--AND    Sd.Name NOT IN (&apos;HSDE-QC&apos;, &apos;Link Document Set&apos;)
AND    Sd.Step_Definition_Id  IN (${DE_REF_VALS})
--AND		Sd.name in (&apos;Data Entry Task&apos;,&apos;Complaint Data Entry Task&apos;,&apos;DE General Reminder Task&apos;,
--&apos;Enrollment Data Entry Task&apos;,&apos;Health Assessment Form Data Entry Task&apos;)
--and si.status &lt;&gt; &apos;TERMINATED&apos;
order by Si.Step_Instance_Id desc</sql>
    <outer_join>Y</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>DOCUMENT_ID</name>
        <type>Number</type>
      </field>
    </parameter>
