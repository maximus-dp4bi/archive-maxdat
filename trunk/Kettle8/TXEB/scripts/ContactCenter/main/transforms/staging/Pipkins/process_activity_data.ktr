  <step>
    <name>Calculate DURATION_IN_INTERVAL_SECONDS</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
