    <optimizationLevel>9</optimizationLevel>
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var DURATION_IN_INTERVAL_SECONDS = 0;

//When both the start and end time of the activity are inside of the interval
if(((INTERVAL_START_DATE &lt;= STARTTIME) &amp;&amp; (STARTTIME &lt;= INTERVAL_END_DATE)) 
	&amp;&amp; ((INTERVAL_START_DATE &lt;= ENDTIME) &amp;&amp; (ENDTIME &lt;= INTERVAL_END_DATE))){

	DURATION_IN_INTERVAL_SECONDS = abs(dateDiff(ENDTIME, STARTTIME, "ss"));

//When the start time of the activity is prior to the interval start,
// but the end time of the activity is inside of the interval
}else if((STARTTIME &lt; INTERVAL_START_DATE) 
	&amp;&amp; ((INTERVAL_START_DATE &lt;= ENDTIME) &amp;&amp; (ENDTIME &lt;= INTERVAL_END_DATE))){

	DURATION_IN_INTERVAL_SECONDS = abs(dateDiff(ENDTIME, INTERVAL_START_DATE, "ss"));

//When the end time is after the interval end
// but the start time of the activity is inside of the interval
}else if(((INTERVAL_START_DATE &lt;= STARTTIME) &amp;&amp; (STARTTIME &lt;= INTERVAL_END_DATE))
	&amp;&amp; (INTERVAL_END_DATE &lt;= ENDTIME)){

	DURATION_IN_INTERVAL_SECONDS = abs(dateDiff(INTERVAL_END_DATE, STARTTIME, "ss"));

//Else the activity spanned the entirety of the interval
}else{

	DURATION_IN_INTERVAL_SECONDS = abs(dateDiff(INTERVAL_END_DATE, INTERVAL_START_DATE, "ss"));
}</jsScript_script>
      </jsScript>
    </jsScripts>
