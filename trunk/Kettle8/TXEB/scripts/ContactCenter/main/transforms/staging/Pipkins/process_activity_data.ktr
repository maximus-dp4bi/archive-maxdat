    <connection>MAXDAT</connection>
    <sql>SELECT EVT.NAME as ACTIVITYNAME,
TPT.LOCAL_TASK_START as STARTTIME,
TPT.LOCAL_TASK_END as ENDTIME,
TPS.NATIONAL_ID as LOGINID,
I.INTERVAL_ID,
I.INTERVAL_START_DATE,
I.INTERVAL_END_DATE,
I.INTERVAL_SECONDS
FROM 
	CC_S_TMP_PIPKINS_TASK TPT
    INNER JOIN CC_S_TMP_PIPKINS_EVENT EVT ON (TPT.EVENT_ID = EVT.EVENT_ID)
	INNER JOIN CC_S_TMP_PIPKINS_STAFF TPS ON (TPT.STAFF_ID = TPS.STAFF_ID)
	INNER JOIN CC_S_TMP_INTERVAL I on 
		(TPT.LOCAL_TASK_START BETWEEN I.INTERVAL_START_DATE AND I.INTERVAL_END_DATE)
		OR (TPT.LOCAL_TASK_END BETWEEN I.INTERVAL_START_DATE AND I.INTERVAL_END_DATE)
		OR (
			TPT.LOCAL_TASK_START &lt; I.INTERVAL_START_DATE
			AND TPT.LOCAL_TASK_END > I.INTERVAL_END_DATE
		)
where task_edit_id = 0
	and scenario_group_id = 5</sql>
