    <sql>SELECT
  IntervalUpperBound1 AS PERIOD_1_UPPER_BOUND
, IntervalUpperBound2 AS PERIOD_2_UPPER_BOUND
, IntervalUpperBound3 AS PERIOD_3_UPPER_BOUND
, IntervalUpperBound4 AS PERIOD_4_UPPER_BOUND
, IntervalUpperBound5 AS PERIOD_5_UPPER_BOUND
, IntervalUpperBound6 AS PERIOD_6_UPPER_BOUND
, IntervalUpperBound7 AS PERIOD_7_UPPER_BOUND
, IntervalUpperBound8 AS PERIOD_8_UPPER_BOUND
, IntervalUpperBound9 AS PERIOD_9_UPPER_BOUND
FROM dbo.Bucket_Intervals
</sql>
