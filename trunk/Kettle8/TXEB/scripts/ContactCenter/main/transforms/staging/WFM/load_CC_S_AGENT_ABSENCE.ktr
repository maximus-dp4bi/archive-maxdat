    <name>Get Source Absences</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>WFM</connection>
    <sql>select a.NAME as ABSENCE_TYPE,
	tohpd.starttime as ABSENCE_START_TIME,
	tohpd.endtime as ABSENCE_END_TIME,
	o.NAME as ORGANIZATION,
	o.TIMEZONE as TIMEZONE,
	CASE
		WHEN a.NAME IN('Unscheduled Absence', 'NCNS', 'Tardy', 'Unscheduled CRITICAL Absence', 'Unscheduled HALF Absence') THEN 1
		ELSE 0
	END as IS_PLANNED_FLAG,
	substring(tohpd.hoursperday, 3, len(hoursperday)) as hoursperday,
	eds.EXTERNALEMPIDENT as LOGIN_ID
from dbo.timeoffhoursperday tohpd
	inner join dbo.activity a
		on tohpd.activityid = a.ID
	inner join dbo.EMPLOYEEAM eam
		on eam.ID = tohpd.employeeid
	inner join dbo.EMPLOYEEDATASOURCE eds
		on eds.EMPLOYEEID = eam.ID
	inner join dbo.WORKRESOURCE wr
		on eam.ID = wr.ID
	inner join dbo.WORKRESOURCEORGANIZATION wro
		on wr.ID = wro.WORKRESOURCEID
	inner join dbo.ORGANIZATION o
		on o.ID = wro.ORGANIZATIONID
where eds.EXTERNALEMPIDENT IS NOT NULL
			and tohpd.starttime between ? and ?
   	AND (wro.ENDTIME > ? OR wro.ENDTIME IS NULL)
	AND (wro.STARTTIME &lt; ?)
group by a.NAME, tohpd.starttime, tohpd.endtime, o.name, o.timezone, tohpd.hoursperday, eds.EXTERNALEMPIDENT</sql>
    <limit>0</limit>
    <lookup>Remove Local Time Info</lookup>
