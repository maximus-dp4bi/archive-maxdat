  <step>
    <name>Parse Absence Entries</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <compatible>N</compatible>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// this Java Script will parse each absence entry and create additional rows where an entry may span a number of days
// 		(i.e. 3|8, this means we need to have three records that account for one day of absence with eight hours each)
// to learn how to create rows via the Java Script step,
// 		reference:  http://type-exit.org/adventures-with-open-source-bi/2010/06/generating-rows-using-javascript-in-pentaho-kettle/

// store the entries in an array (e.g. we can have a single entry:  '6.75'
// 						   			   or multiple entries:  '6.75, 3|8, ...')
var entries = hoursperday.split(",");

// store the cumulative sum of days to later reference when determining the start datetime of each entry
var totalDays = 0;
var cumulativeDays = new Array();
for(var i in entries)
{
	// no pipe character found, so this is a single day entry (e.g. '6.75')
	if(indexOf(entries[i], "|") == -1)
	{
		totalDays++; // add one to our total for single day entries
		cumulativeDays.push(totalDays);
	}
	// pipe character found, so this is a multi-day entry (e.g. '3|8')
	else
	{
		totalDays = totalDays + str2num(substr(entries[i], 0, indexOf(entries[i], "|"))); // grab the first # in the format '#|#', and add it to our total
		cumulativeDays.push(totalDays);
	}
}

// no pipe character found, so this is a single day entry where the time per day is the entry itself
if(indexOf(hoursperday_parsed, "|") == -1)
{
	var timePerDay = hoursperday_parsed * 60; // in minutes
	dayCounter = 1;
}
// pipe character found, so this is a multi-day entry of the format '#|#', 
// where the first # is the number of days and the second # is the number of hours per day
else
{
	var timePerDay = substr(hoursperday_parsed, hoursperday_parsed.indexOf("|") + 1) * 60; // in minutes
	var dayCounter = substr(hoursperday_parsed, 0, indexOf(hoursperday_parsed, "|"));
}

// we now create record(s) for the entry,
// where a single day entry will create just one record,
// and multi-day entries of the format '#|#' will create a number of records equal to the first #
while(dayCounter > 0)
{
	// calculate the start and end times as they pertain to each day in the entry
	var thisDayStartTime = dateAdd(LOCAL_STARTTIME, "d", cumulativeDays[rowNum - 1] - dayCounter);
	var thisDayEndTime = dateAdd(thisDayStartTime, "ss", timePerDay * 60); // in minutes

	// create a new row object that fits our output size
	var row = createRowCopy(getOutputRowMeta().size());

	// find the index of the first field appended by this step
	var idx = getInputRowMeta().size();

	// fill each field (notice how the index is increased)
	row[idx++] = dayCounter;
	row[idx++] = timePerDay;
	row[idx++] = thisDayStartTime;
	row[idx++] = thisDayEndTime;

	// output the row
	putRow(row);

	dayCounter--; // move to the next day (for multi-day entries only)
}
</jsScript_script>
