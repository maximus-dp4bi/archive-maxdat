    <sql>-- pull the last successful run of this job type
SELECT START_DATETIME_PARAM as PREV_START_DATETIME,
	   END_DATETIME_PARAM as PREV_END_DATETIME,
	   SCHEDULED_JOB_ID as PREV_SCHEDULED_JOB_ID 
FROM CC_A_SCHEDULED_JOB
WHERE JOB_START_DATE = 
