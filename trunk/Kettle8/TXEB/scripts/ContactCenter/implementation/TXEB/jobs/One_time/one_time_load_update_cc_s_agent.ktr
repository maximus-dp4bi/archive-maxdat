    <connection>WFM</connection>
    <sql>SELECT 
        distinct(eds.EXTERNALEMPIDENT) as LOGIN_ID,
		wrjt.STARTTIME AS HIRE_DATE,
        wrjt.ENDTIME AS TERMINATION_DATE
FROM 
        EMPLOYEEAM eam
        INNER JOIN EMPLOYEEDATASOURCE eds ON eam.ID = eds.EMPLOYEEID 
        INNER JOIN PERSON p ON eam.PERSONID = p.ID 
        INNER JOIN WORKRESOURCE wr ON eam.ID = wr.ID 
        INNER JOIN WORKRESOURCEJOBTITLE wrjt ON wr.ID = wrjt.WORKRESOURCEID 
        INNER JOIN JOBTITLE jt ON wrjt.JOBTITLEID = jt.ID
		LEFT JOIN WORKRESOURCEORGANIZATION wro ON wr.id = wro.WORKRESOURCEID
		LEFT JOIN ORGANIZATION org on wro.ORGANIZATIONID = org.ID
WHERE EXTERNALEMPIDENT IS NOT NULL -- unable to load agents that do not have a LOGIN_ID
    AND EXTERNALEMPIDENT != ''
</sql>
