    <sql>SELECT TYPE, START_DATE, END_DATE
FROM D_REPORTING_PERIOD
WHERE END_DATE >= TO_DATE('${startDate}', 'YYYY/MM/DD') - 60
AND START_DATE &lt;= TO_DATE('${endDate}', 'YYYY/MM/DD')
AND TYPE = '${reportingPeriodType}'
ORDER BY END_DATE
</sql>
