    </partitioning>
    <connection>MAXDAT</connection>
    <sql>select prj.project_id, prg.program_id, geo.geography_master_id, 0 as attrition_count 
from cc_c_project_config pc
inner join cc_d_project prj on pc.project_name = prj.project_name
inner join cc_d_program prg on pc.program_name = prg.program_name
inner join cc_d_country dc on pc.country_name = dc.country_name
inner join cc_d_state ds on pc.state_name = ds.state_name
inner join cc_d_province dp on pc.province_name = dp.province_name
inner join cc_d_district dd on pc.district_name = dd.district_name
inner join cc_d_region dr on pc.region_name = dr.region_name
inner join cc_d_geography_master geo on 
  dc.country_id = geo.country_id
  and ds.state_id = geo.state_id
  and dp.province_id = geo.province_id
  and dd.district_id = geo.district_id
  and dr.region_id = geo.region_id
where project_id != 0
</sql>
    <limit>0</limit>
