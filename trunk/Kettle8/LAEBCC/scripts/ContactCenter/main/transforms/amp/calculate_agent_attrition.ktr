    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>select 
  da.d_agent_id
  , d_date
  , project_id as d_project_id
  , d_program_id
  , d_geography_master_id
from 
  cc_d_agent da 
  inner join cc_d_dates dd on da.termination_date = dd.d_date
  inner join cc_f_agent_by_date f on da.d_agent_id = f.d_agent_id and dd.d_date_id = f.d_date_id  
  inner join cc_d_project_targets dpt on f.d_project_targets_id = dpt.d_project_targets_id
  inner join cc_d_group dg on f.d_group_id = dg.d_group_id and include_agent_payroll_flg = 'Y'
where 
	da.termination_date between ? and ?
</sql>
