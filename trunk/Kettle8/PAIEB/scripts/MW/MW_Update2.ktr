    <connection>MAXDAT</connection>
    <sql>SELECT  mw.cemw_id
       ,trunc(nvl(mw.complete_date, sysdate) - trunc(mw.create_date))  AGE_IN_CALENDAR_DAYS
       ,bus_days_between(mw.create_date,trunc(nvl(mw.complete_date, sysdate))) AGE_IN_BUSINESS_DAYS
       ,CASE
            WHEN mw.sla_jeopardy_days is null then 'N'
            WHEN mw.sla_days_type = 'B' AND  bus_days_between(mw.create_date,trunc(nvl(mw.complete_date, sysdate))) > mw.sla_jeopardy_days THEN 'Y'
            WHEN mw.sla_days_type = 'C' AND  trunc(nvl(mw.complete_date, sysdate) - trunc(mw.create_date)) >  mw.sla_jeopardy_days THEN 'Y'
            ELSE 'N' 
         END  JEOPARDY_FLAG
       ,CASE
            WHEN mw.complete_flag = 'N' then 'Not Complete'
            WHEN mw.sla_days is null then 'Not Required'
            WHEN mw.sla_days_type = 'B' AND  bus_days_between(mw.create_date,trunc(nvl(mw.complete_date, sysdate))) >  mw.sla_days then 'Untimely'
            WHEN mw.sla_days_type = 'C' AND  trunc(nvl(mw.complete_date, sysdate) - trunc(mw.create_date)) >  mw.sla_days then 'Untimely'
            else 'Timely' END  TIMELINESS_STATUS
       ,DECODE(mw.task_status, 'COMPLETED', 0,trunc(nvl(mw.complete_date, sysdate)) - trunc(mw.status_date))  STATUS_AGE_IN_CAL_DAYS
       ,DECODE(mw.task_status, 'COMPLETED', 0,bus_days_between(mw.status_date,trunc(nvl(mw.complete_date, sysdate)))) STATUS_AGE_IN_BUS_DAYS
       ,trunc(sysdate) system_date
  FROM corp_etl_manage_work_tmp mw
 WHERE mw.stage_done_date IS NULL
   AND mw.cancel_work_flag = 'N'
   AND (mw.age_in_calendar_days IS NULL
    OR  mw.age_in_calendar_days &lt;&gt; trunc(nvl(mw.complete_date, sysdate)) - trunc(mw.create_date)       )
</sql>
