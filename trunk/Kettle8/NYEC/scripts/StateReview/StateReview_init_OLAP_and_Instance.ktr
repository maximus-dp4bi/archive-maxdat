    <sql>
  select  appH.CREATE_TS as "Capture Date",
		to_char(SYSDATE, 'yyyy/mm/dd HH:MI:SS') as "Capture_Date_string",
		'STATE_REVIEW_CAPTURE_DATE' as "Update_Field",
        appH.Renewal_Due_Date as "Approval Date",
        (case when (appM.MI > 0) then 'N' else 'Y' end) as "ALL SATISFIED",
        appH.Application_ID as "QApplication_ID",
		si.step_instance_id as "QTast_ID",
        CC.CALL_RECORD_ID as "CALL_CAMPAIN_ID",
        appH.Status_Cd as "Application Status",

        (select RFE_STATUS_CD from App_Tracker where Application_ID = appH.Application_ID and Client_ID = si.client_id and rownum = 1) as "RFE_STATUS",
        case when ((appH.Renewal_Due_Date > (select CREATE_TS from CALL_RECORD where CALL_RECORD_ID = CC.CALL_RECORD_ID))
              and ((select count(RFE_STATUS_CD) from App_Tracker  where Application_ID = appH.Application_Id and HISTORY_IND = 0 and RFE_STATUS_CD = 'AWAITING_MI')>0)
              and (appH.Status_Cd = 'INPROCESS')) and (select DO_NOT_CALL_IND from client cl where si.client_id = cl.clnt_client_id)=0 and 
              (select case 
       				   	    	when app_form_cd='FPBP' then 
				                   1 
			         	      else 
				                   0 
     			         	  end  from app_header where application_id = appH.Application_Id) = 1
             then 'Y' 
             else 'N' 
        end as "CALL_CAMPAIN_FLAG",
        
		(select case 
       	 	    	when app_form_cd='FPBP' then 
		                   'Y'
		        else 
			               'N' 
     		    end  from app_header where application_id = appH.Application_Id) as "FPBP_FLAG",

        case when (appH.Status_Cd = 'DISREGARD' or appH.Status_Cd = 'TIMEOUT') then appH.Update_Ts else null end as "CANCEL DATE",
        
--        (Select MAX(lr.lmreq_id) as "Letter_REQ_ID"  From letter_request      lr, letter_request_link ll  where lr.lmreq_id = ll.lmreq_id and Ll.reference_id = appH.Application_Id and rownum=1
--              group by Ll.reference_id) as "LETTER_REQ_ID",
--        
--        (Select lr.status_cd as "Letter_Status" From letter_request  lr, letter_request_link ll  
--          where lr.lmreq_id = ll.lmreq_id and Ll.reference_id = appH.Application_ID and lr.lmreq_id = 
--                (Select MAX(lr.lmreq_id) as "Letter_REQ_ID"  From letter_request lr, letter_request_link ll  
--                   where lr.lmreq_id = ll.lmreq_id and Ll.reference_id = appH.Application_ID
--                      group by Ll.reference_id
--                ) and rownum=1
--         ) as "LETTER_STATUS",
--        (Select lr.update_ts as "Letter_Status_date" From letter_request  lr, letter_request_link ll  
--          where lr.lmreq_id = ll.lmreq_id and Ll.reference_id = appH.Application_ID and lr.lmreq_id = 
--                (Select MAX(lr.lmreq_id) as "Letter_REQ_ID"  From letter_request lr, letter_request_link ll  
--                   where lr.lmreq_id = ll.lmreq_id and Ll.reference_id = appH.Application_ID
--                      group by Ll.reference_id
--                ) and rownum=1
--         ) as "LETTER_STATUS_DATE",
		
        -- MI not implemented so I am not sure yet.
        case when (select count(DISTINCT RFE_STATUS_CD) from App_Tracker where Application_ID =  appH.Application_Id ) > 1 then 'N' else 'Y' 
         end  RFE_Status_Flag,
--        HrtCC.Qc_Ind as "State Acceptance indicator",
        (SELECT qc_ind FROM Hrt_Case_Control HrtCC WHERE  HrtCC.Application_Id = appH.Application_Id) state_acpt_Ind,
        
        (select case when (select count(*) from App_Tracker where application_id = appH.Application_Id and HISTORY_IND=0) = 
                          (select count(*) from App_Tracker where application_id = appH.Application_Id and (RFE_STATUS_CD = 'ACCEPTED') and HISTORY_IND=0) then 'Accept'
                     when (select count(*) from App_Tracker where application_id = appH.Application_Id and (RFE_STATUS_CD = 'REJECTED') and HISTORY_IND=0) > 1 then 'Reject'
                     else null
                end  
         from Dual) State_Review_Outcome

        
from App_Header appH
       left outer join (select count(case when SATISFIED_DATE IS NULL then 1 else NULL end) "MI", Application_ID from App_Missing_Info where Application_ID = ? group by Application_ID) appM
         on appH.Application_Id = appM.Application_Id

--        left outer join Hrt_Case_Control HrtCC on appH.Application_Id = HrtCC.Application_Id

--        left outer join (select count(DISTINCT RFE_STATUS_CD) "RFE_STATUS_DIF", Application_ID from App_Tracker where Application_ID = 151212 group by Application_ID) appT
--         on appM.Application_Id = appT.Application_Id

        left outer join (select CALL_RECORD_ID, Application_ID from App_Missing_Info where Application_ID = ?) CC 
             on CC.Application_ID = appH.Application_ID

--        join (select RFE_STATUS_CD, Application_ID from App_Tracker where HISTORY_IND = 0 and Application_ID = 151212) rfestat on rfestat.Application_Id = appH.Application_Id

		join step_instance si on si.step_instance_id = ?
where appH.Application_Id = ? and si.step_instance_id = ? and CC.Application_ID = ? and rownum=1 -- get only one task of an application and this will bring back every task.  This query gets application data with subqueries
</sql>
