  <step>
    <name>Database join 2 -- Get MAXDAT Data</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT (
        select case when step_definition_id in (SELECT ref_id
                                               FROM corp_etl_list_lkup
                                               WHERE NAME = 'TASK_MONITOR_TYPE'
                                                     AND out_var = 'State Review'
                                                     AND SYSDATE BETWEEN start_date AND end_date)
                    then
                        'State Review'
                    when step_definition_id in (SELECT ref_id
                                               FROM corp_etl_list_lkup
                                               WHERE NAME = 'TASK_MONITOR_TYPE'
                                                     AND out_var = 'Data Correction'
                                                     AND SYSDATE BETWEEN start_date AND end_date)
                    then
                        'Data Correction'
                    when step_definition_id in (SELECT ref_id
                                               FROM corp_etl_list_lkup
                                               WHERE NAME = 'TASK_MONITOR_TYPE'
                                                     AND out_var = 'Research'
                                                     AND SYSDATE BETWEEN start_date AND end_date)
                    then
                        'Research'
                    else
                        'Other'
               end
        from step_instance_stg
		where step_instance_id = si_stg.step_instance_id and rownum = 1
		) as "CURRENT_TASK_TYPE",
		(
		select HIST_STATUS from step_instance_stg si_stg2 where step_instance_history_id = (select MAX(step_instance_history_id) 
        		                                                                           from step_instance_stg 
                		                                                                   group by ref_id, step_instance_id 
                        		                                                           having ref_id = si_stg2.ref_id and step_instance_id = si_stg2.step_instance_id)
                                				                and ref_id = si_stg.ref_id and step_instance_id = si_stg.step_instance_id and rownum = 1
        ) as "CURRENT_TASK_STATUS",
		(
		select hist_create_ts from step_instance_stg si_stg2 where hist_create_ts = (select MAX(hist_create_ts) 
        		                                                                    from step_instance_stg 
                		                                                            where ref_id = si_stg.ref_id
                        		                                                    group by ref_id, step_instance_id, hist_status 
                                		                                            having ref_id = si_stg2.ref_id and step_instance_id = si_stg2.step_instance_id and HIST_STATUS = 'CLAIMED')
                                				                and ref_id = si_stg.ref_id and step_instance_id = si_stg.step_instance_id and rownum = 1
		) as "Current_Task_Claim_Date",
		(
		select hist_create_ts from step_instance_stg si_stg2 where hist_create_ts = (select MAX(hist_create_ts) 
        		                                                                    from step_instance_stg 
                		                                                            where ref_id = si_stg.ref_id
                        		                                                    group by ref_id, step_instance_id, hist_status 
                                		                                            having ref_id = si_stg2.ref_id and step_instance_id = si_stg2.step_instance_id and HIST_STATUS = 'CLAIMED')
                                        		        						           and step_definition_id in (SELECT ref_id
                                                		                        						              FROM corp_etl_list_lkup
						                                                        		                              WHERE NAME = 'TASK_MONITOR_TYPE'
                        						                                        		                           AND out_var = 'Research'
                                                						                        		                   AND SYSDATE BETWEEN start_date AND end_date)
                                				                and ref_id = si_stg.ref_id and step_instance_id = si_stg.step_instance_id and rownum = 1
		) as "Research_Task_Claim_Date",
--        Slkup.Display_Name as "ASPD_RECIEVE_STATE_REVIEW",
        si_stg.ref_id as "MDApplication_ID",
		si_stg.step_instance_id as "MDTask_ID",
        bus_days_between((select create_ts from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1), SYSDATE) as "BUSINES DAYS",
        trunc((SYSDATE - (select create_ts from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1))) as "CALENDAR DAYS",
        case when Slkup.First_Name = 'AutoCloseStateReviewTasks' then 'Y' else 'N' end as "Auto Close",
        (select step_instance_id from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1) as "Current_Task_ID",
        (select completed_ts from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1) as "COMPLETED DATE",
        (select step_instance_id from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1) as "State_Review_Task",
--RECEIVE_STATE_REVIEW        
        (select completed_ts from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1) as "ASED_RECEIVE_STATE_REVIEW",
        (select Display_Name from Staff_Lkup Lkup where Lkup.Staff_id = si_stg.hist_owner and si_stg.hist_status = 'COMPLETED') as "ASPB_RECEIVE_STATE_REVIEW",
        (select hist_create_by from step_instance_stg si_stg2 where HIST_STATUS = 'ClAIMED' and  step_instance_id = si_stg.step_instance_id and rownum=1) as "ASSD_RECEIVE_STATE_REVIEW",
-- PROCESS_DC
        (SELECT si_stg2.completed_ts
          FROM step_instance_stg si_stg2
         WHERE step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = 'TASK_MONITOR_TYPE'
                   AND out_var = 'Data Correction'
                   AND SYSDATE BETWEEN start_date AND end_date)
           AND si_stg2.hist_status = 'COMPLETED'
           AND si_stg2.REF_ID = si_stg.REF_ID
           AND rownum = 1) "ASED_PROCESS_DC",
        (SELECT Slkup2.Display_Name
          FROM step_instance_stg si_stg2
          left outer join Staff_LKup Slkup2 on HIST_CREATE_BY = Slkup2.Staff_id
         WHERE step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = 'TASK_MONITOR_TYPE'
                   AND out_var = 'Data Correction'
                   AND SYSDATE BETWEEN start_date AND end_date)
           AND si_stg2.hist_status = 'COMPLETED'
           AND si_stg2.REF_ID = si_stg.REF_ID
           AND rownum = 1) "ASPB_PROCESS_DC",
        (SELECT si_stg2.hist_create_ts
          FROM step_instance_stg si_stg2
         WHERE step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = 'TASK_MONITOR_TYPE'
                   AND out_var = 'Data Correction'
                   AND SYSDATE BETWEEN start_date AND end_date)
           AND si_stg2.hist_status = 'CLAIMED'
           AND si_stg2.REF_ID = si_stg.REF_ID
           AND rownum = 1) as "ASSD_PROCESS_DC",
--RESEARCH_DC
        (SELECT si_stg2.completed_ts
          FROM step_instance_stg si_stg2
         WHERE step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = 'TASK_MONITOR_TYPE'
                   AND out_var = 'Research'
                   AND SYSDATE BETWEEN start_date AND end_date)
           AND si_stg2.hist_status = 'COMPLETED'
           AND si_stg2.REF_ID = si_stg.REF_ID
           AND rownum = 1) "ASED_RESEARCH_DC",
        (SELECT Slkup2.Display_Name
          FROM step_instance_stg si_stg2
          left outer join Staff_LKup Slkup2 on HIST_CREATE_BY = Slkup2.Staff_id
         WHERE step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = 'TASK_MONITOR_TYPE'
                   AND out_var = 'Research'
                   AND SYSDATE BETWEEN start_date AND end_date)
           AND si_stg2.hist_status = 'COMPLETED'
           AND si_stg2.REF_ID = si_stg.REF_ID
           AND rownum = 1) "ASPB_RESEARCH_DC",
           
        (select status from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum=1) as "INSTANCE STATUS",
        -- New State Review Task
        (select step_instance_id from step_instance_stg si
	       where step_instance_id = si_stg.step_instance_id and
    	    	REF_ID = si_stg.REF_ID and 
	        	step_definition_id in (SELECT ref_id FROM corp_etl_list_lkup
                                               WHERE NAME = 'TASK_MONITOR_TYPE'
                                                     AND out_var = 'State Review'
                                                     AND SYSDATE BETWEEN start_date AND end_date) and
	      		? = 'Y' and
  			     EXISTS (select * from step_instance_stg where ref_id = si.ref_id and 
                                                      create_ts &lt; si.create_ts and
                                                      step_definition_id in (SELECT ref_id
                                                                             FROM corp_etl_list_lkup
                                                                             WHERE NAME = 'TASK_MONITOR_TYPE'
                                                                                   AND (out_var = 'Data Correction' or out_var = 'Research')
                                                                                   AND SYSDATE BETWEEN start_date AND end_date))
		        and rownum=1) as "New_State_Review_Task",

				(select LETTER_ID as "LETTER_REQ_ID"
					from Letters_Stg 
					where LETTER_TYPE_CD = 'MI2' and LETTER_APP_ID = si_stg.REF_ID and LETTER_ID = (SELECT MAX(LETTER_ID) from Letters_Stg where LETTER_APP_ID = si_stg.REF_ID)) as "LETTER_REQ_ID",

				(select LETTER_STATUS_CD as "LETTER_STATUS"
					from Letters_Stg
					where LETTER_TYPE_CD = 'MI2' and LETTER_APP_ID = si_stg.REF_ID and LETTER_ID = (SELECT MAX(LETTER_ID) from Letters_Stg where LETTER_APP_ID = si_stg.REF_ID)) as "LETTER_STATUS",

				(select LETTER_UPDATE_TS as "LETTER_STATUS_DATE"
					from Letters_Stg 
					where LETTER_TYPE_CD = 'MI2' and LETTER_APP_ID = si_stg.REF_ID and LETTER_ID = (SELECT MAX(LETTER_ID) from Letters_Stg where LETTER_APP_ID = si_stg.REF_ID)) as"LETTER_STATUS_DATE"

FROM step_instance_stg si_stg
left outer join Staff_LKup Slkup on si_stg.HIST_CREATE_BY = Slkup.Staff_id
WHERE 
--step_definition_id IN 
--            (SELECT ref_id FROM corp_etl_list_lkup WHERE NAME = 'TASK_MONITOR_TYPE' 
--            AND out_var ='State Review'
--            AND SYSDATE BETWEEN start_date AND end_date)
      -- Get based on the Instance step and application
--      AND 
si_stg.step_instance_id = ? AND si_stg.ref_id = ? AND REF_TYPE = 'APP_HEADER' and si_stg.HIST_CREATE_TS >= ?

      -- Get the current state or the last history status
      AND si_stg.step_instance_history_id = (select MAX(step_instance_history_id) 
                                             from step_instance_stg 
                                             group by ref_id, step_instance_id having ref_id = si_stg.ref_id and step_instance_id = si_stg.step_instance_id)
--      AND si_stg.hist_status='UNCLAIMED' -- Every record has an UnClaimed status so this limites it to one record per step_instance.
--      order by step_instance_id -- takes forever and needs to be removed but needed for development testing.
--      rownum=1 -- get the latest
  </sql>
