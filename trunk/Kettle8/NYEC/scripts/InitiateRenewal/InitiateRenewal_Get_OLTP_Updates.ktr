  <step>
    <name>Database join - Retrieve OLTP Updates for an Application</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>5</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT a.receipt_date     ren_receipt_dt
     , a.status_date      app_status_dt
     , a.status_cd        app_status
     , SYSDATE            stg_last_processed_dt
     --, CASE WHEN a.status_cd = 'RENEWAL_INITIATED' 
     --    THEN 'ACTIVE'
     --    ELSE 'COMPLETE'
     --  END                instance_status
     , CASE WHEN a.status_cd in ('DISREGARDED','VOID')
         THEN a.status_date
         ELSE NULL
       END cancel_dt   
     , CASE WHEN a.status_cd = 'TIMEOUT'
         THEN a.status_date
         ELSE NULL
       END close_dt   
     , A.RENEWAL_DUE_DATE       new_auth_end_dt
     , CASE (SELECT COUNT(1) FROM HRT_CASE 
	            WHERE HRT_CASE.APPLICATION_ID = A.APPLICATION_ID
                  AND ( 
                     ( HRT_CASE.TRANSACTION_TYPE_CD in ('07', '08')
				               AND EXISTS ( SELECT 1  FROM HRT_CASE_CONTROL
                 		                 	 WHERE HRT_CASE.HRT_CASE_ID = HRT_CASE_CONTROL.HRT_CASE_ID 
                                       		   AND CURRENT_TIMESTAMP &lt; A.RENEWAL_DUE_DATE
                                       		   AND HRT_CASE_CONTROL.CURRENT_STATUS_CD = '05') 
                      ) 
                  	OR EXISTS (SELECT 1 FROM HRT_CASE_DEFERRAL
                                WHERE HRT_CASE_DEFERRAL.CASE_DEFERRAL_ID = ( SELECT MAX(HCD.CASE_DEFERRAL_ID)
                                                                               FROM HRT_CASE_DEFERRAL HCD
                                                                              WHERE HRT_CASE.HRT_CASE_ID = HCD.HRT_CASE_ID 
                                                                                AND (HCD.DEFERRAL_RES_CD IS NULL OR HCD.DEFERRAL_RES_CD = '04')
                                                                                AND HCD.DEFERRAL_CD = '02' )
	                          )        
                    ))
         WHEN 0 THEN 'N' ELSE 'Y' END CLOCKDOWN_INDICATOR
  FROM app_header a
 WHERE a.application_id = ?
</sql>
