    <rowlimit>0</rowlimit>
    <sql>select trr.value oltp_reason_cd
from STEP_INSTANCE si
inner join STEP_DEFINITION sd on si.step_definition_id = sd.step_definition_id
and sd.step_type_cd in('HUMAN_TASK','VIRTUAL_HUMAN_TASK')
inner join enum_task_research_reason trr on 
      -- Capture tag value only (multiple ENUM can appear within CLOB
      instr(substr(input_data,instr(input_data,'RESEARCH_REF_REASON'),100)
                                   ,'&lt;string>'||trr.value||'&lt;/string>') > 0
where ref_id = ?
and ref_type  IN ('APP_HEADER','DOCUMENT_SET_ID')
and input_data like '%RESEARCH_REF_REASON%'
and step_instance_id = ?
</sql>
