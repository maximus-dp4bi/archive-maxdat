    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>select  distinct
        si.step_instance_id as TASK_ID,
        --si.hist_status      as TASK_STATUS,
        si_cl.step_instance_id as StgMI_TASK_ID,
        si_cl.hist_status      as StgMI_TASK_STATUS,
        (case when si_cl.hist_status = 'CLAIMED' then si_cl.hist_create_ts end) as StgMI_TASK_CLAIMED_DATE,
        (case when si_cl.hist_status = 'CLAIMED' and si_cl.hist_create_by &lt;&gt;'IMG'
                                            then (select first_name ||' ' || last_name 
                                                    from staff_stg
                                                   where staff_id = si_cl.hist_create_by) 
             when si_cl.hist_status = 'CLAIMED' and si_cl.hist_create_by ='IMG' then si_cl.hist_create_by
             else null end)   as StgMI_TASK_CLAIMED_BY,
       (case when si_comp.hist_status = 'COMPLETED' then si_comp.hist_create_ts end) as StgMI_TASK_COMPLETED_DATE,
       (case when si_comp.hist_status = 'COMPLETED' and si_comp.hist_create_by &lt;&gt;'IMG'
                                            then (select first_name ||' ' || last_name 
                                                    from staff_stg
                                                   where staff_id = si_comp.hist_create_by) 
             when si_comp.hist_status = 'COMPLETED' and si_comp.hist_create_by ='IMG' then si_comp.hist_create_by
             else null end)   as StgMI_TASK_COMPLETED_BY
from step_instance_stg si
left outer join (select * from step_instance_stg si_cl_2 
                    where si_cl_2.step_instance_id = ?                
                   and si_cl_2.step_instance_history_id = (select min(si_cl_3.step_instance_history_id)
                                                             from step_instance_stg si_cl_3
                                                            where si_cl_3.step_instance_id = ?
                                                            and si_cl_3.hist_status = 'CLAIMED'
														    and si_cl_3.hist_create_ts &lt;=  ? 
                                                            ) 
                  ) si_cl on si.step_instance_id = si_cl.step_instance_id
left outer join (select * from step_instance_stg si_comp_2 
                    where si_comp_2.step_instance_id = ?
                   and si_comp_2.step_instance_history_id = (select min(si_comp_3.step_instance_history_id)
                                                             from step_instance_stg si_comp_3
                                                            where si_comp_3.step_instance_id = ?
                                                            and si_comp_3.hist_status = 'COMPLETED'
															and si_comp_2.hist_create_ts &lt;=  ? 
                                                            )
                  ) si_comp on si.step_instance_id = si_comp.step_instance_id
where si.step_instance_id = ?</sql>
