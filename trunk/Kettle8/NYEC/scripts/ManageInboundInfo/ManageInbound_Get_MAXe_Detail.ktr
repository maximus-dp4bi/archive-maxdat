AND    Si.Step_Definition_Id = Sd.Step_Definition_Id
AND    Sd.Step_Type_Cd IN ('HUMAN_TASK', 'VIRTUAL_HUMAN_TASK')
AND    Sd.Name NOT IN ('HSDE-QC', 'Link Document Set')
AND    Sd.Step_Definition_Id NOT IN (${RS_REF_VALS})
