  <step>
    <name>Database join - OLTP Doc Task data</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT DISTINCT Si.Ref_Id
     , Si.Ref_Type
     , Si.Step_Definition_Id
     , Si.Step_Instance_Id
     , TRUNC(Si.Create_Ts) Create_Ts
FROM   STEP_INSTANCE SI
     , DOCUMENT_SET   Ds
     , Step_Definition Sd
WHERE  Si.Ref_Type = 'DOCUMENT_SET_ID'
AND    Si.Ref_Id = Ds.Document_Set_Id
AND    Ds.Document_Set_Id = ?
AND    Si.Create_Ts > Ds.Create_Ts   -- Get the Step instance record after App doc data was created
AND    Si.Step_Definition_Id = Sd.Step_Definition_Id
AND    Sd.Step_Type_Cd IN ('HUMAN_TASK', 'VIRTUAL_HUMAN_TASK')
AND    Sd.Name NOT IN ('HSDE-QC', 'Link Document Set')
AND    Sd.Step_Definition_Id NOT IN (${RS_REF_VALS})
order by Si.Step_Instance_Id</sql>
    <outer_join>Y</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>DOCUMENT_SET_ID</name>
        <type>Integer</type>
      </field>
    </parameter>
