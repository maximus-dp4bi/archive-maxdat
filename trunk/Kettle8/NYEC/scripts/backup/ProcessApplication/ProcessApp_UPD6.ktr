    <connection>MAXDAT</connection>
    <sql>SELECT PA.APP_ID INAPP_ID
	  ,SYSDATE CURRENT_DT
--
,PAS.AGE_IN_BUSINESS_DAYS	  stgAGE_IN_BUSINESS_DAYS
,PAS.AGE_IN_CALENDAR_DAYS	  stgAGE_IN_CALENDAR_DAYS
,PAS.APP_CYCLE_BUS_DAYS	  	  stgAPP_CYCLE_BUS_DAYS
,PAS.APP_CYCLE_CAL_DAYS	  	  stgAPP_CYCLE_CAL_DAYS
,PAS.JEOPARDY_FLAG		  	  stgJEOPARDY_FLAG
,PAS.TIMELINESS_STATUS	  	  stgTIMELINESS_STATUS
--
,PA.AGE_IN_BUSINESS_DAYS	  UPD_AGE_IN_BUSINESS_DAYS
,PA.AGE_IN_CALENDAR_DAYS	  UPD_AGE_IN_CALENDAR_DAYS
,PA.APP_CYCLE_BUS_DAYS	  	  UPD_APP_CYCLE_BUS_DAYS
,PA.APP_CYCLE_CAL_DAYS	  	  UPD_APP_CYCLE_CAL_DAYS
,PA.JEOPARDY_FLAG		  	  UPD_JEOPARDY_FLAG
,PA.TIMELINESS_STATUS	  	  UPD_TIMELINESS_STATUS
--
   ,  &apos;N&apos; UPDATED_FLAG
   ,  &apos;Y&apos; CONSTANT_YES

 FROM  NYEC_ETL_PROCESS_APP PA 
      ,PROCESS_APP_STG PAS
WHERE PA.APP_ID = PAS.APP_ID
  AND PA.STAGE_DONE_DATE IS NULL
  AND PA.CANCEL_FLAG = &apos;N&apos;
  AND (PA.AGE_IN_CALENDAR_DAYS IS NULL OR 
       (PA.AGE_IN_CALENDAR_DAYS &lt;&gt; TRUNC(NVL(PA.COMPLETE_DT,SYSDATE))-TRUNC(PA.CREATE_DT))
      )
  AND PAS.APP_PRIORITY_IND = 1
  AND PAS.APP_EXTRACT_COMPLETE_DT IS NULL 
</sql>
