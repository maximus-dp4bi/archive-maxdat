  <step>
    <name>OLTP - Get MI Documents </name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>5</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>select
       ap_doc.application_id,
       ap_doc.app_doc_data_id,
       ap_doc.ecn,
       ap_doc.dcn,
       ap_doc.received_date as Envelope_Received_Date,
       --ap_doc.create_ts     as Envelope_linked_Date,
       lnk.create_ts        as Document_linked_Date,
       lnk.created_by       as Document_Linked_By,
       ap_doc.document_type_cd as Envelope_Type_CD,
       ap_doc.status_cd    as Envelope_Status,
       ap_doc.status_ts    as Envelope_Status_Date,
       ap_doc.media_cd     as Envelope_Media_CD
from  app_doc_data       ap_doc 
join doc_link  lnk on lnk.document_id = ap_doc.document_id    
where 1=1
AND  ap_doc.application_id = ?
--and ap_doc.document_type_cd &lt;&gt; 'APPLICATION'
/*
Below sub-query will avoid any multiple links to the clients. Picks the first link.
*/
and lnk.doc_link_id = (select min(lnk_2.doc_link_id) 
						 from doc_link lnk_2
                        where lnk_2.document_id = lnk.document_id)                        
order by ap_doc.application_id, ecn, dcn</sql>
    <outer_join>N</outer_join>
    <replace_vars>N</replace_vars>
    <parameter>
      <field>
        <name>INMI_TASK_APP_ID</name>
        <type>Integer</type>
      </field>
    </parameter>
