  <step>
    <name>Table input - MEDICAID Selection segments from EB</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>SELECT ss.selection_segment_id source_selection_record_id
       ,'SELECTION_SEGMENT' source_table_name
       ,ss.client_id source_client_id
       ,ss.program_type_cd program_type_code
       ,ss.plan_type_cd plan_type_code   
       ,COALESCE(p.plan_code,'0') source_plan_code
       ,COALESCE(ss.network_id,tmp_provider.network_id,0) source_network_id
       ,COALESCE(ss.disenroll_reason_cd_1,'0') change_reason_code
       ,COALESCE(st.selection_source_cd,ss.selection_source_cd,'0') selection_source_code
       ,CASE WHEN st.choice_reason_cd IS NULL AND ss.choice_reason_cd IS NULL THEN 'N/A' ELSE COALESCE(st.choice_reason_cd,ss.choice_reason_cd) END selection_reason_code
       ,CASE WHEN COALESCE(st.selection_source_cd,ss.selection_source_cd) = 'ST' AND st.transaction_type_cd IS NULL THEN 'DefaultEnroll' 
         ELSE COALESCE(st.transaction_type_cd,'0') END  enrollment_action_status_code               
       ,ss.start_date managed_care_start_date
       ,ss.end_date managed_care_end_date
       ,ss.end_nd --segment end date in number format
       ,ss.status_date enrollment_status_change_date
       ,1 number_count -- always set to 1
       ,'ME' segment_type_cd   --MEDICAID eligibility
        ,TO_CHAR(ss.create_ts,'mm/dd/yyyy') create_ts
        ,TO_CHAR(ss.create_ts,'hh24:mi') create_ts_hrmin
        ,TO_NUMBER(TO_CHAR(ss.create_ts,'ss')) create_ts_second
        ,ct.plan_service_type_cd sub_program_code
        ,ct.plan_service_type_cd plan_service_type
        ,COALESCE(tmp_provider.provider_type_code,'0') provider_type_code
        ,'0' status_in_group_code
        ,'0' risk_group_code
        ,'0' coverage_code
        ,'new' record_status
FROM selection_segment ss 
 INNER JOIN plans p
   ON ss.plan_id = p.plan_id
 INNER JOIN contract ct
   ON ss.contract_id = ct.contract_id
 LEFT OUTER JOIN (SELECT type_cd provider_type_code,n.provider_id_ext, n.plan_id, MAX(network_id) network_id
                  FROM network n
                    INNER JOIN provider p
                       ON n.provider_id = p.provider_id
                  WHERE n.status_cd = 'O'
                  GROUP BY type_cd,n.provider_id_ext, n.plan_id) tmp_provider
   ON ss.provider_id_ext = tmp_provider.provider_id_ext
   AND ss.plan_id = tmp_provider.plan_id
 LEFT OUTER JOIN selection_txn st
   ON ss.selection_segment_id = st.selection_segment_id</sql>
