    <rowlimit>1</rowlimit>
    <sql>SELECT NVL(cl.clnt_citizen,'0') citizenship_status_code
       ,NVL(cl.client_language,'0') language_code
	   ,NVL(cl.clnt_race,'0') race_code
       ,clnt_cin
