--      else null end receipt_date
--,CASE WHEN instr(rd.filename,'\') > 0 
--       THEN substr(rd.filename, instr(rd.filename,'\',-1)+1) 
--       ELSE substr(rd.filename, instr(rd.filename,'/',-1)+1) END transmit_date
,rd.end_ts transmit_date
,null as file_name
