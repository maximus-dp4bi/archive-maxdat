    <sql>select 
mjf.job_id,
mjf.job_name,
mjf.job_detail,
mjf.job_group,
mjf.job_type,
mjf.job_start_dt,
mjf.job_end_dt,
mjf.job_status,
mjf.job_status_enum,
--mjf.receipt_date as receipt_dt,
 case when mjf.job_type = 'Inbound File'
      then mjf.receipt_date end  receipt_dt,
--case when mjf.job_type = 'Inbound File'
--      then to_date(substr(mjf.receipt_date,1,14),'YYYYMMDDHH24MISS') end receipt_dt,
case when mjf.job_type = 'Outbound File'
     then mjf.transmit_date end transmit_dt,
     -- then to_date(substr(substr(REGEXP_substr(mjf.transmit_date, '[[:digit:]]+'),-8,8) || REGEXP_substr(mjf.transmit_date, '[[:digit:]]+',1,2),1,14),'YYYYMMDDHH24MISS') end transmit_dt,
mjf.file_name,
mjf.file_type,
mjf.file_source,
mjf.plan_name,
mjf.file_destination,
mjf.response_file_req,
mjf.RESPONSE_FILE_NAME,
mjf.RECORD_COUNT,
mjf.error_count as RECORD_ERROR_COUNT,
mjf.instance_status,
mjf.last_update_by_name,
mjf.last_update_by_dt,
mjf.start_ts
from (select 
rd.job_run_data_id as job_id
,rd.job_name
--,rd.comments 
,COALESCE(upper(re.filename),upper(rd.filename)) as job_detail
,rd.job_group
--,jd.JOB_DATA as job_type
,CASE WHEN dbms_lob.INSTR(jd.JOB_DATA, utl_raw.cast_to_raw('inputFileParamName'),1,1) > 0
       THEN 'Inbound File'
      WHEN dbms_lob.INSTR(jd.JOB_DATA, utl_raw.cast_to_raw('outputFileParamName'),1,1) > 0
       THEN 'Outbound File'
      ELSE 'Other Job Type' END job_type 
,rd.start_ts as job_start_dt
,rd.end_ts as job_end_dt
,js.report_label as job_status
,js.VALUE  as job_status_enum
, rd.start_ts receipt_date
--,case when rd.status_cd = 'COMPLETED'
