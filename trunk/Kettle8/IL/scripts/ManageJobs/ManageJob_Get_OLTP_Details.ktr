, null as response_file_req
, null as RESPONSE_FILE_NAME
,RE.RECORD_COUNT
,re.error_count
,'Active' instance_status
--,rd.updated_by as last_update_by_name
--,CASE WHEN S.STAFF_ID &lt;0 THEN 'MAXeb' 
--ELSE S.FIRST_NAME||' '||S.LAST_NAME 
--END as last_update_by_name
,coalesce(trim(s.first_name ||' '|| s.last_name),rd.created_by ) last_update_by_name 
--,rd.update_ts as last_update_by_dt
,nvl(rd.create_ts,sysdate) as last_update_by_dt
--,SYSDATE STAGE_CREATE_TS
,re.start_ts
from JOB_RUN_DATA rd,
ETL_REPORT re
,JOB_JOB_DETAILS jd
,staff s
,enum_job_status js 
where 
rd.job_run_data_id = re.job_id(+)
and REGEXP_substr(rd.job_name, '[^0-9][^-]+') = jd.job_name(+)
and rd.created_by = to_char(s.staff_id(+))
and RD.STATUS_CD = JS.VALUE(+)
--and rd.job_run_data_id = 17619
--and re.filename like '%AIDP.PA2733EH.VNDR%'
--and rownum &lt; =100
)mjf
where mjf.job_id = ?
order by 1</sql>
