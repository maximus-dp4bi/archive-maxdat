    <sql>SELECT  ostg.INCIDENT_ID NEW_INCIDENT_ID
, 'Y' as con_yes 
, 'N' as con_no
, SYSDATE as IN_SYSTEM_DATE
, to_char(null) as IN_NULL_CHAR
, 'Complete' as IN_COMPLETE
, 'Incident Withdrawn' as IN_CANCEL_REASON
, 'Normal' as IN_CANCEL_METHOD
, ostg.STAGE_DONE_DT NEW_STAGE_DONE_DT
, ostg.COMPLETE_DT NEW_COMPLETE_DT
, ostg.LAST_UPDATE_BY_DT NEW_LAST_UPDATE_BY_DT
, ostg.LAST_UPDATE_BY_NAME NEW_LAST_UPDATE_BY_NAME
, ostg.INCIDENT_STATUS NEW_INCIDENT_STATUS
, ostg.INCIDENT_STATUS_DT NEW_INCIDENT_STATUS_DT
, ostg.CANCEL_DT NEW_CANCEL_DT
, ostg.INSTANCE_STATUS NEW_INSTANCE_STATUS
, ostg.CANCEL_BY NEW_CANCEL_BY
, ostg.CANCEL_REASON NEW_CANCEL_REASON
, ostg.CANCEL_METHOD NEW_CANCEL_METHOD
, wstg.OLD_INCIDENT_ID
, wstg.OLD_STAGE_DONE_DT
, wstg.OLD_COMPLETE_DT
, wstg.OLD_LAST_UPDATE_BY_DT
, wstg.OLD_LAST_UPDATE_BY_NAME 
, wstg.OLD_INCIDENT_STATUS
, wstg.OLD_INCIDENT_STATUS_DT
, wstg.OLD_CANCEL_DT
, wstg.OLD_INSTANCE_STATUS
, wstg.OLD_UPDATED 
, wstg.OLD_CANCEL_BY
, wstg.OLD_CANCEL_REASON
, wstg.OLD_CANCEL_METHOD
FROM PROCESS_INCIDENTS_OLTP ostg
left outer join (
SELECT INCIDENT_ID AS OLD_INCIDENT_ID,
STAGE_DONE_DT AS OLD_STAGE_DONE_DT,
COMPLETE_DT AS OLD_COMPLETE_DT,
LAST_UPDATE_BY_DT AS OLD_LAST_UPDATE_BY_DT,
LAST_UPDATE_BY_NAME AS OLD_LAST_UPDATE_BY_NAME,
INCIDENT_STATUS AS OLD_INCIDENT_STATUS,
INCIDENT_STATUS_DT AS OLD_INCIDENT_STATUS_DT,
CANCEL_DT AS OLD_CANCEL_DT,
INSTANCE_STATUS AS OLD_INSTANCE_STATUS,
UPDATED AS OLD_UPDATED,
CANCEL_BY AS OLD_CANCEL_BY,
CANCEL_REASON AS OLD_CANCEL_REASON,
CANCEL_METHOD AS OLD_CANCEL_METHOD
FROM PROCESS_INCIDENTS_WIP_BPM
) wstg
on ostg.INCIDENT_ID = wstg.OLD_INCIDENT_ID 
where wstg.OLD_INSTANCE_STATUS='Active'
and wstg.OLD_CANCEL_DT is null




</sql>
