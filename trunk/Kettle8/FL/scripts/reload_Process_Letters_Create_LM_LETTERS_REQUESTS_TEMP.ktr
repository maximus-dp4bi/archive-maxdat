  <step>
    <name>Table input LM_LETTER_IMAGES</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>LETTERS_SOURCE</connection>
    <sql>SELECT 
   	 A.LMLI_ID
    ,A.BFILE_PATH
    ,A.LMREQ_ID
    ,to_date(substr(bfile_path, 34, 10),'YYYY/MM/DD') IMAGE_RECEIVED_DATE
	,'Run_Initialization' as process_name
	,'Load LM_LETTER_IMAGES_TEMP' as job_name
	,sysdate as system_date
	,'LM_LETTER_IMAGES_TEMP' as driver_table_name
	,'CRITICAL' AS ERR_LEVEL
FROM 
	LM_LETTER_IMAGES A
    left outer join LM_LETTER_ATTACHMENTS B on A.LMREQ_ID = B.LMREQ_ID
WHERE
	  trunc(B.INSERT_DATE) > current_date - ${LETTER_LOOKBACK_DAYS} or
