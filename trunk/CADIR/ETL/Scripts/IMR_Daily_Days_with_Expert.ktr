    <sql>SELECT Nvl(expert_license,expert_name)  expert_lic_name
   , to_date(&apos;${gDWRptDate}&apos;,&apos;YYYYMMDD&apos;)  report_date
   , COUNT(*)                           total
   , Round(AVG(to_date(&apos;${gDWRptDate}&apos;,&apos;YYYYMMDD&apos;)-date_sent),2)    avg_days_with_expert
   , Round(Median(to_date(&apos;${gDWRptDate}&apos;,&apos;YYYYMMDD&apos;)-date_sent),2) median_days_with_expert
   , MIN(date_sent)                     min_date_sent
   , MAX(date_sent)                     max_date_sent
FROM  (SELECT (SELECT EN2.c_value 
              FROM  t_imr_expert_review     ER2
                 ,  t_expert_specialty_type EN2
              WHERE I.id = ER2.id_parent 
               AND EN2.id = ER2.c_expert_specialty 
                AND EN2.c_value IS NOT NULL
                AND ROWNUM = 1 
             ) expert_specialty
          ,  (SELECT EN2.c_name 
              FROM  t_imr_expert_review   ER2
                 ,  t_imr_expert_license  EN2
              WHERE I.id = ER2.id_parent 
                AND EN2.c_code = ER2.c_expert_license 
                AND EN2.c_name IS NOT NULL
                AND ROWNUM = 1
             ) expert_license
          ,  (SELECT EN2.c_name
              FROM  t_imr_expert_review ER2
                 ,  t_imr_expert_name   EN2
              WHERE I.id = ER2.id_parent 
                AND EN2.id = ER2.c_expert_name 
                AND EN2.c_name IS NOT NULL
                AND ROWNUM = 1
             ) expert_name
          ,  I.c_case_number
          ,  S.c_name status
          ,  (SELECT MIN(ER2.c_date_sent)
              FROM  t_imr_expert_review ER2
              WHERE I.id = ER2.id_parent
             ) date_sent
          ,  (SELECT MIN(ER2.c_date_clarification_returned)
              FROM  t_imr_expert_review ER2
              WHERE I.id = ER2.id_parent
             ) date_recd
       FROM t_imr                     I
       LEFT OUTER JOIN t_imr_status   S ON I.c_status = s.id 
       LEFT OUTER JOIN etk_assignment A ON I.ID = A.tracking_id
       LEFT OUTER JOIN etk_role       R ON R.role_id = A.role_id 
       WHERE data_object_key=&apos;T_IMR&apos;
         AND is_current = 1
         AND Upper(S.c_name) NOT LIKE &apos;%CLOSED%&apos;
         AND subject_type = 2
         AND R.name IN (&apos;Pending Expert Review Queue&apos;,&apos;Expert Reviewers Queue&apos;)
      ) expert
GROUP BY Nvl(expert_license,expert_name)
   ,  to_date(&apos;${gDWRptDate}&apos;,&apos;YYYYMMDD&apos;)
ORDER BY 3 DESC</sql>
