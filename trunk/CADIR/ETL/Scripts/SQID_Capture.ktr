t.Priority_ Task_priority,
Case When r.Value = &apos;code.application&apos; Then &apos;caseCreation&apos;
	 When r.Value = &apos;code.rfiReceivedDocument&apos; Then &apos;RFIRD&apos;
     When r.Workflow_Queue is null Then r.VALUE
     Else r.Workflow_Queue End Task_type,  
t.Create_ Start_Date,
t.End_ End_Date,
D.Updated_Date Status_Date,
D.Release_Date Release_Date,
D.Received_Date Received_Date,
T.Assignee_ Owner_name,
D.MAS_CASE_ID Case_ID,
D.Execution_ID_ execution_id,
