  <step>
    <name>Last Successful Run Date</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>&#47;*
SQL fetches the  last successful date so as to aviod missing IDs
*&#47;
select job_start_date as last_successful_run_date,
       rnum
from       
(
select 
       
       job_start_date,
       RANK() OVER (partition by 1 order by job_id desc)     as rnum
from corp_etl_job_statistics
where job_status_cd = &apos;COMPLETED&apos;
and job_name = &apos;CRS_IMR_DECISIONS_RUNALL&apos;
)
where rnum = ${g_IMR_NUM_DECISION_RUNS_LOOK_BACK}</sql>
