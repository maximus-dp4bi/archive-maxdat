 QCM.VALUE QC_MODULE_NAME,
 CLASSM.VALUE CLASSIFICATION_MODULE_NAME,
 RECM.VALUE RECOGNITION_MODULE_NAME,
 VALM.VALUE VALIDATION_MODULE_NAME,
 PDFM.VALUE PDF_MODULE_NAME,
 REPM.VALUE REPORT_MODULE_NAME,
 EXPM.VALUE EXPORT_MODULE_NAME,
 BCL.VALUE BATCH_CLASS_LIST


FROM CORP_ETL_CONTROL RSD,
 CORP_ETL_CONTROL CSD,
 CORP_ETL_CONTROL ASD,
 CORP_ETL_CONTROL RSL,
 CORP_ETL_CONTROL CSL,
 CORP_ETL_CONTROL ASL,
 CORP_ETL_CONTROL SCANM,
 CORP_ETL_CONTROL QCM,
 CORP_ETL_CONTROL CLASSM,
 CORP_ETL_CONTROL RECM,
 CORP_ETL_CONTROL VALM,
 CORP_ETL_CONTROL PDFM,
 CORP_ETL_CONTROL REPM,
 CORP_ETL_CONTROL EXPM,
(SELECT SUBSTR (SYS_CONNECT_BY_PATH (value , &apos;&apos;&apos;,&apos;&apos;&apos;), 4) VALUE
      FROM (SELECT value , ROW_NUMBER () OVER (ORDER BY value ) rn,
                   COUNT (*) OVER () cnt
              FROM CORP_ETL_CONTROL where NAME like &apos;MFB_BATCH_CLASS_LIST%&apos;)
     WHERE rn = cnt
START WITH rn = 1
CONNECT BY rn = PRIOR rn + 1
) BCL
WHERE RSD.NAME=&apos;MFB_REMOTE_LAST_UPDATE_DATE&apos;
 AND CSD.NAME=&apos;MFB_CENTRAL_LAST_UPDATE_DATE&apos;
 AND ASD.NAME=&apos;MFB_ARS_LAST_UPDATE_DATE&apos;
 AND RSL.NAME=&apos;MFB_REMOTE_LOOKBACK_DAYS&apos;
 AND CSL.NAME=&apos;MFB_CENTRAL_LOOKBACK_DAYS&apos;
 AND ASL.NAME=&apos;MFB_ARS_LOOKBACK_DAYS&apos;
 AND SCANM.NAME=&apos;MFB_SCAN_MODULE_NAME&apos;
 AND QCM.NAME=&apos;MFB_QC_MODULE_NAME&apos;
 AND CLASSM.NAME=&apos;MFB_CLASSIFICATION_MODULE_NAME&apos;
 AND RECM.NAME=&apos;MFB_RECOGNITION_MODULE_NAME&apos;
 AND VALM.NAME=&apos;MFB_VALIDATION_MODULE_NAME&apos;
 AND PDFM.NAME=&apos;MFB_PDF_MODULE_NAME&apos;
 AND REPM.NAME=&apos;MFB_REPORT_MODULE_NAME&apos;
 AND EXPM.NAME=&apos;MFB_EXPORT_MODULE_NAME&apos;

