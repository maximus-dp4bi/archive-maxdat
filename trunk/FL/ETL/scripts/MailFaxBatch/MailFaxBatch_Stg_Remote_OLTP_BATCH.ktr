         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT  sb.ExternalBatchID &quot;ExternalBatchID&quot; &quot;BatchGUID&quot;,
 sb.ExternalBatchID &quot;ExternalBatchID&quot;,
 sb.BatchName &quot;BatchName&quot;,
 sb.CreationStationID &quot;CreationStationID&quot;,		 	
 sb.CreationUserName &quot;CreationUserName&quot;,			
 sb.CreationUserID &quot;CreationUserID&quot;,			
 sb.BatchClass &quot;BatchClass&quot;,			
 sb.BatchClassDes &quot;BatchClassDescription&quot;,	
 sb.BATCHTYPE,		
 sbm.StartDateTime &quot;StartDateTime&quot;,
 (case when exists(select 1 from CORP_ETL_MFB_SBM_OLTP sbm2 where sbm2.batchguid=sb.batchguid and sbm2.PagesScanned&gt;0) then &apos;Y&apos; else &apos;N&apos; end) &quot;PagesScanned&quot;,
 (case when exists(select 1 from CORP_ETL_MFB_SBM_OLTP sbm2 where sbm2.batchguid=sb.batchguid and sbm2.DocumentsCreated&gt;0) then &apos;Y&apos; else &apos;N&apos; end) &quot;DocumentsCreated&quot;,
 (case when exists(select 1 from CORP_ETL_MFB_SBM_OLTP sbm2 where sbm2.batchguid=sb.batchguid and sbm2.DocumentsDeleted&gt;0) then &apos;Y&apos; else &apos;N&apos; end) &quot;DocumentsDeleted&quot;,
 (case when exists(select 1 from CORP_ETL_MFB_SBM_OLTP sbm2 where sbm2.batchguid=sb.batchguid and sbm2.PagesReplaced&gt;0) then &apos;Y&apos; else &apos;N&apos; end) &quot;PagesReplaced&quot;,
 (case when exists(select 1 from CORP_ETL_MFB_SBM_OLTP sbm2 where sbm2.batchguid=sb.batchguid and sbm2.PagesDeleted&gt;0) then &apos;Y&apos; else &apos;N&apos; end) &quot;PagesDeleted&quot;,
 &apos;Active&apos; INSTANCE_STATUS,
 sbm.Priority &quot;Priority&quot;,
 sbm.Deleted &quot;Deleted&quot;,
 sysdate,
 sb.source_server
FROM
 (SELECT sb.BatchGUID,
  min(sbm.StartDateTime) StartDateTime
 FROM CORP_ETL_MFB_SBM_OLTP sbm, 
  CORP_ETL_MFB_SB_OLTP sb 
 WHERE sb.BatchGUID=sbm.BatchGUID
 AND sbm.ModuleLaunchID in (select ModuleLaunchID from CORP_ETL_MFB_SML_OLTP where ModuleName=&apos;${SCAN_MODULE_NAME}&apos;)
 GROUP BY sb.BatchGUID) BatchFirstScanned,
 CORP_ETL_MFB_SBM_OLTP sbm,
 CORP_ETL_MFB_SB_OLTP sb
WHERE sb.BatchGUID=sbm.BatchGUID
 AND sbm.ModuleLaunchID in (select ModuleLaunchID from CORP_ETL_MFB_SML_OLTP where ModuleName=&apos;${SCAN_MODULE_NAME}&apos;)
 AND sb.BatchGUID=BatchFirstScanned.BatchGUID
 AND sbm.StartDateTime=BatchFirstScanned.StartDateTime
</sql>
