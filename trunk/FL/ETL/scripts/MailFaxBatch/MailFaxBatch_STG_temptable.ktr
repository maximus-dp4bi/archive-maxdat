    <connection>KOFAX_CENTRAL</connection>
    <sql>SELECT distinct sb.&quot;BatchGUID&quot;,
       sb.&quot;ExternalBatchID&quot; BatchID, 
	   &quot;cntr&quot;,
	   &quot;ScanOp&quot;
      ,&quot;IndexOp&quot;
      ,&quot;IndexOp2&quot;
      ,&quot;DocType&quot;
      ,isnull(sb.&quot;batchName&quot;, kfd.&quot;batchName&quot;) batchname
      ,isnull(sb.&quot;BatchClass&quot;, kfd.&quot;BatchClass&quot;) batchclass
      ,&quot;ECN&quot;
      ,&quot;DCN&quot;
      ,&quot;PageCount&quot;
      ,&quot;ExportDate&quot;
      ,&quot;KFDocID&quot;
       ,&quot;Payor&quot;
      ,&quot;BagNum&quot;
      ,&quot;FINum&quot;
      ,&quot;Amount&quot;
FROM &quot;FLCCOFAXPRD&quot;..&quot;kfDocData&quot; kfd
left outer join &quot;FLCCOFAXPRD&quot;..&quot;StatsBatch&quot; sb 
  on kfd.batchName =sb.BatchName and kfd.batchName not in (&apos;Web Import QA&apos;)
where cntr &gt; ${NEW_MAX_KDOC_CNTR_ID}
order by ExternalBatchID, cntr, ecn</sql>
    <limit>0</limit>
