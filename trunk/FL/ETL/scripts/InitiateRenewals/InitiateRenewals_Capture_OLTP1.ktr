    <rowlimit>1</rowlimit>
    <sql>select  count(co.coverage_id)                   COUNT_OF_ELIGIBLE_MEMBERS,
        add_months(co.COVERAGE_BEGIN_DATE, 12)  CALCULATED_RENEW_DATE,
        ER.ELIGIBILITY_RESULT_ID                ELIGIBILITY_RESULT_ID,
        er.schip_status                         elig_status  
from coverage co 
join eligibility_result er on co.ELIGIBILITY_RESULT_ID = ER.ELIGIBILITY_RESULT_ID
where co.account_membership_id = ?
and er.ELIGIBILITY_RESULT_ID = 
(select max(ELIGIBILITY_RESULT_ID) from eligibility_result where account_membership_id = ?)
group by co.COVERAGE_BEGIN_DATE, er.ELIGIBILITY_RESULT_ID, CO.account_membership_id, er.schip_status  
order by  calculated_renew_date desc, er.ELIGIBILITY_RESULT_ID desc</sql>
