    <sql>SELECT R.LMREQ_ID 			  AS LETTER_REQ_ID,
NVL(D.DRIVER_TYPE, &apos;ACCOUNT&apos;) AS DRIVER_TYPE,
CASE WHEN D.DRIVER_TYPE = &apos;ACCOUNT&apos; THEN to_number(R.DRIVER_PRIMARY_KEY) END AS ACCOUNT_ID,
CASE WHEN D.DRIVER_TYPE = &apos;MEMBER&apos; THEN to_number(R.DRIVER_PRIMARY_KEY) END AS MEMBER_ID,
CASE WHEN D.DRIVER_TYPE = &apos;APP_ID&apos; THEN to_number(R.DRIVER_PRIMARY_KEY) END AS APP_ID,
D.NAME 			              AS LETTER_CODE,
D.DESCRIPTION                 AS TYPE
FROM LM_LETTER_REQUESTS_TEMP R
LEFT OUTER JOIN LM_LETTER_DEFINITIONS_TMP D ON R.LMDEF_ID = D.LMDEF_ID 
WHERE R.LMREQ_ID &gt; ${MAX_LETTER_REQID}</sql>
