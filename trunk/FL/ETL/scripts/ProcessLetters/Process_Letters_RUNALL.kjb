    <entry>
      <name>Update calculated values</name>
      <description/>
      <type>SQL</type>
      <sql>UPDATE 
FLHK_ETL_PROCESS_LETTERS 
SET 
AGE_IN_BUSINESS_DAYS =  business_days_between(CREATE_DATE, CURRENT_DATE), 
AGE_IN_CALENDAR_DAYS = round(CURRENT_DATE - create_date,0), 
STATUS_AGE_IN_BUS_DAYS =  business_days_between(STATUS_DATE, CURRENT_DATE), 
STATUS_AGE_IN_CAL_DAYS = round(CURRENT_DATE - STATUS_DATE,0) 
WHERE STAGE_DONE_DATE is null;
COMMIT;

delete from FLHK_ETL_PROCESS_LETTERS where stage_done_date &lt; current_date -90;
commit;</sql>
