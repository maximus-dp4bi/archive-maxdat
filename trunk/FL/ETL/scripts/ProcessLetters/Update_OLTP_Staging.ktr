    <connection>MAXDAT</connection>
    <sql>SELECT  
A.LMREQ_ID,
CASE when A.VALUE = &apos;en&apos; then &apos;ENGLISH&apos;
     when A.VALUE = &apos;es&apos; then &apos;SPANISH&apos;
     when A.VALUE = &apos;ht&apos; then &apos;HAITIAN_CREOLE&apos;
     else &apos;ENGLISH&apos; 
end  LANGUAGE
FROM  LM_VARIABLE_VALUES_TEMP A
where lmreq_id &gt; ${MAX_LETTER_REQID}

</sql>
