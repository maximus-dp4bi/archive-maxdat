  <step>
    <name>Table input - EndLetterId</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>SELECT 
	MAX(A.LMREQ_ID) AS NEW_END_LETTER_LMREQID
FROM
	LM_LETTER_REQUESTS A,
	LM_LETTER_DEFINITIONS B 
WHERE 
	A.LMDEF_ID = B.LMDEF_ID (+)
AND	
	(
		(
		A.STATUS != &apos;MLD&apos; 
		)
		OR
		(
		A.STATUS IN (&apos;MLD&apos;) AND TRUNC(STATUS_DATE) &gt; &apos;01-OCT-2013&apos;
		)
	) 
AND 
	A.LMREQ_ID NOT IN 
		(SELECT LMREQ_ID FROM LM_LETTER_REQUESTS WHERE STATUS=&apos;CANCEL&apos; AND TRUNC(STATUS_DATE)=&apos;27-SEP-2013&apos;)
AND
	A.SOURCE != &apos;MIGRATION&apos;</sql>
    <limit>1</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
