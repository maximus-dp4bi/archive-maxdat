    <sql>SELECT WRSTATUS_ID,
TASK_ID,
WRQ_ID,
WRT_ID,
to_number(DRIVER_KEY, &apos;99999999999&apos;) DRIVER_KEY,
STATUS,
STATUS_DATE,
STATUS_REASON,
PRIORITY,
CONTEXT,
ASSIGNED_TO,
PARENT_TASK_ID,
CREATED_BY,
CREATED_DATE,
UPDATED_BY,
UPDATED_DATE,
DUE_DATE,
WRRULE_ID,
USER_ROLE,	
&apos;Member Eligibility&apos; as process_name,
&apos;Load WR_STATUSES_TMP&apos; as job_name,
sysdate as system_date,
&apos;WR_STATUSES&apos; as driver_table_name,
&apos;CRITICAL&apos; AS ERR_LEVEL	
FROM WR_STATUSES WR
WHERE WR.WRT_id  = 55
and wr.status = &apos;RESOLVED&apos;
and created_by &lt;&gt; &apos;MIGRATION&apos;
and TASK_ID &gt; ?</sql>
