    <connection>TASK</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT       WS.TASK_ID,
             WS.PARENT_TASK_ID,
             WS.WRSTATUS_ID,
             WT.DESCRIPTION    TASK_TYPE,
             WS.STATUS         TASK_STATUS,
             WS.STATUS_DATE,
             WS.STATUS_REASON,
             WS.CREATED_DATE, 
             WS.CREATED_BY     CREATED_BY_NAME,
             WS.PRIORITY,
             WS.DUE_DATE,  
             WS.ASSIGNED_TO,  
             WQ.DESCRIPTION    QUEUE,
             WS.CONTEXT,
             NVL(WT.DRIVER, 'DOCUMENT')         DRIVER_TYPE,
             WS.DRIVER_KEY,
             WA.CATEGORY       ACTION_CATEGORY,
             WR.DESCRIPTION    RULE_EXECUTED, 
             WR.ROLE_CODE      ROLE_NAME,
             WS.UPDATED_DATE   LAST_UPDATE_DATE,
             WS.UPDATED_BY     LAST_UPDATED_BY_NAME,
             WS.STATUS_DATE    ASSD_COMPLETE_TASK,  
             decode(WS.STATUS, 'RESOLVED',     'COMPLETE',
                               'UNASSIGNED',   'ACTIVE',
                               'ON_HOLD',      'ACTIVE',
                               'BEING_WORKED', 'ACTIVE',
                               NULL,           'ACTIVE') INSTANCE_STATUS                            
FROM WR_STATUSES WS
     left outer join WR_TYPES WT
       on WS.WRT_ID  = WT.WRT_ID
     left outer join WR_QUEUES WQ 
       on WS.WRQ_ID = WQ.WRQ_ID
     left outer join WR_RULES WR 
       on WS.WRRULE_ID = WR.WRRULE_ID 
     left outer join WR_ACTIONS WA
       on WR.WRACTION_ID = WA.WRACTION_ID
WHERE  WS.TASK_ID = ?
  AND  WS.WRSTATUS_ID = (SELECT MAX(wrstatus_id) FROM WR_STATUSES W2 
                         where WS.TASK_ID = W2.TASK_ID)</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
