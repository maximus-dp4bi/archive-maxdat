    <sql>select nvl(a.account_id, 0)                                          ACCOUNT_ID,
       nvl(a.account_number, 0)                                      ACCOUNT_NUMBER,
       &apos;N&apos;                                                           ASF_RVW_ELIG,
       &apos;N&apos;                                                           ASF_RECEIVE_PROCESS_MI
from coverage_request cr
     join account_membership  am on cr.account_membership_id = am.account_membership_id
     join account a              on am.account_id = a.account_id
where cr.COVERAGE_REQUEST_ID = ?
