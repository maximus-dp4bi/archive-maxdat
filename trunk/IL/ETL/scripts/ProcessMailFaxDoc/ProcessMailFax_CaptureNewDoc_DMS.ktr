  <step>
    <name>Table input - Documents DMS Source</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DMS</connection>
    <sql>Select  &#47;*+ PARALLEL(a,8) PARALLEL(ad,8) *&#47;
		to_char(d.DCN)							DCN
       ,NVL(d.Creation_date,SYSDATE)		dcn_create_dt
		,SYSDATE							stg_last_update_date
		,&apos;Active&apos;							instance_status
		,NVL(b.name,&apos;Unknown&apos;)				batch_name
		,NVL(decode(b.source ,&apos;SCAN&apos;,&apos;MAIL&apos;,&apos;FAX&apos;,&apos;FAX&apos;),&apos;Unknown&apos;)	batch_channel
		,d.original_dcn						original_dcn
		,d.number_of_pages					document_page_count
		,&apos;Released&apos;							document_status
		,SYSDATE							document_status_dt
		,decode(d.AVAILABILITY_STATUS ,&apos;RESCANNED&apos;,&apos;Y&apos;,&apos;N&apos;)	rescanned
		,&apos;N&apos;								dcn_jeopardy_status
		,sysdate							dcn_jeopardy_status_dt
		,&apos;Not Processed&apos;					dcn_timeliness_status

From	Documents d
		,Batches b
		,ACTIVITY_DETAILS ad
		,ACTIVITIES a 
Where	d.DCN &gt; ${LastDCN}
and		d.BATCH_ID = b.BATCH_ID 
and		ad.DCN = d.DCN 
and		a.ACTIVITY_ID = ad.ACTIVITY_ID
and		a.CREATION_DATE = ( select max(aa.CREATION_DATE) 
                           from   dms.activities aa
                                , dms.activity_details ad1 
                           where  d.dcn = ad1.dcn 
                           and    aa.activity_id = ad1.activity_id
                           and    aa.ACTIVITY_TYPE in( &apos;ENVELOPERELEASE&apos; , &apos;RELEASE&apos; ))
and		d.DCN NOT IN (SELECT DISTINCT(ad.DCN)
                     FROM   dms.ACTIVITY_DETAILS ad
                          , dms.ACTIVITIES a
                     WHERE  a.ACTIVITY_ID = ad.ACTIVITY_ID  
                     and    a.activity_type = &apos;INACTIVE&apos;)	
</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
