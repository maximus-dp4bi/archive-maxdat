    <sql>&#47;*
If the MANAGEENRL_RUN_RUNALL_TODAY global control has a value other than current date, then run ProcessManageEnroll_RUNALL.kjb
*&#47;
select 
       --to_date(value,&apos;yyyy&#47;mm&#47;dd&apos;)  as MANAGEENRL_RUN_RUNALL_TODAY,
	   --trunc(sysdate) as CURR_DATE,
       &apos;Y&apos;            as in_yes,
       case when nvl(to_date(value,&apos;yyyy&#47;mm&#47;dd&apos;), to_date(trunc(sysdate)-1,&apos;yyyy&#47;mm&#47;dd&apos;)) &lt;&gt; trunc(sysdate) then &apos;Y&apos; else &apos;N&apos; end as check_run
  from corp_etl_control
 where name = &apos;MANAGEENRL_RUN_RUNALL_TODAY&apos;</sql>
