    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT * FROM (SELECT ROWNUM AS ROWED,
FILE_NAME,
FILE_TYPE,
FILE_SOURCE,
PLAN_ID,
FILE_DESTINATION,
RESPONSE_FILE_REQ,
RESPONSE_FILE_NAME
FROM (select FILE_NAME,
FILE_TYPE,
FILE_SOURCE,
PLAN_ID,
FILE_DESTINATION,
RESPONSE_FILE_REQ,
RESPONSE_FILE_NAME from (
SELECT FILE_NAME,
FILE_TYPE,
CASE WHEN instr(?,upper(file_name)) &gt;0
THEN FILE_NAME 
END AS FILE_NAME1,
CASE WHEN substr(substr(?, instr(?,&apos;&#47;&apos;,-1)+1),1,2) = upper(filename_beg)
and 
instr(?,upper(filename_cont)) &gt;0 
THEN FILE_NAME 
END AS FILE_NAME2,
CASE WHEN instr(?,upper(filename_beg)) &gt;0
and filename_cont is null
THEN FILE_NAME 
END AS FILE_NAME3,
CASE WHEN instr(?,upper(filename_beg)) &gt;0
and instr(?,upper(filename_cont)) &gt;0
THEN FILE_NAME 
END AS FILE_NAME4,
FILE_SOURCE,
PLAN_ID,
FILE_DESTINATION,
RESPONSE_FILE_REQ,
RESPONSE_FILE_NAME
 FROM CORP_MJ_ETL_FILE_LKUP)
where FILE_NAME1 is not null or FILE_NAME2 is not null or FILE_NAME3 is not null or FILE_NAME4 is not null


UNION ALL
SELECT FILE_NAME,
FILE_TYPE,
FILE_SOURCE,
PLAN_ID,
FILE_DESTINATION,
RESPONSE_FILE_REQ,
RESPONSE_FILE_NAME
FROM CORP_MJ_ETL_FILE_LKUP
WHERE FILE_NAME = &apos;Unknown&apos;
)
)
WHERE ROWED = 1</sql>
