    <type>ORACLE</type>
    <access>Native</access>
    <database>${DB_DMS_NAME}</database>
    <port>${DB_DMS_PORT}</port>
    <username>${DB_DMS_USER}</username>
    <password>${DB_DMS_PASSWORD}</password>
    <servername/>
    <data_tablespace/>
    <index_tablespace/>
