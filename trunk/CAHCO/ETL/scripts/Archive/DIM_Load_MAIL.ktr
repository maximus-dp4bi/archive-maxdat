    <sql>SELECT  CreatedOn mail_sent_date
        ,CASE WHEN ? IS NOT NULL THEN CreatedOn ELSE null END packet_sent_date
FROM (SELECT MailID,ms.CreatedOn,ROW_NUMBER() OVER (PARTITION BY MailID ORDER BY MailTransitionID DESC, CreatedOn DESC) mrn
