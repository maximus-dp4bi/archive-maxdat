  <step>
    <name>Table input -MAIL from EB</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>SELECT ml.MailId mail_id   
       ,ml.MailTypeID mail_type_id
       ,ml.CreatedOn request_date       
       ,ml.LanguageCode language_code
       ,ml.CaseheadBeneID casehead_client_number
       ,ml.CountyCode mail_county_code
       ,ml.LetterDate mail_letter_date
       ,cf.CaseRowID case_number
       ,ml.CreatedOn record_date
       ,ml.CreatedBy record_name
       ,ml.ModifiedOn modified_date
       ,ml.ModifiedBy modified_name
       ,CASE WHEN ml.CustomMailingIndicator = &apos;AnnualBDE&apos; THEN &apos;BD&apos;
             WHEN ml.CustomMailingIndicator = &apos;90DayBDE&apos; THEN &apos;BD90&apos;
        ELSE mt.MailType END mail_type
	  ,CASE WHEN mt.IsPacketRequest = &apos;true&apos; OR mt.MailType IN(${PACKET_TYPE}) THEN mt.MailType ELSE null END packet_type
FROM hcodb.dbo.mail ml (nolock)
  JOIN hcodb.dbo.mailtype mt (nolock) ON ml.MailTypeID = mt.MailTypeID  
  JOIN hcodb.dbo.casefile cf (nolock) ON ml.CaseID = cf.CaseID
WHERE ml.CreatedOn &gt;= CONVERT(datetime,&apos;${HCO_MAIL_CREATE_DATE}&apos;)
  OR ml.ModifiedOn &gt;= CONVERT(datetime,&apos;${HCO_MAIL_UPDATE_DATE}&apos;)


  </sql>
