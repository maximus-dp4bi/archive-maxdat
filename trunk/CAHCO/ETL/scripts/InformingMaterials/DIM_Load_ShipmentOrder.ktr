    <connection>MATERIALORDER_OLTP_SOURCE</connection>
    <sql>SELECT sh.shipment_id
       ,COALESCE(shd.shipment_detail_id,0) shipment_detail_id
	   ,sh.order_id
	   ,shd.order_detail_id
	   ,sh.vendor_id
	   ,sh.enter_by_id
	   ,sh.enter_date
	   ,sh.ship_start_date
	   ,sh.ship_by_date
	   ,sh.scheduled_post_date
	   ,sh.post_date
	   ,sh.process_note
	   ,sh.order_file
	   ,sh.receive_date
	   ,sh.ship_date
	   ,sh.modify_by_id
	   ,sh.modify_date
	   ,sh.actual_id
	   ,shd.material_version_history_id
	   ,shd.amount
	   ,shd.receive_by_id
	   ,shd.receive_amount
FROM dbo.hco_shipment sh (NOLOCK)
  LEFT JOIN dbo.hco_shipment_detail shd (NOLOCK) ON sh.shipment_id = shd.shipment_id
WHERE (sh.modify_date &gt;= CONVERT(datetime,&apos;${HCO_SHIPMENTORDER_ENTER_DATE}&apos;)
   OR sh.enter_date &gt; =  CONVERT(datetime,&apos;${HCO_SHIPMENTORDER_UPDATE_DATE}&apos;) )
