    <sql>SELECT  bad_address_transaction_type,bad_address_record_date,bad_address_flag
FROM (
  SELECT ba.TransactionType bad_address_transaction_type,
      ba.RecordCreateDate bad_address_record_date,
      CASE WHEN ba.DeIdentificationCIN is not null THEN &apos;Y&apos; ELSE &apos;N&apos; END bad_address_flag,
      ROW_NUMBER() OVER(ORDER BY DimBadAddressId) prn
  FROM dbo.DimPacketMailing (nolock) lm
    LEFT JOIN dbo.DimBadAddress ba (nolock) ON lm.DCIN = ba.DeIdentificationCIN
  WHERE lm.DimPacketMailingId = ? ) tmp
WHERE prn = 1</sql>
