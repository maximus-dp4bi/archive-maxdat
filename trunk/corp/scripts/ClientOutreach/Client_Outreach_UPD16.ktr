FROM CORP_ETL_CLNT_OUTREACH_WIP_BPM
WHERE INSTANCE_STATUS = &apos;Active&apos;
AND CANCEL_DT IS NULL
AND ASSD_TERMINATION_TIMER IS NOT NULL 
AND ASED_TERMINATION_TIMER IS NULL
)BPM
ON OLTP.CECO_ID = BPM.CECO_ID</sql>
