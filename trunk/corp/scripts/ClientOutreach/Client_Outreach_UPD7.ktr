    <connection>MAXDAT</connection>
    <sql>SELECT 
&apos;Y&apos;		IN_YES
,&apos;N&apos;	IN_NO
,&apos;Complete&apos; FINAL_STATUS
,${OUT_SUCCESS} AS SUCCESS
,${OUT_UNSUCCESS} AS UNSUCCESS
,OLTP.OUTREACH_ID
,OLTP.OUTREACH_STATUS
,OLTP.FINAL_WAIT_IND
,OLTP.OUTREACH_STEP3_TYPE
,OLTP.LAST_UPDATE_DT
,OLTP.LAST_UPDATE_BY
,BPM.*
FROM (SELECT  OLTP.CECO_ID,
         OLTP.OUTREACH_ID,
         OLTP.HIST_OUTREACH_STATUS OUTREACH_STATUS,
         OLTP.OUTREACH_STEP3_TYPE,         
         OLTP.FINAL_WAIT_IND,
         OLTP.OUTREACH_HIST_STAT_DT LAST_UPDATE_DT,
         OLTP.OUTREACH_HIST_STAT_BY LAST_UPDATE_BY
      FROM CORP_ETL_CLNT_OUTREACH_OLTP OLTP
      WHERE OLTP.HIST_OUTREACH_STATUS IN(${OUT_SUCCESS},${OUT_UNSUCCESS})   
      AND OLTP.INCIDENT_HEADER_STAT_HIST_ID = (SELECT MAX(INCIDENT_HEADER_STAT_HIST_ID)
                                               FROM CORP_ETL_CLNT_OUTREACH_OLTP oltp1
                                               WHERE oltp.outreach_id = oltp1.outreach_id
                                               AND oltp1.HIST_OUTREACH_STATUS IN(${OUT_SUCCESS},${OUT_UNSUCCESS}) )
     UNION
	  SELECT  OLTP.CECO_ID,
         OLTP.OUTREACH_ID,
         OLTP.HIST_OUTREACH_STATUS OUTREACH_STATUS,
         OLTP.OUTREACH_STEP3_TYPE,         
         OLTP.FINAL_WAIT_IND,
         OLTP.LAST_UPDATE_DT,
         OLTP.LAST_UPDATE_BY
      FROM CORP_ETL_CLNT_OUTREACH_OLTP OLTP
      WHERE OLTP.OUTREACH_STEP3_TYPE IS NOT NULL  -- for those that are not in Succes&#47;Unsuccess status, only those that have step3 matters
      AND OLTP.INCIDENT_HEADER_STAT_HIST_ID = (SELECT MAX(INCIDENT_HEADER_STAT_HIST_ID)
                                                 FROM CORP_ETL_CLNT_OUTREACH_OLTP oltp1
                                                 WHERE oltp.outreach_id = oltp1.outreach_id ) ) OLTP
JOIN 
(
SELECT CECO_ID	CECO_ID
,OUTREACH_ID	O_OUTREACH_ID
,TRACKING_NUMBER	O_TRACKING_NUMBER
,OUTREACH_REQ_CATEGORY	O_OUTREACH_REQ_CATEGORY
,OUTREACH_REQ_TYPE	O_OUTREACH_REQ_TYPE
,COMPLETE_DT O_COMPLETE_DT
,GWF_INVALID	O_GWF_INVALID
,GWF_STEP3_REQUIRED O_GWF_STEP3_REQUIRED
,ASSD_OUTREACH_STEP3 O_ASSD_OUTREACH_STEP3
,GWF_UNSUCCESSFUL O_GWF_UNSUCCESSFUL
,GWF_FINAL_WAIT O_GWF_FINAL_WAIT
,ASSD_TERMINATION_TIMER O_ASSD_TERMINATION_TIMER
,ASED_PERFORM_OUTREACH O_ASED_PERFORM_OUTREACH
,ASPB_PERFORM_OUTREACH O_ASPB_PERFORM_OUTREACH
,ASF_PERFORM_OUTREACH O_ASF_PERFORM_OUTREACH
,ASED_OUTREACH_STEP2 O_ASED_OUTREACH_STEP2
--,UPDATED O_UPDATED
,&apos;N&apos; O_UPDATED
FROM CORP_ETL_CLNT_OUTREACH_WIP_BPM
WHERE INSTANCE_STATUS = &apos;Active&apos;
AND CANCEL_DT IS NULL
AND ASED_OUTREACH_STEP2 IS NOT NULL 
AND GWF_STEP3_REQUIRED IS NULL
)BPM
ON OLTP.CECO_ID = BPM.CECO_ID</sql>
