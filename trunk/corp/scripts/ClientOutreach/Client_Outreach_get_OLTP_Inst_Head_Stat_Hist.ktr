    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>select 
incident_header_id,hx_date,hx_create_by
from (
SELECT 
hx.incident_header_id,
  hx.hx_date,
  RANK() OVER (PARTITION BY hx.incident_header_id ORDER BY hx_date desc) CREAT,
--  DECODE(S.LAST_NAME,NULL,NULL,S.LAST_NAME
--  ||&apos;,&apos;
--  ||S.FIRST_NAME) AS hx_create_by
  S.last_name || &apos;,&apos; || S.first_name AS hx_create_by

FROM
  (SELECT 
    incident_header_id,
    MAX(create_ts) AS hx_date,
    CREATED_BY
  FROM INCIDENT_HEADER_STAT_HIST_STG
  WHERE incident_header_id = ?
  and status_cd           IN (&apos;OUTREACH_REQUEST_INITIATED&apos;,&apos;OUTREACH_INVALID_REQUEST&apos;)
  GROUP BY incident_header_id, CREATED_BY
  ) hx
LEFT OUTER JOIN STAFF_STG S
ON hx.CREATED_BY = to_char(S.STAFF_ID)
)
where creat =1
and incident_header_id =  ?

</sql>
