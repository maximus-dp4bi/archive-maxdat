    <sql>with handle_time as (
select round((task_end-task_start),5) handle_time from rt_actuals
WHERE trunc(task_start)=trunc(?)
and (?) like &apos;%&apos; || event_id || &apos;%&apos; )
select avg(handle_time) as ACTL_HANDLE_TIME_AVG ,min(handle_time) as ACTL_HANDLE_TIME_MIN,
max(handle_time)as ACTL_HANDLE_TIME_MAX,stddev(handle_time) as ACTL_HANDLE_TIME_SD, median(handle_time) as ACTL_HANDLE_TIME_MEDIAN, (sum(handle_time)*24) as ACTL_STAFF_HOURS from handle_time</sql>
