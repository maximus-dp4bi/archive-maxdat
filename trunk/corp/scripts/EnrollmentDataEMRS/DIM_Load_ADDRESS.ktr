       ,addr.created_by record_name
       ,addr.last_update_date modified_date
       ,addr.last_updated_by modified_name
       ,TO_CHAR(addr.last_update_date,&apos;HH24MI&apos;) modified_time
       ,addr_begin_date source_addr_begin_date
       ,addr_end_date source_addr_end_date
FROM address addr
  INNER JOIN enum_address_type eat
  ON addr.addr_type_cd = eat.value
WHERE (addr.creation_date &gt;= TO_DATE(&apos;${ETL_RUN_DATE}&apos;,&apos;MM&#47;DD&#47;YYYY&apos;)
  OR addr.last_update_date &gt;= TO_DATE(&apos;${ETL_RUN_DATE}&apos;,&apos;MM&#47;DD&#47;YYYY&apos;))
  </sql>
