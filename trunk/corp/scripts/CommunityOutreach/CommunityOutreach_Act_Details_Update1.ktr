    <sql>select * from (
select 
BPM.acty_det_rec_id,
BPM.acty_det_rec_type,
CASE WHEN NVL(acty_det_rec_value,&apos;X&apos;) &lt;&gt;  NVL(O_acty_det_rec_value,&apos;X&apos;)
     THEN acty_det_rec_value
     ELSE O_acty_det_rec_value
END AS UPD1_2_1020_acty_det_rec_value,
CASE WHEN NVL(acty_det_rec_create_dt,&apos;1-JAN-1111&apos;) &lt;&gt;  NVL(O_acty_det_rec_create_dt,&apos;1-JAN-1111&apos;)
     THEN acty_det_rec_create_dt
     ELSE O_acty_det_rec_create_dt
END AS UPD1_2_1030_acty_create_dt,
CASE WHEN NVL(acty_det_rec_create_by,&apos;X&apos;) &lt;&gt;  NVL(O_acty_det_rec_create_by,&apos;X&apos;)
     THEN acty_det_rec_create_by
     ELSE O_acty_det_rec_create_by
END AS UPD1_2_1040_acty_create_by,
CASE WHEN NVL(acty_det_rec_update_dt,&apos;1-JAN-1111&apos;) &lt;&gt;  NVL(O_acty_det_rec_update_dt,&apos;1-JAN-1111&apos;)
     THEN acty_det_rec_update_dt
     ELSE O_acty_det_rec_update_dt
END AS UPD1_2_1050_acty_update_dt,
CASE WHEN NVL(acty_det_rec_update_by,&apos;X&apos;) &lt;&gt;  NVL(O_acty_det_rec_update_by,&apos;X&apos;)
     THEN acty_det_rec_update_by
     ELSE O_acty_det_rec_update_by
END AS UPD1_2_1060_acty_update_by,
CASE WHEN NVL(community_activity_id,0) &lt;&gt;  NVL(O_community_activity_id,0)
     THEN community_activity_id
     ELSE O_community_activity_id
END AS UPD1_2_1070_comm_act_id,
CASE WHEN NVL(outreach_session_id,0) &lt;&gt;  NVL(O_outreach_session_id,0)
     THEN outreach_session_id
     ELSE O_outreach_session_id
END AS UPD1_2_1080_session_id,

CASE WHEN NVL(acty_det_rec_value,&apos;X&apos;) &lt;&gt;  NVL(O_acty_det_rec_value,&apos;X&apos;)
OR NVL(acty_det_rec_create_dt,&apos;1-JAN-1111&apos;) &lt;&gt;  NVL(O_acty_det_rec_create_dt,&apos;1-JAN-1111&apos;)
OR NVL(acty_det_rec_create_by,&apos;X&apos;) &lt;&gt;  NVL(O_acty_det_rec_create_by,&apos;X&apos;)
OR NVL(acty_det_rec_update_dt,&apos;1-JAN-1111&apos;) &lt;&gt;  NVL(O_acty_det_rec_update_dt,&apos;1-JAN-1111&apos;)
OR NVL(acty_det_rec_update_by,&apos;X&apos;) &lt;&gt;  NVL(O_acty_det_rec_update_by,&apos;X&apos;)
OR NVL(community_activity_id,0) &lt;&gt;  NVL(O_community_activity_id,0)
OR NVL(outreach_session_id,0) &lt;&gt;  NVL(O_outreach_session_id,0) 
THEN &apos;Y&apos;
ELSE &apos;N&apos;
END AS UPDATED

FROM COMMUNITY_ACTVTY_DET_CHLD_OLTP OLTP
JOIN (
select
acty_det_rec_id 				
,acty_det_rec_type 			
,acty_det_rec_value 				O_acty_det_rec_value 
,acty_det_rec_create_dt 				O_acty_det_rec_create_dt 
,acty_det_rec_create_by 				O_acty_det_rec_create_by 
,acty_det_rec_update_dt 				O_acty_det_rec_update_dt 
,acty_det_rec_update_by 				O_acty_det_rec_update_by 
,community_activity_id 				O_community_activity_id 
,outreach_session_id 				O_outreach_session_id 
,DELETED_FROM_SOURCE 				O_DELETED_FROM_SOURCE 
from COMMUNITY_ACTVTY_DET_CHLD_WIP
)BPM
ON OLTP.acty_det_rec_id = BPM.acty_det_rec_id
and OLTP.acty_det_rec_type = BPM.acty_det_rec_type
)
where UPDATED = &apos;Y&apos;</sql>
