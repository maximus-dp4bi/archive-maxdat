  case 
    when a_stg.DCN &lt;&gt; d_stg.DCN then &apos;DCN&apos;
    when a_stg.DESCRIPTION &lt;&gt; d_stg.DESCRIPTION then &apos;DESCRIPTION&apos;
    when a_stg.CREATED_BY &lt;&gt; d_stg.CREATED_BY then &apos;CREATED_BY&apos;
    when a_stg.CREATED_TS &lt;&gt; d_stg.CREATED_TS then &apos;CREATED_TS&apos;
    when a_stg.UPDATED_BY &lt;&gt; d_stg.UPDATED_BY then &apos;UPDATED_BY&apos;
    when a_stg.UPDATED_TS &lt;&gt; d_stg.UPDATED_TS then &apos;UPDATED_TS&apos;
    when a_stg.STATUS &lt;&gt; d_stg.STATUS then &apos;STATUS&apos;
    when a_stg.PROCESSED_IND &lt;&gt; d_stg.PROCESSED_IND then &apos;PROCESSED_IND&apos;
    when a_stg.ACCOUNT_ID &lt;&gt; d_stg.ACCOUNT_ID then &apos;ACCOUNT_ID&apos;
    when a_stg.PROCESS_INSTANCE_ID &lt;&gt; d_stg.PROCESS_INSTANCE_ID then &apos;PROCESS_INSTANCE_ID&apos;
    when a_stg.ERRORS &lt;&gt; d_stg.ERRORS then &apos;ERRORS&apos;
    when a_stg.NOTIFICATION_DATE &lt;&gt; d_stg.NOTIFICATION_DATE then &apos;NOTIFICATION_DATE&apos;
    when a_stg.HX_ACCOUNT_ID &lt;&gt; d_stg.HX_ACCOUNT_ID then &apos;HX_ACCOUNT_ID&apos;
    when a_stg.HXID &lt;&gt; d_stg.HXID then &apos;HXID&apos;            
    when a_stg.ACTION_TYPE &lt;&gt; d_stg.ACTION_TYPE then &apos;ACTION_TYPE&apos;    
  end field_changed
from document_notification_stg a_stg
join dly_doc_notification_stg d_stg on a_stg.DOCUMENT_NOTIFICATION_ID = d_stg.DOCUMENT_NOTIFICATION_ID
where 1=1
and (
a_stg.DCN                     &lt;&gt; d_stg.DCN
