or a_stg.PROCESSED_IND        &lt;&gt; d_stg.PROCESSED_IND
or a_stg.ACCOUNT_ID           &lt;&gt; d_stg.ACCOUNT_ID
or a_stg.PROCESS_INSTANCE_ID  &lt;&gt; d_stg.PROCESS_INSTANCE_ID
or a_stg.ERRORS               &lt;&gt; d_stg.ERRORS
