  <step>
    <name>Table input - Reporting periods</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT *
FROM (
SELECT end_date current_reporting_period
       ,end_date period_end_date
       ,start_date period_start_date
       ,m.d_metric_definition_id   
       ,pgd.name task_type       
       ,p.d_reporting_period_id
       ,p.type report_type
       ,pm.d_process_group_metric_id
       ,CASE WHEN type = &apos;WEEKLY&apos; THEN  TO_DATE(&apos;${MW_CURRENT_REPORTING_PERIOD}&apos;,&apos;YYYY&#47;MM&#47;DD&apos;) - (TO_NUMBER(${MW_NUM_WEEKS_LOOKBACK}) * 7)
         ELSE ADD_MONTHS(TO_DATE(&apos;${MW_CURRENT_REPORTING_PERIOD}&apos;,&apos;YYYY&#47;MM&#47;DD&apos;), TO_NUMBER(${MW_NUM_MONTHS_LOOKBACK}) ) END reporting_period_start
       ,&apos;N&apos; is_trend_indicator_calculated
FROM bpm_d_process_group_metric pm
  INNER JOIN bpm_d_metric_definition m
     ON m.d_metric_definition_id = pm.d_metric_definition_id
  INNER JOIN bpm_d_process_group_detail pgd
     ON pgd.d_process_group_detail_id = pm.d_process_group_detail_id
  INNER JOIN bpm_d_process_group pg
     ON pg.d_process_group_id = pgd.d_process_group_id  
  INNER JOIN bpm_d_process_definition pd
    ON pgd.d_process_definition_id = pd.d_process_definition_id
  CROSS JOIN bpm_d_reporting_period p    
WHERE pd.process_name = &apos;MANAGE_WORK&apos;
AND pg.group_name = &apos;TASK_TYPE&apos; ) 
WHERE report_type = ${MW_REPORT_PERIOD_TYPE}
AND current_reporting_period &gt;= reporting_period_start
AND current_reporting_period &lt;= TO_DATE(&apos;${MW_CURRENT_REPORTING_PERIOD}&apos;,&apos;YYYY&#47;MM&#47;DD&apos;)
</sql>
