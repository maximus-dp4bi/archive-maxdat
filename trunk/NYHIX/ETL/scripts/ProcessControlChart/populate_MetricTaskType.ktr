  <step>
    <name>Database join - Get cycle times and percent completed timely</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT SUM(cnt_timely)&#47;SUM(cnt_completed) * 100 task_completed_timely,
       MEDIAN(cycle_time_caldays) median_cyle_time_caldays,
       MAX(cycle_time_caldays) max_cycle_time_caldays,
       ROUND(AVG(cycle_time_caldays),2) avg_cycle_time_caldays,
       MEDIAN(cycle_time_busdays) median_cyle_time_busdays,
       MAX(cycle_time_busdays) max_cyle_time_busdays,
       ROUND(AVG(cycle_time_busdays),2) avg_cyle_time_busdays
FROM(
  SELECT CASE WHEN timeliness_status = &apos;Timely&apos; THEN 1 ELSE 0 END cnt_timely
         ,1 cnt_completed  
         ,TRUNC(complete_date) - TRUNC(vc.create_date) cycle_time_caldays
         ,BPM_COMMON.BUS_DAYS_BETWEEN(TRUNC(vc.create_date),TRUNC(complete_date)) cycle_time_busdays
         ,task_name
  FROM d_mw_v2_current vc, d_task_types tt
  WHERE vc.task_type_id = tt.task_type_id
  AND complete_date IS NOT NULL
  AND TRUNC(complete_date) &gt;= ?
  AND TRUNC(complete_date) &lt;= ?
  AND tt.task_name = ?)</sql>
    <outer_join>N</outer_join>
    <replace_vars>N</replace_vars>
    <parameter>
      <field>
        <name>PERIOD_START_DATE</name>
        <type>-</type>
