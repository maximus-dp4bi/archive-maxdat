    <sql>SELECT MEDIAN(age_unclaimed_task) median_age_unclaimed ,
       MAX(age_unclaimed_task) max_age_unclaimed, 
       AVG(age_unclaimed_task) avg_age_unclaimed 
FROM 
 (SELECT CASE WHEN sla_days_type = &apos;B&apos; THEN age_in_business_days ELSE age_in_calendar_days END age_unclaimed_task
  FROM d_mw_v2_current vc, d_task_types tt
  WHERE vc.task_type_id = tt.task_type_id
  AND curr_task_status = &apos;UNCLAIMED&apos;
  AND trunc(vc.create_date) &gt;= ?
  AND trunc(vc.create_date) &lt;= ?
  AND tt.task_name = ?)</sql>
