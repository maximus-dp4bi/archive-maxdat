  <step>
    <name>Database join</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT LMREQ_ID, CREATE_TS AS STATUS_DT,CASE WHEN NEWBORN = 1
THEN &apos;Y&apos;
WHEN NEWBORN = 0
THEN &apos;N&apos;
ELSE NULL
END AS NEWBORN_FLAG ,material_request_id,LANGUAGE,FAMILY_MEMBER_COUNT,AIAN_MEMBER_COUNT,
FILE_NAME

 FROM
(SELECT LL.CLIENT_ID,CL.CLNT_DOB,LS.CREATE_TS,
CASE WHEN CL.CLNT_DOB IS NULL 
THEN NULL
WHEN LR.REQUESTED_ON -CL.CLNT_DOB  &lt;= 60 
THEN 1
ELSE 0
END AS NEWBORN, 
LR.LMREQ_ID ,
LR.material_request_id material_request_id,
--(select material_cd from material_request_details where material_request_id = LR.material_request_id) material_cd,
(select max(language_cd ) from material_request_details where material_request_id = LR.material_request_id) LANGUAGE,
(select FAMILY_MEMBER_COUNT from MATERIAL_APP_REQUEST where material_request_id = LR.material_request_id) FAMILY_MEMBER_COUNT,
(select AIAN_MEMBER_COUNT from MATERIAL_APP_REQUEST where material_request_id = LR.material_request_id) AIAN_MEMBER_COUNT,
LR.FILE_NAME AS FILE_NAME
FROM LETTER_REQUEST LR
JOIN (SELECT A.LMREQ_ID,B.STATUS_CD,B.CREATE_TS FROM LETTER_REQUEST A
JOIN letter_req_status_history B
ON A.LMREQ_ID = B.LMREQ_ID
AND B.STATUS_CD in (&apos;REQ&apos;, &apos;QC_REVIEW&apos;)) LS
ON LR.LMREQ_ID = LS.LMREQ_ID
LEFT OUTER JOIN LETTER_REQUEST_LINK LL 
ON LR.LMREQ_ID = LL.LMREQ_ID
LEFT OUTER JOIN CLIENT CL
ON LL.CLIENT_ID = CL.CLNT_CLIENT_ID
WHERE LR.LMREQ_ID = ?
)
--GROUP BY LMREQ_ID,CREATE_TS</sql>
