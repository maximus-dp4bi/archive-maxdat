  <step>
    <name>Table input - Letter ID</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>Select distinct
L.LETTER_ID,
L.LETTER_CREATE_TS,
COALESCE((CASE WHEN S.STAFF_ID &lt;0 THEN &apos;MAXeb&apos; 
ELSE S.DISPLAY_NAME 
END),L.LETTER_CREATED_BY) AS USERNAME,
L.LETTER_REQUESTED_ON,
&apos;Active&apos; AS INSTANCE_STATUS,
L.LETTER_TYPE,
L.PROGRAM,
L.LETTER_CASE_ID,
L.RESIDENCE_COUNTY,
L.RESIDENCE_ZIP_CODE,
L.CREATION_GROUP,
--L.LANGUAGE,
CASE WHEN L.LETTER_REQUEST_TYPE = &apos;R&apos;
THEN &apos;Y&apos;
ELSE &apos;N&apos;
END AS REPRINT,
CASE WHEN L.LETTER_DRIVER_TYPE = &apos;CASE&apos;
THEN &apos;CASE&apos;
WHEN LETTER_DRIVER_TYPE = &apos;CLNT&apos;
THEN &apos;CLNT&apos;
ELSE NULL
END AS REQUEST_DRIVER_TYPE,
L.DRIVER_TABLE_NAME,
&apos;Requested&apos; AS STATUS,
L.LETTER_CREATE_TS AS ASSD_PROCESS_LETTER_REQ,
&apos;N&apos; as  ASF_PROCESS_LETTER_REQ,
&apos;N&apos; as ASF_TRANSMIT,
&apos;N&apos; as ASF_RECEIVE_CONFIRMATION,
&apos;N&apos; as ASF_CREATE_ROUTE_WORK,
NULL AS MANUAL_NOTICE_TYPE
FROM 
LETTERS_STG L
LEFT OUTER JOIN STAFF_LKUP S
ON L.LETTER_CREATED_BY = S.STAFF_ID
WHERE 
LETTER_CREATE_TS &gt;=  to_date(&apos;${LastSuccessfulETLRunDate}&apos;,&apos;yyyy&#47;mm&#47;dd hh24:mi:ss&apos;)
ORDER BY LETTER_ID</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
