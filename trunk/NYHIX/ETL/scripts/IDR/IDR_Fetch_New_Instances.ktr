  ) INCIDENT_TYPE
,(SELECT apl.appellant_type FROM apl_appeal apl WHERE apl.incident_header_id = i.incident_header_id) appellant_type
,(SELECT report_label FROM enum_apl_ob_appelant_type oat,apl_appeal apl WHERE oat.value = apl.appellant_type AND apl.incident_header_id = i.incident_header_id) appellant_type_desc
,i.reporter_first_name || &apos; &apos; ||i.reporter_last_name  REPORTER_NAME
,i.reporter_phone  REPORTER_PHONE
,c.case_cin ACCOUNT_ID
from INCIDENT_HEADER I
left outer join cases C ON I.CASE_ID = C.CASE_ID
Where 1=1
AND ((i.status_cd not in (${gv_IDR_INCIDENT_CLOSED}))  or  ((i.status_cd in (${gv_IDR_INCIDENT_CLOSED})) and i.status_ts&gt;sysdate-${gv_INC_LOOK_BACK_DAYS}))
and i.INCIDENT_HEADER_TYPE_CD IN (${gv_IDR_INCIDENT_HEADER_TYPE})
and I.INCIDENT_HEADER_ID &gt; ${gv_LastIncID} - ${gv_INCIDNTS_TO_LOOK_BACK}
