  <step>
    <name>Database join - Get updates from OLTP</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <copies>4</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>select A.*,
	   (case when CANCEL_DT is not null then ${gv_IDR_INCIDENT_NORMAL} else null end) as cancel_method
from
(
Select 
 (select P.REPORT_LABEL from ENUM_PRIORITY P where P.value=I.PRIORITY_CD) PRIORITY
--,(select s.report_label from ENUM_INCIDENT_HEADER_STATUS s where s.value=I.STATUS_CD) INCIDENT_STATUS
--,i.status_ts INCIDENT_STATUS_DT
,i.update_ts LAST_UPDATE_BY_DT
,coalesce((select TRIM(S.LAST_NAME)||&apos;,&apos;||TRIM(S.FIRST_NAME)||&apos; &apos;||TRIM(S.MIDDLE_NAME) from staff s where to_char(s.staff_id)=i.updated_by),i.updated_by) LAST_UPDATE_BY_NAME
,i.updated_by as Updated_by
,(select a.REPORT_LABEL from ENUM_ACTION_TAKEN a where a.value=I.ACTION_TAKEN_CD) ACTION_TYPE
,replace(replace(substrb(DBMS_LOB.SUBSTR(xmltype.createxml(i.actions_taken).extract(&apos;&#47;actions_taken&#47;action_taken&#47;action&apos;).getClobVal(),2000,1),1,2000)||substrb(DBMS_LOB.SUBSTR(xmltype.createxml(i.actions_taken).extract(&apos;&#47;actions_taken&#47;action_taken&#47;action&apos;).getClobVal(),2000,2001),1,2000), &apos;&lt;action&gt;&apos;, CHR(13)||CHR(10)), &apos;&lt;&#47;action&gt;&apos;, &apos;&apos;) ACTION_COMMENTS
--,(SELECT dbms_lob.substr( I.ACTIONS_TAKEN, 4000, 1 ) FROM dual) ACTION_COMMENTS
--,NULL action_comments --20140115 workaround for now to avoid getting the numeric value error
,(select R.REPORT_LABEL from ENUM_RESOLUTION_TYPE R where R.value=I.RESOLUTION_TYPE_CD) RESOLUTION_TYPE
--, null RESOLUTION_DESCRIPTION
,substrb(dbms_lob.substr(i.resolution,2000, 1 ),1,2000)|| substrb(dbms_lob.substr(i.resolution,2000, 2001),1,2000) RESOLUTION_DESCRIPTION
,(select T.REPORT_LABEL from ENUM_INCIDENT_HEADER_TYPE T where T.value=I.INCIDENT_HEADER_TYPE_CD) INCIDENT_TYPE
,(select pa.report_label from ENUM_AFFECTED_PARTY_TYPE pa where pa.value=I.AFFECTED_PARTY_TYPE_CD) INCIDENT_ABOUT
,substrb(dbms_lob.substr(description,2000, 1 ),1,2000)|| substrb(dbms_lob.substr(description,2000, 2001),1,2000) INCIDENT_DESCRIPTION
--, null INCIDENT_DESCRIPTION
,(select N.PROVIDER_ID from network N where N.NETWORK_ID_EXT=I.NETWORK_ID_EXT) ABOUT_PROVIDER_ID
,(select plan_code from (select P.PLAN_CODE from PLANS P, INCIDENT_HEADER I where P.PLAN_ID_EXT=I.PLAN_ID_EXT and I.incident_header_id = ? order by p.plan_id asc)  where rownum=1) ABOUT_PLAN_CODE
,(select r.report_label from enum_reporter_type r where r.value=i.reporter_type_cd) REPORTED_BY
,(select RR.REPORT_LABEL from ENUM_RELATIONSHIP RR where RR.value=I.REPORTER_RELATIONSHIP) REPORTER_RELATIONSHIP
,(select O.REPORT_LABEL from ENUM_OTHER_PARTY_TYPE O where O.value=I.OTHER_PARTY_TYPE_CD) OTHER_PARTY_NAME
,COALESCE((select IH.STATUS_TS 
    from INCIDENT_HEADER IH 
    where IH.INCIDENT_HEADER_ID=I.INCIDENT_HEADER_ID 
      and IH.STATUS_CD in (${gv_IDR_CLOSED_NOT_SUCCESSFUL},${gv_IDR_CLOSED_SUCCESSFUL})
   ),?) COMPLETE_DT
,(select IH.STATUS_TS 
    FROM INCIDENT_HEADER IH 
    where IH.INCIDENT_HEADER_ID=I.INCIDENT_HEADER_ID 
      AND IH.STATUS_CD in (${gv_IDR_INCIDENT_CLOSED}, ${gv_IDR_INC_CLOSED_DUP})
    ) CANCEL_DT
,(select  coalesce( (select TRIM(S.LAST_NAME)||&apos;,&apos;||TRIM(S.FIRST_NAME)||&apos; &apos;||TRIM(S.MIDDLE_NAME)
                       from STAFF S 
                      where to_char(s.staff_id) = IH.updated_by),TO_CHAR(IH.updated_by)
                   )
 FROM INCIDENT_HEADER IH 
    where IH.INCIDENT_HEADER_ID=I.INCIDENT_HEADER_ID 
      AND IH.STATUS_CD in (${gv_IDR_INCIDENT_CLOSED}, ${gv_IDR_INC_CLOSED_DUP})
  )  CANCEL_BY
,I.CLIENT_ID
,i.case_id
,(select MAX(step_instance_id) 
    from STEP_INSTANCE si, step_definition sd 
    where SI.REF_TYPE=&apos;INCIDENT_HEADER&apos;
     and SI.STEP_DEFINITION_ID = SD.STEP_DEFINITION_ID
     and SD.STEP_TYPE_CD in (&apos;VIRTUAL_HUMAN_TASK&apos;,&apos;HUMAN_TASK&apos;)
     and si.completed_ts is null
     and ref_id=i.incident_header_id) CURRENT_TASK_ID
,(SELECT apl.appellant_type FROM apl_appeal apl WHERE apl.incident_header_id = i.incident_header_id) appellant_type
,(SELECT report_label FROM enum_apl_ob_appelant_type oat,apl_appeal apl WHERE oat.value = apl.appellant_type AND apl.incident_header_id = i.incident_header_id) appellant_type_desc
,i.reporter_first_name || &apos; &apos; ||i.reporter_last_name  REPORTER_NAME
,i.reporter_phone  REPORTER_PHONE
,c.case_cin ACCOUNT_ID
,(SELECT o.report_label FROM enum_incident_origin o WHERE o.value=i.origin_cd) CHANNEL
from INCIDENT_HEADER I
left outer join CASES C on I.CASE_ID = C.CASE_ID
where  i.incident_header_id = ?
) A
</sql>
