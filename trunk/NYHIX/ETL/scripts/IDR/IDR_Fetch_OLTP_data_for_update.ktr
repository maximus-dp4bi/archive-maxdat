  <step>
    <name>Database join - Get enrollment status</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <copies>4</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>select 
CASE WHEN EXISTS (select 1 from CLIENT_ELIG_STATUS EG,CLIENT_ENROLL_STATUS EN
where EG.CLIENT_ID=EN.CLIENT_ID(+)
and EG.END_DATE is null
and EN.END_DATE is null
and EG.ELIG_STATUS_CD in (&apos;M&apos;,&apos;V&apos;)
and EN.ENROLL_STATUS_CD in (${gv_IDR_ENROLL_STATUS_ENROLLEE}) --(&apos;J&apos;,&apos;L&apos;)
and EG.CLIENT_ID=?) then &apos;Enrollee&apos;
 when exists (select 1 from CLIENT_ELIG_STATUS EG,CLIENT_ENROLL_STATUS EN
where EG.CLIENT_ID=EN.CLIENT_ID(+)
and EG.END_DATE is null
and EN.END_DATE is null
and EG.ELIG_STATUS_CD in (&apos;M&apos;,&apos;V&apos;)
and EN.ENROLL_STATUS_CD not in (${gv_IDR_ENROL_STATS_POTNTIAL_ENRLE}) --(&apos;J&apos;,&apos;L&apos;,&apos;O&apos;)
and EG.CLIENT_ID=?) then &apos;Potential Enrollee&apos;
else
&apos;Other&apos;
end ENROLLMENT_STATUS
from dual
</sql>
