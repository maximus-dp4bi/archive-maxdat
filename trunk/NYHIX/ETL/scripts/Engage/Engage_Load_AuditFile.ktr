  <step>
    <name>Get total_score</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DP_SCORECARD</connection>
    <sql>SELECT s.evaluation_reference_id
      ,s.evaluation_reference
      ,t.score_total
      ,sysdate last_update_date
      ,s.deleted_flag
FROM engage_actuals s
join engage_actuals_stg t
on s.evaluation_reference = t.evaluation_reference
WHERE s.evaluation_date_time &gt;=  TO_DATE(&apos;${AUDIT_START_DATE}&apos;,&apos;YYYY&#47;MM&#47;DD HH24:MI:SS&apos;)
AND s.evaluation_date_time &lt;=  TO_DATE(&apos;${AUDIT_END_DATE}&apos;,&apos;YYYY&#47;MM&#47;DD HH24:MI:SS&apos;)
and s.score_total != t.score_total
</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
