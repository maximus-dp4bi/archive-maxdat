    <sql>SELECT evaluation_reference_id
      ,evaluation_reference
      ,score_total
      ,sysdate last_update_date
      ,deleted_flag
      ,&apos;Y&apos; in_yes
FROM engage_actuals s
WHERE evaluation_date_time &gt;=  TO_DATE(&apos;${AUDIT_START_DATE}&apos;,&apos;YYYY&#47;MM&#47;DD HH24:MI:SS&apos;)
AND evaluation_date_time &lt;=  TO_DATE(&apos;${AUDIT_END_DATE}&apos;,&apos;YYYY&#47;MM&#47;DD HH24:MI:SS&apos;)
AND NOT EXISTS(SELECT 1 FROM engage_actuals_stg t
               WHERE s.evaluation_reference = t.evaluation_reference)</sql>
