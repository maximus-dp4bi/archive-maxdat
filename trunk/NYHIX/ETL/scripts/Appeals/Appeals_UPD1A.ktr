  , ostg.INSTANCE_STATUS                  NEW_INSTANCE_STATUS
  , ostg.INCIDENT_STATUS                  NEW_INCIDENT_STATUS
  , ostg.LAST_UPDATE_BY_NAME              NEW_LAST_UPDATE_BY_NAME
  , ostg.INCIDENT_STATUS_DT               NEW_INCIDENT_STATUS_DT  
  , ostg.INCIDENT_HEADER_STAT_HIST_ID	  NEW_MAX_INCI_STAT_HIST_ID
  ,LAG(ostg.incident_status,1) OVER (PARTITION BY incident_id ORDER BY incident_header_stat_hist_id) PREV_INCIDENT_STATUS
  ,COALESCE(LEAD(ostg.incident_header_stat_hist_id,1) OVER (PARTITION BY incident_id ORDER BY incident_header_stat_hist_id),ostg.incident_header_stat_hist_id+1) NEXT_INCI_HIST_ID
 FROM NYHBE_PROCESS_APPEALS_OLTP ostg 
 WHERE incident_status  NOT IN (${PA_EXCLUDE_STATUS}) 
) ostg
INNER JOIN
 (SELECT 
 INCIDENT_ID              AS   OLD_INCIDENT_ID            
,INCIDENT_STATUS     	  AS   OLD_INCIDENT_STATUS     
,LAST_UPDATE_BY_NAME      AS   OLD_LAST_UPDATE_BY_NAME
,INCIDENT_STATUS_DT       AS   OLD_INCIDENT_STATUS_DT 
,MAX_INCI_STAT_HIST_ID    AS   OLD_MAX_INCI_STAT_HIST_ID
--,UPDATED                  AS   OLD_UPDATED             
,&apos;N&apos;                      AS   OLD_UPDATED             
FROM NYHBE_PROCESS_APPEALS_WIP_BPM
WHERE CANCEL_DT IS NULL
AND INSTANCE_STATUS = &apos;Active&apos;
AND INCIDENT_STATUS = &apos;APPEAL INITIATED&apos;
) wstg
on ostg.NEW_INCIDENT_ID = wstg.OLD_INCIDENT_ID
AND ostg.PREV_INCIDENT_STATUS = wstg.OLD_INCIDENT_STATUS
WHERE ostg.prev_incident_status IS NOT NULL
AND NEW_INCIDENT_STATUS = &apos;Incident Open&apos;
AND NEW_MAX_INCI_STAT_HIST_ID BETWEEN OLD_MAX_INCI_STAT_HIST_ID AND NEXT_INCI_HIST_ID</sql>
