       ELSE null END)  CREATED_BY_NAME
,i.incident_header_id INCIDENT_ID
,i.tracking_number TRACKING_NUMBER
,i.hearing_date APPEAL_HEARING_DT
,apl.appeal_date appeal_dt
--,i.create_ts CREATE_DT
,coalesce ((select min(ad.received_date) from doc_link d, app_doc_data ad where d.document_id = ad.document_id and link_type_cd = &apos;INCIDENT_HEADER&apos; and link_ref_id = i.INCIDENT_HEADER_ID),i.create_ts ) create_dt
,(SELECT t.report_label FROM enum_incident_header_type t WHERE t.value=i.incident_header_type_cd) INCIDENT_TYPE
,i.received_ts RECEIPT_DT
,apl.hearing_time appeal_hearing_time
,apl.hearing_loc appeal_hearing_location
,DECODE(i.aid_to_continue_ind,1,&apos;Y&apos;,&apos;N&apos;) aid_to_continue
,(SELECT plan_code FROM (SELECT p.plan_code FROM plans p, incident_header i WHERE p.plan_id_ext=i.plan_id_ext and i.incident_header_id=? order by p.plan_id asc)  WHERE rownum=1) ABOUT_PLAN_CODE
,(SELECT plan_name FROM (SELECT p.plan_name FROM plans p, incident_header i WHERE p.plan_id_ext=i.plan_id_ext and i.incident_header_id=? order by p.plan_id asc)  WHERE rownum=1) ABOUT_PLAN_NAME
,(SELECT provider_id 
 FROM (SELECT n.provider_id,n.network_id FROM network n,incident_header i WHERE n.network_id_ext=i.network_id_ext and i.incident_header_id=? order by n.network_id)
 WHERE network_id=(SELECT max(network_id) FROM (SELECT n.provider_id,n.network_id FROM network n,incident_header i WHERE n.network_id_ext=i.network_id_ext and i.incident_header_id=? order by n.network_id))
 ) ABOUT_PROVIDER_ID
,(select last_name||&apos;,&apos;||first_name||&apos; &apos;||middle_name from provider
where provider_id=
(SELECT provider_id 
 from (select n.provider_id,n.network_id from network n,incident_header i where n.network_id_ext=i.network_id_ext and i.incident_header_id=? order by n.network_id)
 where network_id=(select max(network_id) from (select n.provider_id,n.network_id from network n,incident_header i where n.network_id_ext=i.network_id_ext and i.incident_header_id=? order by n.network_id)))
 ) ABOUT_PROVIDER_NAME
--,i.actions_taken ACTION_COMMENTS
, replace(replace(DBMS_LOB.SUBSTR(xmltype.createxml(i.actions_taken).extract(&apos;&#47;actions_taken&#47;action_taken&#47;action&apos;).getClobVal(),4000,1), &apos;&lt;action&gt;&apos;, CHR(13)||CHR(10)), &apos;&lt;&#47;action&gt;&apos;, &apos;&apos;) ACTION_COMMENTS
,(SELECT a.report_label FROM enum_action_taken a WHERE a.value=i.action_taken_cd) ACTION_TYPE
,(SELECT step_instance_id FROM step_instance si, step_definition sd WHERE ref_type=&apos;INCIDENT_HEADER&apos;and si.step_definition_id = sd.step_definition_id
   and sd.step_type_cd IN (&apos;VIRTUAL_HUMAN_TASK&apos;,&apos;HUMAN_TASK&apos;)
   and si.completed_ts IS NULL
   and ref_id=i.incident_header_id) CURRENT_TASK_ID
,(SELECT pa.report_label FROM enum_affected_party_type pa WHERE pa.value=i.affected_party_type_cd) INCIDENT_ABOUT
--,(SELECT ii.description FROM incident_issue ii WHERE ii.incident_header_id=i.incident_header_id) INCIDENT_DESCRIPTION
,(SELECT dbms_lob.substr( i.description, 4000, 1 ) FROM dual) INCIDENT_DESCRIPTION
--,(SELECT s.report_label FROM enum_incident_header_status s WHERE s.value=i.status_cd) INCIDENT_STATUS
,(SELECT  s.report_label
  FROM enum_incident_header_status s WHERE s.value= hs.status_cd) INCIDENT_STATUS
--,i.status_ts INCIDENT_STATUS_DT
,hs.create_ts INCIDENT_STATUS_DT
,i.update_ts LAST_UPDATE_BY_DT
,COALESCE((SELECT s.last_name||&apos;,&apos;||s.first_name||&apos; &apos;||s.middle_name FROM staff s WHERE to_char(s.staff_id)=i.updated_by),i.updated_by)LAST_UPDATE_BY_NAME   
,i.client_id LINKED_CLIENT
,(SELECT o.report_label FROM enum_other_party_type o WHERE o.value=i.other_party_type_cd) OTHER_PARTY_NAME
,(SELECT p.report_label FROM enum_priority p WHERE p.value=i.priority_cd) PRIORITY
,(SELECT r.report_label FROM enum_reporter_type r WHERE r.value=i.reporter_type_cd) REPORTED_BY
,COALESCE((SELECT rr.report_label FROM enum_relationship rr WHERE rr.value=i.reporter_relationship),${PA_REPORTER_RELATIONSHIP_DEFLT}) REPORTER_RELATIONSHIP
,(SELECT dbms_lob.substr( i.resolution, 4000, 1 ) FROM dual) RESOLUTION_DESCRIPTION
,(SELECT r.report_label FROM enum_resolution_type r WHERE r.value=i.resolution_type_cd) RESOLUTION_TYPE
,i.case_id CASE_ID
--,(SELECT pa.report_label FROM enum_affected_party_subtype pa WHERE pa.value=i.affected_party_subtype_cd) INCIDENT_REASON
, apl.appellant_type appellant_type
,(SELECT report_label FROM enum_apl_ob_appelant_type oat WHERE oat.value = apl.appellant_type) appellant_type_desc
, tmp_ar.requested_day
, tmp_ar.requested_time
, apl.exp_appeal_req expected_request
,COALESCE((SELECT ih.status_ts FROM incident_header ih WHERE ih.incident_header_id=i.incident_header_id and i.status_cd in (${PA_INCIDENT_STATUS_CLSD_OLTP})),?) COMPLETE_DT
--,(SELECT ih.status_ts FROM incident_header ih WHERE ih.incident_header_id=i.incident_header_id and i.status_cd in (${PA_INCIDNT_STATS_WITHDRAN_OLTP})) CANCEL_DT
--,(SELECT MAX(ih.create_ts) FROM incident_header_stat_hist ih WHERE ih.incident_header_id=i.incident_header_id and ih.status_cd in (${PA_INCIDENT_STATUS_CLSD_OLTP})) COMPLETE_DT
--,(SELECT MAX(ih.create_ts) FROM incident_header_stat_hist ih WHERE ih.incident_header_id=i.incident_header_id and ih.status_cd in (${PA_INCIDNT_STATS_WITHDRAN_OLTP})) CANCEL_DT
,hs.incident_header_stat_hist_id 
FROM incident_header i
  LEFT OUTER JOIN apl_appeal apl
    ON i.incident_header_id = apl.incident_header_id
  LEFT OUTER JOIN (SELECT appeal_id,report_label requested_day, requested_time
				   FROM apl_requested_appeal_time ar, enum_apl_ob_appl_day_type oad
				   WHERE ar.requested_day = oad.value
			       AND ar.rowid = (SELECT MAX(rowid)  --had to use rowid to get max row, no other unique key in table
					               FROM apl_requested_appeal_time mar
					               WHERE mar.appeal_id = ar.appeal_id)) tmp_ar
    ON apl.appeal_id = tmp_ar.appeal_id
  LEFT OUTER JOIN incident_header_stat_hist hs
    ON i.incident_header_id = hs.incident_header_id    
    AND hs.incident_header_stat_hist_id = ?
--    AND hs.incident_header_stat_hist_id &gt; ?
WHERE  i.incident_header_type_cd=${PA_INCIDENT_HEADER_TYPE}
and i.incident_header_id=?</sql>
