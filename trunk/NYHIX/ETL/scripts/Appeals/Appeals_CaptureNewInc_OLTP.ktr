  <step>
    <name>Get - Incidents OLTP Source</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>SELECT i.incident_header_id incident_id
,(SELECT t.report_label FROM enum_incident_header_type t WHERE t.value=i.incident_header_type_cd) incident_type
--,i.create_ts create_dt
,coalesce ((select min(ad.received_date) from doc_link d, app_doc_data ad where d.document_id = ad.document_id and link_type_cd = &apos;INCIDENT_HEADER&apos; and link_ref_id = i.INCIDENT_HEADER_ID), i.create_ts) create_dt
,(select min(ad.received_date) from doc_link d, app_doc_data ad where d.document_id = ad.document_id and link_type_cd = &apos;INCIDENT_HEADER&apos; and link_ref_id = i.INCIDENT_HEADER_ID) received_dt
,(CASE when i.responsible_staff_id is NOT NULL THEN COALESCE((SELECT s.last_name||&apos;,&apos;||s.first_name||&apos; &apos;||s.middle_name FROM staff s WHERE s.staff_id=i.responsible_staff_id),to_char(i.responsible_staff_id))
       when i.responsible_staff_id IS NULL THEN COALESCE((SELECT s.last_name||&apos;,&apos;||s.first_name||&apos; &apos;||s.middle_name FROM staff s WHERE to_char(s.staff_id)=i.created_by),i.created_by) 
       ELSE null END)  created_by_name
,(SELECT g.group_name FROM groups g WHERE g.group_id=i.responsible_staff_group_id) created_by_group
,&apos;Active&apos; instance_status
,(SELECT o.report_label FROM enum_incident_origin o WHERE o.value=i.origin_cd) channel
,i.tracking_number
,CASE when (i.status_cd NOT IN (${PA_INCIDENT_STATUS_OPEN_OLTP})) THEN 
(SELECT h.report_label FROM enum_incident_header_status h, incident_header_stat_hist ihh 
 WHERE h.value=ihh.status_cd and ihh.incident_header_id=i.incident_header_id and ihh.incident_header_stat_hist_id=
 (SELECT min(hh.incident_header_stat_hist_id)FROM incident_header_stat_hist hh WHERE hh.incident_header_id=i.incident_header_id )) 
 ELSE (SELECT s.report_label FROM enum_incident_header_status s WHERE s.value=i.status_cd)
END incident_status
,CASE when (i.status_cd NOT IN (${PA_INCIDENT_STATUS_OPEN_OLTP})) THEN 
(SELECT ihh.create_ts FROM incident_header_stat_hist ihh 
 WHERE ihh.incident_header_id=i.incident_header_id and ihh.incident_header_stat_hist_id=
 (SELECT min(hh.incident_header_stat_hist_id)FROM incident_header_stat_hist hh WHERE hh.incident_header_id=i.incident_header_id )) 
ELSE i.status_ts
END incident_status_dt
,i.client_id linked_client
,(SELECT pa.report_label FROM enum_affected_party_type pa WHERE pa.value=i.affected_party_type_cd) incident_about
--,(SELECT pa.report_label FROM enum_affected_party_subtype pa WHERE pa.value=i.affected_party_subtype_cd) incident_reason
,sysdate stg_extract_date
,appeal_date appeal_dt
,(SELECT MIN(hh.INCIDENT_HEADER_STAT_HIST_ID) FROM INCIDENT_HEADER_STAT_HIST hh WHERE hh.INCIDENT_HEADER_ID = i.INCIDENT_HEADER_ID ) INCIDENT_HEADER_STAT_HIST_ID
,i.FAIR_HEARING_TRACKING_NBR
,apl.SPECIALIZED_FLAG SPECIALIZED_FLAG
,apl.APL_OUTRCH_IND APL_OUTRCH_IND
,apl.UNBL_CNTCT_START_DATE UNBL_CNTCT_START_DATE
FROM incident_header i
LEFT OUTER JOIN apl_appeal apl
  ON i.incident_header_id = apl.incident_header_id
WHERE  ((i.status_cd NOT IN (${PA_INCIDENT_STATUS_CLSD_OLTP}))  or  ((i.status_cd in (${PA_INCIDENT_STATUS_CLSD_OLTP})) and i.status_ts&gt;sysdate-${INC_LOOK_BACK_DAYS}))
AND i.incident_header_type_cd=${PA_INCIDENT_HEADER_TYPE}
AND i.incident_header_id &gt; ${LastIncID}-${INCIDNTS_TO_LOOK_BACK}




</sql>
