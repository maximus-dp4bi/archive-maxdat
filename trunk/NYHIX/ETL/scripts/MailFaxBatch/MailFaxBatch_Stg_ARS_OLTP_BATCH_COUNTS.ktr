  when exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;DocClass&quot; like &apos;%Application%&apos;)
   and not exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;DocClass&quot; not like &apos;%Application%&apos;)
   then &apos;Application Only&apos;

  when exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;DocClass&quot; like &apos;%Application%&apos;)
   and exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;DocClass&quot; like &apos;%Verification%&apos;)
   and not exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;FormType&quot; like &apos;%Renewal%&apos;)
   and not exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;FormType&quot; like &apos;%Employee%&apos;)
   and not exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;FormType&quot; like &apos;%Employer%&apos;)
  then &apos;Application + Verification Documents&apos;

  when exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;DocClass&quot; like &apos;%Application%&apos;)
   and exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;FormType&quot; like &apos;%Renewal%&apos;)
   and not exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;FormType&quot; like &apos;%Employee%&apos;)
   and not exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;FormType&quot; like &apos;%Employer%&apos;)
  then &apos;Renewals Only&apos;

  when exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;DocClass&quot; like &apos;%Application%&apos;)
   and exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;DocClass&quot; like &apos;%Verification%&apos;)
   and exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;FormType&quot; like &apos;%Renewal%&apos;)
   and not exists(select 1 from &quot;${DB_KOFAX_ARS_DB}&quot;..&quot;Document&quot; where &quot;Document&quot;.&quot;BatchID&quot;=&quot;Batch&quot;.&quot;BatchID&quot; and &quot;Document&quot;.&quot;FormType&quot; like &apos;%Employee%&apos;)
