    <sql>SELECT &apos;Y&apos; yes, bpm.itm_indicator, bpm.itm_update_ts
      ,SYSDATE curr_date,bpm.info_req_id, bpm.info_req_group, bpm.inreq_id
      , bpm.letter_status_dt, bpm.letter_image_link_dt
      ,bpm.info_req_status, oltp.info_req_status new_letter_status
      ,bpm.assd_mail_letter_req, bpm.ased_mail_letter_req
  FROM sendinfotradpart_bpm_stg  bpm
      ,sendinfotradpart_oltp_stg oltp
 WHERE bpm.info_req_group  IN (&apos;Letter&apos;  -- MI Letter, Renewal Reminder Letter
                               ,&apos;Material Request&apos;)
   AND bpm.info_req_id     = oltp.info_req_id
   AND bpm.info_req_source = oltp.info_req_source
   AND bpm.cancel_date IS NULL
   AND bpm.ased_mail_letter_req IS NULL
&#47;*
   AND EXISTS
       (SELECT 1 FROM sendinfotradpart_bpm_stg bpm
         WHERE bpm.info_req_id     = bpm.info_req_id
           AND bpm.info_req_source = bpm.info_req_source)
*&#47;
--and rownum &lt;= 200
</sql>
