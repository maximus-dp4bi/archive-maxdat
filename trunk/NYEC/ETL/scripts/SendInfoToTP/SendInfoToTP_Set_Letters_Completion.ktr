      ,gwf_ltr_mailed, new_ltr_req_id, assd_create_new_letter_req, aspb_create_new_letter_req
  FROM sendinfotradpart_bpm_stg bpm
 WHERE info_req_group  IN (&apos;Letter&apos;  -- MI Letter, Renewal Reminder Letter
