  <step>
    <name>Table Input - OLTP Staged Requests</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>
SELECT &apos;Y&apos; itm_indicator, SYSDATE itm_update_ts
      ,info_req_id, info_req_source
      ,&apos;Y&apos; asf_mail_letter_req, letter_status_dt ased_mail_letter_req
      ,staff_id, s.first_name||&apos; &apos;||s.last_name aspb_mail_letter_request
  FROM sendinfotradpart_oltp_stg oltp
      ,letters_stg r
      ,staff_stg   s
 WHERE info_req_group  IN (&apos;Letter&apos;  -- MI Letter, Renewal Reminder Letter
                               ,&apos;Material Request&apos;)
   AND info_req_status IN (&apos;Mailed&apos;,&apos;Errored&apos;)
   AND info_req_source     = info_req_source
   AND r.letter_created_by = s.staff_id
   AND r.letter_id         = info_req_id
   AND cancel_date IS NULL
   AND ased_mail_letter_req IS NULL

--and rownum &lt; 100
</sql>
