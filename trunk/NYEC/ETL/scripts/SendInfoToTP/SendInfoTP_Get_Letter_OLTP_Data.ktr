  <step>
    <name>Table Input - New Letter Requests</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT NULL                itm_indicator
      ,SYSDATE             itm_insert_ts
      ,NULL                itm_update_ts
      ,&apos;Y&apos;                 yes
      ,r.letter_id         info_req_id
      ,it.flex_char3       info_req_source
      ,it.flex_char2       info_req_type
      ,it.flex_char1       info_req_group
      ,ls.flex_char1       info_req_status  -- per Workbook
      ,&apos;Active&apos;            instance_status
      ,&apos;Timely&apos;            timeliness_status 
      ,r.letter_create_ts  info_req_create_dt
      ,r.letter_create_ts  assd_receive_info_req
      ,r.letter_app_id     app_id 
      ,r.letter_sent_on    info_req_sent_dt
      ,it.flex_char4       gwf_request_type
      ,r.letter_requested_on  letter_req_dt
      ,r.letter_update_ts     letter_status_dt
      ,NULL                   letter_types
      ,&apos;XX&apos;                   ltr_type_xx
      ,&apos;MI2&apos;                  ltr_type_mi
      ,&apos;RR1&apos;                  ltr_type_rr
      ,it.flex_char6                                 sla_days_type
      ,it.flex_int1                                  sla_days
      ,it.flex_int2                                  sla_jeopardy_days
      ,it.flex_int3                                  sla_target_days
      ,(SELECT &apos;Y&apos; FROM nyec_etl_sendinfotradpart sitp
         WHERE sitp.info_req_id     = r.letter_id
           AND sitp.info_req_source = it.flex_char3) nyec_exists
  FROM letters_stg r
      ,corp_etl_list_lkup it  -- Instance Types
      ,corp_etl_list_lkup ls  -- Letter Status (ASF Mailed)
 WHERE r.letter_type_cd    = it.value
   AND it.name             = &apos;SITP_INSTANCE_TYPE&apos;
   AND ls.name             = &apos;SITP_LETTER_STATUS&apos;
   AND r.letter_status_cd  = ls.value
   AND it.ref_type IN (&apos;LETTER&apos;,&apos;MATERIAL&apos;)
--   AND INSTR(ls.ref_type,it.ref_type) &gt; 0
   AND r.letter_create_ts &gt;= ( SYSDATE - ${vSITP_LETTER_DAYS} )

-- Test
--and letter_id in (9798814,9796792) -- Manual &apos;S&apos; request
--   AND letter_id = 9796792  -- Cancelled in stage
--and rownum &lt;= 200  AND ls.flex_char1 NOT IN (&apos;Mailed&apos;,&apos;Errored&apos;)
</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
