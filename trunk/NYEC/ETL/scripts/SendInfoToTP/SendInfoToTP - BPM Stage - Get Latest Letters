           </partitioning>
    <connection>MAXDAT</connection>
    <sql>-- A new App request supercedes an existing App request.

SELECT oltp.inreq_id,info_req_source,info_req_type,info_req_group
      ,oltp.info_req_id,latest_ltr.max_letter_id new_ltr_req_id
      ,&apos;Y&apos; asf_create_new_letter_req
      ,oltp.info_req_status
      ,&apos;Complete&apos; instance_status
      ,oltp.gwf_request_type,oltp.letter_types
  FROM sendinfotradpart_oltp_stg oltp
      ,(SELECT letter_case_id,letter_app_id,letter_type
              ,MIN(letter_id) min_letter_id, MAX(letter_id) max_letter_id
          FROM letters_stg r
              ,corp_etl_list_lkup it  -- Instance Types
         WHERE r.letter_type_cd = it.value
           AND it.name            = &apos;SITP_INSTANCE_TYPE&apos;
        GROUP BY letter_case_id,letter_app_id,letter_type) latest_ltr
 WHERE oltp.info_req_source  = &apos;Letter Req&apos;
   AND oltp.info_req_status  IN (&apos;Errored&apos;,&apos;Voided&apos;,&apos;Cancelled&apos;)
   AND oltp.app_id           = latest_ltr.letter_app_id
   AND oltp.info_req_type    = latest_ltr.letter_type
   AND oltp.info_req_id     != latest_ltr.max_letter_id
--and rownum &lt;= 100</sql>
