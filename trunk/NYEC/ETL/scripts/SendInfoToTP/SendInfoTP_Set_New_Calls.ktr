    <sql>SELECT bpm.inreq_id, bpm.info_req_id, bpm.info_req_type, bpm.info_req_source
      ,&apos;Y&apos; itm_indicator, SYSDATE itm_update_ts
      ,bpm.info_req_create_dt,bpm.app_id, bpm.event_type_cd, bpm.call_retry_limit
