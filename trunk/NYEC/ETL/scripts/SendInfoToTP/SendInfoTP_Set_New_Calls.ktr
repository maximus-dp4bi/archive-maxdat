  FROM sendinfotradpart_bpm_stg bpm
      ,nyec_etl_sendinfotradpart nyec
 WHERE bpm.info_req_group   = &apos;Outbound Call&apos;
   AND bpm.info_req_group   = nyec.info_req_group
   AND bpm.info_req_id      = nyec.info_req_id
   AND bpm.info_req_source  = nyec.info_req_source
   AND bpm.info_req_status != nyec.info_req_status
   AND bpm.call_flag        = &apos;Y&apos;
-- Test
and 1 =2</sql>
