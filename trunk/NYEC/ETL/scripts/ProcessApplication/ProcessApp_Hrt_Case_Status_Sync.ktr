    <sql>Select To_number(app_id) Maxdat_Appid,  heart_case_status as Maxdat_hrt_case_status
	   , To_number(cepa_id) Incepa_id	
  from nyec_etl_process_app
 where complete_dt &gt;= (Select trunc(sysdate) - to_number(value) from corp_etl_control where name = &apos;AP_WAIT_DAYS_BEFORE_STAGE_DONE&apos;) 
