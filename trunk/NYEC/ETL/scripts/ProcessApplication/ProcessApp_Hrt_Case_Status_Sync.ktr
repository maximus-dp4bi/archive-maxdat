    <rowlimit>1</rowlimit>
    <sql> SELECT DISTINCT aas.report_label Oltp_Hrt_Case_Status
  FROM hrt_case_control hcc
     , enum_app_agency_app_status aas 
      , app_header ah
  WHERE hcc.current_status_cd = aas.value 
