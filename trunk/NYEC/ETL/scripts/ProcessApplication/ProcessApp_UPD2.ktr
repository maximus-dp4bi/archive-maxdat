       pa.sla_target_days updSLA_TARGET_DAYS, 
	   pa.days_until_timeout updDAYS_UNTIL_TIMEOUT,

       pas.APP_ID stgAPP_ID,
       pas.CHANNEL stgCHANNEL,
       pas.receipt_dt stgRECEIPT_DT,
       pas.APP_CYCLE_START_DT stgAPP_CYCLE_START_DT,
       pas.APP_CYCLE_END_DT stgAPP_CYCLE_END_DT,
       pas.APP_CYCLE_BUS_DAYS stgAPP_CYCLE_BUS_DAYS,
       pas.APP_CYCLE_CAL_DAYS stgAPP_CYCLE_CAL_DAYS,
       pas.SLA_JEOPARDY_DT stgSLA_JEOPARDY_DT,
       pas.JEOPARDY_FLAG stgJEOPARDY_FLAG,
       pas.TIMELINESS_STATUS stgTIMELINESS_STATUS,
       pas.DE_TASK_ID stgDE_TASK_ID,
       pas.CLOCKDOWN_INDICATOR stgCLOCKDOWN_INDICATOR,
       pas.ASSD_CLOSE_APP stgASSD_CLOSE_APP,
       pas.ASPB_CLOSE_APP stgASPB_CLOSE_APP,
       pas.sla_days stgSLA_DAYS,
       pas.sla_days_type stgSLA_DAYS_TYPE,
       pas.sla_jeopardy_days stgSLA_JEOPARDY_DAYS,
       pas.sla_target_days stgSLA_TARGET_DAYS,
	   pas.days_until_timeout stgDAYS_UNTIL_TIMEOUT,

       &apos;N&apos; UPDATED,
       &apos;Y&apos; cYes,
	   SYSDATE CURRENTDT
 FROM  process_app_stg  pas,
       nyec_etl_process_app pa 
 WHERE pas.app_id = pa.app_id
   AND nvl(pa.Reactivation_nbr,0) = pas.Reactivation_nbr
   AND pa.stage_done_date is NULL
   AND pa.complete_dt IS NULL 
   AND pa.receipt_dt IS NULL
   AND (nvl(pas.clockdown_indicator,&apos;n&apos;)  &lt;&gt; nvl(pa.clockdown_indicator,&apos;n&apos;)
    OR  pas.receipt_dt IS NOT NULL)
   AND pas.app_priority_ind = 1
   AND pas.app_extract_complete_dt IS NULL

