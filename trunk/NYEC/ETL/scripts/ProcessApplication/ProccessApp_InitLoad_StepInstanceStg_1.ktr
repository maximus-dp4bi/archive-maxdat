    <sql>SELECT
  SI.STEP_INSTANCE_ID              
, SI.STATUS                        
, SI.CREATE_TS                     
, SI.COMPLETED_TS                  
, SH.ESCALATED_IND
, SI.STEP_DUE_TS
, SH.FORWARDED_IND
, SH.GROUP_ID
, SH.TEAM_ID
, SI.REF_ID
, SI.REF_TYPE
, SI.STEP_DEFINITION_ID
, SI.CREATED_BY
, SH.ESCALATE_TO
, SH.FORWARDED_BY
, SH.OWNER                         
, SI.SUSPENDED_TS   
, SH.STEP_INSTANCE_HISTORY_ID 
, SH.STATUS                        HIST_STATUS
, SH.CREATE_TS                     HIST_CREATE_TS
, SH.CREATED_BY                    HIST_CREATE_BY
,&apos;Y&apos; MW_PROCESSED
,&apos;N&apos; AP_PROCESSED
,&apos;Y&apos; MIB_PROCESSED
,SYSDATE STAGE_CREATE_TS
FROM STEP_INSTANCE SI,
     STEP_INSTANCE_HISTORY SH, step_definition sd
where SH.STEP_INSTANCE_ID = SI.STEP_INSTANCE_ID
AND si.step_definition_id = sd.step_definition_id
AND sd.step_type_cd IN (&apos;HUMAN_TASK&apos;,&apos;VIRTUAL_HUMAN_TASK&apos;)
AND SI.REF_TYPE = &apos;APP_HEADER&apos;
AND SI.REF_ID IN (SELECT APPLICATION_ID FROM APP_HEADER AH WHERE AH.STATUS_CD NOT IN (&apos;COMPLETED&apos;,&apos;DISREGARDED&apos;,&apos;TIMEOUT&apos;)) 
UNION
SELECT
  SI.STEP_INSTANCE_ID              
, SI.STATUS                        
, SI.CREATE_TS                     
, SI.COMPLETED_TS                  
, SH.ESCALATED_IND
, SI.STEP_DUE_TS
, SH.FORWARDED_IND
, SH.GROUP_ID
, SH.TEAM_ID
, SI.REF_ID
, SI.REF_TYPE
, SI.STEP_DEFINITION_ID
, SI.CREATED_BY
, SH.ESCALATE_TO
, SH.FORWARDED_BY
, SH.OWNER                         
, SI.SUSPENDED_TS   
, SH.STEP_INSTANCE_HISTORY_ID 
, SH.STATUS                        HIST_STATUS
, SH.CREATE_TS                     HIST_CREATE_TS
, SH.CREATED_BY                    HIST_CREATE_BY
,&apos;Y&apos; MW_PROCESSED
,&apos;N&apos; AP_PROCESSED
,&apos;Y&apos; MIB_PROCESSED
,SYSDATE STAGE_CREATE_TS
FROM STEP_INSTANCE SI,
     STEP_INSTANCE_HISTORY SH, step_definition sd
where SH.STEP_INSTANCE_ID = SI.STEP_INSTANCE_ID
AND si.step_definition_id = sd.step_definition_id
AND sd.step_type_cd IN (&apos;HUMAN_TASK&apos;,&apos;VIRTUAL_HUMAN_TASK&apos;)
AND SI.REF_TYPE = &apos;APP_HEADER&apos;
AND  SI.REF_ID IN (SELECT APPLICATION_ID FROM APP_HEADER AH WHERE AH.STATUS_CD IN (&apos;COMPLETED&apos;,&apos;DISREGARDED&apos;,&apos;TIMEOUT&apos;) AND ah.status_date &gt; trunc(SYSDATE-31))</sql>
