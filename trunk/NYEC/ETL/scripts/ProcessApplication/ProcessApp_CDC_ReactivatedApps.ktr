    <sql>-- Identify Applications that have been reactivated 

Select To_Number(Application_ID) Oltp_App_id
      ,Reactivation_Ind Oltp_Reactivation_Ind
      ,Reactivation_TS  Oltp_Reactivation_DT
      ,(Select report_label From enum_app_reactivation_reason Where value = Reactivation_reason_cd ) Oltp_Reactivation_Reason  
      ,to_char(Reactivated_By)         Oltp_Reactivated_By

	  ,(Select WorkFlow_Reactivated_Ind From app_header_react_detail rd 
		 Where rd.application_id = ah.application_id and rd.Reactivation_TS = ah.Reactivation_TS) Oltp_Workflow_React_Ind

	  ,AH.Create_TS Oltp_App_Crt_dt
	  ,(Select Min(mailed_date) From  App_Mailed_Calendar Where mailed_date &gt;= trunc(AH.Create_TS)) New_Last_Mail_dt
 from App_Header AH
Where Reactivation_Ind &lt;&gt; 0
  and NVL(ah.app_form_cd,&apos;***&apos;) != &apos;PAP&apos;</sql>
