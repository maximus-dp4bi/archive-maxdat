  <step>
    <name>Database join - Get FPBP Details</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>Select New_wms_reason
     , New_ma_reason
     , New_Rev_Clear_Ind
     , New_Rev_Clear_Outcome
     , New_Rev_Clear_Reason
     , New_Office_id
     , New_Provider_name

	 , (Select Client_CIN From App_Individual a Where a.Application_id = Ah.Application_id and a.applicant_ind = 1 and Rownum = 1) New_CIN    
	 , (Select App_individual_id From App_Individual b Where b.Application_id = Ah.Application_id and b.applicant_ind = 1 and Rownum = 1) New_Applicant_id

 from app_header ah
 
Left Outer join ( Select Case_Reas_cd New_wms_reason
          , ma_reas_cd New_ma_reason
          , to_char(Reverse_Clear_Ind) New_Rev_Clear_Ind
          , to_char(Reverse_Clear_Outcome) New_Rev_Clear_Outcome
          , Reverse_Clear_Reas New_Rev_Clear_Reason
          , ref_id 
       from wms_data w
      Where w.ref_type = &apos;APP_HEADER&apos;
        and w.applicant_number is null
        and w.Hist_Ind = 0 ) wms on ah.application_id = wms.ref_id

Left Outer Join  (Select to_number(O.Office_id) New_Office_ID
 					   , O.Office_name New_Provider_Name
					   , apl.application_id	
                    From App_Provider_Link apl , Office O 
                   Where apl.office_id = O.Office_Id   
                     and exists ( Select &apos;x&apos; from App_Provider_Link s
                                   where apl.application_id = s.application_id
                                  having apl.app_provider_link_id = max(s.app_provider_link_id))) Prov  on ah.application_id = Prov.application_id
Where ah.application_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
