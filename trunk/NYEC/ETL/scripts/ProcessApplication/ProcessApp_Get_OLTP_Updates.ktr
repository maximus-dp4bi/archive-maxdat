  <step>
    <name>Database join - Retrieve OLTP Updates for an Application</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>20</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>Select AH.Receipt_date Receipt_dt
      ,AH.Status_Date Status_dt
      ,AH.App_Media_CD Channel
	  ,AH.Updated_By Completed_By
	  ,SYSDATE STG_LAST_PROCESSED_DT

	  ,${SLA_DAYS} 	AP_SLA_DAYS
	  ,&apos;${SLA_DAYS_TYPE}&apos; AP_SLA_DAYS_TYPE
	  ,${SLA_JEOPARDY_DAYS} AP_SLA_JEOPARDY_DAYS
	  ,${SLA_TARGET_DAYS} AP_SLA_TARGET_DAYS


	  ,	CASE WHEN ( Select count(1) from App_doc_data ad 
                     where AD.Application_id = AH.Application_ID and Document_Type_CD = &apos;APPLICATION&apos; 
                       and Document_Sub_Type LIKE &apos;%FPBP%&apos; ) &gt; 0 
             THEN &apos;FPBP&apos;
             ELSE ( Select Distinct Document_Sub_Type from App_doc_data ad 
                     where AD.Application_id = AH.Application_ID and Document_Type_CD = &apos;APPLICATION&apos; 
		               and Renewal_Ind = 1 and Primary_App_Ind = 1)
              END App_Type

      ,(select E.Report_label from enum_app_status E where E.Value = AH.Status_cd) App_Status

      ,  CASE AH.STATUS_CD 
         WHEN  &apos;RENEWAL_INITIATED&apos; THEN &apos;Renewal Initiated&apos;
         WHEN  &apos;RECEIVED&apos; THEN &apos;Received&apos;
         WHEN  &apos;DISREGARDED&apos;  THEN &apos;Cancelled&apos;
		 WHEN  &apos;VOID&apos;  THEN &apos;Cancelled&apos;
         WHEN  &apos;COMPLETED&apos; THEN &apos;Done&apos; 
         WHEN  &apos;TIMEOUT&apos; THEN &apos;Done&apos; 
         WHEN  &apos;INPROCESS&apos; THEN 
              (CASE (select Distinct &apos;MI&apos; from app_Tracker t 
					  where t.application_id = ah.application_id and History_Ind = 0 and RFE_STATUS_CD = &apos;AWAITING_MI&apos; )
               WHEN &apos;MI&apos; Then &apos;Awaiting Missing Information&apos; 
               ELSE &apos;In Process&apos; End )
         ELSE AH.STATUS_CD  END App_Status_Group
         
      ,  CASE WHEN AH.STATUS_CD IN (&apos;DISREGARDED&apos;,&apos;VOID&apos;) THEN &apos;Y&apos; ELSE &apos;N&apos; END Cancel_Flag
         
      ,  CASE WHEN AH.STATUS_CD IN (&apos;DISREGARDED&apos;,&apos;VOID&apos;) THEN AH.Status_Date ELSE NULL END Cancel_dt
      
      ,  CASE WHEN AH.STATUS_CD IN (&apos;COMPLETED&apos;,&apos;TIMEOUT&apos;,&apos;DISREGARDED&apos;,&apos;VOID&apos;) THEN AH.Status_Date 
              ELSE NULL END Complete_dt

      ,  CASE WHEN AH.STATUS_CD IN (&apos;COMPLETED&apos;,&apos;TIMEOUT&apos;,&apos;DISREGARDED&apos;,&apos;VOID&apos;) THEN &apos;Complete&apos;
              ELSE &apos;Active&apos; END Instance_Status

       , CASE WHEN AH.STATUS_CD IN (&apos;COMPLETED&apos;,&apos;TIMEOUT&apos;,&apos;DISREGARDED&apos;,&apos;VOID&apos;) THEN AH.Status_Date 
              ELSE NULL END Instance_Complete_dt
             
       , CASE WHEN AH.STATUS_CD IN (&apos;COMPLETED&apos;,&apos;TIMEOUT&apos;,&apos;DISREGARDED&apos;,&apos;VOID&apos;) THEN 
                 (CASE (select Distinct T.RFE_STATUS_CD from app_Tracker T 
						 where T.application_id = AH.application_id and T.History_Ind = 0 and T.RFE_STATUS_CD = &apos;ACCEPTED&apos; )
                  WHEN &apos;ACCEPTED&apos; Then &apos;Renewed&apos; 
                  ELSE &apos;Not Renewed&apos; End )
               ELSE NULL END Eligibility_Action
             
       , CASE WHEN AH.REF_VALUE_1 IS NULL THEN &apos;N&apos; ELSE &apos;Y&apos; END Heart_Synch_Flag
       , (Select Distinct AAS.Report_label 
            From hrt_case_control HCC, enum_app_agency_app_status AAS 
           where HCC.current_status_cd = AAS.value and HCC.application_id = AH.Application_id) Heart_App_Status
           
       , CASE (SELECT COUNT(1) FROM HRT_CASE, HRT_CASE_CONTROL
	            WHERE HRT_CASE.HRT_CASE_ID = HRT_CASE_CONTROL.HRT_CASE_ID 
       			  AND HRT_CASE.APPLICATION_ID = AH.APPLICATION_ID
		          AND HRT_CASE_CONTROL.CURRENT_STATUS_CD=&apos;03&apos;
		          AND HRT_CASE.REPROCESS_IND IN (&apos;1&apos;, &apos;Y&apos;)
		          AND EXISTS ( SELECT 1
				   	             FROM HRT_CHECKLIST 
					            WHERE HRT_CASE.HRT_CASE_ID = HRT_CHECKLIST.HRT_CASE_ID ))     
         WHEN 0 THEN &apos;N&apos; ELSE &apos;Y&apos; END AUTO_REPROCESS_FLAG 
         
       , CASE (SELECT COUNT(1) FROM HRT_CASE
	           WHERE HRT_CASE.APPLICATION_ID= AH.APPLICATION_ID
			     AND EXISTS ( 
				              SELECT 1
					            FROM HRT_CASE_REFERRAL 
				               WHERE HRT_CASE_REFERRAL.CASE_REFERRAL_ID = ( SELECT MAX(HCR.CASE_REFERRAL_ID)
														                      FROM HRT_CASE_REFERRAL HCR
														                     WHERE HCR.HRT_CASE_ID = HRT_CASE.HRT_CASE_ID
																               AND HCR.REFERRAL_RES_CD IS NULL 
																			   AND HCR.REFERRAL_CD = &apos;01&apos; )                
					         ))
         WHEN 0 THEN &apos;N&apos; ELSE &apos;Y&apos; END REFER_LDSS_FLAG  

       , CASE (SELECT COUNT(1) FROM HRT_CASE 
	            WHERE HRT_CASE.APPLICATION_ID = AH.APPLICATION_ID
                  AND ( 
                     ( HRT_CASE.TRANSACTION_TYPE_CD in (&apos;07&apos;, &apos;08&apos;)
				               AND EXISTS ( SELECT 1  FROM HRT_CASE_CONTROL
                 		                 	 WHERE HRT_CASE.HRT_CASE_ID = HRT_CASE_CONTROL.HRT_CASE_ID 
                                       		   AND CURRENT_TIMESTAMP &lt; AH.RENEWAL_DUE_DATE
                                       		   AND HRT_CASE_CONTROL.CURRENT_STATUS_CD = &apos;05&apos;) 
                      ) 
                  	OR EXISTS (SELECT 1 FROM HRT_CASE_DEFERRAL
                                WHERE HRT_CASE_DEFERRAL.CASE_DEFERRAL_ID = ( SELECT MAX(HCD.CASE_DEFERRAL_ID)
                                                                               FROM HRT_CASE_DEFERRAL HCD
                                                                              WHERE HRT_CASE.HRT_CASE_ID = HCD.HRT_CASE_ID 
                                                                                AND (HCD.DEFERRAL_RES_CD IS NULL OR HCD.DEFERRAL_RES_CD = &apos;04&apos;)
                                                                                AND HCD.DEFERRAL_CD = &apos;02&apos; )
	                          )        
                    ))
         WHEN 0 THEN &apos;N&apos; ELSE &apos;Y&apos; END CLOCKDOWN_INDICATOR

       ,(Select Min(mailed_date) From  App_Mailed_Calendar Where mailed_date &gt;= trunc(AH.Create_TS)) Last_Mail_dt

	   ,(Select Substr(Reason_Cd,1,50) from app_Event_log el where el.application_id = ah.application_id 
             and Action_Cd = &apos;REFERTORESEARCH&apos; and App_Event_CD = &apos;USER_ACTION&apos; and Rownum = 1 ) Research_Reason 

       , (select ec.REPORT_LABEL  from  app_case_link ac, enum_county ec
           where ac.APPLICATION_ID(+) = AH.Application_ID  and ec.VALUE(+) = SUBSTR(ac.CASE_CIN,1,3)
             and ec.EFFECTIVE_END_DATE is NULL ) County	

       , AH.Renewal_Due_Date App_Cycle_End_dt      
       
       , (Select Max (trunc(m.Satisfied_date)) From app_missing_info m where m.application_id = ah.application_id) Assd_Receive_Process_MI

	   , CASE WHEN AH.STATUS_CD = &apos;COMPLETED&apos; THEN 
				   (Select Distinct Satisfied_by From app_missing_info m1 
					 Where m1.application_id = ah.application_id
					   and exists ( select &apos;x&apos; from app_missing_info m2
					                 where m1.application_id = m2.application_id
						            having m1.Satisfied_date = max(m2.Satisfied_date))
					   and rownum = 1	)	
			  ELSE NULL END ASPB_RECEIVE_PROCESS_MI	

       , (Select MIN(Sent_on) From letter_request lr, letter_request_link ll , letter_definition ld
           where lr.lmreq_id = ll.lmreq_id 
           	 and lr.lmdef_id = ld.lmdef_id
             and ld.Name in (&apos;MI1&apos;,&apos;MI2&apos;,&apos;MI4&apos;)
             and ll.reference_type = &apos;APP_HEADER&apos; and ll.reference_id = ah.Application_id
		 	 and lr.requested_on &gt;= nvl(ah.reactivation_ts,ah.create_ts) ) Notify_Client_Pended_App_dt

       , (Select MAX(lr.lmreq_id) From letter_request lr, letter_request_link ll , letter_definition ld
           where lr.lmreq_id = ll.lmreq_id 
           	 and lr.lmdef_id = ld.lmdef_id
             and ld.Name in (&apos;MI1&apos;,&apos;MI2&apos;,&apos;MI4&apos;)
             and ll.reference_type = &apos;APP_HEADER&apos; and ll.reference_id = ah.Application_id
 		     and lr.requested_on &gt;= nvl(ah.reactivation_ts,ah.create_ts) ) Current_Mi_Letter_ID

	   ,(Select Count(Missing_Info_ID)
		   From app_missing_info m, enum_app_mi_type mt 
		  Where application_id = ah.application_id
		    and Satisfied_date is null
		    and Mi_Type_Cd = mt.Value
		    and Satisfied_without_tangible_ind = 1 ) Pending_Mi_Data_Count

	   ,(Select Count(Missing_Info_ID)
		   From app_missing_info m, enum_app_mi_type mt 
		  Where application_id = ah.application_id
		    and Satisfied_date is null
		    and Mi_Type_Cd = mt.Value
		    and Satisfied_without_tangible_ind = 0 ) Pending_Mi_Doc_Count

	   , CASE WHEN RTRIM(AH.ref_value_1,&apos;0123456789&apos;) IS NOT NULL THEN NULL -- Alphanumeric 
			  ELSE	
					(Select Distinct AAS.Report_label 
			           From hrt_case_control HCC, enum_app_agency_app_status AAS 
			          where HCC.current_status_cd = AAS.value and hcc.hrt_case_id = to_number(AH.ref_value_1))
			  END Heart_Case_Status

	   , (Select Case_CIN from app_case_link l where L.Application_id = AH.Application_id) State_Case_Iden

	   , CASE WHEN RTRIM(AH.ref_value_1,&apos;0123456789&apos;) IS NOT NULL THEN NULL -- Alphanumeric 
			  ELSE (Select QC_IND From hrt_case_control HCC where hcc.hrt_case_id = to_number(AH.ref_value_1))
			   END QC_IND

	   , (Select &apos;Y&apos; From app_Event_log el where el.application_id = ah.application_id and App_Event_CD = &apos;STOP_APPLICATION&apos; and rownum = 1 ) Stop_App_Exists
	   , Reactivation_Ind 	
       , &apos;0&apos; CURR_MODE_CD 
  	   , &apos;0&apos; CURR_MODE_LABEL
	   , &apos;N&apos; OUTCOME_NOTIFY_RQRD_FLAG
	   , &apos;0&apos; STOP_APP_REASON  

	   , CASE WHEN RTRIM(AH.ref_value_1,&apos;0123456789&apos;) IS NOT NULL THEN NULL -- Alphanumeric 
			  ELSE ( Select Case when Incomplete_application_ind = &apos;1&apos; THEN &apos;Y&apos; 
		               Else &apos;N&apos; END HEART_INCMPLT_APP_IND
					   From hrt_case hc Where hc.hrt_case_id = to_number(AH.ref_value_1))
			   END HEART_INCMPLT_APP_IND
	   
	   ,(Select count(1) from app_individual I where I.application_id = AH.application_id and I.applicant_ind = 1) Applicant_Cnt

	   ,(Select distinct First_Value(substr(document_sub_type,1,1)) OVER ( Partition by application_id order by document_sub_type desc) || 
			    COUNT(distinct document_sub_type) OVER (Partition by application_id) 
           from App_doc_data ad 
          where AD.Application_id = AH.Application_ID and Document_Type_CD = &apos;APPLICATION&apos; 
		    and Document_Sub_Type like &apos;%FPBP%&apos; ) FPBP_TYPE
 	   , Ref_Value_2 UPSTATE_INDICATOR

	   , &apos;0&apos; CIN
	   , &apos;0&apos; Provider_Name
	   , &apos;0&apos; WMS_Reason
	   , &apos;0&apos; MA_Reason
	   , &apos;0&apos; Rev_Clear_Reason
	   , &apos;0&apos; Rev_Clear_Outcome		
	   , &apos;0&apos; Rev_Clear_Ind
	   , to_number(&apos;0&apos;) Office_ID
	   , to_number(&apos;0&apos;) Applicant_ID	

  From App_Header AH 
 Where AH.Application_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>CURRENTAPPID</name>
        <type>Number</type>
      </field>
    </parameter>
