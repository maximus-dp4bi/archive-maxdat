  <step>
    <name>Database join - Get Reactivation Details</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>Select 
	   (Select Substr(E.report_label, 1,100)
    	   From ( Select Substr(Reason_Cd,1,50) Stop_Reas 
                 	   , Rank () over (partition by App_Event_CD order by App_Event_log_id desc) rk 
             		From app_Event_log el where el.application_id = ? 
              		 And App_Event_CD = &apos;STOP_APPLICATION&apos; 
					 And el.Create_TS &gt;= NVL(?,el.Create_ts)) SR, Enum_Stop_App_Reason E
	      Where Rk = 1 and Stop_Reas = E.Value ) New_Stop_App_Reason  


	   , CASE WHEN RTRIM(AH.ref_value_1,&apos;0123456789&apos;) IS NOT NULL THEN NULL -- Alphanumeric 
			  ELSE	
					(Select Current_Reactivation_Mode_Cd From hrt_reactivation_control rc 
			          where rc.hrt_case_id = to_number(AH.ref_value_1)
		                and reactivation_control_id = ( Select max(reactivation_control_id) From hrt_reactivation_control rcc where rc.hrt_case_id = rcc.hrt_case_id ))
			  END NEW_CURR_MODE_CD 


	   , CASE WHEN RTRIM(AH.ref_value_1,&apos;0123456789&apos;) IS NOT NULL THEN NULL -- Alphanumeric 
			  ELSE	
					(Select Distinct HRM.Report_label 
		               From hrt_reactivation_control rc , enum_hrt_reactivation_mode_cd hrm
			          where RC.Current_Reactivation_Mode_Cd = HRM.value and rc.hrt_case_id = to_number(AH.ref_value_1)
		                and Reactivation_control_id = ( Select max(Reactivation_control_id) From hrt_reactivation_control rcc where rc.hrt_case_id = rcc.hrt_case_id ))
			  END NEW_CURR_MODE_LABEL

	   , CASE WHEN 

				  (Select E.LMDEF_ID
			    	 From ( Select Substr(Reason_Cd,1,50) Stop_Reas 
                 	 			  , Rank () over (partition by App_Event_CD order by App_Event_log_id desc) rk 
		               		  From app_Event_log el where el.application_id = ? 
              		 		   And App_Event_CD = &apos;STOP_APPLICATION&apos; ) SR, Enum_Stop_App_Reason E
	      			Where Rk = 1 and Stop_Reas = E.Value ) IS NOT NULL  

					AND

				  (Select WorkFlow_Reactivated_IND 
				     From app_header_react_detail rd 
				    Where rd.application_id = ah.application_id 
					  And History_Ind = 0) = 1

			  THEN &apos;Y&apos;
			  ELSE &apos;N&apos; END NEW_OUTCOME_NOTIFY_RQRD_FLAG

From App_header AH
Where Application_id = ?	  </sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>CURRENTAPPID</name>
        <type>Integer</type>
      </field>
      <field>
        <name>REACTIVATION_DT</name>
        <type>Date</type>
      </field>
      <field>
        <name>CURRENTAPPID</name>
        <type>Integer</type>
      </field>
      <field>
        <name>CURRENTAPPID</name>
        <type>Integer</type>
      </field>
    </parameter>
