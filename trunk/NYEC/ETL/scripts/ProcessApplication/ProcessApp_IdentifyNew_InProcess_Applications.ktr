  <step>
    <name>Database join - Identify Apps </name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>5</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>1</rowlimit>
    <sql>Select App_ID Stg_App_id
	 , &apos;Y&apos; New_App_In_Process
     ,  1  New_App_Priority_Ind
	 , SYSDATE STG_LAST_PROCESSED_DT
  from Process_App_Stg A
 where App_In_Process = &apos;Y&apos;
   and App_Extract_complete_dt is null
   and App_Priority_Ind = 0
   and App_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>CURRENTAPPID</name>
        <type>Integer</type>
      </field>
    </parameter>
     <cluster_schema/>
 <remotesteps>   <input>   </input>   <output>   </output> </remotesteps>    <GUI>
      <xloc>412</xloc>
      <yloc>262</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>Table input - Identify Renewal App to Timeout or Withdrawn</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>Select To_Number(Application_id) CURRENTAPPID 
  from App_Header
 Where Status_cd in (&apos;DISREGARDED&apos;,&apos;TIMEOUT&apos;)
   and trunc(Status_Date) &gt;= trunc(Current_Timestamp) - ${AP_PROCESS_DAYS} </sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
