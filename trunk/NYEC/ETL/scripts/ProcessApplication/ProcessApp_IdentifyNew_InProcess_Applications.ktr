  <step>
    <name>Table input - IdentifyNew In Process App</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>Select To_Number(App_id) CURRENTAPPID 
	 , &apos;Y&apos; New_App_In_Process
     ,  1  New_App_Priority_Ind
	 , SYSDATE STG_LAST_PROCESSED_DT
  from Process_App_Stg A
 Where A.App_extract_complete_dt is null
   and exists ( Select 1 
				  From Step_Instance_Stg S
			     where S.Ref_type = &apos;APP_HEADER&apos; and S.Ref_ID = A.App_ID
           		   and trunc(Hist_create_ts) &gt;= trunc(sysdate) - ${AP_PROCESS_DAYS} )</sql>
    <limit>0</limit>
