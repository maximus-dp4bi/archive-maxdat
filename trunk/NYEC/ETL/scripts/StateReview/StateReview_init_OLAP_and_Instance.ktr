  <step>
    <name>Database join 2 -- Get MAXDAT Data</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT (
        select case when step_definition_id in (SELECT ref_id
                                               FROM corp_etl_list_lkup
                                               WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                                     AND out_var = &apos;State Review&apos;
                                                     AND SYSDATE BETWEEN start_date AND end_date)
                    then
                        &apos;State Review&apos;
                    when step_definition_id in (SELECT ref_id
                                               FROM corp_etl_list_lkup
                                               WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                                     AND out_var = &apos;Data Correction&apos;
                                                     AND SYSDATE BETWEEN start_date AND end_date)
                    then
                        &apos;Data Correction&apos;
                    when step_definition_id in (SELECT ref_id
                                               FROM corp_etl_list_lkup
                                               WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                                     AND out_var = &apos;Research&apos;
                                                     AND SYSDATE BETWEEN start_date AND end_date)
                    then
                        &apos;Research&apos;
                    else
                        &apos;Other&apos;
               end
        from step_instance_stg
		where step_instance_id = si_stg.step_instance_id and rownum = 1
		) as &quot;CURRENT_TASK_TYPE&quot;,
		(
		select HIST_STATUS from step_instance_stg si_stg2 where step_instance_history_id = (select MAX(step_instance_history_id) 
        		                                                                           from step_instance_stg 
                		                                                                   group by ref_id, step_instance_id 
                        		                                                           having ref_id = si_stg2.ref_id and step_instance_id = si_stg2.step_instance_id)
                                				                and ref_id = si_stg.ref_id and step_instance_id = si_stg.step_instance_id and rownum = 1
        ) as &quot;CURRENT_TASK_STATUS&quot;,
		(
		select hist_create_ts from step_instance_stg si_stg2 where hist_create_ts = (select MAX(hist_create_ts) 
        		                                                                    from step_instance_stg 
                		                                                            where ref_id = si_stg.ref_id
                        		                                                    group by ref_id, step_instance_id, hist_status 
                                		                                            having ref_id = si_stg2.ref_id and step_instance_id = si_stg2.step_instance_id and HIST_STATUS = &apos;CLAIMED&apos;)
                                				                and ref_id = si_stg.ref_id and step_instance_id = si_stg.step_instance_id and rownum = 1
		) as &quot;Current_Task_Claim_Date&quot;,
		(
		select hist_create_ts from step_instance_stg si_stg2 where hist_create_ts = (select MAX(hist_create_ts) 
        		                                                                    from step_instance_stg 
                		                                                            where ref_id = si_stg.ref_id
                        		                                                    group by ref_id, step_instance_id, hist_status 
                                		                                            having ref_id = si_stg2.ref_id and step_instance_id = si_stg2.step_instance_id and HIST_STATUS = &apos;CLAIMED&apos;)
                                        		        						           and step_definition_id in (SELECT ref_id
                                                		                        						              FROM corp_etl_list_lkup
						                                                        		                              WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                        						                                        		                           AND out_var = &apos;Research&apos;
                                                						                        		                   AND SYSDATE BETWEEN start_date AND end_date)
                                				                and ref_id = si_stg.ref_id and step_instance_id = si_stg.step_instance_id and rownum = 1
		) as &quot;Research_Task_Claim_Date&quot;,
--        Slkup.Display_Name as &quot;ASPD_RECIEVE_STATE_REVIEW&quot;,
        si_stg.ref_id as &quot;MDApplication_ID&quot;,
		si_stg.step_instance_id as &quot;MDTask_ID&quot;,
        bus_days_between((select create_ts from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1), SYSDATE) as &quot;BUSINES DAYS&quot;,
        trunc((SYSDATE - (select create_ts from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1))) as &quot;CALENDAR DAYS&quot;,
        case when Slkup.First_Name = &apos;AutoCloseStateReviewTasks&apos; then &apos;Y&apos; else &apos;N&apos; end as &quot;Auto Close&quot;,
        (select step_instance_id from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1) as &quot;Current_Task_ID&quot;,
        (select completed_ts from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1) as &quot;COMPLETED DATE&quot;,
        (select step_instance_id from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1) as &quot;State_Review_Task&quot;,
--RECEIVE_STATE_REVIEW        
        (select completed_ts from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum = 1) as &quot;ASED_RECEIVE_STATE_REVIEW&quot;,
        (select Display_Name from Staff_Lkup Lkup where Lkup.Staff_id = si_stg.hist_owner and si_stg.hist_status = &apos;COMPLETED&apos;) as &quot;ASPB_RECEIVE_STATE_REVIEW&quot;,
        (select hist_create_by from step_instance_stg si_stg2 where HIST_STATUS = &apos;ClAIMED&apos; and  step_instance_id = si_stg.step_instance_id and rownum=1) as &quot;ASSD_RECEIVE_STATE_REVIEW&quot;,
-- PROCESS_DC
        (SELECT si_stg2.completed_ts
          FROM step_instance_stg si_stg2
         WHERE step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                   AND out_var = &apos;Data Correction&apos;
                   AND SYSDATE BETWEEN start_date AND end_date)
           AND si_stg2.hist_status = &apos;COMPLETED&apos;
           AND si_stg2.REF_ID = si_stg.REF_ID
           AND rownum = 1) &quot;ASED_PROCESS_DC&quot;,
        (SELECT Slkup2.Display_Name
          FROM step_instance_stg si_stg2
          left outer join Staff_LKup Slkup2 on HIST_CREATE_BY = Slkup2.Staff_id
         WHERE step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                   AND out_var = &apos;Data Correction&apos;
                   AND SYSDATE BETWEEN start_date AND end_date)
           AND si_stg2.hist_status = &apos;COMPLETED&apos;
           AND si_stg2.REF_ID = si_stg.REF_ID
           AND rownum = 1) &quot;ASPB_PROCESS_DC&quot;,
        (SELECT si_stg2.hist_create_ts
          FROM step_instance_stg si_stg2
         WHERE step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                   AND out_var = &apos;Data Correction&apos;
                   AND SYSDATE BETWEEN start_date AND end_date)
           AND si_stg2.hist_status = &apos;CLAIMED&apos;
           AND si_stg2.REF_ID = si_stg.REF_ID
           AND rownum = 1) as &quot;ASSD_PROCESS_DC&quot;,
--RESEARCH_DC
        (SELECT si_stg2.completed_ts
          FROM step_instance_stg si_stg2
         WHERE step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                   AND out_var = &apos;Research&apos;
                   AND SYSDATE BETWEEN start_date AND end_date)
           AND si_stg2.hist_status = &apos;COMPLETED&apos;
           AND si_stg2.REF_ID = si_stg.REF_ID
           AND rownum = 1) &quot;ASED_RESEARCH_DC&quot;,
        (SELECT Slkup2.Display_Name
          FROM step_instance_stg si_stg2
          left outer join Staff_LKup Slkup2 on HIST_CREATE_BY = Slkup2.Staff_id
         WHERE step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                   AND out_var = &apos;Research&apos;
                   AND SYSDATE BETWEEN start_date AND end_date)
           AND si_stg2.hist_status = &apos;COMPLETED&apos;
           AND si_stg2.REF_ID = si_stg.REF_ID
           AND rownum = 1) &quot;ASPB_RESEARCH_DC&quot;,
           
        (select status from step_instance_stg si_stg2 where step_instance_id = si_stg.step_instance_id and rownum=1) as &quot;INSTANCE STATUS&quot;,
        -- New State Review Task
        (select step_instance_id from step_instance_stg si
	       where step_instance_id = si_stg.step_instance_id and
    	    	REF_ID = si_stg.REF_ID and 
	        	step_definition_id in (SELECT ref_id FROM corp_etl_list_lkup
                                               WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                                     AND out_var = &apos;State Review&apos;
                                                     AND SYSDATE BETWEEN start_date AND end_date) and
	      		? = &apos;Y&apos; and
  			     EXISTS (select * from step_instance_stg where ref_id = si.ref_id and 
                                                      create_ts &lt; si.create_ts and
                                                      step_definition_id in (SELECT ref_id
                                                                             FROM corp_etl_list_lkup
                                                                             WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                                                                   AND (out_var = &apos;Data Correction&apos; or out_var = &apos;Research&apos;)
                                                                                   AND SYSDATE BETWEEN start_date AND end_date))
		        and rownum=1) as &quot;New_State_Review_Task&quot;,

				(select LETTER_ID as &quot;LETTER_REQ_ID&quot;
					from Letters_Stg 
					where LETTER_TYPE_CD = &apos;MI2&apos; and LETTER_APP_ID = si_stg.REF_ID and LETTER_ID = (SELECT MAX(LETTER_ID) from Letters_Stg where LETTER_APP_ID = si_stg.REF_ID)) as &quot;LETTER_REQ_ID&quot;,

				(select LETTER_STATUS_CD as &quot;LETTER_STATUS&quot;
					from Letters_Stg
					where LETTER_TYPE_CD = &apos;MI2&apos; and LETTER_APP_ID = si_stg.REF_ID and LETTER_ID = (SELECT MAX(LETTER_ID) from Letters_Stg where LETTER_APP_ID = si_stg.REF_ID)) as &quot;LETTER_STATUS&quot;,

				(select LETTER_UPDATE_TS as &quot;LETTER_STATUS_DATE&quot;
					from Letters_Stg 
					where LETTER_TYPE_CD = &apos;MI2&apos; and LETTER_APP_ID = si_stg.REF_ID and LETTER_ID = (SELECT MAX(LETTER_ID) from Letters_Stg where LETTER_APP_ID = si_stg.REF_ID)) as&quot;LETTER_STATUS_DATE&quot;

FROM step_instance_stg si_stg
left outer join Staff_LKup Slkup on si_stg.HIST_CREATE_BY = Slkup.Staff_id
