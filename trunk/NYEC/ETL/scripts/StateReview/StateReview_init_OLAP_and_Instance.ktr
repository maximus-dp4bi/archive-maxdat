  <step>
    <name>Database join -- Get OLTP Data</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>
  select  appH.CREATE_TS as &quot;Capture Date&quot;,
		to_char(SYSDATE, &apos;yyyy&#47;mm&#47;dd HH:MI:SS&apos;) as &quot;Capture_Date_string&quot;,
		&apos;STATE_REVIEW_CAPTURE_DATE&apos; as &quot;Update_Field&quot;,
        appH.Renewal_Due_Date as &quot;Approval Date&quot;,
        (case when (appM.MI &gt; 0) then &apos;N&apos; else &apos;Y&apos; end) as &quot;ALL SATISFIED&quot;,
        appH.Application_ID as &quot;QApplication_ID&quot;,
		si.step_instance_id as &quot;QTast_ID&quot;,
        CC.CALL_RECORD_ID as &quot;CALL_CAMPAIN_ID&quot;,
        appH.Status_Cd as &quot;Application Status&quot;,

        (select RFE_STATUS_CD from App_Tracker where Application_ID = appH.Application_ID and Client_ID = si.client_id and rownum = 1) as &quot;RFE_STATUS&quot;,
        case when ((appH.Renewal_Due_Date &gt; (select CREATE_TS from CALL_RECORD where CALL_RECORD_ID = CC.CALL_RECORD_ID))
              and ((select count(RFE_STATUS_CD) from App_Tracker  where Application_ID = appH.Application_Id and HISTORY_IND = 0 and RFE_STATUS_CD = &apos;AWAITING_MI&apos;)&gt;0)
              and (appH.Status_Cd = &apos;INPROCESS&apos;)) and (select DO_NOT_CALL_IND from client cl where si.client_id = cl.clnt_client_id)=0 and 
              (select case 
       				   	    	when app_form_cd=&apos;FPBP&apos; then 
				                   1 
			         	      else 
				                   0 
     			         	  end  from app_header where application_id = appH.Application_Id) = 1
             then &apos;Y&apos; 
             else &apos;N&apos; 
        end as &quot;CALL_CAMPAIN_FLAG&quot;,
        
		(select case 
       	 	    	when app_form_cd=&apos;FPBP&apos; then 
		                   &apos;Y&apos;
		        else 
			               &apos;N&apos; 
     		    end  from app_header where application_id = appH.Application_Id) as &quot;FPBP_FLAG&quot;,

        case when (appH.Status_Cd = &apos;DISREGARD&apos; or appH.Status_Cd = &apos;TIMEOUT&apos;) then appH.Update_Ts else null end as &quot;CANCEL DATE&quot;,
        
--        (Select MAX(lr.lmreq_id) as &quot;Letter_REQ_ID&quot;  From letter_request      lr, letter_request_link ll  where lr.lmreq_id = ll.lmreq_id and Ll.reference_id = appH.Application_Id and rownum=1
--              group by Ll.reference_id) as &quot;LETTER_REQ_ID&quot;,
--        
--        (Select lr.status_cd as &quot;Letter_Status&quot; From letter_request  lr, letter_request_link ll  
--          where lr.lmreq_id = ll.lmreq_id and Ll.reference_id = appH.Application_ID and lr.lmreq_id = 
--                (Select MAX(lr.lmreq_id) as &quot;Letter_REQ_ID&quot;  From letter_request lr, letter_request_link ll  
--                   where lr.lmreq_id = ll.lmreq_id and Ll.reference_id = appH.Application_ID
--                      group by Ll.reference_id
--                ) and rownum=1
--         ) as &quot;LETTER_STATUS&quot;,
--        (Select lr.update_ts as &quot;Letter_Status_date&quot; From letter_request  lr, letter_request_link ll  
--          where lr.lmreq_id = ll.lmreq_id and Ll.reference_id = appH.Application_ID and lr.lmreq_id = 
--                (Select MAX(lr.lmreq_id) as &quot;Letter_REQ_ID&quot;  From letter_request lr, letter_request_link ll  
--                   where lr.lmreq_id = ll.lmreq_id and Ll.reference_id = appH.Application_ID
--                      group by Ll.reference_id
--                ) and rownum=1
--         ) as &quot;LETTER_STATUS_DATE&quot;,
		
        -- MI not implemented so I am not sure yet.
        case when (select count(DISTINCT RFE_STATUS_CD) from App_Tracker where Application_ID =  appH.Application_Id ) &gt; 1 then &apos;N&apos; else &apos;Y&apos; 
         end  RFE_Status_Flag,
--        HrtCC.Qc_Ind as &quot;State Acceptance indicator&quot;,
        (SELECT qc_ind FROM Hrt_Case_Control HrtCC WHERE  HrtCC.Application_Id = appH.Application_Id) state_acpt_Ind,
        
        (select case when (select count(*) from App_Tracker where application_id = appH.Application_Id and HISTORY_IND=0) = 
                          (select count(*) from App_Tracker where application_id = appH.Application_Id and (RFE_STATUS_CD = &apos;ACCEPTED&apos;) and HISTORY_IND=0) then &apos;Accept&apos;
                     when (select count(*) from App_Tracker where application_id = appH.Application_Id and (RFE_STATUS_CD = &apos;REJECTED&apos;) and HISTORY_IND=0) &gt; 1 then &apos;Reject&apos;
                     else null
                end  
         from Dual) State_Review_Outcome

        
from App_Header appH
       left outer join (select count(case when SATISFIED_DATE IS NULL then 1 else NULL end) &quot;MI&quot;, Application_ID from App_Missing_Info where Application_ID = ? group by Application_ID) appM
         on appH.Application_Id = appM.Application_Id

--        left outer join Hrt_Case_Control HrtCC on appH.Application_Id = HrtCC.Application_Id

--        left outer join (select count(DISTINCT RFE_STATUS_CD) &quot;RFE_STATUS_DIF&quot;, Application_ID from App_Tracker where Application_ID = 151212 group by Application_ID) appT
--         on appM.Application_Id = appT.Application_Id

        left outer join (select CALL_RECORD_ID, Application_ID from App_Missing_Info where Application_ID = ?) CC 
             on CC.Application_ID = appH.Application_ID

--        join (select RFE_STATUS_CD, Application_ID from App_Tracker where HISTORY_IND = 0 and Application_ID = 151212) rfestat on rfestat.Application_Id = appH.Application_Id

		join step_instance si on si.step_instance_id = ?
where appH.Application_Id = ? and si.step_instance_id = ? and CC.Application_ID = ? and rownum=1 -- get only one task of an application and this will bring back every task.  This query gets application data with subqueries
</sql>
