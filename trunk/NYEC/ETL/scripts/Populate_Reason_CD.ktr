    <connection>MAXDAT</connection>
    <sql>SELECT DISTINCT sis.step_instance_id
      ,sis.ref_id as app_id
FROM step_instance_stg sis, corp_etl_list_lkup ll
WHERE reason_cd IS NULL
  AND step_definition_id = ll.ref_id
  AND name         = &apos;TASK_MONITOR_TYPE&apos;
  AND out_var      = &apos;Research&apos;
  AND ll.ref_type  = &apos;STEP_DEFINITION_ID&apos; 
  AND sis.ref_type IN (&apos;APP_HEADER&apos;,&apos;DOCUMENT_SET_ID&apos;)
  AND SYSDATE BETWEEN start_date AND end_date
  AND step_instance_history_id &gt; ?
--and sis.step_instance_id IN (5540461,5540831)
