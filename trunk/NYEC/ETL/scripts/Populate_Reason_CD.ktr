    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>select trr.value oltp_reason_cd
from step_instance si
inner join step_definition sd on si.step_definition_id = sd.step_definition_id
and sd.step_type_cd in(&apos;HUMAN_TASK&apos;,&apos;VIRTUAL_HUMAN_TASK&apos;)
inner join enum_task_research_reason trr on 
      -- Capture tag value only (multiple ENUM can appear within CLOB
      instr(substr(input_data,instr(input_data,&apos;RESEARCH_REF_REASON&apos;),100)
                                   ,&apos;&lt;string&gt;&apos;||trr.value||&apos;&lt;&#47;string&gt;&apos;) &gt; 0
where ref_id = ?
and ref_type  IN (&apos;APP_HEADER&apos;,&apos;DOCUMENT_SET_ID&apos;)
and input_data like &apos;%RESEARCH_REF_REASON%&apos;
and step_instance_id = ?
</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>APP_ID</name>
        <type>Number</type>
      </field>
      <field>
        <name>STEP_INSTANCE_ID</name>
        <type>Number</type>
      </field>
    </parameter>
