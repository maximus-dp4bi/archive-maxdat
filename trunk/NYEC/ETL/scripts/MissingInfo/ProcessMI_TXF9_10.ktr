  <step>
    <name>Table input</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>select B.*,       
      (CASE
            WHEN  In_MI_CYCLE_END_DATE IS NULL then &apos;Not Complete&apos;
            WHEN In_SLA_DAYS is null then &apos;Not Required&apos;
            WHEN In_SLA_DAYS_TYPE = &apos;B&apos; AND  In_Age_In_Business_Days &gt; In_SLA_DAYS then &apos;Untimely&apos;
            WHEN In_SLA_DAYS_TYPE = &apos;C&apos; AND  In_Age_in_calendar_days &gt; In_SLA_DAYS then &apos;Untimely&apos;
            else &apos;Timely&apos;
        END) as IN_TIMELINESS_STATUS
from (
select A.*,
       trunc(nvl(in_mi_task_cancel_date,sysdate)) - trunc(in_mi_task_create_date) as In_Age_in_calendar_days,
       bus_days_between(trunc(in_mi_task_create_date), trunc(nvl(in_mi_task_cancel_date,sysdate))) as in_Age_In_Business_Days,
       bus_days_between(trunc(mi_cycle_start_dt), trunc(nvl(in_mi_cycle_end_date,sysdate))) as in_mi_cycle_bus_days
from        
(
select distinct 
     mi_task_id as in_mi_task_id,
     mi_task_create_date as in_mi_task_create_date,    
     mi_cycle_start_dt,     
     &apos;Y&apos;        as YES,
     SYSDATE    as SYS_DATE,    
     &apos;Complete&apos; as Complete_d,
     TO_NUMBER(Null) as null_col,
     si.hist_create_ts as in_mi_task_cancel_date,     
     --Age_in_calendar_days as in_Age_in_calendar_days,
     sla_days        as in_sla_days,
     sla_days_type   as in_sla_days_type,
     si.hist_create_ts as in_mi_cycle_end_date,
	   &apos;Y&apos; as Inyes
from 
     process_mi_stg    stg,
     step_instance_stg si
where 1=1
and stg.mi_task_id = si.step_instance_id
and stg.cancel_date is null
and stg.instance_status = &apos;Active&apos;
and si.step_instance_history_id = (select max(step_instance_history_id)
                                     from step_instance_stg si_2
                                    where si_2.step_instance_id = si.step_instance_id
                                      and si_2.hist_create_ts &lt;= ?)
and si.status in (&apos;TIMEOUT&apos;, &apos;TERMINATED&apos;)
) A
) B</sql>
