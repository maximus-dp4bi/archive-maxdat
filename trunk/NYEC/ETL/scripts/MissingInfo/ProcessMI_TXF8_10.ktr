    <notepad>
      <note>TXF8	Determine if instance has ended the process and no longer needs to be updated
	&quot;Check When ASF_CREATE_STATE_ACCEPT = Y
or gwf_MI_outcome = &apos;S&apos; 
or gwf_MI_outcome = &apos;M&apos; 
or ASF_SEND_MI_LETTER = &apos;Y&apos; 

TXF8_10	
       set STAGE_DONE_DATE = SYSDATE
	   set INSTANCE_COMPLETE_DATE = SYSDATE
	   set INSTANCE_STATUS = &apos;Complete&apos;
	   set CURRENT_TASK_ID = NULL
       set MI_CYCLE_END_DT = INSTANCE_COMPLETE_DATE
       set MI_CYCLE_BUS_DAYS
       set TIMELINESS_STATUS
       set AGE_IN_BUSINESS_DAYS = Business Days between instance start date and instance complete date
       set AGE_IN_CALENDAR_DAYS = calendar days between instance start date and instance complete date

</note>
