    <connection>MAXDAT</connection>
    <sql>select B.*,
       (CASE
            WHEN  In_MI_CYCLE_END_DATE IS NULL then &apos;Not Complete&apos;
            WHEN In_SLA_DAYS is null then &apos;Not Required&apos;
            WHEN In_SLA_DAYS_TYPE = &apos;B&apos; AND  In_Age_In_Business_Days &gt; In_SLA_DAYS then &apos;Untimely&apos;
            WHEN In_SLA_DAYS_TYPE = &apos;C&apos; AND  In_Age_in_calendar_days &gt; In_SLA_DAYS then &apos;Untimely&apos;
            else &apos;Timely&apos;
        END) as IN_TIMELINESS_STATUS
from (        
select A.*,
       in_Instance_Complete_Date as in_mi_cycle_end_date,
       trunc(nvl(in_Instance_Complete_Date,sysdate)) - trunc(in_mi_task_create_date) as In_Age_in_calendar_days,
       bus_days_between(trunc(in_mi_task_create_date), trunc(nvl(in_Instance_Complete_Date,sysdate))) as in_Age_In_Business_Days,
       bus_days_between(trunc(mi_cycle_start_dt), trunc(nvl(in_Instance_Complete_Date,sysdate))) as in_MI_cycle_Bus_Days
from        
(
select
    mi_task_id as in_mi_task_id,
    app_id     as in_app_id,
    mi_task_create_date as in_mi_task_create_date,
    mi_cycle_start_dt,            
    &apos;Y&apos;        as YES,
    SYSDATE    as SYS_DATE,    
    &apos;Complete&apos; as Complete_d,
    TO_NUMBER(Null) as null_col,
    (
    CASE when NOT(ASF_CREATE_STATE_ACCEPT = &apos;Y&apos; 
                  or gwf_MI_outcome = &apos;S&apos;  
                  or gwf_MI_outcome = &apos;M&apos;  
                  or ASF_SEND_MI_LETTER = &apos;Y&apos;) then CANCEL_DATE
    ELSE COALESCE(ASED_CREATE_STATE_ACCEPT,ASED_SEND_MI_LETTER,ASED_PERFORM_RESEARCH,ASED_PERFORM_QC,ASED_COMPLETE_MI_TASK) END
    ) as in_Instance_Complete_Date,         
     sla_days        as in_sla_days,
     sla_days_type   as in_sla_days_type
from process_mi_stg stg
where 1=1
and stg.instance_status = &apos;Active&apos;
and  (ASF_CREATE_STATE_ACCEPT = &apos;Y&apos;
      or gwf_MI_outcome = &apos;S&apos; 
      or gwf_MI_outcome = &apos;M&apos; 
      or ASF_SEND_MI_LETTER = &apos;Y&apos;)

) A
) B</sql>
