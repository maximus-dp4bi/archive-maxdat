    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>select  distinct
        si.step_instance_id as TASK_ID,
        --si.hist_status      as TASK_STATUS,
        si_cl.step_instance_id as StgRsrch_TASK_ID,
        si_cl.hist_status      as StgRsrch_TASK_STATUS,
        (case when si_cl.hist_status = &apos;CLAIMED&apos; then si_cl.hist_create_ts end) as StgRsrch_TASK_CLAIMED_DATE,
        (case when si_cl.hist_status = &apos;CLAIMED&apos; and si_cl.hist_create_by &lt;&gt;&apos;IMG&apos;
                                            then coalesce( (select first_name ||&apos; &apos; || last_name 
                                                    from staff_stg
                                                   where to_char(staff_id) = si_cl.hist_create_by)  ,si_cl.hist_create_by ) 
             when si_cl.hist_status = &apos;CLAIMED&apos; and si_cl.hist_create_by =&apos;IMG&apos; then si_cl.hist_create_by
             else null end)   as StgRsrch_TASK_CLAIMED_BY,
       (case when si_comp.hist_status in (&apos;COMPLETED&apos;,&apos;TERMINATED&apos;) then si_comp.hist_create_ts end) as StgRsrch_TASK_COMPLETED_DATE,
       (case when si_comp.hist_status in (&apos;COMPLETED&apos;,&apos;TERMINATED&apos;) and si_comp.hist_create_by &lt;&gt;&apos;IMG&apos;
                                            then coalesce( (select first_name ||&apos; &apos; || last_name 
                                                    from staff_stg
                                                   where to_char(staff_id) = si_comp.hist_create_by)  ,si_comp.hist_create_by )   
             when si_comp.hist_status in (&apos;COMPLETED&apos;,&apos;TERMINATED&apos;) and si_comp.hist_create_by =&apos;IMG&apos; then si_comp.hist_create_by
             else null end)   as StgRsrch_TASK_COMPLETED_BY
from step_instance_stg si
left outer join (select * from step_instance_stg si_cl_2 
                    where si_cl_2.step_instance_id = ? 
                   and si_cl_2.step_instance_history_id = (select min(si_cl_3.step_instance_history_id)
                                                             from step_instance_stg si_cl_3
                                                            where si_cl_3.step_instance_id = ?
                                                            and si_cl_3.hist_status = &apos;CLAIMED&apos;
                                                            and si_cl_3.hist_create_ts &lt;=  ?  
                                                            ) 
                   ) si_cl on si.step_instance_id = si_cl.step_instance_id
left outer join (select * from step_instance_stg si_comp_2 
                    where si_comp_2.step_instance_id = ? 
                   and si_comp_2.step_instance_history_id = (select min(si_comp_3.step_instance_history_id)
                                                             from step_instance_stg si_comp_3
                                                            where si_comp_3.step_instance_id = ?
                                                            and si_comp_3.hist_status in (&apos;COMPLETED&apos;,&apos;TERMINATED&apos;)
                                                            and si_comp_3.hist_create_ts &lt;=  ?
                                                            ) 
                   ) si_comp on si.step_instance_id = si_comp.step_instance_id
where si.step_instance_id = ?</sql>
