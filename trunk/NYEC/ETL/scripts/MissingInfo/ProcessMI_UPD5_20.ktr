    <connection>MAXDAT</connection>
    <sql>select B.*,
        case when nvl(stg_QC_Task_ID,9)           &lt;&gt; nvl(oltp_QC_Task_ID,9) then &apos;Y&apos;
             when nvl(stg_Research_Task_ID,9)     &lt;&gt; nvl(oltp_Research_Task_ID,9) then &apos;Y&apos;
             when nvl(stg_State_Review_Task_ID,9) &lt;&gt; nvl(oltp_State_Review_Task_ID,9) then &apos;Y&apos;
             when nvl(stg_Current_Task_ID,9)      &lt;&gt; nvl(oltp_Current_Task_ID,9) then &apos;Y&apos;
        else &apos;N&apos;            
        end as IN_any_upd_for_upd_5_20,
        &apos;Y&apos; as IN_yes
from (       
select A.*,
       nvl(nvl(nvl(A.oltp_State_Review_Task_ID, A.oltp_Research_Task_ID), A.oltp_QC_Task_ID),stg_MI_task_ID) as oltp_Current_Task_Id
from (           
select  
        stg.mi_task_id           as stg_MI_task_ID,
        stg.app_id               as stg_App_ID,
		    stg.qc_task_id           as stg_QC_Task_ID,
		    stg.research_task_id     as stg_Research_Task_ID,
		    stg.state_review_task_id as stg_State_Review_Task_ID,
		    stg.current_task_id      as stg_Current_Task_ID, 
        (select min(step_instance_id)
          from step_instance_stg   si
         where si.ref_id = stg.app_id  
           and si.ref_type = &apos;APP_HEADER&apos;
           and si.step_definition_id in (SELECT ref_id FROM corp_etl_list_lkup
                                          WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                            AND out_var = &apos;QC&apos; 
                                            AND SYSDATE BETWEEN start_date AND end_date)
           and si.hist_create_ts &gt;= stg.mi_task_create_date
           and si.hist_create_ts &lt;= nvl((select min(stg_2.mi_task_create_date)
                                       from process_mi_stg stg_2
                                      where stg_2.app_id = stg.app_id
                                        and stg_2.mi_task_create_date &gt; stg.mi_task_create_date), si.hist_create_ts)
           ) as oltp_QC_Task_ID,
        (select min(step_instance_id)
          from step_instance_stg   si
         where si.ref_id = stg.app_id  
           and si.ref_type = &apos;APP_HEADER&apos;
           and si.step_definition_id in (SELECT ref_id FROM corp_etl_list_lkup
                                          WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                          AND out_var = &apos;Research&apos; 
                                          AND SYSDATE BETWEEN start_date AND end_date)
           and si.hist_create_ts &gt;= stg.mi_task_create_date
           and si.hist_create_ts &lt;= nvl((select min(stg_2.mi_task_create_date)
                                       from process_mi_stg stg_2
                                      where stg_2.app_id = stg.app_id
                                        and stg_2.mi_task_create_date &gt; stg.mi_task_create_date), si.hist_create_ts)
           ) as oltp_Research_Task_ID,
        (select min(step_instance_id)
          from step_instance_stg   si
         where si.ref_id = stg.app_id
           and si.ref_type = &apos;APP_HEADER&apos;
           and si.step_definition_id in (SELECT ref_id FROM corp_etl_list_lkup
                                          WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                            AND out_var = &apos;State Review&apos; 
                                            AND SYSDATE BETWEEN start_date AND end_date)
           and si.hist_create_ts &gt;= stg.mi_task_create_date
           and si.hist_create_ts &lt;= nvl((select min(stg_2.mi_task_create_date)
                                       from process_mi_stg stg_2
                                      where stg_2.app_id = stg.app_id
                                        and stg_2.mi_task_create_date &gt; stg.mi_task_create_date), si.hist_create_ts)
           ) as oltp_State_Review_Task_ID
from process_mi_stg  stg       
  where 1=1
  and stg.instance_status = &apos;Active&apos;
  and stg.mi_task_type &lt;&gt; &apos;State Review Task - MI Reprocess Result&apos;
  --and stg.app_id = 314266
) A
) B</sql>
