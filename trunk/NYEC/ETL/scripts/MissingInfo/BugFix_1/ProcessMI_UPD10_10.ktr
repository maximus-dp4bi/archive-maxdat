  <step>
    <name>MI Letter</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>Select letter_id           as StgLetter_ID,
       letter_status_cd    as Stgetter_status,
       letter_requested_on as Stgletter_requested_on,        
       letter_sent_on      as Stgletter_sent_on   
From LETTERS_STG let_1
where 1=1
and let_1.letter_App_id = ?
and let_1.letter_id = (select min(letter_id) 
                         from LETTERS_STG let_2
                        where let_2.letter_App_id = ?
                          and let_2.letter_sent_on &gt;= ?
                          and let_2.Letter_request_type = &apos;L&apos; --Not a Manual Letter
                          and let_2.letter_type_cd like &apos;MI%&apos;
                          )</sql>
    <outer_join>Y</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
