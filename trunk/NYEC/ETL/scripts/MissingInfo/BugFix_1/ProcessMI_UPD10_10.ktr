  <step>
    <name>Table input</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>select stg.mi_task_id           as inMI_Task_ID,
	   stg.app_id               as inApp_ID,
       stg.mi_call_campaign     as inMI_Call_Campaign,
       stg.mi_letter_request_id as in_MI_Letter_Request_ID,
       stg.mi_letter_status     as inMI_Letter_Status, 
       stg.mi_type	            as inMI_Type,
       stg.mi_task_create_date  as inMI_Task_Create_Date,
	    case when exists (select 1 from app_mis_stg mi
                         where mi.mi_task_app_id = stg.app_id
                           and mi.mi_type_doc_data = &apos;DATA&apos;                           
                           and mi.mi_satisfied_date &gt;= mi.mi_task_creation_date
                           and exists (select 1 from app_mis_stg mi_2
                                         where mi_2.mi_task_app_id = stg.app_id                                           
                                           and mi_2.mi_type_doc_data = &apos;DOCUMENT&apos;
                                           and mi_2.mi_satisfied_date &gt;= mi.mi_task_creation_date)
                         ) then &apos;BOTH&apos;
           when exists (select 1 from app_mis_stg mi
                         where mi.mi_task_app_id = stg.app_id
                           and mi.mi_type_doc_data = &apos;DATA&apos;
                           and mi.mi_satisfied_date &gt;= mi.mi_task_creation_date
                         ) then &apos;DATA&apos;
             when exists (select 1 from app_mis_stg mi
                         where mi.mi_task_app_id = stg.app_id
                           and mi.mi_type_doc_data = &apos;DOCUMENT&apos;
                           and mi.mi_satisfied_date &gt;= mi.mi_task_creation_date
                         ) then &apos;DOCUMENT&apos;             
             else null end as StgMI_TYPE,
	   &apos;Y&apos; as InYes
from process_mi_stg    stg,
     PROCESS_APP_STG   APP
 where stg.app_id = app.app_id
   AND stg.cancel_date is null
   and stg.instance_status = &apos;ACTIVE&apos;
   and stg.mi_task_type &lt;&gt; &apos;State Review Task - MI Reprocess Result&apos; 
   and stg.asf_complete_mi_task = &apos;Y&apos;
   and stg.Asf_Send_Mi_Letter = &apos;N&apos;      
   and stg.gwf_mi_outcome = &apos;A&apos;
   and app.app_status_group in (&apos;Awaiting Missing Information&apos;,&apos;Done&apos;)
            and stg.mi_task_id  in (select distinct mi_task_id from Pro_MI_multiple_qc_rsch_strw)</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
     <cluster_schema/>
 <remotesteps>   <input>   </input>   <output>   </output> </remotesteps>    <GUI>
