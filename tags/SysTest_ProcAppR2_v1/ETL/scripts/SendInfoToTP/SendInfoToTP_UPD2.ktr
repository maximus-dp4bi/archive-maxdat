  <step>
    <name>Table input - Get UPD2 Records</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT s.inreq_id          
     , s.info_req_source
     , s.info_req_id
     , s.info_req_create_dt
     , s.info_req_type
     , s.gwf_request_type
     , s.info_req_group         new_info_req_group
     , s.info_req_status        new_info_req_status
     , s.letter_req_dt          new_letter_req_dt
     , s.letter_status_dt       new_letter_status_dt
     , s.assd_mail_letter_req   new_assd_mail_letter_req
     , s.man_letter_flag        new_man_letter_flag
     , s.app_id                 new_app_id
     , CASE
         WHEN s.info_req_group = &apos;Outbound Call&apos; THEN
           CASE
             WHEN s.event_type_cd IN (&apos;OB_DIALER_MISSING_DATA&apos;,&apos;OB_DIALER_MISSING_DOC&apos;)
              AND ( s.auth_end_date IS NOT NULL AND s.auth_end_date &gt; s.info_req_create_dt )
              AND s.rfe_status = &apos;AWAITING_MI&apos;
              AND s.mi_exists = &apos;Y&apos;
              AND s.app_status = &apos;INPROCESS&apos;
              AND s.no_call_ind = &apos;N&apos;
              AND s.call_retry_ct &lt; s.call_retry_limit
             THEN &apos;Y&apos;
             WHEN s.event_type_cd IN (&apos;OB_DIALER_PRE_RENEWAL&apos;,&apos;OB_DAILER_RENEWAL_REM&apos;)
              AND s.app_status = &apos;RENEWAL_INITIATED&apos;
              AND s.no_call_ind = &apos;N&apos;
              AND s.call_retry_ct &lt; s.call_retry_limit
             THEN &apos;Y&apos;
             ELSE &apos;N&apos;
           END
         ELSE NULL
       END                      new_call_flag
     , e.sitp_id                sitp_id
     , e.app_id                 app_id
     , e.man_letter_flag        man_letter_flag
     , e.info_req_status        info_req_status
     , e.info_req_group         info_req_group
     , e.assd_receive_info_req  assd_receive_info_req
     , e.letter_req_dt          letter_req_dt
     , e.letter_status_dt       letter_status_dt
     , e.assd_mail_letter_req   assd_mail_letter_req
     , e.assd_perform_outbound_call
     , e.call_flag              call_flag 
     , SYSDATE                  current_ts
     , TRUNC(SYSDATE)           todays_date
     , &apos;N&apos;                      updated_flag
     , &apos;Y&apos;                      yes
   FROM sendinfotradpart_stg s
     , nyec_etl_sendinfotradpart e
 WHERE s.info_req_source = e.info_req_source
   AND s.info_req_id = e.info_req_id
   AND s.new_ind = &apos;N&apos;
   AND s.app_in_process = &apos;Y&apos;
   AND e.stage_done_date IS NULL 
   AND e.cancel_date IS NULL
   AND e.info_req_group IS NULL </sql>
