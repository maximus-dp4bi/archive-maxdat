  <step>
    <name>Table input -- App ID and Task ID</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT a.*, &apos;Y&apos; as &quot;Insert&quot;, &apos;STATE_REVIEW_LAST_STEP_HIST_ID&apos; as  &quot;HistoryFieldName&quot; --application_id, task_id, COUNT(*) 
FROM (
-- in process       
SELECT Ref_ID              application_id, 
       step_instance_id    task_id,
       create_ts           assd_received_state_review,
       &apos;Active&apos;            instance_status,
	   step_instance_history_id HistoryID

  FROM step_instance_stg sis  
 WHERE sis.hist_status = &apos;UNCLAIMED&apos; 
   AND NOT EXISTS (SELECT 1 FROM step_instance_stg sis2 
                   WHERE sis2.step_instance_id = sis.step_instance_id  
                   AND sis2.status IN  (&apos;COMPLETED&apos; ,&apos;TERMINATED&apos;))
   AND sis.step_instance_history_id  IN (SELECT MIN(step_instance_history_id) 
                                         FROM step_instance_stg sigm  
                                         WHERE sigm.step_instance_id  = sis.step_instance_id  
                                         AND sigm.hist_status = sis.hist_status)
   AND (
       sis.step_definition_id IN (SELECT ref_id
                                  FROM corp_etl_list_lkup
                                  WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                  AND out_var = &apos;State Review&apos;
                                  AND SYSDATE BETWEEN start_date AND end_date)
       )
       
     and sis.step_instance_history_id &gt; (select VALUE from corp_etl_control where name = &apos;STATE_REVIEW_LAST_STEP_HIST_ID&apos;)      
--   and sis.step_instance_history_id Great then Max his for last state review run
UNION ALL -- Completed in last n days
SELECT Ref_ID              application_id, 
       step_instance_id    task_id,
       create_ts           assd_recieved_state_review,
       &apos;Active&apos;            instance_status,
	   step_instance_history_id HistoryID

  FROM step_instance_stg sis
 WHERE sis.status IN  (&apos;COMPLETED&apos; ,&apos;TERMINATED&apos;)
   AND sis.hist_status IN  (&apos;COMPLETED&apos; ,&apos;TERMINATED&apos;)
   AND  sis.completed_ts &gt;= trunc(SYSDATE - (select VALUE from corp_etl_control where name = &apos;STATE_REVIEW_LOOK_BACK_DAYS&apos;))  -- Replace days with the  global control variable 
   AND sis.step_instance_history_id  IN (SELECT MIN(step_instance_history_id) 
                                         FROM step_instance_stg sigm  
                                         WHERE sigm.step_instance_id  = sis.step_instance_id  
                                         AND sigm.hist_status = sis.hist_status)
   AND (
       sis.step_definition_id IN (SELECT ref_id
                                  FROM corp_etl_list_lkup
                                  WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                  AND out_var = &apos;State Review&apos;
                                  AND SYSDATE BETWEEN start_date AND end_date)
       )
     and sis.step_instance_history_id &gt; (select VALUE from corp_etl_control where name = &apos;STATE_REVIEW_LAST_STEP_HIST_ID&apos;)
--   and sis.step_instance_history_id Great then Max his for last state review run
) a
WHERE  NOT EXISTS (SELECT 1 FROM maxdat.STATE_REVIEW_STG psed
                    WHERE psed.app_id = a.Application_ID and psed.CURRENT_TASK_ID = a.task_id) 
--AND rownum &lt; 2000</sql>
