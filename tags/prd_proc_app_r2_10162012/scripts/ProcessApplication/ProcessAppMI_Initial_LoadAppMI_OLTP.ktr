  <step>
    <name>Table input - App MI OLTP Source</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>SELECT mi.missing_info_id
     , mi.application_id app_id

     , CASE WHEN mi.app_individual_id IS NULL
         THEN &apos;Application&apos;
         ELSE &apos;Client&apos;
       END mi_item_level

     , CASE mit.satisfied_without_tangible_ind
         WHEN 0 THEN &apos;Data&apos;
         WHEN 1 THEN &apos;Documentation&apos;
       END mi_item_type

	 , mi.created_by 
     , mi.create_ts mi_item_create_dt

     , mi.call_record_id outbound_call_id 
     , &apos;Y&apos; in_process_ind
  FROM app_missing_info mi
     , enum_app_mi_type mit
 WHERE mit.value = mi.mi_type_cd
   AND mi.missing_info_id &gt;= ?</sql>
