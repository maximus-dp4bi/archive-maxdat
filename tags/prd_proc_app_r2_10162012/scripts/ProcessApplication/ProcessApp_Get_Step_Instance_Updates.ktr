  <step>
    <name>Table input - Identify Applications To Be Updated</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>20</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT A.App_ID
	  ,CASE WHEN A.App_Type IS NULL THEN &apos;RENEWAL&apos; ELSE A.App_Type END App_Type	

      ,CASE WHEN A.Complete_Dt IS NOT NULL THEN Bus_Days_Between(A.Create_dt,A.Complete_dt)
            ELSE Bus_Days_Between(A.Create_dt,SYSDATE) END Age_In_Business_Days
            
      ,CASE WHEN A.Complete_Dt IS NOT NULL THEN Trunc(A.Complete_dt) - Trunc(A.Create_dt) 
            ELSE Trunc(SYSDATE) - Trunc(A.Create_dt)  END Age_In_Calendar_Days
            
      ,CASE WHEN A.Complete_Dt IS NOT NULL THEN
                 CASE WHEN App_Status = &apos;Completed(Timeout)&apos; THEN &apos;Not Received Prior to Authorization&apos;
                      WHEN Refer_LDSS_Flag = &apos;Y&apos; THEN &apos;Referred to District&apos;
                      WHEN App_Status = &apos;Completed(Withdrawn)&apos; AND Heart_App_Status = &apos;Withdrawn from HEART&apos; THEN &apos;Withdrawn from HEART&apos;
                      WHEN App_Status = &apos;Completed(Withdrawn)&apos; THEN &apos;Disregarded&apos;
                      WHEN Eligibility_Action = &apos;Not Renewed&apos; THEN &apos;Ineligible&apos;
					  WHEN Eligibility_Action = &apos;Renewed&apos; THEN &apos;Eligible&apos;
                      ELSE NULL END
            ELSE NULL END App_Complete_Result
      
      , CASE
            WHEN Sla_Jeopardy_days IS NULL THEN &apos;N&apos;
            WHEN A.Complete_Dt IS NOT NULL THEN JEOPARDY_FLAG
            WHEN Sla_Days_type = &apos;B&apos; AND A.Receipt_dt  IS NOT NULL AND get_bus_date(TRUNC(A.Receipt_dt), sla_Jeopardy_days) &lt;= TRUNC(SYSDATE)  THEN &apos;Y&apos;
            WHEN Sla_Days_type = &apos;C&apos; AND A.Receipt_dt  IS NOT NULL AND TRUNC(A.Receipt_dt)+sla_Jeopardy_days &lt;= TRUNC(SYSDATE) THEN &apos;Y&apos;
            ELSE &apos;N&apos; END JEOPARDY_FLAG
      
      , CASE
            WHEN A.Complete_Dt IS NULL THEN &apos;Not Complete&apos;  
            WHEN Sla_days IS NULL THEN &apos;Not Required&apos;
            WHEN Sla_days_type = &apos;B&apos; AND A.Receipt_dt  IS NOT NULL AND Bus_Days_Between(A.Receipt_dt, NVL(A.Complete_dt, SYSDATE)) &gt;  sla_days THEN &apos;Untimely&apos;
            WHEN sla_days_type = &apos;C&apos; AND A.Receipt_dt  IS NOT NULL AND  TRUNC(NVL(A.Complete_dt, SYSDATE)) - TRUNC(A.Receipt_dt) &gt;  sla_days THEN &apos;Untimely&apos;
            ELSE &apos;Timely&apos;  END    TIMELINESS_STATUS      
      
      ,CASE WHEN Sla_Jeopardy_days IS NULL THEN NULL
                   WHEN Sla_Days_type = &apos;B&apos; AND A.Receipt_dt  IS NOT NULL THEN get_bus_date(A.Receipt_dt, sla_Jeopardy_days)
                   WHEN Sla_Days_type = &apos;C&apos; AND A.Receipt_dt  IS NOT NULL THEN TRUNC(A.Receipt_dt)+sla_Jeopardy_days
                   ELSE NULL END SLA_Jeopardy_dt

      ,A.Receipt_dt App_Cycle_Start_dt

      ,CASE WHEN A.Complete_Dt IS NOT NULL AND A.Receipt_dt  IS NOT NULL THEN Bus_Days_Between(A.Receipt_dt,A.Complete_dt)
			WHEN A.Receipt_dt  IS NOT NULL THEN Bus_Days_Between(A.Receipt_dt,SYSDATE) 
			ELSE NULL END App_Cycle_Bus_Days
            
      ,CASE WHEN A.Complete_Dt IS NOT NULL THEN Trunc(A.Complete_dt) - Trunc(A.Receipt_dt) 
			WHEN A.Receipt_dt  IS NOT NULL THEN Trunc(SYSDATE) - Trunc(A.Receipt_dt)
			ELSE NULL END App_Cycle_Cal_Days

      , (Select Max(step_Instance_id) 
		   from Step_Instance_Stg s WHERE s.Ref_type = &apos;APP_HEADER&apos; AND s.Ref_id = A.App_ID AND s.Status != &apos;TERMINATED&apos;) Current_Task_ID

      , CASE WHEN RVW_ENTER_DATA_DT IS NULL THEN 
				 (SELECT MIN (Max(Step_Instance_History_id)) 
				    FROM step_instance_stg s WHERE s.ref_type = &apos;APP_HEADER&apos; AND s.ref_id = A.App_ID AND Status != &apos;TERMINATED&apos;
		             AND Step_definition_id IN ( SELECT Ref_id 
												   FROM corp_etl_list_lkup 
 												  WHERE Name = &apos;TASK_MONITOR_TYPE&apos; AND Out_Var = &apos;State Data Entry&apos;)
				   GROUP BY step_Instance_id)

			 ELSE DE_HIST_TASK_ID END DE_HIST_TASK_ID
            
      , CASE WHEN PERFORM_QC_DT IS NULL THEN 
				 (SELECT MIN (Max(Step_Instance_History_id)) 
				    FROM step_instance_stg s WHERE s.ref_type = &apos;APP_HEADER&apos; AND s.ref_id = A.App_ID AND Status != &apos;TERMINATED&apos;
		             AND Step_definition_id IN ( SELECT Ref_id 
												   FROM corp_etl_list_lkup 
 												  WHERE Name = &apos;TASK_MONITOR_TYPE&apos; AND Out_Var = &apos;QC&apos;)
				   GROUP BY step_Instance_id)

			 ELSE QC_HIST_TASK_ID END QC_HIST_TASK_ID

      , CASE WHEN ASED_PERFORM_RESEARCH IS NULL THEN 
				 (SELECT MIN (Max(Step_Instance_History_id)) 
				    FROM step_instance_stg s WHERE s.ref_type = &apos;APP_HEADER&apos; AND s.ref_id = A.App_ID AND Status != &apos;TERMINATED&apos;
		             AND Step_definition_id IN ( SELECT Ref_id 
												   FROM corp_etl_list_lkup 
 												  WHERE Name = &apos;TASK_MONITOR_TYPE&apos; AND Out_Var = &apos;Research&apos;)
				   GROUP BY step_Instance_id)

			 ELSE RESEARCH_HIST_TASK_ID END RESEARCH_HIST_TASK_ID

      , CASE WHEN ASED_WAIT_STATE_APPROVAL IS NULL THEN 
				 (SELECT MAX (Max(Step_Instance_History_id)) 
				    FROM step_instance_stg s WHERE s.ref_type = &apos;APP_HEADER&apos; AND s.ref_id = A.App_ID AND Status != &apos;TERMINATED&apos;
		             AND Step_definition_id IN ( SELECT Ref_id 
												   FROM corp_etl_list_lkup 
 												  WHERE Name = &apos;TASK_MONITOR_TYPE&apos; AND Out_Var = &apos;State Review&apos;)
				   GROUP BY step_Instance_id)

			 ELSE STATE_REVIEW_HIST_TASK_ID END STATE_REVIEW_HIST_TASK_ID


      , (SELECT Count(Distinct(step_Instance_id)) FROM step_instance_stg s WHERE s.ref_type = &apos;APP_HEADER&apos; AND s.ref_id = A.App_ID AND Status != &apos;TERMINATED&apos;
            AND Step_definition_id IN ( SELECT Ref_id FROM corp_etl_list_lkup WHERE Name = &apos;TASK_MONITOR_TYPE&apos; AND Out_Var = &apos;State Review&apos;)) STATE_REVIEW_TASK_COUNT

      , (SELECT Count(Distinct(step_Instance_id)) FROM step_instance_stg s WHERE s.ref_type = &apos;APP_HEADER&apos; AND s.ref_id = A.App_ID AND Status != &apos;TERMINATED&apos;
            AND Step_definition_id IN ( SELECT Ref_id FROM corp_etl_list_lkup WHERE Name = &apos;MI_TASK_TYPE&apos; AND Out_Var = &apos;Missing Information&apos;)) MI_RECEIVED_TASK_COUNT


      , CASE WHEN AUTO_REPROCESS_FLAG_DT IS NULL AND AUTO_REPROCESS_FLAG = &apos;Y&apos; THEN TRUNC(SYSDATE)
			 ELSE AUTO_REPROCESS_FLAG_DT END AUTO_REPROCESS_FLAG_DT

      , CASE WHEN REFER_LDSS_FLAG_DT IS NULL AND REFER_LDSS_FLAG = &apos;Y&apos; THEN TRUNC(SYSDATE)
			 ELSE REFER_LDSS_FLAG_DT END REFER_LDSS_FLAG_DT

	  , CASE WHEN pending_mi_data_count = 0 AND pending_mi_doc_count &gt; 0 THEN &apos;Document&apos;
			 WHEN pending_mi_data_count &gt; 0 AND pending_mi_doc_count = 0 THEN &apos;Data&apos;
			 WHEN pending_mi_data_count &gt; 0 AND pending_mi_doc_count &gt; 0 THEN &apos;Both&apos;
			 ELSE NULL END PENDING_MI_TYPE

	  , (SELECT Count(Event_id) FROM cc_event_stg WHERE ref_id = A.App_ID  
			AND ((call_campaign_type like &apos;OB_DIALER_MISSING%FAIL&apos;) OR (call_campaign_type like &apos;OB_DIALER_MISSING%SUCCESS&apos;))) OUTBOUND_CALL_COUNT

	  , (SELECT MAX(Event_id) FROM cc_event_stg WHERE ref_id = A.App_ID  
			AND ((call_campaign_type like &apos;OB_DIALER_MISSING%FAIL&apos;) OR (call_campaign_type like &apos;OB_DIALER_MISSING%SUCCESS&apos;))) OUTBOUND_CALL_EVENT_ID

	  , SYSDATE STG_LAST_PROCESSED_DT
  from Process_App_Stg A
 where App_id = ?
</sql>
    <limit>1</limit>
