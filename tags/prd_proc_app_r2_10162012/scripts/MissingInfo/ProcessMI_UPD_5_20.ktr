    <connection>MAXDAT</connection>
    <sql>select  stg.mi_task_id as inMI_task_ID,
        stg.app_id     as inApp_ID,
		stg.qc_task_id as stg_QC_Task_ID,
		stg.research_task_id as stgResearch_Task_ID,
		stg.state_review_task_id as stgState_Review_Task_ID,
		stg.current_task_id as stgCurrent_Task_ID,
       (select max(step_instance_id)
          from step_instance_stg   si
         where si.ref_id = stg.app_id  
           and si.ref_type = &apos;APP_HEADER&apos;
           and si.step_definition_id in (SELECT ref_id FROM corp_etl_list_lkup
                                          WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                            AND out_var = &apos;QC&apos; 
                                            AND SYSDATE BETWEEN start_date AND end_date)
           and si.hist_create_ts &gt;= stg.mi_task_create_date) as InQC_Task_ID,
        (select max(step_instance_id)
          from step_instance_stg   si
         where si.ref_id = stg.app_id  
           and si.ref_type = &apos;APP_HEADER&apos;
           and si.step_definition_id in (SELECT ref_id FROM corp_etl_list_lkup
                                              WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                              AND out_var = &apos;Research&apos; 
                                              AND SYSDATE BETWEEN start_date AND end_date)
           and si.hist_create_ts &gt;= stg.mi_task_create_date) as InResearch_Task_ID,   
        (select max(step_instance_id)
          from step_instance_stg   si
         where si.ref_id = stg.app_id  
           and si.ref_type = &apos;APP_HEADER&apos;
           and si.step_definition_id in (SELECT ref_id FROM corp_etl_list_lkup
                                              WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                              AND out_var = &apos;State Review&apos; 
                                              AND SYSDATE BETWEEN start_date AND end_date)
           and si.hist_create_ts &gt;= stg.mi_task_create_date) as InState_Review_Task_ID,
       (select max(step_instance_id)
          from step_instance_stg   si,
               step_definition_stg si_def
         where si.ref_id = stg.app_id  
           and si.ref_type = &apos;APP_HEADER&apos;
           and si.step_definition_id = si_def.step_definition_id
           and (si_def.name in (&apos;State Data Entry Task - MI&apos;,&apos;State Review Task - MI Reprocess Result&apos;)
                OR 
                si.step_definition_id in (SELECT ref_id FROM corp_etl_list_lkup
                                           WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                             AND out_var in (&apos;QC&apos;,&apos;Research&apos;,&apos;State Review&apos;) 
                                             AND SYSDATE BETWEEN start_date AND end_date)
                 )           
           and si.hist_create_ts &gt;= stg.mi_task_create_date) as InCurrent_Task_Id,
	   &apos;Y&apos; as Inyes
from process_mi_stg    stg       
  where stg.cancel_date is null
  and stg.instance_status = &apos;ACTIVE&apos;
  and stg.mi_task_type &lt;&gt; &apos;State Review Task - MI Reprocess Result&apos;</sql>
