  <step>
    <name>Database join - Get DMS Documents</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <copies>5</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>NYECDMS</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT &#47;*+ PARALLEL(a,8) PARALLEL(ad,8) *&#47;
	   DISTINCT a.Status Availability_Status
     , a.Date_Created    Date_Received
     , b.Document_Count  DMS_Document_Count
     , &apos;N&apos; Processed_Flag
FROM   dms.DOCUMENTS d
     , dms.BATCHES b
     , dms.ACTIVITY_DETAILS ad
     , dms.ACTIVITIES a 
WHERE  d.BATCH_ID = b.BATCH_ID 
and    ad.DCN = d.DCN 
and    a.ACTIVITY_ID = ad.ACTIVITY_ID
AND    b.Capture_batch_Id = ?
and    a.CREATION_DATE = ( select max(aa.CREATION_DATE) 
                           from   dms.activities aa
                                , dms.activity_details ad1 
                           where  d.dcn = ad1.dcn 
                           and    aa.activity_id = ad1.activity_id
                           and    aa.ACTIVITY_TYPE in( &apos;ENVELOPERELEASE&apos; , &apos;RELEASE&apos; ))
and    d.DCN NOT IN (SELECT DISTINCT(ad.DCN)
                     FROM   dms.ACTIVITY_DETAILS ad
                          , dms.ACTIVITIES a
                     WHERE  a.ACTIVITY_ID = ad.ACTIVITY_ID  
                     and    a.activity_type = &apos;INACTIVE&apos;)</sql>
    <outer_join>Y</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
