    <sql>with driver as (
select 2 as WEIGHT, p.plan_code, pr.PROVIDER_ID,s.client_id,s.start_date
from SELECTION_SEGMENT S
inner join PLANS P
on S.PLAN_ID = P.PLAN_ID
left outer join PROVIDER PR
on S.PROVIDER_ID_EXT = PR.PROVIDER_ID_EXT
where S.CLIENT_ID=?
and (S.END_DATE is null or S.END_DATE &gt; sysdate)
--and s.provider_id_ext is not null

union

select 1 as WEIGHT, p.plan_code, s.PROVIDER_ID,s.client_id,s.start_date
from SELECTION_TXN s
inner join PLANS P
on S.PLAN_ID = P.PLAN_ID
where S.CLIENT_ID=?
and (S.END_DATE is null or S.END_DATE &gt; sysdate)

union

select 0 as weight,null,null,?,sysdate from dual )

select plan_code, provider_id from driver where weight = (select max(weight) from driver) and start_date=(select min(start_date) from driver)</sql>
