    <rowlimit>0</rowlimit>
    <sql>&#47;* 
For IL, there is only one program code so, it has been hardcoded here *&#47;

with DRIVER as (select SUBPROGRAM_TYPE,
case when ELIG_STATUS_CD = &apos;M&apos; then 3 
when ELIG_STATUS_CD = &apos;V&apos; and SUBPROGRAM_TYPE in (&apos;PCCM&apos;) then 2 
when ELIG_STATUS_CD = &apos;V&apos; and SUBPROGRAM_TYPE not in (&apos;PCCM&apos;) then 1 
else 0 
end weight 
from CLIENT_ELIG_STATUS 
where CLIENT_ID = ?
and (END_DATE is null or END_DATE&gt;sysdate)) 
select distinct case when weight &gt; 0 then SUBPROGRAM_TYPE
else null 
end PROGRAM_SUBTYPE,&apos;MEDICAID&apos; PROGRAM_TYPE 
from DRIVER 
where weight = (select max(weight) from driver)</sql>
