    <rowlimit>0</rowlimit>
    <sql>Select i.incident_header_id INCIDENT_ID
,i.tracking_number
,i.received_ts receipt_dt
,i.create_ts create_dt
,(case when I.RESPONSIBLE_STAFF_ID is not null then (select S.FIRST_NAME||S.LAST_NAME from STAFF S where S.STAFF_ID=I.RESPONSIBLE_STAFF_ID)
       when I.RESPONSIBLE_STAFF_ID is null then (select S.FIRST_NAME||S.LAST_NAME from STAFF S where S.STAFF_ID=I.CREATED_BY)
       else null end)  CREATED_BY_NAME 
,(select g.group_name from groups g where g.group_id=i.responsible_staff_group_id) CREATED_BY_GROUP
,i.origin_id
,(select O.REPORT_LABEL from ENUM_INCIDENT_ORIGIN O where O.value=I.ORIGIN_CD) CHANNEL
,(select P.REPORT_LABEL from ENUM_PRIORITY P where P.value=I.PRIORITY_CD) PRIORITY
,(select s.report_label from ENUM_INCIDENT_HEADER_STATUS s where s.value=I.STATUS_CD) INCIDENT_STATUS
,i.status_ts INCIDENT_STATUS_DT
,i.update_ts LAST_UPDATE_BY_DT
,(select S.FIRST_NAME||S.LAST_NAME from STAFF S where S.STAFF_ID=I.UPDATED_BY) LAST_UPDATE_BY_NAME
,(select a.REPORT_LABEL from ENUM_ACTION_TAKEN a where a.value=I.ACTION_TAKEN_CD) ACTION_TYPE
,I.ACTIONS_TAKEN ACTION_COMMENTS
,(select R.REPORT_LABEL from ENUM_RESOLUTION_TYPE R where R.value=I.RESOLUTION_TYPE_CD) RESOLUTION_TYPE
,I.RESOLUTION RESOLUTION_DESCRIPTION
--,CURRENT_TASK_ID
,(select T.REPORT_LABEL from ENUM_INCIDENT_HEADER_TYPE T where T.value=I.INCIDENT_HEADER_TYPE_CD) INCIDENT_TYPE
,(select IA.DESCRIPTION from ENUM_AFFECTED_PARTY_TYPE IA,INCIDENT_ISSUE II where IA.value=II.ABOUT_CD and II.INCIDENT_HEADER_ID=I.INCIDENT_HEADER_ID) INCIDENT_ABOUT
,(select S.REPORT_LABEL from ENUM_AFFECTED_PARTY_SUBTYPE S,INCIDENT_ISSUE II where S.value=II.REASON_CD and II.INCIDENT_HEADER_ID=I.INCIDENT_HEADER_ID) INCIDENT_REASON
,(select II.DESCRIPTION from INCIDENT_ISSUE II where II.INCIDENT_HEADER_ID=I.INCIDENT_HEADER_ID) INCIDENT_DESCRIPTION
,(select N.PROVIDER_ID from network N where N.NETWORK_ID_EXT=I.NETWORK_ID_EXT) ABOUT_PROVIDER_ID
,(select P.PLAN_CODE from PLANS P where P.PLAN_ID_EXT=I.PLAN_ID_EXT) ABOUT_PLAN_CODE
,(select r.report_label from enum_reporter_type r where r.value=i.reporter_type_cd) REPORTED_BY
,(select RR.REPORT_LABEL from ENUM_RELATIONSHIP RR where RR.value=I.REPORTER_RELATIONSHIP) REPORTER_RELATIONSHIP
,(case when (I.EB_FOLLOW_UP_NEEDED_IND is null or I.EB_FOLLOW_UP_NEEDED_IND=0 ) then &apos;N&apos;
       when I.EB_FOLLOW_UP_NEEDED_IND=1 then &apos;Y&apos; else null END) EB_FOLLOWUP_NEEDED_IND
,(select O.REPORT_LABEL from ENUM_OTHER_PARTY_TYPE O where O.value=I.OTHER_PARTY_TYPE_CD) OTHER_PARTY_NAME
--,ENROLLMENT_STATUS
--,PROGRAM_TYPE
--,SUB_PROGRAMTYPE
--,PLAN_CODE
,I.CASE_ID CASE_ID
--,PROVIDER_ID
,(select min(H.CREATE_TS) from INCIDENT_HEADER_STAT_HIST H where H.INCIDENT_HEADER_ID=I.INCIDENT_HEADER_ID and H.STATUS_CD=&apos;REFERRED_TO_STATE&apos;) ESCALATE_DT
--,RETURNED_DT undetermined
,(select IH.STATUS_TS from INCIDENT_HEADER IH where IH.INCIDENT_HEADER_ID=I.INCIDENT_HEADER_ID and I.STATUS_CD=&apos;CLOSED&apos;) COMPLETE_DT
,(select IH.STATUS_TS FROM INCIDENT_HEADER IH where IH.INCIDENT_HEADER_ID=I.INCIDENT_HEADER_ID AND I.STATUS_CD=&apos;WITHDRAWN&apos;) CANCEL_DT
,I.CLIENT_ID
,I.GENERIC_FIELD1
,I.GENERIC_FIELD2
,I.GENERIC_FIELD3
,I.GENERIC_FIELD4
,I.GENERIC_FIELD5
,(select step_instance_id from STEP_INSTANCE si, step_definition sd where REF_TYPE=&apos;INCIDENT_HEADER&apos;and SI.STEP_DEFINITION_ID = SD.STEP_DEFINITION_ID
   and SD.STEP_TYPE_CD in (&apos;VIRTUAL_HUMAN_TASK&apos;,&apos;HUMAN_TASK&apos;)
   and si.completed_ts is null
   and ref_id=i.incident_header_id) CURRENT_TASK_ID
from INCIDENT_HEADER I
where  I.INCIDENT_HEADER_TYPE_CD=&apos;COMPLAINT&apos;
and i.incident_header_id=?</sql>
