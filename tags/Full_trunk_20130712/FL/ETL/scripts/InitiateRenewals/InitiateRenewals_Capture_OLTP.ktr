    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>select distinct
       ar.ACCOUNT_ID                                                ACCOUNT_ID, 
       nvl(ar.renewal_date, &apos;01-JAN-1900&apos;)                          RENEWAL_PERIOD_START_DATE,
       nvl(ap.application_id, 0)                                    APPLICATION_ID,
       ap.RECEIVED_DATE                                             APP_RECEIVED_DATE,
       ap.APP_ENTERED_BY                                            APP_ENTERED_BY,
       nvl(ar.CONTINUOUS_ELIG_END_DATE, &apos;01-JAN-1900&apos;)              RENEWAL_PERIOD_END_DATE,
       ar.renewal_status                                            RENEWAL_STATUS,
       nvl(ar.renewal_status_date, &apos;01-JAN-1900&apos;)                   RENEWAL_STATUS_DATE,
       case when ar.renewal_date is not null 
       then ADD_MONTHS(ar.renewal_date, -4)
       else to_date(&apos;01-JAN-1900&apos;,&apos;DD-MON-YYYY&apos;)
       END                                                          ASSD_IDENTIFY_RENEWAL_ACCTS,  
       nvl(d.ins_cancelled_date, &apos;01-JAN-1900&apos;)                     CANCEL_DATE,
       esr.STATUS_REASON                                            CANCEL_REASON,
       nvl(arr.insert_date, &apos;01-JAN-1900&apos;)                          COMPLETE_DATE,
       case when months_between(ar.renewal_date, c.insert_date) &gt; 2 
       then c.contact_id                
       END                                                          TIME_TO_RENEWAL_CALL_ID, 
       case when months_between(ar.renewal_date, c.insert_date) &lt; 2 
             and months_between(ar.renewal_date, c.insert_date) &gt; 1
       then c.contact_id
       END                                                          RENEWAL_REMINDER_CALL_ID,
       case when months_between(ar.renewal_date, c.insert_date) &gt; 1 
            and months_between(ar.renewal_date, c.insert_date) &lt; 0
       then c.contact_id 
       END                                                          RENEWAL_CANCELLATION_CALL_ID,
       case when interface_process_name like &apos;DOR%&apos;
       then ip.interface_process_status                             
       END                                                          DOR_MATCH_STATUS,
       case when interface_process_name like &apos;DEO%&apos;
       then ip.interface_process_status                             
       END                                                          DEO_MATCH_STATUS,
       case when interface_process_name like &apos;DOR%&apos;
       then ip.interface_process_status_date
       END                                                          DOR_MATCH_STATUS_DATE,
       case when interface_process_name like &apos;DEO%&apos;
       then ip.interface_process_status_date
       END                                                          DEO_MATCH_STATUS_DATE,
       case when ar.renewal_status = &apos;SENT_TO_FAMILY&apos;
       then ar.renewal_status_date                                       
       END                                                          ASED_IDENTIFY_RENEWAL_ACCTS,
       case when ar.renewal_status = (&apos;SENT_TO_FAMILY&apos;) 
       then &apos;Y&apos;  
       else &apos;N&apos;
       END                                                          ASF_IDENTIFY_RENEWAL_ACCTS,
       ar.last_update_user                                          ASPB_IDENTIFY_RENEWAL_ACCTS,
       case when ar.renewal_status in (&apos;INTENT_RECEIVED&apos;)
       then ar.renewal_status_date 
       end                                                          ASED_REQUEST_NOTIFICATION,
       case when (sysdate - ar.renewal_status_date) &gt; 2 
            and ar.renewal_status in (&apos;INTENT_RECEIVED&apos;)
       then &apos;Y&apos;
       else &apos;N&apos; 
       END                                                          ASF_REQUEST_NOTIFICATION,
       case when (sysdate - ar.renewal_status_date) &lt; 2 
            and ar.renewal_status in (&apos;INTENT_RECEIVED&apos;)
       then ar.last_update_user
       END                                                          ASPB_REQUEST_NOTIFICATION,
       case when arr.renewal_result in (&apos;NON_COMPLIANT_WITH_INTENT&apos;, &apos;NON_COMPLIANT_WITHOUT_INTENT&apos;, &apos;NO_ELIGIBILITY&apos;)
       then add_months(ar.renewal_date, 1) + 1                                                      
       END                                                          ASSD_WAIT_FOR_ALL_DISENROLLED,
        case when rh.intent_to_renew = &apos;F&apos;
             and arr.renewal_result not in (&apos;RENEWAL_GRANTED&apos;)
       then &apos;Y&apos; 
       else &apos;N&apos;                                                     
       END                                                          ASF_CANCEL_RENEWAL,
       case when ADD_MONTHS(ar.RENEWAL_DATE, 1) &gt; sysdate 
             and arr.renewal_result  not in (&apos;RENEWAL_GRANTED&apos;) 
       then &apos;N&apos; 
       when arr.renewal_result = &apos;RENEWAL_GRANTED&apos;
       then &apos;Y&apos;
       end                                                          GWF_APP_RECEIVED,
       &apos;ACTIVE&apos;                                                  as INSTANCE_STATUS,
       sysdate                                                   as STG_EXTRACT_DATE,
       sysdate                                                   as STG_LAST_UPDATE_DATE,
       &apos;Initate Renewals&apos;                                        as process_name,
       &apos;Initate Renewals Insert&apos;                                 as job_name,
        ar.account_id                                            as driver_key_number, 
        sysdate                                                  as system_date,
       &apos;FLHK_ETL_INITIATE_RENEWALS&apos;                              as driver_table_name,
       &apos;CRITICAL&apos;                                                as ERROR_LEVEL        
from account_renewal ar
join account_membership am 
  on am.account_id = ar.account_id
left outer join ELIGIBILITY_RESULT ER
  on am.account_membership_id = er.ACCOUNT_MEMBERSHIP_ID
left outer join application ap 
  on (ap.account_id = AR.ACCOUNT_ID
 and ap.app_type = &apos;RENEWAL&apos;)
left outer join deter_factor d
  on er.ELIGIBILITY_RESULT_ID  = d.ELIGIBILITY_RESULT_ID
left outer join eligibility_status_reason esr
  on er.eligibility_result_id  = esr.eligibility_result_id 
  and er.schip_status = &apos;NOT_ELIGIBLE&apos;
left outer join account_renewal_result arr 
  on ar.account_id = arr.account_id
 and arr.RENEWAL_DATE &gt;= ADD_MONTHS(sysdate, -2)
left outer join contact_account ca
  on ar.account_id = ca.account_id
left outer join contact c
  on ca.contact_id = c.contact_id
 and c.contact_source = &apos;PHONE&apos;
left outer join interface_process ip
  on ar.account_id = ip.account_id 
 and ip.interface_process_name in (&apos;DOR&apos;, &apos;DOR Renewal&apos;, &apos;DEO&apos;, &apos;DEO Renewal&apos;)
left outer join account_renewal_result arr
  on ar.account_id = arr.account_id
left outer join renewal_history rh
  on ar.account_id = rh.account_id 
where ADD_MONTHS(sysdate, 6) &gt;= CONTINUOUS_ELIG_END_DATE
  and sysdate &lt;= CONTINUOUS_ELIG_END_DATE
  and ar.account_id = ?
 order by ar.ACCOUNT_ID
</sql>
