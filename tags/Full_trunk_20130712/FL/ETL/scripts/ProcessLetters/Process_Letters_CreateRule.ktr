    <connection>LETTERS_SOURCE</connection>
    <sql>SELECT DISTINCT
	&apos;ProcessLetters&apos; as PROCESS_NAME,
	&apos;ProcessLetters_CreateRule&apos; as JOB_NAME,
	&apos;FLHK_ETL_PROCESS_LETTERS&apos; as DRIVER_TABLE_NAME,
	sysdate as ERROR_DATE,
	&apos;CRITICAL&apos; as ERR_LEVEL,
	A.LMREQ_ID AS LETTER_REQ_ID,
	A.REQUESTED_ON AS REQUESTED_ON_DATE,
	B.DESCRIPTION AS TYPE,
	&apos;REQ&apos; AS STATUS,
	B.TRIGERRING_METHOD AS TRIGGERING_METHOD,  -- THIS WILL COLUMN IS MISPELLED IN HER SCHEMA, WILL NEED TO CHANGE ONCE WE GO LIVE,
	A.BATCH_ID AS BATCH_ID,
	B.DRIVER_TYPE,
	A.DRIVER_PRIMARY_KEY,
	A.SOURCE,
	&apos;ACTIVE&apos; AS INSTANCE_STATUS,
	SYSDATE AS STG_EXTRACT_DATE,
	A.REQUESTED_ON AS ASSD_PROCESS_LETTER_REQUEST
FROM
    LM_LETTER_REQUESTS A,
	LM_LETTER_DEFINITIONS B 
WHERE 
	A.LMDEF_ID = B.LMDEF_ID (+)
AND	
	(
		(A.STATUS NOT IN (&apos;Mailed&apos;))
	OR
		(A.STATUS IN (&apos;Mailed&apos;) AND TRUNC(STATUS_DATE) &gt; TRUNC(SYSDATE)-60)
	)
AND
	LMREQ_ID &gt; ${MAX_LETTER_REQID}


