    <rowlimit>0</rowlimit>
    <sql>SELECT  DISTINCT
LR.LMREQ_ID,
LL.CLIENT_ID,
CS.ELIG_STATUS_CD,
CS.SUBPROGRAM_TYPE
FROM LETTER_REQUEST LR
LEFT OUTER JOIN LETTER_REQUEST_LINK LL
ON LL.LMREQ_ID= LR.LMREQ_ID
LEFT OUTER JOIN CLIENT_ELIG_STATUS CS
ON CS.CLIENT_ID = LL.CLIENT_ID
WHERE LR.LMREQ_ID = ?
AND END_DATE IS NULL
AND CS.ELIG_STATUS_CD IN (&apos;M&apos;)
UNION ALL
SELECT DISTINCT LMREQ_ID,CLIENT_ID,ELIG_STATUS_CD,
CASE WHEN CT_STATS = 1
THEN SUBPROGRAM_TYPE
WHEN CT_STATS = 2
THEN &apos;PCCM&apos;
END AS SUBPROGRAM_TYPE
FROM (SELECT  
LR.LMREQ_ID,
LL.CLIENT_ID,
CS.ELIG_STATUS_CD,
COUNT(CS.ELIG_STATUS_CD) OVER (PARTITION BY LR.LMREQ_ID,LL.CLIENT_ID,CS.ELIG_STATUS_CD) AS CT_STATS,
CS.SUBPROGRAM_TYPE
FROM LETTER_REQUEST LR
LEFT OUTER JOIN LETTER_REQUEST_LINK LL
ON LL.LMREQ_ID= LR.LMREQ_ID
LEFT OUTER JOIN CLIENT_ELIG_STATUS CS
ON CS.CLIENT_ID = LL.CLIENT_ID
WHERE LR.LMREQ_ID = ?
AND END_DATE IS NULL
AND CS.ELIG_STATUS_CD &lt;&gt; (&apos;X&apos;)
and CS.CLIENT_ID NOT IN (SELECT CLIENT_ID FROM (SELECT  
LR.LMREQ_ID,
LL.CLIENT_ID,
CS.ELIG_STATUS_CD,
CS.SUBPROGRAM_TYPE
FROM LETTER_REQUEST LR
LEFT OUTER JOIN LETTER_REQUEST_LINK LL
ON LL.LMREQ_ID= LR.LMREQ_ID
LEFT OUTER JOIN CLIENT_ELIG_STATUS CS
ON CS.CLIENT_ID = LL.CLIENT_ID
WHERE LR.LMREQ_ID = ?
AND END_DATE IS NULL
AND CS.ELIG_STATUS_CD IN (&apos;M&apos;)))
ORDER BY 2,3)
</sql>
