    <sql>SELECT application_id, status_cd app_status, renewal_due_date auth_end_date
      ,&apos;N&apos; mi_exists,&apos;N&apos; no_call_ind
      ,&apos;N&apos; no, &apos;Y&apos; yes
      ,&apos;RENEWAL_INITIATED&apos; app_stat_renewinit
      ,&apos;INPROCESS&apos; app_stat_inproc
      ,&apos;AWAITING_MI&apos; rfe_await_mi
      ,(SELECT MAX(missing_info_id) max_id
          FROM app_missing_info mi
         WHERE mi.application_id = ah.application_id
           AND mi.satisfied_date IS NULL) max_mi_id
      ,(SELECT DISTINCT FIRST_VALUE(rfe_status_cd) 
              OVER (PARTITION BY t.application_id ORDER BY app_tracker_id DESC)
          FROM app_tracker t
         WHERE t.application_id = ah.application_id 
           AND t.history_ind =0) rfe_status 
      ,(SELECT MAX(do_not_call_ind) FROM client c,app_individual ai
         WHERE ai.client_id = clnt_client_id
           AND ai.application_id = ah.application_id
           and do_not_call_ind = 1) max_do_not_call
  FROM app_header ah
 WHERE application_id = ?</sql>
