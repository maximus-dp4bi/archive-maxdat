      ,&apos;Call Campaign - Missing Data&apos;                  campaign_miss_data
      ,&apos;Call Campaign - Missing Document&apos;              campaign_miss_doc
      ,&apos;Call Campaign - 30 day prior Renewal Reminder&apos; campaign_pre_renew
      ,&apos;Call Campaign - 10 day Renewal Reminder&apos;       campaign_renew
      ,(SELECT COUNT(1) FROM cc_event_stg cc
         WHERE cc.ref_id      = oltp.app_id 
           AND cc.case_id     = oltp.case_id
           AND cc.create_ts   &gt; info_req_create_dt
           AND INSTR(cc.call_campaign_type,event_type_cd) &gt; 0
           AND INSTR(cc.call_campaign_type,&apos;FAIL&apos;) &gt; 0) call_retry_ct
  FROM sendinfotradpart_oltp_stg oltp
 WHERE info_req_group = &apos;Outbound Call&apos;
