  <step>
    <name>Get Task IDs</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>  select stg.mi_task_id as inMI_task_ID, 
         stg.app_id     as inApp_ID,
         stg.mi_task_create_date,
         stg.new_mi_creation_date,         
         stg.qc_task_id,
         qc_task_completed_date,
         stg.research_task_id,
         stg.rsrch_task_completed_date,
         stg.mi_task_completed_by as inMI_task_completed_by, 
         stg.mi_task_complete_date as inMI_task_complete_date,       
        &#47;*
        Find out the earliest MI created that is still outstanding after the MI task completion and 
        before research task completion (if a research tasks exists) or before QC task completion (if a QC tasks exists).
        *&#47;
        ( select min(mi.mi_creation_date) 
           from app_mis_stg mi
          where mi.mi_task_app_id = stg.app_id
            and mi_creation_date &gt;= stg.mi_task_create_date            
            AND (
                  (stg.rsrch_task_completed_date is not null and stg.research_task_id is not null and mi.mi_creation_date &lt;= stg.rsrch_task_completed_date)
                  or
                  (stg.qc_task_completed_date is not null and stg.qc_task_id is not null and stg.research_task_id is null and mi.mi_creation_date &lt;= stg.qc_task_completed_date)
                  or
                  (stg.mi_task_complete_date is not null and stg.qc_task_id is null and stg.research_task_id is null and mi.mi_creation_date &lt;= stg.mi_task_complete_date)
                 )
          ) as oltp_New_MI_Creation_Dt,
      &#47;*
      Get the most recent date when the New MIs only (NOT the older MIs) is satisfied; and NO OTHER New MI should be Outstanding.
      *&#47;   
      (select max(mi_2.mi_satisfied_date)
       from app_mis_stg mi_2
      where mi_2.mi_task_app_id = stg.app_id   
      and mi_2.mi_creation_date &gt;= stg.new_mi_creation_date
      and mi_2.mi_satisfied_date is not null
      and (
            (stg.rsrch_task_completed_date is not null and stg.research_task_id is not null and mi_2.mi_creation_date &lt;= stg.rsrch_task_completed_date)
            or
            (stg.qc_task_completed_date is not null and stg.qc_task_id is not null and stg.research_task_id is null and mi_2.mi_creation_date &lt;= stg.qc_task_completed_date)
            or
            (stg.mi_task_complete_date is not null and stg.qc_task_id is null and stg.research_task_id is null and mi_2.mi_creation_date &lt;= stg.mi_task_complete_date)
           )
      and not exists (select 1
                         from app_mis_stg mi
                        where mi.mi_task_app_id = stg.app_id
                          and mi_creation_date &gt;= stg.new_mi_creation_date                                   
                          and mi.mi_satisfied_date is null
                          and   ((stg.rsrch_task_completed_date is not null and stg.research_task_id is not null and mi.mi_creation_date &lt;= stg.rsrch_task_completed_date)
                                   or
                                  (stg.qc_task_completed_date is not null and stg.qc_task_id is not null and stg.research_task_id is null and mi.mi_creation_date &lt;= stg.qc_task_completed_date)
                                  or
                                  (stg.mi_task_complete_date is not null and stg.qc_task_id is null and stg.research_task_id is null and mi.mi_creation_date &lt;= stg.mi_task_complete_date)
                                  )
                        ) 
        ) as oltp_New_MI_Satisfied_Dt,    
	   &apos;Y&apos; as Inyes
  from process_mi_stg    stg
 where 1=1
   and stg.instance_status = &apos;Active&apos;
   and stg.mi_task_type &lt;&gt; &apos;State Review Task - MI Reprocess Result&apos;   
   and stg.mi_task_complete_date is not null
   --and stg.app_id = 324076</sql>
