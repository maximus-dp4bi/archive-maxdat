  <step>
    <name>OLTP Application MIs</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>5</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>select &#47;*+ ORDERED *&#47;
       mi_task.task_id            as MI_TASK_ID,
       mi_task.Task_Creation_date as MI_TASK_CREATION_DATE,
       mi_task.Task_Created_By    as MI_TASK_CREATED_BY,
       mi_task.task_type          AS MI_TASK_TYPE,
       mi_task.Task_App_ID        AS MI_TASK_APP_ID,
       mi.missing_info_id         AS MI_MISSING_INFO_ID,
       mi.app_individual_id       as MI_app_individual_id,
       mi.create_ts               as MI_creation_date,
       mi.reported_date           as MI_reported_date,
       mi.satisfied_date          as MI_satisfied_date,
       mi.satisfied_by            as MI_satisfied_by,
       mi_type_cd                 AS MI_TYPE_CD,
       decode(mi_type.satisfied_without_tangible_ind,1,&apos;Data&apos;,&apos;Document&apos;) as mi_type_doc_data,      
       mi.satisfied_reason_cd     AS MI_SATISFIED_REASON_CD,
       sysdate                    as PROCESS_MI_START_TIME	
from 
  (
  --Below SQL fetches MI tasks only.
SELECT distinct 
         si.step_instance_id as task_ID,
         si.ref_id           as Task_App_ID,
         si.create_ts        as Task_Creation_date,
         si.created_by       as Task_Created_By,
	     sd.name 			 as Task_type
   FROM STEP_INSTANCE   SI,
        STEP_DEFINITION SD
  WHERE  sd.step_type_cd IN (&apos;HUMAN_TASK&apos;,&apos;VIRTUAL_HUMAN_TASK&apos;)
    AND SD.STEP_DEFINITION_ID  = SI.STEP_DEFINITION_ID
    and si.ref_type = &apos;APP_HEADER&apos;
    and sd.STEP_DEFINITION_ID in (${MI_TASK_TYPE_IDS})
    and si.ref_id = 315149
     &#47;*   and ( status not in (&apos;COMPLETED&apos;,&apos;TERMINATED&apos;)
        	  OR completed_ts &gt;=trunc(sysdate)- ${PRO_MI_CDC_MI_TASKS_DAYS}
              OR si.step_instance_id = ?
		     )  *&#47;
   ) MI_task
join app_missing_info  mi          on MI_task.Task_App_ID = mi.application_id
join ENUM_APP_MI_TYPE  mi_type on mi_type.value = mi.mi_type_cd
where 1=1 
--and mi_task.task_app_id =   2743 --&amp;p_app_id
order by task_id</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
