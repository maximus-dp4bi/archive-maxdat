  <step>
    <name>Table Input - UPD2_10 - Corp_ETL_Manage_Work</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT  mw.cemw_id
       ,trunc(nvl(mw.complete_date, sysdate) - trunc(mw.create_date))  AGE_IN_CALENDAR_DAYS
       ,bus_days_between(mw.create_date,trunc(nvl(mw.complete_date, sysdate))) AGE_IN_BUSINESS_DAYS
       ,CASE
            WHEN mw.sla_jeopardy_days is null then &apos;N&apos;
            WHEN mw.sla_days_type = &apos;B&apos; AND  bus_days_between(mw.create_date,trunc(nvl(mw.complete_date, sysdate))) &gt; mw.sla_jeopardy_days THEN &apos;Y&apos;
            WHEN mw.sla_days_type = &apos;C&apos; AND  trunc(nvl(mw.complete_date, sysdate) - trunc(mw.create_date)) &gt;  mw.sla_jeopardy_days THEN &apos;Y&apos;
            ELSE &apos;N&apos; 
         END  JEOPARDY_FLAG
       ,CASE
            WHEN mw.complete_flag = &apos;N&apos; then &apos;Not Complete&apos;
            WHEN mw.sla_days is null then &apos;Not Required&apos;
            WHEN mw.sla_days_type = &apos;B&apos; AND  bus_days_between(mw.create_date,trunc(nvl(mw.complete_date, sysdate))) &gt;  mw.sla_days then &apos;Untimely&apos;
            WHEN mw.sla_days_type = &apos;C&apos; AND  trunc(nvl(mw.complete_date, sysdate) - trunc(mw.create_date)) &gt;  mw.sla_days then &apos;Untimely&apos;
            else &apos;Timely&apos; END  TIMELINESS_STATUS
       ,DECODE(mw.task_status, &apos;COMPLETED&apos;, 0,trunc(nvl(mw.complete_date, sysdate)) - trunc(mw.status_date))  STATUS_AGE_IN_CAL_DAYS
       ,DECODE(mw.task_status, &apos;COMPLETED&apos;, 0,bus_days_between(mw.status_date,trunc(nvl(mw.complete_date, sysdate)))) STATUS_AGE_IN_BUS_DAYS
       ,trunc(sysdate) system_date
  FROM corp_etl_manage_work_tmp mw
 WHERE mw.stage_done_date IS NULL
   AND mw.cancel_work_flag = &apos;N&apos;
   AND (mw.age_in_calendar_days IS NULL
    OR  mw.age_in_calendar_days &lt;&gt; trunc(nvl(mw.complete_date, sysdate)) - trunc(mw.create_date)       )
</sql>
