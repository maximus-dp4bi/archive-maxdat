         ELSE &apos;Client&apos;
       END mi_item_level
     , CASE mit.satisfied_without_tangible_ind
         WHEN 0 THEN &apos;Data&apos;
         WHEN 1 THEN &apos;Documentation&apos;
       END mi_item_type
     , mi.create_ts mi_item_create_dt
     , mi.created_by
     , mi.updated_by
     , NULL rfe_status
     , TRUNC(SYSDATE) rfe_status_dt
     , CASE 
         WHEN SUBSTR(mi.updated_by,1,1) IN (&apos;-&apos;,&apos;1&apos;,&apos;2&apos;,&apos;3&apos;,&apos;4&apos;,&apos;5&apos;,&apos;6&apos;,&apos;7&apos;,&apos;8&apos;,&apos;9&apos;,&apos;0&apos;) THEN &apos;Y&apos;
         ELSE &apos;N&apos;
       END updated_by_is_number
     , CASE WHEN NVL(mi.satisfied_reason_cd,&apos;void&apos;) = &apos;void&apos; THEN NULL
            WHEN s2.staff_id IS NULL THEN mi.updated_by
            ELSE s2.last_name||DECODE(s2.first_name,NULL,NULL,&apos;,&apos;)||s2.first_name
       END mi_item_status_performer
     , CASE 
         WHEN s.staff_id IS NULL THEN mi.created_by
         ELSE s.last_name||DECODE(s.first_name,NULL,NULL,&apos;,&apos;)||s.first_name
       END mi_item_requested_by
     , CASE WHEN mi.satisfied_reason_cd IS NULL THEN mi.create_ts
         ELSE NVL(mi.satisfied_date,mi.update_ts)
       END mi_item_status_dt
     , CASE WHEN mi.satisfied_reason_cd IS NULL THEN &apos;Unsatisfied&apos;
            WHEN mi.satisfied_reason_cd = &apos;void&apos; THEN &apos;Removed&apos;
            ELSE &apos;Satisfied&apos;
        END mi_item_status
     , CASE 
         WHEN mi.app_individual_id IS NULL 
         THEN 0
         ELSE ( SELECT MAX(at.app_tracker_id)
                  FROM app_tracker at
                 WHERE at.application_id = mi.application_id
                   AND at.app_individual_id = mi.app_individual_id
                   AND at.history_ind = 0  )
       END app_tracker_id
     , mi.call_record_id outbound_call_id 
     , ( SELECT COUNT(1)
           FROM app_tracker at1
          WHERE at1.application_id = mi.application_id
            AND at1.app_individual_id = mi.app_individual_id
            AND at1.history_ind = 0
            AND at1.rfe_status_cd &lt;&gt; &apos;ACCEPTED&apos; ) rfe_not_approved_count
     , ( SELECT MIN(lr.lmreq_id)
           FROM letter_request lr
              , letter_request_link ll 
              , letter_definition ld
          WHERE lr.lmreq_id = ll.lmreq_id 
            AND lr.lmdef_id = ld.lmdef_id
            AND ll.reference_id = mi.application_id
            AND ld.name in (&apos;MI1&apos;,&apos;MI2&apos;)
            AND ll.reference_type = &apos;APP_HEADER&apos;
            AND lr.requested_on &gt;= mi.create_ts ) mi_letter_id
     , NULL null_value
     , &apos;Y&apos; in_process_ind
  FROM app_missing_info mi
     , enum_app_mi_type mit
     , staff s
     , staff s2
 WHERE mit.value = mi.mi_type_cd
   AND mi.created_by = s.ext_staff_number(+)
   AND mi.updated_by = s2.ext_staff_number(+)
   AND mi.missing_info_id &gt; ?</sql>
