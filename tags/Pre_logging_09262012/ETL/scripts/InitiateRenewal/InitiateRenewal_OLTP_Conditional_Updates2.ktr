    <sql>select notification_request_id  notice_3_source_id
 from notification_request nr
where ref_type1 =&apos;APP_HEADER&apos;
and ref_type1_value = ?
and type_cd = &apos;RR2&apos;
and notification_request_id in (select min(notification_request_id) from notification_request nr2
                                where nr2.ref_type1 =&apos;APP_HEADER&apos;
                                and nr2.ref_type1_value = nr.ref_type1_value
