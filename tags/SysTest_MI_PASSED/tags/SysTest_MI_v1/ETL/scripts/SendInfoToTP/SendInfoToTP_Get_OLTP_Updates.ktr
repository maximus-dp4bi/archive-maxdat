  <step>
    <name>Database join - OLTP Updates for Events</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT a.status_cd       app_status
     , h.auth_to_dt      auth_end_date
     , ( SELECT MAX(at.rfe_status_cd)
           FROM app_tracker at
          WHERE at.application_id = a.application_id
            AND at.history_ind = 0 ) rfe_status
     , e.create_ts      info_req_sent_dt
     , CASE
         ( SELECT COUNT(1)
             FROM app_missing_info mi
            WHERE mi.application_id = e.ref_id
              AND mi.satisfied_date IS NULL ) 
         WHEN 0 THEN &apos;N&apos;
         ELSE &apos;Y&apos;
       END              mi_exists  
  FROM event e
     , app_header a
     , hrt_case h
 WHERE e.ref_id = a.application_id
   AND a.application_id = h.application_id
   AND e.event_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>INFO_REQ_ID</name>
        <type>Number</type>
      </field>
    </parameter>
     <cluster_schema/>
 <remotesteps>   <input>   </input>   <output>   </output> </remotesteps>    <GUI>
      <xloc>279</xloc>
