      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>Database join - OLTP Updates for Letters</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT r.sent_on           info_req_create_dt
     , CASE
         WHEN r.status_cd IN (&apos;VOID&apos;,&apos;CANC&apos;,&apos;REJ&apos; ) THEN r.update_ts
         ELSE NULL
       END cancel_date
     , s.report_label      info_req_status
     , r.sent_on           info_req_sent_dt
     , CASE r.status_cd
        WHEN &apos;ERR&apos; THEN &apos;N&apos;
        WHEN &apos;MAIL&apos; THEN &apos;Y&apos;
        ELSE NULL
       END                 gwf_ltr_mailed
     , ( SELECT MAX(sh.create_ts)
           FROM letter_req_status_history sh
          WHERE sh.lmreq_id = r.lmreq_id
            AND sh.status_cd = r.status_cd ) letter_status_dt
     , CASE r.request_type 
         WHEN &apos;L&apos; THEN r.create_ts
         WHEN &apos;R&apos; THEN r.create_ts
         WHEN &apos;S&apos; THEN   
            ( SELECT MIN(r1.create_ts)
                FROM letter_request r1
               WHERE r1.lmdef_id = r.lmdef_id
                 AND r1.status_cd NOT IN (&apos;REQ&apos;,&apos;CANC&apos;,&apos;VOID&apos;)
                 AND r1.mailed_date IS NOT NULL
                 AND r1.lmreq_id &lt; r.lmreq_id
                 AND r1.case_id = r.case_id 
                 AND r1.create_ts &lt; ( r.create_ts - 60 ))
         ELSE r.create_ts
       END                 info_req_cycle_start_dt
     , ( SELECT MIN(i.create_ts)
                  FROM letter_image i
                 WHERE i.lmreq_id = r.lmreq_id ) letter_image_link_dt
     , CASE
         WHEN r.status_cd IN (&apos;VOID&apos;,&apos;CANC&apos;,&apos;REJ&apos; ) THEN r.update_ts
         WHEN r.status_cd = &apos;MAIL&apos; THEN r.mailed_date
         WHEN r.status_cd = &apos;ERR&apos; THEN
            ( SELECT MIN(lr.create_ts)
                FROM letter_request lr
               WHERE lr.parent_lmreq_id = r.lmreq_id
                 AND lr.create_ts &gt; r.create_ts )
         ELSE NULL
       END                          info_req_cycle_end_dt
     , CASE
         WHEN r.status_cd &lt;&gt; &apos;MAIL&apos; THEN NULL
         ELSE ( SELECT MIN(i.create_ts)
                  FROM letter_image i
                 WHERE i.lmreq_id = r.lmreq_id )
       END                          ased_mail_letter_req
     , r.sent_on                    assd_mail_letter_req
     , CASE 
         WHEN r.request_type = &apos;S&apos; THEN o.create_ts
         ELSE NULL
       END                          assd_create_new_letter_req  
     , CASE 
         WHEN r.request_type = &apos;S&apos; THEN
           CASE 
             WHEN o.mailed_date IS NOT NULL THEN o.mailed_date
             ELSE o.update_ts 
           END
         ELSE NULL
       END                          ased_create_new_letter_req  
  FROM letter_request r
     , letter_definition d
     , enum_lm_status s
     , letter_request o
 WHERE r.lmdef_id = d.lmdef_id
   AND r.parent_lmreq_id = o.lmreq_id(+) 
   AND r.status_cd = s.value
   AND r.lmreq_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
