    <connection>MAXDAT</connection>
    <sql>SELECT s.inreq_id
     , s.info_req_source
     , s.info_req_id
     , s.info_req_group
     , s.gwf_request_type
     , ( SELECT COUNT(1) 
           FROM cc_event_stg e
          WHERE e.ref_id = s.app_id
            AND e.call_campaign_type LIKE s.event_type_cd||&apos;%&apos;
            AND e.call_campaign_type &lt;&gt; s.event_type_cd ) call_retry_ct
     , ( SELECT MIN(e2.event_create_ts) 
           FROM cc_event_stg e2
          WHERE e2.ref_id = s.app_id
            AND e2.call_campaign_type = s.event_type_cd ) info_req_cycle_start_dt
     , ( SELECT MAX(e3.event_create_ts) 
           FROM cc_event_stg e3
          WHERE e3.ref_id = s.app_id
            AND e3.call_campaign_type LIKE s.event_type_cd||&apos;%&apos;
            AND e3.call_campaign_type &lt;&gt; s.event_type_cd ) info_req_cycle_end_dt
     , SYSDATE current_dt
  FROM sendinfotradpart_stg s
 WHERE s.info_req_source = &apos;Event ID&apos;
   AND s.new_ind = &apos;N&apos;
   AND s.app_in_process = &apos;Y&apos;</sql>
