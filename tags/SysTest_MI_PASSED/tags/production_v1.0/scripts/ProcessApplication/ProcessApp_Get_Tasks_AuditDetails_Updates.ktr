  <step>
    <name>Table input - Identify Applications To Be Updated</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT A.App_id 
	 , DE.Completed_TS Rvw_Enter_Data_Dt 
     , DE.Completed_TS Process_App_Info_Dt
     , QC.Completed_TS Perform_QC_Dt
     , RS.Completed_TS Perform_Research_Dt
     , SR.Completed_TS Wait_State_Approval_Dt

     , A.Cancel_DT     Assd_Cancel_App
     , A.Cancel_DT     Ased_Cancel_App
     , CASE WHEN A.App_Status = &apos;Completed(Timeout)&apos; THEN A.Complete_DT ELSE NULL END Assd_Close_App
     , CASE WHEN A.App_Status = &apos;Completed(Timeout)&apos; THEN A.Complete_DT ELSE NULL END Ased_Close_App     
     , CASE WHEN A.App_Status = &apos;Completed&apos; 		 THEN A.Complete_DT ELSE NULL END Assd_Complete_App
     , CASE WHEN A.App_Status = &apos;Completed&apos; 		 THEN A.Complete_DT ELSE NULL END Ased_Complete_App     

     , A.Notify_Client_Pended_App_DT  Assd_Notify_Client_Pended_App
     , A.Notify_Client_Pended_App_DT  Ased_Notify_Client_Pended_App
     , QC.Create_TS                   Assd_Perform_Qc
     , QC.Completed_TS                Ased_Perform_Qc 
     , RS.Create_TS                   Assd_Perform_Research
     , RS.Completed_TS                Ased_Perform_Research    
      
     , DE.Completed_TS                Assd_Process_App_Info
     , CASE WHEN QC.Step_Instance_ID IS NOT NULL THEN QC.Create_TS
            WHEN QC.Step_Instance_ID IS NULL AND RS.Step_Instance_ID IS NOT NULL THEN RS.Create_TS
            WHEN QC.Step_Instance_ID IS NULL AND RS.Step_Instance_ID IS NULL THEN SR.Create_TS 
            ELSE NULL END Ased_Process_App_Info     

	 , CASE WHEN A.App_Status = &apos;Completed&apos; AND A.Auto_Reprocess_flag = &apos;N&apos; AND A.MI_Received_Task_Count &lt;&gt; 0 THEN A.Assd_Receive_Process_MI
			WHEN A.App_Status = &apos;Completed&apos; AND A.Auto_Reprocess_flag = &apos;Y&apos; AND A.MI_Received_Task_Count &lt;&gt; 0 THEN A.Complete_Dt
			ELSE NULL END Assd_Receive_Process_MI

	 , CASE WHEN A.App_Status = &apos;Completed&apos; AND A.MI_Received_Task_Count &lt;&gt; 0 THEN A.Complete_Dt
			ELSE NULL END Ased_Receive_Process_MI
     
     , A.Receipt_DT                   Assd_Receive_App
     , A.Receipt_DT                   Ased_Receive_App     
     , DE.Create_TS                   Assd_Rvw_Enter_Data
     , DE.Completed_TS                Ased_Rvw_Enter_Data     

     , SR.Create_TS                   Assd_Wait_State_Approval
     , SR.Completed_TS                Ased_Wait_State_Approval
     
     , CASE WHEN A.Cancel_Flag = &apos;Y&apos; THEN 
                 (SELECT Display_name FROM Staff_lkup S WHERE S.Staff_ID = A.Completed_By) 
            ELSE NULL END             Aspb_Cancel_App
            
     , CASE WHEN A.APP_STATUS = &apos;Completed(Timeout)&apos; THEN 
                 (SELECT Display_name FROM Staff_lkup S WHERE S.Staff_ID = A.Completed_By) 
            ELSE NULL END             Aspb_Close_App
            
     , CASE WHEN A.APP_STATUS = &apos;Completed(Timeout)&apos; THEN 
                 (SELECT Display_name FROM Staff_lkup S WHERE S.Staff_ID = A.Completed_By) 
            ELSE NULL END             Aspb_Complete_App
            
     ,  CASE WHEN A.Notify_Client_Pended_App_DT IS NOT NULL THEN
				 (SELECT Display_name FROM Staff_lkup S where S.Staff_ID = &apos;-19&apos;)  
			 ELSE NULL END  Aspb_Notify_Client_Pended_App

     ,  (SELECT Display_name FROM Staff_lkup S where S.Staff_ID = QC.Owner)          Aspb_Perform_QC
     ,  (SELECT Display_name FROM Staff_lkup S where S.Staff_ID = RS.Owner)          Aspb_Perform_Research
     ,  (SELECT Display_name FROM Staff_lkup S where S.Staff_ID = DE.Owner)          Aspb_Process_App_Info

	 , CASE WHEN A.App_Status = &apos;Completed&apos; AND A.MI_Received_Task_Count &lt;&gt; 0 
			THEN (Select Display_name From Staff_lkup S 
				   where S.Staff_ID = A.Aspb_Receive_Process_MI 
					 and ASCII(substr(A.Aspb_Receive_Process_MI,-1)) between 49 and 57 ) 
			ELSE NULL END          													 Aspb_Receive_Process_MI

     ,  (SELECT Display_name FROM Staff_lkup S where S.Staff_ID = DE.Created_By)     Aspb_Receive_App
     ,  (SELECT Display_name FROM Staff_lkup S where S.Staff_ID = DE.Owner)          Aspb_Rvw_Enter_Data
     ,  (SELECT Display_name FROM Staff_lkup S where S.Staff_ID = SR.Owner)          Aspb_Wait_State_Approval


     , DE.Step_Instance_ID DE_TASK_ID
     , QC.Step_Instance_ID QC_TASK_ID 
     , RS.Step_Instance_ID RESEARCH_TASK_ID
     , SR.Step_Instance_ID STATE_REVIEW_TASK_ID
     
     , SYSDATE Stg_Last_Processed_DT
     
  FROM Process_App_Stg A 
  LEFT OUTER JOIN  Step_Instance_Stg DE ON  DE.Step_Instance_History_id = DE_HIST_TASK_ID 
  LEFT OUTER JOIN  Step_Instance_Stg QC ON  QC.Step_Instance_History_id = QC_HIST_TASK_ID 
  LEFT OUTER JOIN  Step_Instance_Stg RS ON  RS.Step_Instance_History_id = RESEARCH_HIST_TASK_ID 
  LEFT OUTER JOIN  Step_Instance_Stg SR ON  SR.Step_Instance_History_id = STATE_REVIEW_HIST_TASK_ID 
 where App_in_process = &apos;Y&apos;
   and App_extract_complete_dt is null
   and App_Priority_Ind = 1
</sql>
