    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT a.status_cd       app_status
     , h.auth_to_dt      auth_end_date
     , ( SELECT MAX(at.rfe_status_cd)
           FROM app_tracker at
          WHERE at.application_id = a.application_id
            AND at.history_ind = 0 ) rfe_status
     , ( SELECT MIN(d.etl_e_834_event_staging_id) 
           FROM etl_e_dialer d
              , etl_report j
          WHERE d.job_id = j.job_id
            AND d.application_id = a.application_id
            AND d.campaign_name = ?
            AND j.start_ts &gt; e.create_ts ) new_event_staging_id
     , CASE
         ( SELECT COUNT(1)
             FROM app_missing_info mi
            WHERE mi.application_id = e.ref_id
              AND mi.satisfied_date IS NULL ) 
         WHEN 0 THEN &apos;N&apos;
         ELSE &apos;Y&apos;
       END              mi_exists
     , ( SELECT COUNT(1)
           FROM etl_e_dialer d
              , etl_report r 
          WHERE d.job_id = r.job_id 
            AND d.application_id = a.application_id
            AND d.campaign_name = ? ) call_retry_ct 
     , ( SELECT MIN(etl_l_dialer_response_id) 
           FROM etl_l_dialer_response r
          WHERE r.application_id = e.ref_id
            AND r.campaign_name = ?
            AND TO_DATE(SUBSTR(TRIM(TO_CHAR(TO_NUMBER(r.call_start_time),&apos;09999999999999&apos;)),1,8),&apos;MMDDYYYY&apos;) &gt; e.create_ts ) new_dialer_response_id
     , a.application_id
  FROM event e
     , app_header a
     , hrt_case h
 WHERE e.ref_id = a.application_id
   AND a.application_id = h.application_id(+)
   AND e.event_id = ?</sql>
