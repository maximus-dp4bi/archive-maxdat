  <step>
    <name>Database join - Call Req</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT CASE 
         WHEN d.error_count IS NULL THEN NULL
         ELSE j.end_ts  
       END new_info_req_sent_dt
     , CASE 
         WHEN d.error_count IS NULL THEN &apos;Sent&apos;
         ELSE &apos;Canceled&apos;    
       END new_info_req_status
  FROM etl_e_dialer d
     , etl_report j
 WHERE d.job_id = j.job_id
   AND etl_e_834_event_staging_id = ? </sql>
    <outer_join>Y</outer_join>
    <replace_vars>N</replace_vars>
    <parameter>
      <field>
        <name>NEW_EVENT_STAGING_ID</name>
        <type>BigNumber</type>
