  <step>
    <name>Database join - Letters OLTP Updates</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT ( SELECT MAX(sh.create_ts)
           FROM letter_req_status_history sh
          WHERE sh.lmreq_id = r.lmreq_id
            AND sh.status_cd = r.status_cd ) letter_status_dt
     , ( SELECT MIN(i.create_ts)
                  FROM letter_image i
                 WHERE i.lmreq_id = r.lmreq_id ) letter_image_link_dt
     , CASE
         WHEN r.status_cd &lt;&gt; &apos;MAIL&apos; THEN NULL
         ELSE ( SELECT MIN(i.create_ts)
                  FROM letter_image i
                 WHERE i.lmreq_id = r.lmreq_id )
       END                          ased_mail_letter_req
  FROM letter_request r
 WHERE r.lmreq_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>INFO_REQ_ID</name>
        <type>Number</type>
      </field>
    </parameter>
     <cluster_schema/>
 <remotesteps>   <input>   </input>   <output>   </output> </remotesteps>    <GUI>
      <xloc>377</xloc>
      <yloc>120</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>Database join - MAXDAT Updates for Letters</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT r.letter_sent_on           info_req_create_dt
     , CASE
         WHEN r.letter_status_cd IN (&apos;VOID&apos;,&apos;CANC&apos;,&apos;REJ&apos; ) THEN r.letter_update_ts
         ELSE NULL
       END cancel_date
     , s.out_var      info_req_status
     , r.letter_sent_on           info_req_sent_dt
     , CASE r.letter_status_cd
        WHEN &apos;ERR&apos; THEN &apos;N&apos;
        WHEN &apos;MAIL&apos; THEN &apos;Y&apos;
        ELSE NULL
       END                 gwf_ltr_mailed
     , CASE r.letter_request_type 
         WHEN &apos;L&apos; THEN r.letter_create_ts
         WHEN &apos;R&apos; THEN r.letter_create_ts
         WHEN &apos;S&apos; THEN   
            ( SELECT MIN(r1.letter_create_ts)
                FROM letters_stg r1
               WHERE r1.letter_type_cd = r.letter_type_cd
                 AND r1.letter_status_cd NOT IN (&apos;REQ&apos;,&apos;CANC&apos;,&apos;VOID&apos;)
                 AND r1.letter_mailed_date IS NOT NULL
                 AND r1.letter_id &lt; r.letter_id
                 AND r1.letter_case_id = r.letter_case_id 
                 AND r1.letter_create_ts &lt; ( r.letter_create_ts - 60 ))
         ELSE r.letter_create_ts
       END                 info_req_cycle_start_dt
     , CASE
         WHEN r.letter_status_cd IN (&apos;VOID&apos;,&apos;CANC&apos;,&apos;REJ&apos; ) THEN r.letter_update_ts
         WHEN r.letter_status_cd = &apos;MAIL&apos; THEN r.letter_mailed_date
         WHEN r.letter_status_cd = &apos;ERR&apos; THEN
            ( SELECT MIN(lr.letter_create_ts)
                FROM letters_stg lr
               WHERE lr.letter_parent_lmreq_id = r.letter_id
                 AND lr.letter_create_ts &gt; r.letter_create_ts )
         ELSE NULL
       END                          info_req_cycle_end_dt
     , r.letter_sent_on                    assd_mail_letter_req
     , CASE 
         WHEN r.letter_request_type = &apos;S&apos; THEN o.letter_create_ts
         ELSE NULL
       END                          assd_create_new_letter_req  
     , CASE 
         WHEN r.letter_request_type = &apos;S&apos; THEN
           CASE 
             WHEN o.letter_mailed_date IS NOT NULL THEN o.letter_mailed_date
             ELSE o.letter_update_ts 
           END
         ELSE NULL
       END                          ased_create_new_letter_req  
  FROM letters_stg r
     , letters_stg o
     , corp_etl_list_lkup s
 WHERE r.letter_status_cd = s.value
   AND s.name = &apos;ENUM_LM_STATUS&apos;
   AND r.letter_parent_lmreq_id = o.letter_id(+) 
   AND r.letter_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>INFO_REQ_ID</name>
        <type>Number</type>
      </field>
    </parameter>
