  <step>
    <name>Database join 2 -- Get Process_DC</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>2</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT 
        (case when si_comp.hist_status = &apos;COMPLETED&apos; then si_comp.hist_create_ts end) as &quot;ASED_PROCESS_DC&quot;,
        Slkup2.Display_Name as &quot;ASPB_PROCESS_DC&quot;,
        (case when si_cl.hist_status = &apos;CLAIMED&apos; then si_cl.hist_create_ts end) as &quot;ASSD_PROCESS_DC&quot;
           
from step_instance_stg si
left outer join (select * from step_instance_stg si_cl_2 
                    where si_cl_2.step_instance_id = ?
                   and si_cl_2.step_instance_history_id = (select min(si_cl_3.step_instance_history_id)
                                                             from step_instance_stg si_cl_3
                                                            where si_cl_3.step_instance_id = si_cl_2.step_instance_id
                                                            and si_cl_3.hist_status = &apos;CLAIMED&apos;
                                                            and si_cl_3.hist_create_ts &lt;=  ?
															and si_cl_3.step_definition_id   IN
                                                                                                (SELECT ref_id
                                                                                                 FROM corp_etl_list_lkup
                                                                                                 WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                                                                                       AND out_var = &apos;Data Correction&apos;
                                                                                                       AND SYSDATE BETWEEN start_date AND end_date)

                                                            ) 
                   ) si_cl on si.step_instance_id = si_cl.step_instance_id
left outer join (select * from step_instance_stg si_comp
                    where si_comp.step_instance_id = ?
                   and si_comp.step_instance_history_id = (select min(si_comp_3.step_instance_history_id)
                                                             from step_instance_stg si_comp_3
                                                            where si_comp_3.step_instance_id = si_comp.step_instance_id
                                                            and si_comp_3.hist_status = &apos;COMPLETED&apos;
                                                            and si_comp_3.hist_create_ts &lt;=  ?
															and si_comp_3.step_definition_id  IN
                                                                                                (SELECT ref_id
                                                                                                 FROM corp_etl_list_lkup
                                                                                                 WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                                                                                                       AND out_var = &apos;Data Correction&apos;
                                                                                                       AND SYSDATE BETWEEN start_date AND end_date)
                                                            ) 
                   ) si_comp on si.step_instance_id = si_comp.step_instance_id
left outer join Staff_LKup Slkup2  on si.hist_create_by = Slkup2.Staff_id
where si.step_instance_id = ?
      AND 
      si.step_definition_id IN
               (SELECT ref_id
                  FROM corp_etl_list_lkup
                 WHERE NAME = &apos;TASK_MONITOR_TYPE&apos;
                   AND out_var = &apos;Data Correction&apos;
                   AND SYSDATE BETWEEN start_date AND end_date)
      AND si.REF_ID = ?
</sql>
