  <step>
    <name>Database join - Retrieve OLTP Updates for an Application</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <copies>5</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT a.receipt_date     ren_receipt_dt
     , a.status_date      app_status_dt
     , a.status_cd        app_status
     , SYSDATE            stg_last_processed_dt
     , CASE WHEN a.status_cd = &apos;RENEWAL_INITIATED&apos; 
         THEN &apos;ACTIVE&apos;
         ELSE &apos;COMPLETE&apos;
       END                instance_status
     , CASE WHEN a.status_cd = &apos;DISREGARDED&apos;
         THEN a.status_date
         ELSE NULL
       END cancel_dt   
     , CASE WHEN a.status_cd = &apos;TIMEOUT&apos;
         THEN a.status_date
         ELSE NULL
       END close_dt   
     , c.auth_to_dt       new_auth_end_dt
     , CASE WHEN c.transaction_type_cd IN (&apos;07&apos;,&apos;08&apos;) THEN
         CASE WHEN 
            ( SELECT COUNT(1) 
                FROM hrt_case_control cc
               WHERE cc.hrt_case_id = c.hrt_case_id 
                 AND cc.current_status_dt &lt; a.renewal_due_date
                 AND cc.current_status_cd = &apos;05&apos; ) = 0
           THEN &apos;N&apos; 
           WHEN ( SELECT COUNT(1) 
                    FROM hrt_case_deferral cd
                   WHERE c.hrt_case_id = cd.hrt_case_id 
                     AND ( cd.deferral_res_cd IS NULL OR cd.deferral_res_cd = &apos;04&apos;)
                     AND cd.deferral_cd = &apos;02&apos; ) = 0        
           THEN &apos;N&apos;
           ELSE &apos;Y&apos;
         END
         ELSE &apos;Y&apos;
       END clockdown_indicator
  FROM app_header a
     , hrt_case c
 WHERE a.application_id = c.application_id(+)
   AND a.application_id = ?
</sql>
