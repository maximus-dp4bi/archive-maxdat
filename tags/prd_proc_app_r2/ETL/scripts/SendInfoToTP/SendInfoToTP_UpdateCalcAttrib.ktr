    <connection>MAXDAT</connection>
    <sql>SELECT s.inreq_id
     , CASE
         WHEN s.cancel_date IS NOT NULL THEN bus_days_between(info_req_create_dt,cancel_date)
         WHEN s.instance_complete_dt IS NOT NULL THEN bus_days_between(info_req_create_dt,instance_complete_dt)
         ELSE bus_days_between(info_req_create_dt,SYSDATE)
       END age_in_business_days
     , CASE
         WHEN s.cancel_date IS NOT NULL THEN TRUNC(cancel_date) - TRUNC(info_req_create_dt)
         ELSE TRUNC(NVL(instance_complete_dt, SYSDATE)) - TRUNC(info_req_create_dt) 
       END age_in_calendar_days
     , CASE 
         WHEN s.info_req_source = &apos;Letter Req ID&apos;
          AND s.info_req_status IN (&apos;Voided&apos;,&apos;Canceled&apos;,&apos;Errored&apos;)
         THEN &apos;Complete&apos;
         WHEN s.cancel_date IS NOT NULL
           OR gwf_request_type = &apos;R&apos;
           OR asf_process_image = &apos;Y&apos;
           OR gwf_retry_call IN (&apos;C&apos;,&apos;D&apos;)
           OR asf_create_new_call_req = &apos;Y&apos;
           OR gwf_ltr_mailed = &apos;Y&apos;
           OR asf_create_new_letter_req = &apos;Y&apos;
         THEN &apos;Complete&apos;
         ELSE &apos;Active&apos;
       END instance_status
     , CASE 
         WHEN s.info_req_source = &apos;Letter Req ID&apos;
          AND s.info_req_status IN (&apos;Voided&apos;,&apos;Canceled&apos;,&apos;Errored&apos;)
         THEN SYSDATE
         ELSE s.cancel_date
       END cancel_date
     , CASE
         WHEN s.cancel_date IS NULL 
         THEN bus_days_between( s.info_req_cycle_start_dt, NVL(s.info_req_cycle_end_dt, SYSDATE))
         ELSE NULL
       END info_req_cycle_bus_days
     , CASE s.instance_status
         WHEN &apos;Active&apos; THEN
            CASE
              WHEN s.sla_days_type = &apos;B&apos; 
               AND bus_days_between(TRUNC(s.info_req_cycle_start_dt), TRUNC(NVL(s.info_req_cycle_end_dt, SYSDATE))) &gt; s.sla_jeopardy_days 
              THEN &apos;Y&apos;
              WHEN s.sla_days_type = &apos;C&apos; 
               AND TRUNC(s.info_req_cycle_start_dt) - NVL(TRUNC(s.info_req_cycle_end_dt), TRUNC(SYSDATE)) &gt;  sla_jeopardy_days 
              THEN &apos;Y&apos;
              ELSE &apos;N&apos;
            END
         ELSE s.jeopardy_flag
       END jeopardy_flag
     , CASE
         WHEN s.call_result IS NULL THEN NULL
         WHEN s.call_result IN (&apos;Error&apos;,&apos;Fax&apos;,&apos;Invalid Number&apos;) THEN &apos;Invalid Number&apos;
         WHEN s.call_result IN (&apos;Live Call and Transfer&apos;,&apos;Live Call and Hang-up&apos;) THEN &apos;Successful Outreach&apos;
         WHEN s.call_result IN (&apos;Voice Mail&apos;,&apos;Busy Signal&apos;,&apos;Ring No Answer&apos;) THEN &apos;Unsuccessful Outreach&apos;
         ELSE NULL
       END  call_success_result
     , CASE
         WHEN s.info_req_group = &apos;Outbound Call&apos; THEN
           CASE
             WHEN s.event_type_cd IN (&apos;OB_DIALER_MISSING_DATA&apos;,&apos;OB_DIALER_MISSING_DOC&apos;)
              AND ( s.auth_end_date IS NOT NULL AND s.auth_end_date &gt; s.info_req_create_dt )
              AND s.rfe_status = &apos;AWAITING_MI&apos;
              AND s.mi_exists = &apos;Y&apos;
              AND s.app_status = &apos;INPROCESS&apos;
              AND s.no_call_ind = &apos;N&apos;
              AND s.call_retry_ct &lt; s.call_retry_limit
             THEN &apos;Y&apos;
             WHEN s.event_type_cd IN (&apos;OB_DIALER_PRE_RENEWAL&apos;,&apos;OB_DAILER_RENEWAL_REM&apos;)
              AND s.app_status = &apos;RENEWAL_INITIATED&apos;
              AND s.no_call_ind = &apos;N&apos;
              AND s.call_retry_ct &lt; s.call_retry_limit
             THEN &apos;Y&apos;
             ELSE &apos;N&apos;
           END
         ELSE NULL
       END call_flag
  FROM sendinfotradpart_stg s
 WHERE s.new_ind = &apos;N&apos;
   AND s.app_in_process = &apos;Y&apos;</sql>
