    <connection>OLTP_SOURCE</connection>
    <sql>SELECT * 
FROM (
      SELECT value reason_code
             ,description reason       
             ,CASE WHEN LENGTH(value) = 1THEN &apos;CHIP&apos; ELSE &apos;MEDICAID&apos;  END managed_care_program        
             ,effective_start_date
      FROM enum_disenrollment_reason d
      WHERE  (INSTR(value,&apos;-&apos;) = 0 OR LENGTH(value) = 1) 
      AND value NOT IN (&apos;A1&apos;,&apos;B1&apos;,&apos;DS&apos;,&apos;UR&apos;,&apos;B2&apos;,&apos;44&apos;,&apos;46&apos;,&apos;47&apos;,&apos;48&apos;,&apos;49&apos;,&apos;50&apos;,&apos;54&apos;)
      ) tmp_chg,
  (SELECT DISTINCT plan_code plan_name, program_type_cd
    FROM plans p, contract c
    WHERE p.plan_id = c.plan_id
    AND end_date IS NULL
    AND status_cd = &apos;ACTIVE&apos;
    AND INSTR(plan_code, &apos;ZZ&apos;) = 0) tmp_plans
WHERE tmp_chg.managed_care_program = tmp_plans.program_type_cd    
ORDER BY managed_care_program, reason_code
</sql>
