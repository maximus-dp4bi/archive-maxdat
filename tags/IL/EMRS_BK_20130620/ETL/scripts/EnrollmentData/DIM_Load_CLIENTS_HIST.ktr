    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT managed_care_program
       ,receiving_ssi_flag
       ,receiving_tanf_flag
       ,CASE WHEN cscl_elig_end_dt IS NULL AND cscl_status_cd = &apos;O&apos; THEN &apos;Y&apos; ELSE &apos;N&apos; END current_flag
--      ,CASE WHEN cscl_elig_end_dt IS NOT NULL AND cnt_rec != version THEN &apos;N&apos; ELSE &apos;Y&apos; END current_flag
      ,version
      ,cscl_elig_begin_dt date_of_validity_start
      ,date_of_validity_end
FROM (
    SELECT cc.cscl_clnt_client_id
                  ,cc.program_cd managed_care_program
                 ,cc.cscl_elig_begin_dt 
                 ,cc.cscl_elig_end_dt
                 ,cc.cscl_status_cd
                 ,CASE WHEN cc.cscl_elig_end_dt IS NULL AND cc.cscl_status_cd = &apos;O&apos; THEN TO_DATE(&apos;12&#47;31&#47;2050&apos;,&apos;MM&#47;DD&#47;YYYY&apos;) ELSE COALESCE(cc.cscl_elig_end_dt,cc.cscl_elig_begin_dt) END date_of_validity_end
                 , CASE WHEN INSTR(ac.description,&apos;SSI&apos;) &gt; 0 THEN &apos;Y&apos; ELSE &apos;N&apos; END receiving_ssi_flag
                 , CASE WHEN INSTR(ac.description,&apos;TANF&apos;) &gt; 0 THEN &apos;Y&apos; ELSE &apos;N&apos; END receiving_tanf_flag
                 ,ROW_NUMBER() OVER (PARTITION BY cscl_clnt_client_id ORDER BY cc.cscl_elig_begin_dt,cc.cscl_status_cd) version   
                 ,COUNT(1) over (PARTITION BY cscl_clnt_client_id) cnt_rec     
    FROM case_client cc
       LEFT OUTER JOIN enum_aid_category ac
         ON cc.cscl_adlk_id = ac.value)    
WHERE cscl_clnt_client_id = ?</sql>
