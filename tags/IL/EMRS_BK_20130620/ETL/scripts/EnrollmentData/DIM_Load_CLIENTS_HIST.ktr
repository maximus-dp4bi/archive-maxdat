               ,CASE WHEN segment_type_cd  IN( &apos;CHIP&apos;,&apos;CHIP-PER&apos;) THEN DECODE(segment_detail_value_3,&apos;Y&apos;,&apos;Y&apos;,&apos;N&apos;) ELSE &apos;N&apos; END managed_via_state_fund_flag
               ,CASE WHEN segment_type_cd =&apos;CHIP-PER&apos; THEN DECODE(segment_detail_value_4,&apos;Y&apos;,&apos;Y&apos;,&apos;N&apos;) ELSE &apos;N&apos; END pregnant_member
			   ,CASE WHEN INSTR(segment_type_cd,&apos;CLNT&apos;) &gt; 0 THEN segment_detail_value_1 ELSE NULL END base_plan 
FROM elig_segment_and_details
WHERE segment_type_cd IN (&apos;CHIP&apos;,&apos;CHIP-PER&apos;,&apos;CLNT-MED&apos;,&apos;CLNT-DEN&apos;,&apos;CLNT-NS&apos;)
AND (end_date IS NULL OR end_date &gt;= SYSDATE)
AND client_id = ?
</sql>
