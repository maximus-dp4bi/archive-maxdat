    <name>Table input -CLIENTS from EB</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>SELECT client_number
       ,source_record_id
       ,date_of_birth
       ,first_name
       ,last_name
       ,middle_initial
       ,full_name
       ,race
       ,sex
       ,social_security_number
       ,ssn_railroad_claim_number
       ,date_of_death
       ,date_of_death_source
       ,client_reported_shcn
       ,plan_reported_shcn
       ,third_party_resources_avail
       ,modified_date
       ,modified_name
       ,modified_time                
       ,record_date
       ,record_name
       ,record_time
       ,CASE WHEN client_age &lt; 1 THEN &apos;Y&apos; ELSE &apos;N&apos; END newborn  
       ,CASE WHEN client_age &lt; 19 THEN &apos;C&apos; ELSE &apos;A&apos; END adult_or_child
      ,add1
       ,add2                
      , city
      ,county
      ,zip
      ,st    
FROM
(SELECT cl.clnt_client_id client_number  
      ,cl.clnt_client_id source_record_id
      ,cl.clnt_dob date_of_birth
      ,cl.clnt_fname  first_name
      ,cl.clnt_lname last_name
      ,SUBSTR(cl.clnt_mi,1,1) middle_initial
      ,cl.clnt_fname||CASE WHEN SUBSTR(cl.clnt_mi,1,1) IS NULL THEN &apos; &apos; ELSE &apos; &apos;||SUBSTR(cl.clnt_mi,1,1)||&apos; &apos; END ||cl.clnt_lname full_name
      ,cl.clnt_race race
      ,cl.clnt_gender_cd sex
      ,cl.clnt_ssn social_security_number                
      ,cl.clnt_generic_field6_txt ssn_railroad_claim_number                
      ,cl.clnt_dod date_of_death
      ,cl.dod_source_cd date_of_death_source
      ,CASE WHEN cl.clnt_generic_field3_num = 1 THEN &apos;Y&apos; ELSE &apos;N&apos; END client_reported_shcn
      ,CASE WHEN cl.clnt_generic_field4_num = 1 THEN &apos;Y&apos; ELSE &apos;N&apos; END plan_reported_shcn                
      ,CASE WHEN (SELECT COUNT(1) FROM other_health_care ohc WHERE ohc.ohc_client_id = cl.clnt_client_id AND (ohc.ohc_end_date &gt; SYSDATE OR ohc.ohc_end_date IS NULL)) &gt; 0 THEN &apos;Y&apos; ELSE &apos;N&apos; END third_party_resources_avail
      ,cl.last_update_date modified_date
      ,cl.last_updated_by modified_name
      ,TO_CHAR(cl.last_update_date,&apos;HH24MI&apos;) modified_time                
      ,cl.creation_date record_date
      ,cl.created_by record_name
      ,TO_CHAR(cl.creation_date,&apos;HH24MI&apos;) record_time 
      ,EXTRACT(YEAR FROM(TRUNC(SYSDATE) - clnt_dob) YEAR TO MONTH) client_age
      ,cs.addr_street_1 add1
      ,cs.addr_street_2 add2                
      ,cs.addr_city city
      ,cs.addr_county county
      ,cs.addr_zip zip
      ,cs.addr_state_cd  st    
  FROM client cl
   INNER JOIN client_supplementary_info cs
      ON cl.clnt_client_id = cs.client_id
  )  
</sql>
