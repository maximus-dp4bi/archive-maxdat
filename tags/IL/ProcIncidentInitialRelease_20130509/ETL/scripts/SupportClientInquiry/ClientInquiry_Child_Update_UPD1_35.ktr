    <sql>SELECT SYSDATE                           system_date
     , &apos;CORP_ETL_CLNT_INQRY_WIP&apos;         driver_table_wip
     , &apos;ABORT&apos;                           error_abort_level
     , &apos;CRITICAL&apos;                        error_critical_level
     , &apos;LOG&apos;                             error_log_level
     , ceci_id
     , contact_record_id
     , contact_end_dt
     , TO_DATE(NULL) na, &apos;N&apos; no, &apos;Y&apos; yes
     , gwf_work_identified
     , asf_create_route_work
     , assd_create_route_work
     , ased_create_route_work
     , &apos;Y&apos; bpm_ind
     ,(SELECT MAX(event_create_dt) FROM corp_etl_client_inquiry_event eve
        WHERE supp_event_type_cd = &apos;MANUAL_TASK_CREATED&apos;
          AND eve.ceci_id        = inq.ceci_id) last_event_create_dt
  FROM corp_etl_client_inquiry inq
 WHERE instance_status = &apos;Active&apos;
   AND gwf_work_identified IS NULL
   AND contact_end_dt IS NOT NULL

</sql>
