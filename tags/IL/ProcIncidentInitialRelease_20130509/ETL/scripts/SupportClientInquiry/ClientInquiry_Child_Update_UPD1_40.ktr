    <connection>MAXDAT</connection>
    <sql>SELECT SYSDATE                           system_date
     , &apos;CORP_ETL_CLNT_INQRY_OLTP&apos;        driver_table_oltp
     , &apos;ABORT&apos;                           error_abort_level
     , &apos;CRITICAL&apos;                        error_critical_level
     , &apos;LOG&apos;                             error_log_level
     , oltp.*
     , DECODE(oltp.supp_latest_note_id, NULL, &apos;N&apos;, &apos;Y&apos;) note_present
     , &apos;Y&apos; bpm_ind, &apos;Complete&apos; complete_status
  FROM corp_etl_clnt_inqry_wip  wip
     , corp_etl_clnt_inqry_oltp oltp
 WHERE oltp.ceci_id = wip.ceci_id
   AND wip.complete_dt IS NOT NULL
   AND wip.instance_status != &apos;Complete&apos;
   AND TRUNC(SYSDATE) - TRUNC(wip.contact_end_dt) &gt; 3
</sql>
