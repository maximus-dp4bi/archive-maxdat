  <step>
    <name>Table input - OLTP Source Table</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <sql>select 
mjf.job_id,
mjf.create_dt,
mjf.created_by,
mjf.job_name,
mjf.job_detail,
mjf.job_group,
mjf.job_type,
mjf.job_start_dt,
--mjf.job_end_dt,
mjf.job_status,
--mjf.receipt_date,
case when mjf.job_type = &apos;Inbound File&apos;
      then to_date(substr(mjf.receipt_date,1,14),&apos;YYYYMMDDHH24MISS&apos;) end receipt_dt,
--mjf.transmit_date,
--case when mjf.job_type = &apos;Outbound File&apos;
--     then to_date(substr(REGEXP_substr(mjf.transmit_date, &apos;[[:digit:]]+&apos;),-8,8),&apos;YYYYMMDD&apos;) end transmit_dt,
mjf.file_name,
mjf.file_type,
mjf.file_source,
mjf.file_destination,
mjf.response_file_req,
mjf.RESPONSE_FILE_NAME,
--mjf.RECORD_COUNT,
--mjf.error_count,
mjf.instance_status,
mjf.last_update_by_name,
mjf.last_update_by_dt,
mjf.assd_identify_job
from (select 
rd.job_run_data_id as job_id
,nvl(rd.create_ts,sysdate) as create_dt
--,coalesce((select first_name ||&apos; &apos; || last_name from staff where to_char(staff_id) =  rd.created_by) ,rd.created_by ) created_by 
,coalesce(trim(s.first_name ||&apos; &apos;|| s.last_name),rd.created_by) created_by 
--,CASE WHEN S.STAFF_ID &lt;0 THEN &apos;MAXeb&apos; 
--ELSE S.FIRST_NAME||&apos; &apos;||S.LAST_NAME 
--END as created_by 
,rd.job_name
--,rd.comments 
,COALESCE(upper(re.filename),upper(rd.filename)) as job_detail
,rd.job_group
--,jd.JOB_DATA as job_type
,CASE WHEN dbms_lob.INSTR(jd.JOB_DATA, utl_raw.cast_to_raw(&apos;inputFileParamName&apos;),1,1) &gt; 0
       THEN &apos;Inbound File&apos;
      WHEN dbms_lob.INSTR(jd.JOB_DATA, utl_raw.cast_to_raw(&apos;outputFileParamName&apos;),1,1) &gt; 0
       THEN &apos;Outbound File&apos;
      ELSE &apos;Other Job Type&apos; END job_type 
,rd.start_ts as job_start_dt
--,rd.end_ts as job_end_dt
--,rd.status_cd as job_status
,js.report_label as job_status
, rd.filename receipt_date
,CASE WHEN instr(rd.filename,&apos;\&apos;) &gt; 0 
       THEN substr(rd.filename, instr(rd.filename,&apos;\&apos;,-1)+1) 
       ELSE substr(rd.filename, instr(rd.filename,&apos;&#47;&apos;,-1)+1) END transmit_date
--,re.filename
--, CASE WHEN instr(re.filename,&apos;\&apos;) &gt; 0 
--       THEN substr(re.filename, instr(re.filename,&apos;\&apos;,-1)+1) 
--       ELSE substr(re.filename, instr(re.filename,&apos;&#47;&apos;,-1)+1) END interface_file_name 
--, transmit_dt
,null as file_name
, null as file_type
, null as file_source
, null as file_destination
, null as response_file_req
, null as RESPONSE_FILE_NAME
,RE.RECORD_COUNT
,re.error_count
,&apos;Active&apos; instance_status
,coalesce(trim(s.first_name ||&apos; &apos;|| s.last_name),rd.created_by) last_update_by_name
--,rd.update_ts as last_update_by_dt
,nvl(rd.create_ts,sysdate) as last_update_by_dt
,nvl(rd.create_ts,sysdate) as assd_identify_job
--,SYSDATE STAGE_CREATE_TS
from JOB_RUN_DATA rd,
ETL_REPORT re
,JOB_JOB_DETAILS jd
,staff s
,enum_job_status js 
where 
rd.job_run_data_id = re.job_id(+)
and REGEXP_substr(rd.job_name, &apos;[^0-9][^-]+&apos;) = jd.job_name(+)
and rd.created_by = s.staff_id(+)
and RD.STATUS_CD = JS.VALUE(+)
--and rd.job_run_data_id = 17619
--and re.filename like &apos;%AIDP.PA2733EH.VNDR%&apos;
--and rownum &lt; =100
)mjf
where mjf.job_id &gt; ${LastJOB} 
--in (15940, 16345,15949, 16282, 16333, 15943, 15939, 16326)
order by 1</sql>
