  <step>
    <name>Database join -- Get the MI Letters</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>4</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <rowlimit>0</rowlimit>
    <sql>Select to_number(letter_id) as oltp_Letter_Req_ID,
       (case when letter_sent_on is not null then &apos;SENT&apos; end)  as oltp_Letter_Status,
       (case when letter_sent_on is not null then  letter_sent_on end) as oltp_Letter_Status_Date
From LETTERS_STG let_1
where 1=1
and let_1.letter_App_id = ?
and let_1.letter_id = (select min(letter_id) 
                         from LETTERS_STG let_2
                        where let_2.letter_App_id = ?
                          and let_2.letter_sent_on &gt;= ?
                          and let_2.Letter_request_type = &apos;L&apos; --Not a Manual Letter
                          and let_2.letter_type_cd like &apos;MI%&apos;
                          )
</sql>
    <outer_join>N</outer_join>
