  <step>
    <name>Database Join - CSI Call Response</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT etl_l_dialer_response_id
      ,application_id, phone_dialed, campaign_name, reported_call_outcome
      ,NULL call_retry_flag, NULL call_success_result
      ,&apos;N&apos; no,&apos;Y&apos; yes
      ,&apos;Invalid Number&apos; invalid_result
      ,&apos;Successful Outreach&apos; success_result
      ,&apos;Unsuccessful Outreach&apos; unsuccess_result
  FROM etl_l_dialer_response r
 WHERE r.application_id = ?  -- APP_ID
   AND ? BETWEEN r.create_ts - 1&#47;(24*60) * ${vSITP_GRACE_MINS} AND
       r.create_ts + 1&#47;(24*60) * ${vSITP_GRACE_MINS}  -- CREATE_TS and SITP_GRACE_MINS
   AND INSTR(?,campaign_name) &gt; 0  -- EVENT_TYPE_CD
</sql>
