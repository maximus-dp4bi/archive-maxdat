  <step>
    <name>Table input - Identify Applications To Be Updated</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT mi.missing_info_id
     , mi.app_id
     , mi.mi_item_level
     , mi.mi_item_type
     , mi.mi_item_create_dt
     , mi.created_by
     , mi.mi_item_requested_by
     , mi.mi_letter_required_status
     , mi.mi_letter_id
     , mi.rfe_status
     , mi.rfe_status_dt
     , mi.mi_item_status
     , mi.mi_item_status_dt
     , mi.mi_item_status_performer
     , mi.mi_item_satisfied_channel
     , mi.mi_letter_cycle_start_dt
     , mi.mi_letter_cycle_end_dt 
     , mi.undeliverable_ind
     , mi.undeliverable_ind_dt
     , mi.mi_validated_dt
     , mi.outbound_call_id
     , mi.in_process_ind
     , mi.mi_extract_complete_dt
     , ( SELECT MAX(trunc(si1.completed_ts))
           FROM step_instance_stg si1
          WHERE si1.ref_id = mi.app_id
            AND si1.ref_type = &apos;APP_HEADER&apos;
            AND si1.status = &apos;COMPLETED&apos;
            AND mi.mi_item_create_dt BETWEEN si1.create_ts AND si1.completed_ts
            AND si1.step_definition_id =
              ( SELECT ll.ref_id
                  FROM corp_etl_list_lkup ll
                 WHERE ll.name = &apos;TASK_MONITOR_TYPE&apos;
                   AND ll.out_var = &apos;State Data Entry&apos;
                   AND ll.value &lt;&gt; &apos;State Data Entry Task - Undeliverable&apos; )) sde_task_complete_dt
     , ( SELECT MAX(trunc(si2.create_ts))
           FROM step_instance_stg si2
          WHERE si2.ref_id = mi.app_id
            AND si2.ref_type = &apos;APP_HEADER&apos;
            AND si2.status &lt;&gt; &apos;TERMINATED&apos;
            AND mi.mi_item_create_dt BETWEEN si2.create_ts AND si2.completed_ts
            AND si2.step_definition_id =
              ( SELECT ll2.ref_id
                  FROM corp_etl_list_lkup ll2
                 WHERE ll2.name = &apos;TASK_MONITOR_TYPE&apos;
                   AND ll2.value = &apos;State Review Task - MI Reprocess Result&apos; )) reprocess_task_create_dt
    , CASE 
         WHEN ( SELECT COUNT(1)
                  FROM step_instance_stg s1 
                 WHERE s1.ref_type = &apos;APP_HEADER&apos; 
                   AND s1.ref_id = mi.app_id 
                   AND s1.Status &lt;&gt; &apos;TERMINATED&apos;
                   AND mi.mi_item_create_dt BETWEEN s1.create_ts AND s1.completed_ts
                   AND s1.step_definition_id IN 
                     ( SELECT ll1.ref_id 
                         FROM corp_etl_list_lkup ll1
                        WHERE ll1.name = &apos;TASK_MONITOR_TYPE&apos;  
                          AND ll1.out_var = &apos;State Data Entry&apos;
                          AND ll1.value &lt;&gt; &apos;State Data Entry Task - Undeliverable&apos; )) &gt; 0
         THEN &apos;SDE&apos;
         WHEN app.qc_task_id IS NOT NULL
          AND mi.mi_item_create_dt BETWEEN qc.create_ts AND qc.completed_ts
         THEN &apos;QC&apos;
         WHEN ( SELECT COUNT(1)
                  FROM step_instance_stg s2 
                 WHERE s2.ref_type = &apos;APP_HEADER&apos; 
                   AND s2.ref_id = mi.app_id 
                   AND s2.Status != &apos;TERMINATED&apos;
                   AND mi.mi_item_create_dt BETWEEN s2.create_ts AND s2.completed_ts
                   AND s2.Step_definition_id IN 
                     ( SELECT ll2.ref_id 
                         FROM corp_etl_list_lkup ll2
                        WHERE ll2.name = &apos;TASK_MONITOR_TYPE&apos;  
                          AND ll2.value = &apos;Application Problem Resolution&apos; )) &gt; 0
         THEN &apos;Research&apos;
         WHEN ( SELECT COUNT(1)
                  FROM step_instance_stg s3 
                 WHERE s3.ref_type = &apos;APP_HEADER&apos; 
                   AND s3.ref_id = mi.app_id 
                   AND s3.Status != &apos;TERMINATED&apos;
                   AND mi.mi_item_create_dt BETWEEN s3.create_ts AND s3.completed_ts
                   AND s3.Step_definition_id IN 
                     ( SELECT ll3.ref_id 
                         FROM corp_etl_list_lkup ll3
                        WHERE ll3.name = &apos;TASK_MONITOR_TYPE&apos;  
                          AND ll3.value = &apos;Data Correction&apos; )) &gt; 0
         THEN &apos;Data Correction&apos;
         WHEN app.state_review_task_id IS NOT NULL
          AND mi.mi_item_create_dt BETWEEN sde.create_ts AND sde.completed_ts
         THEN &apos;State Acceptance&apos;
         WHEN st.first_name = &apos;HeartDataProcessorJob&apos;
         THEN &apos;HEART - No Task&apos;
         WHEN st.staff_type_cd IN (&apos;PROJECT_STAFF&apos;,&apos;STATE&apos;)
         THEN &apos;Manual&apos;
         ELSE &apos;Other&apos;
       END mi_item_create_task_type
      , &apos;Mail&#47;Fax&apos; mail_constant
      , &apos;Reprocess Report&apos; reprocess_rpt_constant
      , &apos;Phone&apos; phone_constant
   FROM process_app_mi_stg mi
     , process_app_stg app
     , staff st
     , step_instance_stg qc
     , step_instance_stg sde
 WHERE mi.app_id = app.app_id
   AND mi.created_by = st.staff_id(+)
   AND app.qc_task_id = qc.step_instance_id(+)
   AND app.state_review_task_id = sde.step_instance_id(+)
   AND app.app_in_process = &apos;Y&apos;
   and app.app_extract_complete_dt is null
   and app.app_priority_ind = 1</sql>
