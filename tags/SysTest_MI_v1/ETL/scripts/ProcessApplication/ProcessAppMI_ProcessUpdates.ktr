  <step>
    <name>Database join - Get MI Status</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OLTP_SOURCE</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT CASE mi.satisfied_reason_cd
         WHEN NULL THEN NULL
         WHEN &apos;void&apos; THEN &apos;Removed&apos;
         ELSE &apos;Satisfied&apos;
       END new_mi_status
     , CASE WHEN mi.app_individual_id IS NULL
         THEN NULL
         ELSE ( SELECT at.rfe_status_cd
                  FROM app_tracker at
                 WHERE at.application_id = mi.application_id
                   AND at.app_individual_id = mi.app_individual_id
                   AND at.history_ind = 0 )
       END new_rfe_status
     , SYSDATE new_rfe_status_dt
     , SYSDATE new_item_status_dt
     , ( SELECT COUNT(1)
           FROM app_tracker at1
          WHERE at1.application_id = mi.application_id
            AND at1.app_individual_id = mi.app_individual_id
            AND at1.history_ind = 0
            AND at1.rfe_status_cd &lt;&gt; &apos;ACCEPTED&apos; ) new_rfe_not_approved_ct
     , CASE WHEN mi.satisfied_reason_cd IS NULL
         THEN NULL
         ELSE ( SELECT st.first_name||&apos; &apos;||st.last_name
                  FROM staff st
                 WHERE st.staff_id = mi.updated_by )
       END new_performer
  FROM app_missing_info mi
 WHERE mi.missing_info_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>MISSING_INFO_ID</name>
        <type>Number</type>
      </field>
    </parameter>
