  <step>
    <name>Table input - get dates to calculate</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MOTS</connection>
    <sql>WITH tmp_metric AS
( SELECT mp.d_metric_project_id
	,mp.d_project_id
    ,md.name
	,pg.d_program_id
	,d_geography_master_id
	,rp.type 
	,rp.end_date
	,f.d_reporting_period_id
	,md.d_metric_definition_id
    ,mp.calculate_control_chart_ind
	,actual_value observation  
    ,mp.upper_specification_limit
    ,mp.lower_specification_limit
    ,tvar.min_num_obs
    ,tvar.max_num_obs
    ,tvar.num_allowed_gaps
    ,tvar.numstderrors
	,CASE WHEN rp.type = &apos;WEEKLY&apos; THEN
       CASE WHEN end_date - LAG(end_date) over (partition by mp.d_metric_project_id,rp.type order by end_date) &lt;= ((tvar.num_allowed_gaps+1) *7)
           THEN null ELSE TO_NUMBER(TO_CHAR(end_date,&apos;yyyymmdd&apos;)) END
     ELSE
       CASE WHEN MONTHS_BETWEEN(end_date, LAG(end_date) over (partition by mp.d_metric_project_id,rp.type order by end_date)) &lt;= (tvar.num_allowed_gaps+1)
           THEN null ELSE TO_NUMBER(TO_CHAR(end_date,&apos;yyyymmdd&apos;)) END
     END rn
  FROM f_metric f
  INNER JOIN d_metric_project mp ON f.d_metric_project_id = mp.d_metric_project_id
  INNER JOIN d_metric_definition md ON mp.d_metric_definition_id = md.d_metric_definition_id
  INNER JOIN d_project p ON mp.d_project_id = p.d_project_id
  INNER JOIN d_program pg ON mp.d_program_id = pg.d_program_id
  INNER JOIN d_reporting_period rp ON f.d_reporting_period_id = rp.d_reporting_period_id
  CROSS JOIN (SELECT (SELECT TO_NUMBER(value) FROM corp_etl_control WHERE name = &apos;NUM_ALLOWABLE_GAPS&apos;) num_allowed_gaps,              
                     (SELECT TO_NUMBER(value) FROM corp_etl_control WHERE name = &apos;MIN_NUM_OBSERVATIONS&apos;) min_num_obs,
                     (SELECT TO_NUMBER(value) FROM corp_etl_control WHERE name = &apos;MAX_NUM_OBSERVATIONS&apos;) max_num_obs,
                     (SELECT TO_NUMBER(value) FROM corp_etl_control WHERE name = &apos;NUM_STANDARD_ERRORS&apos;) numstderrors
              FROM DUAL) tvar    
  WHERE actual_value IS NOT NULL
 -- AND mp.d_metric_project_id in(74,75)
 )
SELECT 
  MAX(calc_start_date) calc_start_date
  ,MAX(calc_end_date) calc_end_date
  ,d_metric_project_id
  ,report_type
  ,metric_name
  ,numstderrors
  ,upper_specification_limit
  ,lower_specification_limit
  ,calculate_control_chart_ind
  ,TRUNC(SYSDATE) parameter_eff_dt
  ,TO_DATE(&apos;12&#47;31&#47;2199&apos;,&apos;mm&#47;dd&#47;yyyy&apos;) parameter_end_dt
FROM(
 SELECT CASE WHEN report_type = &apos;WEEKLY&apos; THEN MAX(max_rpt_end_date) -(LEAST(cnt_contiguous_rec,max_num_obs)*7) ELSE ADD_MONTHS(MAX(max_rpt_end_date),-(LEAST(cnt_contiguous_rec,max_num_obs))) END calc_start_date
       ,MAX(max_rpt_end_date) calc_end_date
       ,d_metric_project_id
       ,report_type
       ,metric_name
       ,numstderrors
       ,upper_specification_limit
       ,lower_specification_limit
       ,calculate_control_chart_ind
       ,cnt_contiguous_rec       
FROM       
  (SELECT MIN(end_date) min_rpt_end_date
        ,MAX(end_date) max_rpt_end_date
        ,COUNT(1) cnt_contiguous_rec       
        ,group_num
        ,d_metric_project_id
        ,type report_type
        ,name metric_name
        ,min_num_obs
        ,max_num_obs
        ,numstderrors
        ,upper_specification_limit
        ,lower_specification_limit
        ,calculate_control_chart_ind
  FROM(SELECT tmp_metric.*, MAX(rn) OVER (PARTITION BY d_metric_project_id, type ORDER BY end_date ) group_num
       FROM tmp_metric)
  GROUP BY group_num,d_metric_project_id, type,name,min_num_obs,max_num_obs,numstderrors,calculate_control_chart_ind,upper_specification_limit,lower_specification_limit
   )
 WHERE cnt_contiguous_rec &gt;= min_num_obs  
 AND calculate_control_chart_ind  = &apos;Y&apos;
 GROUP BY d_metric_project_id,report_type,metric_name,max_num_obs,numstderrors,calculate_control_chart_ind,upper_specification_limit,lower_specification_limit,cnt_contiguous_rec)
GROUP BY d_metric_project_id
  ,report_type
  ,metric_name
  ,numstderrors
  ,upper_specification_limit
  ,lower_specification_limit
  ,calculate_control_chart_ind
ORDER BY d_metric_project_id, report_type</sql>
