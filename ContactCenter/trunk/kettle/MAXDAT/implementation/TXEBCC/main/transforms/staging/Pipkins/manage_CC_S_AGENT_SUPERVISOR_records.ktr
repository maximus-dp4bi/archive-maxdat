    <replace_variables>N</replace_variables>
    <sql>--Update old agent-supervisor records

MERGE INTO CC_S_AGENT_SUPERVISOR AGSUP
USING 
(
SELECT mxeffdate, AG.AGENT_SUPERVISOR_ID, AG.AGENT_ID, AG.SUPERVISOR_AGENT_ID
FROM cc_s_agent_supervisor AG
JOIN 
(
	SELECT agent_id, max(record_eff_dt) mxeffdate
	FROM cc_s_agent_supervisor
	WHERE 
		record_end_dt &gt; sysdate
		group by agent_id
		having count(*) &gt; 1
) MX ON MX.agent_id = AG.agent_id
WHERE AG.record_eff_dt &lt;&gt; MX.mxeffdate AND AG.record_end_dt &gt; sysdate
) SUP ON (SUP.AGENT_SUPERVISOR_ID = AGSUP.AGENT_SUPERVISOR_ID)
WHEN MATCHED THEN UPDATE 
    SET AGSUP.RECORD_END_DT = SUP.MXEFFDATE;

COMMIT;

</sql>
