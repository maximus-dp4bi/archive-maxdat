    <sql>--Terminate record if Supervisors are terminated

MERGE INTO CC_S_AGENT_SUPERVISOR AGTERM
USING 
(
SELECT AGSUP.AGENT_ID, AGSUP.AGENT_SUPERVISOR_ID, AGSUP.SUPERVISOR_AGENT_ID, TERMINATION_DATE
FROM cc_s_agent_supervisor AGSUP
JOIN 
(
	SELECT agent_id, TERMINATION_DATE
	FROM cc_s_agent
	WHERE termination_date is not null
  ) TERM_AGENT ON TERM_AGENT.agent_id = AGSUP.SUPERVISOR_AGENT_ID
WHERE trunc(record_end_dt) = to_date(&apos;12&#47;31&#47;2999&apos;,&apos;mm&#47;dd&#47;yyyy&apos;)
)AG ON (AG.AGENT_SUPERVISOR_ID = AGTERM.AGENT_SUPERVISOR_ID)
WHEN MATCHED THEN UPDATE 
    SET AGTERM.RECORD_END_DT = AG.TERMINATION_DATE;

commit;</sql>
