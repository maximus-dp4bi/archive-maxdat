  <step>
    <name>Terminate Old Agent records</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <execute_each_row>N</execute_each_row>
    <single_statement>N</single_statement>
    <replace_variables>N</replace_variables>
    <sql>--Terminate record if Agents are terminated

MERGE INTO CC_S_AGENT_SUPERVISOR AGTERM
USING 
(
SELECT AGSUP.AGENT_ID, AGSUP.AGENT_SUPERVISOR_ID, TERMINATION_DATE
FROM cc_s_agent_supervisor AGSUP
JOIN 
(
	SELECT agent_id, TERMINATION_DATE
	FROM cc_s_agent
	WHERE termination_date is not null
  ) TERM_AGENT ON TERM_AGENT.agent_id = AGSUP.agent_id
WHERE trunc(record_end_dt) = to_date(&apos;12&#47;31&#47;2999&apos;,&apos;mm&#47;dd&#47;yyyy&apos;)
)AG ON (AG.AGENT_SUPERVISOR_ID = AGTERM.AGENT_SUPERVISOR_ID)
WHEN MATCHED THEN UPDATE 
    SET AGTERM.RECORD_END_DT = AG.TERMINATION_DATE;

commit;
</sql>
