    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47; this Java Script will parse each absence entry and create additional rows where an entry may span a number of days
&#47;&#47; 		(i.e. 3|8, this means we need to have three records that account for one day of absence with eight hours each)
&#47;&#47; to learn how to create rows via the Java Script step,
&#47;&#47; 		reference:  http:&#47;&#47;type-exit.org&#47;adventures-with-open-source-bi&#47;2010&#47;06&#47;generating-rows-using-javascript-in-pentaho-kettle&#47;


&#47;&#47; number of days since last successful run
var missingDays = numDays - 1;

&#47;&#47; the starting day for those days found missing
var missingDay = new Date(PREV_END_DATETIME.getTime());
missingDay = dateAdd(missingDay, &quot;d&quot;, 1);

&#47;&#47; for each missing day, create a record
while(missingDays &gt; 1)
{
	&#47;&#47; create a new row object that fits our output size
	var row = createRowCopy(getOutputRowMeta().size());

	&#47;&#47; find the index of the first field appended by this step
	var idx = getInputRowMeta().size();

	&#47;&#47; fill each field (notice how the index is increased)
	row[idx++] = missingDays;
	row[idx++] = missingDay;

	&#47;&#47; output the row
	putRow(row);

	missingDays--; &#47;&#47; move to the next day
	missingDay = dateAdd(missingDay, &quot;d&quot;, 1); &#47;&#47; a missing day
}</jsScript_script>
      </jsScript>      <jsScript>        <jsScript_type>-1</jsScript_type>
        <jsScript_name>dateAdd_Sample</jsScript_name>
        <jsScript_script>&#47;&#47; Add&apos;s the given number to  the given date Value.
&#47;&#47;
&#47;&#47; Usage:
&#47;&#47; dateAdd(var,var,var);
&#47;&#47; 1: Date - The Variable with the Date.
&#47;&#47; 2: String - The Type which represents the value
&#47;&#47; to add.
&#47;&#47; y - Years
&#47;&#47; m - Months
&#47;&#47; d - Days
&#47;&#47; w - Weeks
&#47;&#47; wd - Workingdays
&#47;&#47; hh - Hours
&#47;&#47; mi - Minutes
&#47;&#47; ss - Seconds
&#47;&#47; 3: Numeric - The Value to add.
&#47;&#47; 
&#47;&#47; 2006-11-15
&#47;&#47;
var d1 = new Date();
Alert(dateAdd(d1, &quot;y&quot;, 1));
Alert(dateAdd(d1, &quot;m&quot;, 4));
Alert(dateAdd(d1, &quot;d&quot;, 5));
