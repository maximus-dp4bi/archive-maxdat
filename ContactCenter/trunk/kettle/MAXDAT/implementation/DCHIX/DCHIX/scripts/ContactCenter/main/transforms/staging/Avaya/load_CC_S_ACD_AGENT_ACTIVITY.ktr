    <sql>select min (to_date(&apos;19700101&apos;, &apos;YYYYMMDD&apos;) + ( 1 &#47; 24 &#47; 60 &#47; 60) * login_utc ) min_login
, max((case logout_utc
    when 0
    then (row_date+1)
    else (to_date(&apos;19700101&apos;, &apos;YYYYMMDD&apos;) + ( 1 &#47; 24 &#47; 60 &#47; 60) * logout_utc )
    end)) max_logout
&#47;*, sum((case logout_utc
	when 0
	then ((((row_date + 1) - to_date(&apos;19700101&apos;, &apos;YYYYMMDD&apos;))*60*60*24) - login_utc)
	else (logout_utc - login_utc) 
  end)) login_seconds*&#47;
from haglog
where
row_date = ?
and trim(logid) = ?

</sql>
