    <connection>MAXDATAVAYA</connection>
    <sql>SELECT 
dagent.row_date
,TRIM(dagent.logid) logid
,sum(dagent.acdcalls) handle_calls_count
,sum((dagent.acdtime+dagent.holdtime+dagent.acwtime)) handle_time_seconds
,sum((dagent.auxoutcalls+dagent.auxincalls)) internal_calls_count
,sum((dagent.i_acdaux_outtime+dagent.i_acdauxintime)) internal_seconds
,sum(dagent.auxoutoffcalls) external_calls_count
,sum(dagent.auxoutofftime) external_seconds
,sum(dagent.holdtime) hold_seconds
,sum(dagent.ringtime) ring_seconds
,sum(dagent.acdtime) talk_seconds
,sum(dagent.acwtime) wrap_seconds
,sum(dagent.i_stafftime) login_seconds
,sum(dagent.i_availtime) idle_seconds
,sum(dagent.i_auxtime) not_ready_seconds
from
dagent
where row_date &gt;= TRUNC(to_date(&apos;${startDateTime}&apos;, &apos;yyyy-mm-dd hh24:mi:ss&apos;), &apos;dd&apos;)
and row_date &lt; to_date(&apos;${endDateTime}&apos;, &apos;yyyy-mm-dd hh24:mi:ss&apos;)
group by dagent.row_date
,TRIM(dagent.logid)
--where dagent.row_date &gt;= TRUNC(to_date(&apos;2018-09-25 10:00:00&apos;, &apos;yyyy-mm-dd hh24:mi:ss&apos;), &apos;dd&apos;)
