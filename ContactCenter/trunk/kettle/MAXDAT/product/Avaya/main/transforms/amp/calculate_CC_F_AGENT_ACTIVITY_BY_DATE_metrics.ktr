  <step>
    <name>CC_F_AGENT_ACTIVITY_BY_DATE</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>SELECT  dd.D_DATE, 
		aabd.D_AGENT_ID, 
		aabd.D_ACTIVITY_TYPE_ID, 
		aabd.D_PROJECT_ID, 
		aabd.D_PROGRAM_ID, 
		aabd.D_GEOGRAPHY_MASTER_ID, 
		aabd.ACTIVITY_MINUTES,
		aabd.F_AGENT_ACTIVITY_BY_DATE_ID
FROM CC_F_AGENT_ACTIVITY_BY_DATE_SV aabd
INNER JOIN CC_D_DATES dd on aabd.D_DATE_ID = dd.D_DATE_ID
inner join cc_d_group dg on aabd.d_group_id = dg.d_group_id
WHERE dd.D_DATE BETWEEN to_date(&apos;${startDate}&apos;, &apos;yyyy&#47;mm&#47;dd&apos;)
  AND to_date(&apos;${endDate}&apos;, &apos;yyyy&#47;mm&#47;dd&apos;)
   and dg.include_agent_payroll_flg = &apos;Y&apos;</sql>
