    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MAXDAT</connection>
    <sql>Select t1.Dt as work_date,
       t1.login_id as staff_id,
       t1.AGT, 
	   t1.Duration-t2.Duration as Duration,
     t1.local_task_end,
     t1.local_task_start
from
(select to_char(sch.start_datetime,&apos;DD-MON-YY&apos;) as Dt, agt.login_id, agt.last_name||&apos;, &apos;||agt.first_name as AGT, 
        max(sch.end_datetime) as local_task_end, min(sch.start_datetime) as local_task_start, 
         round((max(sch.end_datetime) - min(sch.start_datetime))*24*60,2) as Duration
  from
cc_s_tmp_nice_agent_schedules sch
join cc_s_agent agt on sch.agent_login = agt.agent_id
join cc_c_activity_type e on sch.activity_name = e.activity_type_name
group by to_char(sch.start_datetime,&apos;DD-MON-YY&apos;),agt.login_id, agt.last_name||&apos;, &apos;||agt.first_name) t1
left outer join (select to_char(sch.start_datetime,&apos;DD-MON-YY&apos;) as Dt, agt.last_name||&apos;, &apos;||agt.first_name as AGT, 
round(sum(case when (e.IS_PAID_FLAG = 1 and e.IS_ABSENCE_FLAG = 1) or (e.IS_PAID_FLAG = 0 and e.IS_ABSENCE_FLAG = 1) 
  or (e.IS_PAID_FLAG = 0 and e.IS_ABSENCE_FLAG = 0)  then 
  sch.end_datetime - sch.start_datetime else 0 end) *24*60,2) as Duration
  from
cc_s_tmp_nice_agent_schedules sch
join cc_s_agent agt on sch.agent_login = agt.agent_id
join cc_c_activity_type e on sch.activity_name = e.activity_type_name
group by to_char(sch.start_datetime,&apos;DD-MON-YY&apos;), agt.last_name||&apos;, &apos;||agt.first_name) t2
  on t1.Dt = t2.Dt and t1.AGT = t2.AGT
order by 1,2</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
