    <connection>MAXDAT</connection>
    <sql>select a.login_id as staff_id, 
           e.name, 
           to_char(min(START_DATETIME),&apos;DD-MON-YY&apos;) as work_date,
           start_datetime as local_task_start, 
           end_datetime as local_task_end,
           sum(duration_sec&#47;60) as duration
from  cc_s_tmp_nice_agent_actuals t
join cc_s_agent a on t.login_id = a.agent_id
join cc_c_activity_type e on t.activty_name = e.activity_type_name
inner join cc_s_tmp_nice_event e
    on t.ACTIVTY_ID = e.ACTIVITY_ID
inner join cc_c_activity_type at
  on e.name = at.activity_type_name
where (e.IS_PAID_FLAG = 1 and e.IS_ABSENCE_FLAG = 0) 
  group by a.login_id, e.name, start_datetime, end_datetime
order by a.login_id, start_datetime</sql>
