  <step>
    <name>Get Agent Schedules</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>WFM</connection>
    <sql>select a.c_id      as agent_login, 
       act.c_legid as activity_key, 
       c_agt       as agent_id,               ---- Pull agent schedule based activity 
       act.c_name  as activity_name, 
       c_stime     as start_datetime, 
       c_etime     as end_datetime,
       sum(extract(epoch from (asa.c_etime - asa.c_stime)))&#47;60 as Duration_Min
from R_AGTSCHEDACT asa 
left outer join  r_excp act on asa.c_excp = act.c_oid
join  r_agt a on asa.c_agt = a.c_oid
join r_person p on a.c_person = p.c_oid
join r_user u on p.c_oid = u.c_person
where c_stime &gt;= &apos;${startDateTime}&apos;
  and c_stime &lt;= &apos;${endDateTime}&apos;  
  group by a.c_id,  act.c_legid, c_agt, act.c_name, c_stime,c_etime 
union all
select a.c_id      as agent_login,
       act.c_legid as activity_key,
       asd.c_agt   as agent_id,
       act.c_name  as activity_name,           --- Pulls agent schedule non-base activity detail with activity code names
       asa.c_stime as start_datetime, 
       asa.c_etime as end_datetime,
       sum(extract(epoch from (asa.c_etime - asa.c_stime)))&#47;60 as Duration_Min
from R_AGTSCHEDDTLACT asa
left outer join r_excp act on asa.c_excp = act.c_oid
join R_AGTSCHEDACT asd on asa.c_agtsched = asd.c_oid
join r_agt a on asd.c_agt = a.c_oid 
join r_person p on a.c_person = p.c_oid
join r_user u on p.c_oid = u.c_person
where  asa.c_stime &gt;= &apos;${startDateTime}&apos;
  and  asa.c_stime &lt;=  &apos;${endDateTime}&apos;  
and c_agtsched in (select asa.c_oid as c_agtsched
