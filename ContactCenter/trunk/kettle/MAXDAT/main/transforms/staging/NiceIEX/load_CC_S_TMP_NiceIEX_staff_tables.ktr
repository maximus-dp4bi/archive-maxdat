  <step>
    <name>Get Agent Info</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>WFM</connection>
    <sql>select distinct p.c_lname as Last_name
,p.c_fname as First_name
,a.C_ID as login_id
,a.C_CTIME as Hire_date
,Case when ent.c_name = &apos;FHK_Termed&apos; then mr.c_sdate||&apos; 23:59:59&apos; else null end as Termination_date
,r.c_name as Job_title
,adv.c_alphaval as Agent_group_title
,adv.c_armid as Agent_group_id
,lang.c_alphaval as language
,p.c_suffix as SUFFIX
,a.c_mod as modify_date
,a.c_externalid as modify_user
,a.c_sendate as SENIORITY_EFFECTIVE_DATE
,a.c_dtime as DELETE_DATE
,al.c_agtlogon as NATIONAL_ID
,case when emplr.c_alphaval = &apos;MAXIMUS&apos; then &apos;PERM&apos; 
      when emplr.c_alphaval is not null then &apos;TEMP&apos;
      end as employee_type
,tier.c_alphaval as Tier
,aw.c_fulltime   as SCHEDULE_TYPE
from r_agt a 
join r_person p    on  a.c_person = p.c_oid
join r_user u      on  p.c_oid  = u.c_person
join r_userrole ur on  u.c_oid  = ur.c_user
join r_role r      on ur.c_role = r.c_oid
join r_agtacdlogon al on a.c_oid = al.c_agt
join r_agtworkrules aw on a.c_oid = aw.c_agt 
left outer join r_muroster mr on a.c_oid = mr.c_agt
left outer join r_mu mu on mr.c_mu = mu.c_oid
left outer join r_entity ent on ent.c_oid = mu.c_oid AND ent.c_act = &apos;A&apos;
left outer join (select ad.c_agt, adv1.c_oid, adv1.c_alphaval, adv1.c_armid
  from r_agtdataval adv1
  join r_agtdata ad on adv1.c_oid = ad.c_agtdataval
  join r_agtdatadef add1 on adv1.c_agtdatadef = add1.c_oid and add1.c_descr = &apos;Department&apos;
  where ad.c_edate is null) as adv on a.c_oid = adv.c_agt
left outer join (select ad1.c_agt, adv2.c_oid, adv2.c_alphaval 
  from r_agtdataval adv2
  join r_agtdata ad1 on adv2.c_oid = ad1.c_agtdataval
  join r_agtdatadef add2 on adv2.c_agtdatadef = add2.c_oid and add2.c_descr = &apos;Language&apos;
  where ad1.c_edate is null) as lang on a.c_oid = lang.c_agt
left outer join (select ad2.c_agt, adv3.c_oid, adv3.c_alphaval 
  from r_agtdataval adv3
  join r_agtdata ad2 on adv3.c_oid = ad2.c_agtdataval
  join r_agtdatadef add3 on adv3.c_agtdatadef = add3.c_oid and add3.c_descr = &apos;Employer&apos;
  where ad2.c_edate is null) as emplr on a.c_oid = emplr.c_agt
left outer join (select ad3.c_agt, adv4.c_oid, adv4.c_alphaval 
  from r_agtdataval adv4
  join r_agtdata ad3 on adv4.c_oid = ad3.c_agtdataval
  join r_agtdatadef add4 on adv4.c_agtdatadef = add4.c_oid and add4.c_descr = &apos;Tier&apos;
  where ad3.c_edate is null) as tier on a.c_oid = tier.c_agt
where a.c_act = &apos;A&apos; -- Active item
order by Agent_group_title, login_id</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
