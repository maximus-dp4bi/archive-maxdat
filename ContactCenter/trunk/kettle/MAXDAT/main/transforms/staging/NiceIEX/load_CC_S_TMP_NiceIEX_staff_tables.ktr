    <name>Get Agent Actuals</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>WFM</connection>
    <sql>select p.c_lname as Last_name
,p.c_fname as First_name
,a.C_ID as login_id
,agacd.c_agtlogon as acd_login_id
,a.C_CTIME as Hire_date
,a.C_DTIME as Termination_date
,exc.c_name as Activity_name
,act.c_stime as Start_DT
,act.c_etime as End_DT
,exc.c_legid as activity_id
from r_activitylogdtl act
join r_agtacdlogon agacd on act.c_agtacdlogon = agacd.c_oid
join r_agt a on agacd.c_agt = a.c_oid
join r_person p on a.c_person = p.c_oid
join r_user u on p.c_oid = u.c_person
left outer join r_excp exc on act.c_excp = exc.c_oid
where act.c_stime &lt;= &apos;${endDateTime}&apos;
  and act.c_stime &gt;= &apos;${startDateTime}&apos;
order by Start_DT, Last_name</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
