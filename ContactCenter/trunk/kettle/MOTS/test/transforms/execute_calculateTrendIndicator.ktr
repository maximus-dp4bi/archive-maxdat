  <step>
    <name>Get calcTrendIndicator mapping params</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MOTS</connection>
    <sql>SELECT DISTINCT mp.D_METRIC_PROJECT_ID, mp.D_PROJECT_ID, rp.D_REPORTING_PERIOD_ID, rp.TYPE as REPORT_TYPE
FROM F_METRIC fm
INNER JOIN D_METRIC_PROJECT mp on fm.D_METRIC_PROJECT_ID = mp.D_METRIC_PROJECT_ID
INNER JOIN D_REPORTING_PERIOD rp on fm.D_REPORTING_PERIOD_ID = rp.D_REPORTING_PERIOD_ID
WHERE mp.TREND_INDICATOR_CALCULATION = &apos;CONTROL CHART&apos;
AND F_METRIC_ID IN (
    SELECT MAX(F_METRIC_ID)
    FROM F_METRIC fm
    INNER JOIN D_METRIC_PROJECT mp on fm.D_METRIC_PROJECT_ID = mp.D_METRIC_PROJECT_ID
    INNER JOIN D_REPORTING_PERIOD rp on fm.D_REPORTING_PERIOD_ID = rp.D_REPORTING_PERIOD_ID
    GROUP BY mp.D_METRIC_PROJECT_ID, rp.TYPE)</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
