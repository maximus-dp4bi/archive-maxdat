  <step>
    <name>Calculate Actuals Trend by Control Chart Parameters</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>N</compatible>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Actuals Trend Values
&#47;&#47;---------------------
&#47;&#47;0 = Not Calculated
&#47;&#47;1 = Dramatic Increase 
&#47;&#47;2 = Increase 
&#47;&#47;3 = Same
&#47;&#47;4 = Decrease
&#47;&#47;5 = Dramatic Decrease

var calculatedActualsTrend = 0;

if ((ACTUAL_VALUE &gt; UPPER_CONTROL_LIMIT) ||
    ((ACTUAL_VALUE &gt; CENTER_LINE) &amp;&amp; (ACTUALLAG1 &gt; CENTER_LINE) &amp;&amp; (ACTUALLAG2 &gt; CENTER_LINE) 
	&amp;&amp; (ACTUALLAG3 &gt; CENTER_LINE) &amp;&amp; (ACTUALLAG4 &gt; CENTER_LINE) &amp;&amp; (ACTUALLAG5 &gt; CENTER_LINE)
	&amp;&amp; (ACTUALLAG6 &gt; CENTER_LINE) &amp;&amp; (ACTUALLAG7 &gt; CENTER_LINE)) || 
    (CURRENT_MR &gt; UCL_OF_SIGMA) )
  {
	calculatedActualsTrend = 1; 
   }

else if ((ACTUAL_VALUE &lt; LOWER_CONTROL_LIMIT) ||
    ((ACTUAL_VALUE &lt; CENTER_LINE) &amp;&amp; (ACTUALLAG1 &lt; CENTER_LINE) &amp;&amp; (ACTUALLAG2 &lt; CENTER_LINE) 
	&amp;&amp; (ACTUALLAG3 &lt; CENTER_LINE) &amp;&amp; (ACTUALLAG4 &lt; CENTER_LINE) &amp;&amp; (ACTUALLAG5 &lt; CENTER_LINE)
	&amp;&amp; (ACTUALLAG6 &lt; CENTER_LINE) &amp;&amp; (ACTUALLAG7 &lt; CENTER_LINE)) || 
    (CURRENT_MR &lt; LCL_OF_SIGMA) )
  {
	calculatedActualsTrend = 5;
   }

else if ( ((ACTUAL_VALUE &gt;= UPPER_WARNING_LIMIT &amp;&amp; ACTUAL_VALUE &lt;= UPPER_CONTROL_LIMIT) &amp;&amp;
		   (ACTUALLAG1   &gt;= UPPER_WARNING_LIMIT &amp;&amp; ACTUALLAG1 &lt;= UPPER_CONTROL_LIMIT)) ||
          ((ACTUAL_VALUE &gt;= UPPER_WARNING_LIMIT &amp;&amp; ACTUAL_VALUE &lt;= UPPER_CONTROL_LIMIT) &amp;&amp;
		   (ACTUALLAG2   &gt;= UPPER_WARNING_LIMIT &amp;&amp; ACTUALLAG2 &lt;= UPPER_CONTROL_LIMIT)) ||
          ((ACTUALLAG1 &gt;= UPPER_WARNING_LIMIT &amp;&amp; ACTUALLAG1 &lt;= UPPER_CONTROL_LIMIT) &amp;&amp;
		   (ACTUALLAG2 &gt;= UPPER_WARNING_LIMIT &amp;&amp; ACTUALLAG2 &lt;= UPPER_CONTROL_LIMIT)) ||
           (T45UP == &quot;Y&quot; )
         )
  {
	calculatedActualsTrend = 2;
   }


else if ( ((ACTUAL_VALUE &gt;= LOWER_CONTROL_LIMIT &amp;&amp; ACTUAL_VALUE &lt;= LOWER_WARNING_LIMIT) &amp;&amp;
		   (ACTUALLAG1   &gt;= LOWER_CONTROL_LIMIT &amp;&amp; ACTUALLAG1 &lt;= LOWER_WARNING_LIMIT)) ||
          ((ACTUAL_VALUE &gt;= LOWER_CONTROL_LIMIT &amp;&amp; ACTUAL_VALUE &lt;= LOWER_WARNING_LIMIT) &amp;&amp;
		   (ACTUALLAG2   &gt;= LOWER_CONTROL_LIMIT &amp;&amp; ACTUALLAG2 &lt;= LOWER_WARNING_LIMIT)) ||
          ((ACTUALLAG1 &gt;= LOWER_CONTROL_LIMIT &amp;&amp; ACTUALLAG1 &lt;= LOWER_WARNING_LIMIT) &amp;&amp;
		   (ACTUALLAG2 &gt;= LOWER_CONTROL_LIMIT &amp;&amp; ACTUALLAG2 &lt;= LOWER_WARNING_LIMIT)) ||
           (T45DOWN == &quot;Y&quot; )
         )
  {
	calculatedActualsTrend = 4;
   }

else{
	calculatedActualsTrend = 3;
  }


	</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>calculatedActualsTrend</name>
