    <connection>MOTS</connection>
    <rowlimit>0</rowlimit>
    <sql>WITH t as (
  SELECT 
   mp.D_METRIC_PROJECT_ID  d_metric_project_id_o  
   ,mp.trend_indicator_calculation
   ,rp.d_reporting_period_id d_reporting_period_id_o
   ,p.PROJECT_NAME
   ,md.name METRIC_NAME  
   ,rp.end_date
   ,f.actual_value
   ,rp.type report_type_o
   ,LAG(ACTUAL_VALUE, 1, NULL) OVER (PARTITION BY mp.D_METRIC_PROJECT_ID ORDER BY END_DATE) as actualLag1   
   ,LAG(ACTUAL_VALUE, 2, NULL) OVER (PARTITION BY mp.D_METRIC_PROJECT_ID ORDER BY END_DATE) as actualLag2  
   ,LAG(ACTUAL_VALUE, 3, NULL) OVER (PARTITION BY mp.D_METRIC_PROJECT_ID ORDER BY END_DATE) as actualLag3  
   ,LAG(ACTUAL_VALUE, 4, NULL) OVER (PARTITION BY mp.D_METRIC_PROJECT_ID ORDER BY END_DATE) as actualLag4  
   ,LAG(ACTUAL_VALUE, 5, NULL) OVER (PARTITION BY mp.D_METRIC_PROJECT_ID ORDER BY END_DATE) as actualLag5  
   ,LAG(ACTUAL_VALUE, 6, NULL) OVER (PARTITION BY mp.D_METRIC_PROJECT_ID ORDER BY END_DATE) as actualLag6  
   ,LAG(ACTUAL_VALUE, 7, NULL) OVER (PARTITION BY mp.D_METRIC_PROJECT_ID ORDER BY END_DATE) as actualLag7  
   ,LAG(ACTUAL_VALUE, 8, NULL) OVER (PARTITION BY mp.D_METRIC_PROJECT_ID ORDER BY END_DATE) as actualLag8  
  FROM D_METRIC_PROJECT mp 
   INNER JOIN D_PROJECT p on mp.D_PROJECT_ID = p.D_PROJECT_ID
   INNER JOIN D_METRIC_DEFINITION md on mp.D_METRIC_DEFINITION_ID = md.D_METRIC_DEFINITION_ID
   CROSS JOIN d_reporting_period rp 
   LEFT OUTER JOIN f_metric f
     ON f.d_reporting_period_id = rp.d_reporting_period_id
     AND f.D_METRIC_PROJECT_ID = mp.D_METRIC_PROJECT_ID
  WHERE rp.type = ?
  AND p.d_project_id = ?
) 
SELECT 
  PROJECT_NAME
  , METRIC_NAME
  , END_DATE
  , ACTUAL_VALUE  
  , actualLag1
  , actualLag2
  , actualLag3
  , actualLag4
  , actualLag5
  , actualLag6
  , actualLag7
  , ABS(actualLag1 - actual_value) current_mr
  , (ACTUAL_VALUE - actualLag1) as lagDiff1
  , (actualLag1 - actualLag2) as lagDiff2
  , (actualLag2 - actualLag3) as lagDiff3
  , (actualLag3 - actualLag4) as lagDiff4
  , (actualLag4 - actualLag5) as lagDiff5
  , p.upper_control_limit
  , p.center_line
  , p.lower_control_limit
  , p.upper_warning_limit
  , p.lower_warning_limit
  , p.ucl_of_sigma
  , p.lcl_of_sigma
  , p.mu  
  , p.d_control_chart_id
  , trend_indicator_calculation
  , trend_ind_calc1
  , trend_ind_calc2
  , trend_ind_calc3
  , DECODE(trend_indicator_calculation,trend_ind_calc1,1,trend_ind_calc2,2,3) trend_calculation
  , d_metric_project_id_o
  , report_type_o  
  , d_reporting_period_id_o
  , CASE WHEN actual_value &lt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tdown_current
  , CASE WHEN actualLag1 &lt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tdown1
  , CASE WHEN actualLag2 &lt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tdown2
  , CASE WHEN actualLag3 &lt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tdown3
  , CASE WHEN actualLag4 &lt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tdown4
  , CASE WHEN actualLag5 &lt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tdown5
  , CASE WHEN actualLag6 &lt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tdown6
  , CASE WHEN actualLag7 &lt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tdown7
  , CASE WHEN actual_value &gt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tup_current
  , CASE WHEN actualLag1 &gt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tup1
  , CASE WHEN actualLag2 &gt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tup2
  , CASE WHEN actualLag3 &gt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tup3
  , CASE WHEN actualLag4 &gt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tup4
  , CASE WHEN actualLag5 &gt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tup5
  , CASE WHEN actualLag6 &gt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tup6
  , CASE WHEN actualLag7 &gt; center_line THEN &apos;Y&apos; ELSE &apos;N&apos; END tup7
FROM t
 LEFT OUTER JOIN d_control_chart_parameters p
   ON d_metric_project_id_o = p.d_metric_project_id
   AND report_type_o = p.report_type
   AND SYSDATE BETWEEN p.parameter_eff_dt AND p.parameter_end_dt
 CROSS JOIN (SELECT (SELECT TRIM(value) FROM corp_etl_control WHERE name = &apos;TREND_INDICATOR_CALCULATION_1&apos;) trend_ind_calc1
                   ,(SELECT TRIM(value) FROM corp_etl_control WHERE name = &apos;TREND_INDICATOR_CALCULATION_2&apos;) trend_ind_calc2
				   ,(SELECT TRIM(value) FROM corp_etl_control WHERE name = &apos;TREND_INDICATOR_CALCULATION_3&apos;) trend_ind_calc3
             FROM dual) 
WHERE D_METRIC_PROJECT_ID_o = ?
AND D_REPORTING_PERIOD_ID_o = ?
--AND TREND_INDICATOR_CALCULATION != trend_ind_calc3
</sql>
