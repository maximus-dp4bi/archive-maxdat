  <step>
    <name>Calculate Actuals Trend by Delta</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>N</compatible>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
&#47;&#47;Actuals Trend Values
&#47;&#47;---------------------
&#47;&#47;0 = Not Calculated
&#47;&#47;1 = Dramatic Increase 
&#47;&#47;2 = Increase 
&#47;&#47;3 = Same
&#47;&#47;4 = Decrease
&#47;&#47;5 = Dramatic Decrease

var calculatedActualsTrend = 0;

if (TREND_CALCULATION == 3)
  {calculatedActualsTrend = 0}
else
  {
  if((LAGDIFF1 &gt; 0) &amp;&amp; (LAGDIFF2 &gt; 0) &amp;&amp; (LAGDIFF3 &gt; 0) &amp;&amp; (LAGDIFF4 &gt; 0) &amp;&amp; (LAGDIFF5 &gt; 0)){
	calculatedActualsTrend = 1;
  }else if ((LAGDIFF1 &gt; 0) &amp;&amp; (LAGDIFF2 &gt; 0) &amp;&amp; (LAGDIFF3 &gt; 0)){
	calculatedActualsTrend = 2;
  }else if ((LAGDIFF1 &lt; 0) &amp;&amp; (LAGDIFF2 &lt; 0) &amp;&amp; (LAGDIFF3 &lt; 0) &amp;&amp; (LAGDIFF4 &lt; 0) &amp;&amp; (LAGDIFF5 &lt; 0)){
	calculatedActualsTrend = 5;
  }else if ((LAGDIFF1 &lt; 0) &amp;&amp; (LAGDIFF2 &lt; 0) &amp;&amp; (LAGDIFF3 &lt; 0)){
	calculatedActualsTrend = 4;
  }else{
	calculatedActualsTrend = 3;
  }   
 }
</jsScript_script>
