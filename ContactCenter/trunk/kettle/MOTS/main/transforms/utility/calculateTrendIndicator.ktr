    <connection>MOTS</connection>
    <rowlimit>1</rowlimit>
    <sql>SELECT CASE WHEN cnt_yn &gt;= 6 THEN &apos;Y&apos; ELSE &apos;N&apos; END t45down
FROM(
  SELECT value,COUNT (1) cnt_yn
  FROM (SELECT ? tdown_current,
               ? tdown1, 
               ? tdown2,
               ? tdown3, 
               ? tdown4,
               ? tdown5,
               ? tdown6,
               ? tdown7
          from dual)
  UNPIVOT (value for node in (tdown_current,
                              tdown1,
                              tdown2,
                              tdown3,
                              tdown4,
                              tdown5,
                              tdown6,
                              tdown7))
  GROUP BY value)
WHERE value = &apos;Y&apos;
</sql>
