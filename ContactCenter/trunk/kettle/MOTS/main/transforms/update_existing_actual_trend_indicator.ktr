    <connection>MOTS</connection>
    <sql>SELECT tmp.*
	   ,CASE WHEN reporting_period_type = &apos;Monthly&apos; THEN ADD_MONTHS(recalculation_end_date, -5)
        ELSE recalculation_end_date - (5*7) END recalculation_start_date  --start date to recalculate trend
FROM(
SELECT s_metric_template_id
	  ,reporting_period
	  ,project_name
	  ,program_name
	  ,geography_name
	  ,metric
	  ,reporting_period_type
      ,CASE WHEN reporting_period_type = &apos;Monthly&apos; THEN
         CASE WHEN ADD_MONTHS(reporting_period,5) &gt; TRUNC(SYSDATE,&apos;MM&apos;) THEN TRUNC(SYSDATE,&apos;MM&apos;) ELSE ADD_MONTHS(reporting_period,5) END 
       ELSE
         CASE WHEN reporting_period + (5*7) &gt; SYSDATE THEN TRUNC(NEXT_DAY(SYSDATE,&apos;Saturday&apos;) - 7) --5th week is in the future then just get the last rp
           ELSE reporting_period + (5*7) END     
       END  recalculation_end_date      
FROM s_metric_template
WHERE TRUNC(create_date) = TRUNC(SYSDATE)
AND is_processed = &apos;Y&apos;
AND is_actual = &apos;Y&apos;
AND is_actuals_trend_processed = &apos;Y&apos;
AND IS_ERROR &lt;&gt; &apos;Y&apos;
) tmp</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
