    <sql>SELECT f.f_metric_id,f.d_reporting_period_id, f.d_metric_project_id, rp.type, p.d_project_id
FROM f_metric f
 INNER JOIN d_reporting_period rp
   ON f.d_reporting_period_id = rp.d_reporting_period_id
 INNER JOIN d_metric_project dp
   ON f.d_metric_project_id = dp.d_metric_project_id
 INNER JOIN d_metric_definition md
   ON dp.d_metric_definition_id = md.d_metric_definition_id
 INNER JOIN d_project p
   ON dp.d_project_id = p.d_project_id
