    <connection>MOTS</connection>
    <sql>SELECT  dd.D_DATE, 
		aabd.D_AGENT_ID, 
		aabd.D_ACTIVITY_TYPE_ID, 
		aabd.D_PROJECT_ID, 
		aabd.D_PROGRAM_ID, 
		aabd.D_GEOGRAPHY_MASTER_ID, 
		aabd.ACTIVITY_MINUTES,
		aabd.F_AGENT_ACTIVITY_BY_DATE_ID
FROM CC_F_AGENT_ACTIVITY_BY_DATE aabd
INNER JOIN CC_D_DATES dd on aabd.D_DATE_ID = dd.D_DATE_ID
INNER JOIN CC_D_ACTIVITY_TYPE at on aabd.D_ACTIVITY_TYPE_ID = at.D_ACTIVITY_TYPE_ID
WHERE dd.D_DATE BETWEEN to_date(&apos;${reportingPeriodStartDate}&apos;, &apos;yyyy&#47;MM&#47;dd HH24:mi:ss&apos;)
  AND to_date(&apos;${reportingPeriodEndDate}&apos;, &apos;yyyy&#47;MM&#47;dd HH24:mi:ss&apos;)
  AND at.ACTIVITY_TYPE_CATEGORY like &apos;%Training%&apos;</sql>
    <limit/>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
