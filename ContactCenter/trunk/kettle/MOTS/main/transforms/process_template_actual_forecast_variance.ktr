    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Forecast Variance Format Rules
&#47;&#47;------------------------------
&#47;&#47;0 - Not Calculated
&#47;&#47;3 - Green [(-5%) to 5% ]
&#47;&#47;2 - Yellow [(-25%) to (-5%) and 5% to 25% ]
&#47;&#47;1 - Red [Less (-25%) and Over 25% ]

var calculatedForecastVariance = 0;
var absPctVar = abs(1 - (FORECAST_VALUE &#47; ACTUAL_VALUE));
var apv = absPctVar.toString()

&#47;&#47; check for divide by 0 error
if (isNaN(absPctVar)) {
 calculatedForecastVariance = 0;
}
&#47;&#47;  check for green
else if (absPctVar &lt;= 0.05){
 calculatedForecastVariance = 3;
}
&#47;&#47;  check for yellow
else if(absPctVar &lt;= 0.25){
 calculatedForecastVariance = 2;
}
&#47;&#47;  otherwise, red
else{
 calculatedForecastVariance = 1;
}</jsScript_script>
