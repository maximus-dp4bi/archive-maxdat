INNER JOIN CC_D_AGENT da ON fabd.D_AGENT_ID = da.D_AGENT_ID
INNER JOIN CC_D_PROJECT_TARGETS dpt ON fabd.D_PROJECT_TARGETS_ID = dpt.D_PROJECT_TARGETS_ID
