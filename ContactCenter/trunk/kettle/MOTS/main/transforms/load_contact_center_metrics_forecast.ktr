    <connection>MOTS</connection>
    <sql>select sum(fi.CONTACTS_CREATED) as CONTACTS_CREATED_SUM
	, sum(fi.CONTACTS_OFFERED) as CONTACTS_OFFERED_SUM
	, sum(fi.CONTACTS_ABANDONED) as CONTACTS_ABANDONED_SUM
	, sum(fi.CONTACTS_HANDLED) as CONTACTS_HANDLED_SUM
	, sum(fi.LABOR_MINUTES_TOTAL) as LABOR_MINUTES_TOTAL_SUM
	, sum(fi.MEAN_HANDLE_TIME) as MEAN_HANDLE_TIME_SUM
	, pp.project_id
	, pp.program_id
	, pp.geography_master_id
from cc_f_forecast_interval fi
inner join cc_d_production_plan_horizon pph
	on fi.d_production_plan_horizon_id = pph.production_plan_hrzn_id
inner join cc_d_production_plan pp
	on pph.production_plan_id = pp.production_plan_id
inner join cc_d_dates dd
	on fi.d_date_id = dd.d_date_id
where dd.d_date between to_date(&apos;${startDate}&apos;, &apos;yyyy&#47;mm&#47;dd&apos;) and to_date(&apos;${endDate}&apos;, &apos;yyyy&#47;mm&#47;dd&apos;)
group by pp.project_id, pp.program_id, pp.geography_master_id</sql>
