  <step>
    <name>S_PROJECT_REPORT</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>MOTS</connection>
    <sql>SELECT tmp.*
	   , CASE 
			WHEN UPPER(reporting_period_type) = &apos;MONTHLY&apos; THEN ADD_MONTHS(recalculation_end_date, -5)
	        ELSE recalculation_end_date - (5*7) 
		END recalculation_start_date  --start date to recalculate trend
FROM (
	SELECT 
		S_PROJECT_REPORT_ID
		, D_PROJECT_ID
		, D_PROGRAM_ID
		, D_GEOGRAPHY_MASTER_ID
		, D_METRIC_DEFINITION_ID
		, PR.D_REPORTING_PERIOD_ID
		, RP.TYPE AS REPORTING_PERIOD_TYPE
        , CASE WHEN UPPER(RP.TYPE) = &apos;MONTHLY&apos; THEN
         	CASE WHEN ADD_MONTHS(RP.END_DATE,5) &gt; TRUNC(SYSDATE,&apos;MM&apos;) THEN TRUNC(SYSDATE,&apos;MM&apos;) 
			ELSE ADD_MONTHS(RP.END_DATE,5) END 
		  ELSE
          	CASE WHEN RP.END_DATE + (5*7) &gt; SYSDATE THEN TRUNC(NEXT_DAY(SYSDATE,&apos;Saturday&apos;) - 7) --5th week is in the future then just get the last rp
           	ELSE RP.END_DATE + (5*7) END     
       	  END  recalculation_end_date      
FROM 
	S_PROJECT_REPORT PR
	INNER JOIN S_METRIC M ON PR.S_PROJECT_REPORT_ID = M.S_ACTUALS_PROJECT_REPORT_ID
	INNER JOIN D_REPORTING_PERIOD RP ON PR.D_REPORTING_PERIOD_ID = RP.D_REPORTING_PERIOD_ID
WHERE STATUS = &apos;Approved&apos;
	AND is_future_trends_processed &lt;&gt; &apos;Y&apos;
	AND is_actuals_trend_processed = &apos;Y&apos;
	AND IS_ERROR &lt;&gt; &apos;Y&apos;
) tmp</sql>
