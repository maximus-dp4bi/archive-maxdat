  <step>
    <name>Table input</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>bp new</connection>
    <sql>select
distinct(EXTERNALEMPIDENT) as agent_id, --number
EMPLOYEEAM.EMPLOYEENUMBER EMPLOYEE_NUMBER, -- varchar2(20)
PERSON.LASTNAME  EMPLOYEE_LASTNAME, --varchar2(50)
PERSON.FIRSTNAME EMPLOYEE_FIRSTNAME, --varchar2(50)
PERSON.MIDDLEINITIAL EMPLOYEE_MI, --varchar2(1)
managerPERSON.LASTNAME  SUPERVISOR_LASTNAME, --varchar2(50)
managerPERSON.FIRSTNAME SUPERVISOR_FIRSTNAME,--varchar2(50)
managerPERSON.MIDDLEINITIAL SUPERVISOR_MI,--varchar2(1)
dateadd(minute, tr_startTZ.BIAS, ACTUALEVENTTIMELINE.TIMERECORDSTARTTIME) TIMEREC_STARTTIME_MIN,  --number
dateadd(minute, tr_endTZ.BIAS, ACTUALEVENTTIMELINE.TIMERECORDENDTIME) TIMEREC_ENDTIME_MIN, --number
datediff(second, ACTUALEVENTTIMELINE.TIMERECORDSTARTTIME, ACTUALEVENTTIMELINE.TIMERECORDENDTIME) DURATION_WORKTIME_MIN, --number
ACTUALEVENTTIMELINE.ISPAID ISPAID, --number
dateadd(minute, startTZ.BIAS, ACTUALEVENTTIMELINE.STARTTIME) TIMEENTRYEVENT_STARTTIME_MIN, --number
dateadd(minute, endTZ.BIAS, ACTUALEVENTTIMELINE.ENDTIME) TIMEENTRYEVENT_ENDTIME_MIN, --number
datediff(second, ACTUALEVENTTIMELINE.STARTTIME, ACTUALEVENTTIMELINE.ENDTIME) DURATION_MIN, --number
ORGANIZATION.ID ORG_ID, -- number
ORGANIZATION.NAME ORG_NAME,--varchar2(50)
ORGANIZATIONDAY.LOCALDAY,--date
--convert(varchar(10), ORGANIZATIONDAY.LOCALDAY ,112) LOCALDAY,
ACTIVITY.NAME ACTIVITY_NAME -- varchar2(50)
,getdate() as LOAD_DATE
from	ORGANIZATIONDAY
join	WORKRESOURCEORGANIZATION
on	ORGANIZATIONDAY.ORGANIZATIONID = WORKRESOURCEORGANIZATION.ORGANIZATIONID
and  WORKRESOURCEORGANIZATION.STARTTIME &lt;= ORGANIZATIONDAY.STARTTIME
and  (ORGANIZATIONDAY.STARTTIME &lt; WORKRESOURCEORGANIZATION.ENDTIME or WORKRESOURCEORGANIZATION.ENDTIME is null)
join	EMPLOYEEFILTERWORKRESOURCE
on	EMPLOYEEFILTERWORKRESOURCE.WORKRESOURCEID = WORKRESOURCEORGANIZATION.WORKRESOURCEID
join 	ACTUALEVENTTIMELINE
on	WORKRESOURCEORGANIZATION.WORKRESOURCEID = ACTUALEVENTTIMELINE.EMPLOYEEID
and ACTUALEVENTTIMELINE.TIMERECORDSTARTTIME &gt;= ORGANIZATIONDAY.STARTTIME
and ACTUALEVENTTIMELINE.TIMERECORDSTARTTIME &lt; ORGANIZATIONDAY.ENDTIME
join	ACTIVITY
on	ACTUALEVENTTIMELINE.ACTIVITYID = ACTIVITY.ID
join	EMPLOYEEAM
on	ACTUALEVENTTIMELINE.EMPLOYEEID = EMPLOYEEAM.ID
join 	PERSON
on	PERSON.ID = EMPLOYEEAM.PERSONID
join dbo.EMPLOYEEDATASOURCE
on EMPLOYEEAM.id=EMPLOYEEDATASOURCE.EMPLOYEEID
left join
ORGANIZATION
on	WORKRESOURCEORGANIZATION.ORGANIZATIONID = ORGANIZATION.ID
left join
SUPERVISOR
on	SUPERVISOR.EMPLOYEEID = EMPLOYEEAM.ID and
(ACTUALEVENTTIMELINE.STARTTIME &lt; SUPERVISOR.ENDTIME or SUPERVISOR.ENDTIME is null) and
ACTUALEVENTTIMELINE.ENDTIME &gt; SUPERVISOR.STARTTIME
left join
EMPLOYEEAM as manager
on	manager.ID = SUPERVISOR.SUPERVISOREMPLOYEEID
left join
PERSON as managerPerson
on	managerPerson.ID = manager.PERSONID
left join
TIMEZONEAM as startTZ
on
(startTZ.STARTTIME &lt;= ACTUALEVENTTIMELINE.STARTTIME or startTZ.STARTTIME is null)
and (ACTUALEVENTTIMELINE.STARTTIME &lt; startTZ.ENDTIME or startTZ.ENDTIME is null)
and ORGANIZATION.TIMEZONE = startTZ.TIMEZONE
left join
TIMEZONEAM as endTZ
on
(endTZ.STARTTIME &lt;= ACTUALEVENTTIMELINE.ENDTIME or endTZ.STARTTIME is null)
and (ACTUALEVENTTIMELINE.ENDTIME &lt; endTZ.ENDTIME or endTZ.ENDTIME is null)
and ORGANIZATION.TIMEZONE = endTZ.TIMEZONE
left join
TIMEZONEAM as tr_startTZ
on
(tr_startTZ.STARTTIME &lt;= ACTUALEVENTTIMELINE.TIMERECORDSTARTTIME or tr_startTZ.STARTTIME is null)
and (ACTUALEVENTTIMELINE.TIMERECORDSTARTTIME &lt; tr_startTZ.ENDTIME or tr_startTZ.ENDTIME is null)
and ORGANIZATION.TIMEZONE = tr_startTZ.TIMEZONE
left join
TIMEZONEAM as tr_endTZ
on
(tr_endTZ.STARTTIME &lt;= ACTUALEVENTTIMELINE.TIMERECORDENDTIME or tr_endTZ.STARTTIME is null)
and (ACTUALEVENTTIMELINE.TIMERECORDENDTIME &lt; tr_endTZ.ENDTIME or tr_endTZ.ENDTIME is null)
and ORGANIZATION.TIMEZONE = tr_endTZ.TIMEZONE
where
	--ORGANIZATIONDAY.LOCALDAY &gt;= @fromDate and ORGANIZATIONDAY.LOCALDAY &lt; @toDate
convert(varchar(10), ORGANIZATIONDAY.LOCALDAY ,112) = convert(varchar(10),getdate()-1,112)
--convert(varchar(10), ORGANIZATIONDAY.LOCALDAY ,112) &gt;= convert(varchar(10),getdate()-21,112)
--and convert(varchar(10), ORGANIZATIONDAY.LOCALDAY ,112) &lt;= convert(varchar(10),getdate()-1,112)
and ACTIVITY.ID != -4001
and EXTERNALEMPIDENT is not null
and ORGANIZATION.NAME in (&apos;CHIP Austin EEU&apos;,&apos;CHIP QC&apos;,&apos;CHIP SEU English&apos;,&apos;CHIP SEU Spanish&apos;,
&apos;CHIP.ATHENS ENG&apos;,&apos;CHIP.ATHENS SPN&apos;,&apos;CHIP.Austin ENG&apos;,&apos;CHIP.Austin SPN&apos;,&apos;EB ENG Austin&apos;,
&apos;EB SPN Austin&apos;,&apos;EB VM&apos;,&apos;ES English Athens&apos;,&apos;ES English Edinburg&apos;,&apos;ES English Midland&apos;,
&apos;ES QC Athens&apos;,&apos;ES QC Edinburg&apos;,&apos;ES QC Midland&apos;,&apos;ES SEU English Athens&apos;,&apos;ES SEU English Edinburg&apos;,
&apos;ES SEU English Midland&apos;,&apos;ES SEU Spanish Athens&apos;,&apos;ES SEU Spanish Edinburg&apos;,&apos;ES SEU Spanish Midland&apos;,
&apos;ES Spanish Athens&apos;,&apos;ES Spanish Edinburg&apos;,&apos;ES Spanish Midland&apos;,&apos;THS ENG Austin&apos;,&apos;THS SPN Austin&apos;,
&apos;THS VM&apos;)
ORDER BY ORGANIZATIONDAY.LOCALDAY
</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
