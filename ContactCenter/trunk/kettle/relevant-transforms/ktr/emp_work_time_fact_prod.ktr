    <connection>esrepo_prod</connection>
    <sql>SELECT 
    ACTIVITY_DATE_ID  AS DIM_DATE_ID,
    AGENT_ID,
	SUM(DECODE(ispaid,&apos;Y&apos;,duration_min,0)) login_time,
	SUM(DECODE(ACTIVITY_NAME,&apos;Technical Problems (Cisco Code 31)&apos;, DURATION_MIN,0)) 
	+ 	SUM(DECODE(ACTIVITY_NAME,&apos;Technical Problems (Cisco Code 11)&apos;, DURATION_MIN,0))
	AS TECHNICAL_DIFFICULTY_TIME, 
	SUM(DECODE(ACTIVITY_NAME,&apos;Ready&apos;, DURATION_MIN,0)) AS READY,
	SUM(DECODE(ACTIVITY_NAME,&apos;Break (Cisco Code 1)&apos;, DURATION_MIN,0)) AS BREAK,
	SUM(DECODE(ACTIVITY_NAME,&apos;Lunch (Cisco Code 3)&apos;, DURATION_MIN,0)) AS LUNCH,
	SUM(DECODE(ACTIVITY_NAME,&apos;Training (Cisco Code 8)&apos;, DURATION_MIN,0)) AS TRAINING,
	SUM(DECODE(ACTIVITY_NAME,&apos;Meeting (Cisco Code 4)&apos;, DURATION_MIN,0)) AS MEETING,
	SUM(DECODE(ACTIVITY_NAME,&apos;Supervisor Meeting (Cisco Code 2)&apos;, DURATION_MIN,0)) AS SUPERVISOR_MEETING_2,
	SUM(DECODE(ACTIVITY_NAME,&apos;Personal - Restroom (Cisco Code 30)&apos;, DURATION_MIN,0)) AS PERSONAL_RESTROOM,
	SUM(DECODE(ACTIVITY_NAME,&apos;Special Projects (Cisco Code 6)&apos;, DURATION_MIN,0)) AS SPECIAL_PROJECTS,
	SUM(DECODE(ACTIVITY_NAME,&apos;Technical Problems (Cisco Code 31)&apos;, DURATION_MIN,0)) AS TECHNICAL_PROBLEMS_31,
	SUM(DECODE(ACTIVITY_NAME,&apos;ACW (Cisco Code 32)&apos;, DURATION_MIN,0)) AS ACW,
	SUM(DECODE(ACTIVITY_NAME,&apos;Coaching&apos;, DURATION_MIN,0)) AS COACHING,
	SUM(DECODE(ACTIVITY_NAME,&apos;Extra Hours&apos;, DURATION_MIN,0)) AS EXTRA_HOURS,
	SUM(DECODE(ACTIVITY_NAME,&apos;OJT&apos;, DURATION_MIN,0)) AS OJT,
	SUM(DECODE(ACTIVITY_NAME,&apos;Talking&apos;, DURATION_MIN,0)) AS TALKING,
	SUM(DECODE(ACTIVITY_NAME,&apos;Technical Problems (Cisco Code 11)&apos;, DURATION_MIN,0)) AS TECHNICAL_PROBLEMS_11,
	SUM(DECODE(ACTIVITY_NAME,&apos;Supervisor Meeting (Cisco Code 22)&apos;, DURATION_MIN,0)) AS SUPERVISOR_MEETING_22,
	SUM(DECODE(ACTIVITY_NAME,&apos;Not ready&apos;, DURATION_MIN,0)) AS NOT_READY,
    SYSDATE AS LOAD_DATE
  FROM ST_AGENT_ACTIVITY
  WHERE CALL_DAY = TRUNC(SYSDATE)-1
--    AND 
--TRUNC(CALL_DAY) &gt;= &apos;1-MAY-2010&apos; AND   TRUNC(CALL_DAY) &lt; &apos;14-MAY-2010&apos;
  GROUP BY AGENT_ID, ACTIVITY_DATE_ID </sql>
