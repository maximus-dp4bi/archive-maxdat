        <jsScript_script>&#47;&#47;Script here

var column_combo = COLUMN_NAME;
var errors = &quot;&quot;;

if (DATA_TYPE == &quot;int&quot; || DATA_TYPE == &quot;smallint&quot;) {
  column_combo = column_combo + &quot; &quot; + DATA_TYPE;
} else if (DATA_TYPE == &quot;varchar&quot;) {
  column_combo = column_combo + &quot; varchar(&quot; + CHARACTER_MAXIMUM_LENGTH + &quot;)&quot;;
} else if (DATA_TYPE == &quot;char&quot;) {
  if (CHARACTER_MAXIMUM_LENGTH == 1) {
	column_combo = column_combo + &quot; bit&quot;;
  } else {
	errors = column_combo + &quot; of type char(&quot; + CHARACTER_MAXIMUM_LENGTH + &quot;) removed&quot;;
  }
} else if (DATA_TYPE == &quot;smalldatetime&quot; || DATA_TYPE == &quot;datetime&quot;) {
  column_combo = column_combo + &quot; datetime&quot;;
} else if (DATA_TYPE == &quot;float&quot; || DATA_TYPE == &quot;real&quot;) {
  column_combo = column_combo + &quot; &quot; + DATA_TYPE;
} else {
  	errors = column_combo + &quot; of type &quot; + DATA_TYPE + &quot; removed&quot;;
}

if (column_combo != COLUMN_NAME) {
	if (IS_NULLABLE == &quot;YES&quot;) {
  		column_combo = column_combo + &quot; NULL&quot;;
	} else {
  		column_combo = column_combo + &quot; NOT NULL&quot;;
	}
} else {
	column_combo = &quot;&quot;;
}

