        <jsScript_script>var TRANSFORM_NAME = null;
var JOB_NAME = null;

var trans = _step_.getTrans();

var parentTrans = trans.getParentTrans();
if(parentTrans !== null){
	var parentTransMeta = parentTrans.getTransMeta();
	var TRANSFORM_NAME= parentTransMeta.getName();
}

var parentJob = trans.getParentJob();
if(parentJob === null){
